# Configuration defaults for development environment.
<%= clear_secrets %>

dashboard_enable_pegasus: true

db_cluster_id: localhost
db_proxy_reporting_endpoint: localhost
db_writer_endpoint: localhost
# FOLLOW ON: Remove the above naming convention to the `db_endpoint_[type]` convention below.
db_endpoint_writer: localhost
db_endpoint_proxy_writer: localhost
db_endpoint_proxy_reader: localhost
db_endpoint_proxy_reporting: localhost

db_credential_admin: {"username":"root", "password":""}
db_admin_user: root
db_admin_password: ''
db_credential_writer: {"username":"root", "password":""}
db_writer_user: root
db_writer_password: ''
db_credential_reader: {"username":"root", "password":""}
db_reader_user: root
db_reader_password: ''
db_writer: 'mysql://root@localhost/'

firebase_name: cdo-v3-dev
# provide a unique path for firebase channels data for development, to avoid conflicts in channel ids.
firebase_secret: !Secret
firebase_shared_secret: !Secret
firebase_channel_id_suffix: -DEVELOPMENT-<%=ENV['USER']%>

lint: true
optimize_webpack_assets: false
optimize_rails_assets: false
stub_school_data: true
daemon: true

# Disable this for developers by default, it won't make much sense because we
# have our own db.
disable_s3_image_uploads: true

chef_local_mode: true

devinternal_db_writer: !Secret
dashboard_honeybadger_api_key: '00000000'
pegasus_honeybadger_api_key:   '00000000'
google_safe_browsing_key: fake_api_key
pusher_app_id:             fake_app_id
pusher_application_key:    fake_application_key
pusher_application_secret: fake_application_secret
poste_secret: not a real secret
channels_api_secret: not a real secret
dashboard_devise_pepper: not a pepper!
dashboard_secret_key_base: not a secret

custom_error_response: false

# Used for exporting the workshop data to a gsheet
applications_gsheet_key: !Secret
eir_teacher_enrollments_gsheet_key: !Secret

# Used for decoding certain protected level sources
properties_encryption_key: !Secret

javabuilder_private_key: !Secret
javabuilder_key_password: !Secret

pardot_private_key: !Secret

redshift_host: !Secret
redshift_password: !Secret
redshift_username: !Secret

slack_bot_token: !Secret
