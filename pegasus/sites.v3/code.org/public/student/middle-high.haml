---
title: Computer Science Curriculum for Grades 6-12
theme: responsive_full_width
---

%link{href: "/css/generated/design-system-pegasus.css", rel: "stylesheet"}

%section.no-padding-bottom
  .wrapper
    %h1=hoc_s(:middle_high_page_main_title)
    %p.body-one
      =hoc_s(:middle_high_page_main_description)

%section
  .wrapper
    %h2
      =hoc_s(:middle_high_page_hero_title)
    %p=hoc_s(:middle_high_page_hero_description)
    .action-block.action-block--two-col
      .media-wrapper.col-50
        %img{src: "/images/houreofcode-students-banner.png", alt: ""}
      .text-wrapper.col-50
        %h3
          =hoc_s(:middle_high_page_hero_title)
        %p.heading-sm
          =hoc_s(:middle_high_page_hero_description)
        %a.link-button{href: "https://hourofcode.com/learn"}
          =hoc_s(:middle_high_page_hourofcode_link)

= view :section_divider_line

%section
  .wrapper
    %h2
      =hoc_s(:middle_high_page_modules_title)
    %p
      =hoc_s(:middle_high_page_modules_description)

    .action-block__wrapper.action-block__wrapper--three-col
      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline
            =hoc_s(:module_age_11)
          %h3
            =hoc_s(:middle_high_page_modules_card_title_1)
          %img{src: "/images/introduction-game-lab-banner.png", alt: ""}
          %p
            =hoc_s(:middle_high_page_modules_card_description_1)
        .content-footer
          %a.link-button{href: "https://studio.code.org/s/csd3-virtual", target: "_blank", rel: "noopener noreferrer"}
            =hoc_s(:middle_high_page_modules_card_link)
      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline
            =hoc_s(:module_age_13)
          %h3
            =hoc_s(:middle_high_page_modules_card_title_2)
          %img{src: "/images/turtle-programming-banner.png", alt: ""}
          %p
            =hoc_s(:middle_high_page_modules_card_description_2)
        .content-footer
          %a.link-button{href: "https://studio.code.org/s/csp3-virtual", target: "_blank", rel: "noopener noreferrer"}
            =hoc_s(:middle_high_page_modules_card_link)
      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline
            =hoc_s(:module_age_13)
          %h3
            =hoc_s(:middle_high_page_modules_card_title_3)
          %img{src: "/images/event-driven-programming-banner.png", alt: ""}
          %p
            =hoc_s(:middle_high_page_modules_card_description_3)
        .content-footer
          %a.link-button{href: "https://studio.code.org/s/csp5-virtual", target: "_blank", rel: "noopener noreferrer"}
            =hoc_s(:middle_high_page_modules_card_link)

= view :section_divider_line

