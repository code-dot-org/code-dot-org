      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline=hoc_s(:module_duration_month)
          %h3=hoc_s(:curriculum_name_ai)
          %img{src: "/images/ai/ai-curriclum-machine-learning.png", alt: ""}
          %p=hoc_s(:ai_curriculum_desc_01)
        .content-footer
          %a.link-button{href: CDO.studio_url("/s/aiml-2023?viewAs=Instructor"), aria:{label: hoc_s(:ai_curriculum_btn_aria_label_01)}}
            =hoc_s(:call_to_action_explore_unit)

= view :section_divider_line

