        .content-footer
          %a.link-button{href: CDO.studio_url("/s/web-development-2023?viewAs=Instructor"), aria:{label: hoc_s(:aria_label_call_to_action_curriculum_web_development)}}
            =hoc_s(:call_to_action_explore_unit)
      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline=hoc_s(:module_duration_month)
          %h3=hoc_s(:curriculum_name_design_process)
          %img{src: "/images/csd-unit-design-process.png", alt: ""}
          %p=hoc_s(:curriculum_desc_design_process)
        .content-footer
          %a.link-button{href: CDO.studio_url("/s/design-process-2023?viewAs=Instructor"), aria:{label: hoc_s(:aria_label_call_to_action_curriculum_design_process)}}
            =hoc_s(:call_to_action_explore_unit)
      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline=hoc_s(:module_duration_month)
          %h3=hoc_s(:curriculum_name_data_society)
          %img{src: "/images/csd-unit-data-society.png", alt: ""}
          %p=hoc_s(:curriculum_desc_data_society)
        .content-footer
          %a.link-button{href: CDO.studio_url("/s/data-society-2023?viewAs=Instructor"), aria:{label: hoc_s(:aria_label_call_to_action_curriculum_data_society)}}
            =hoc_s(:call_to_action_explore_unit)
      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline=hoc_s(:module_duration_quarter)
