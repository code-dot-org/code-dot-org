---
title: AI 101 for Teachers
theme: responsive_full_width
---

%link{href: "/css/generated/design-system-pegasus.css", rel: "stylesheet"}
%link{href: "/css/generated/ai-pl-101-styles.css", rel: "stylesheet"}

-# FontAwesome icons
- icon_sparkles = "fa-solid fa-sparkles"
- icon_lightbulb = "fa-solid fa-lightbulb"
- icon_magnifying_glass = "fa-solid fa-magnifying-glass"
- icon_book = "fa-solid fa-book"
- icon_person_chalkboard = "fa-solid fa-person-chalkboard"
- icon_video = "fa-solid fa-video"
- icon_megaphone = "fa-solid fa-megaphone"

%section.hero-banner-basic.hero-ai-pl-webinar{style: "display: block !important"}
  .wrapper
    .text-wrapper.col-75
      %h1{style: "color: white"}=hoc_s(:ai_pl_webinar_hero_heading)
      %p{style: "color: white"}=hoc_s(:ai_pl_webinar_hero_desc)
      .hero-ai-pl-webinar__by
        %span{style: "color: white"}=hoc_s(:ai_pl_webinar_hero_presented)
        .hero-ai-pl-webinar__list
          %img{src: "/images/ai/pl/101/code--w.png", height: "24px", alt: "CODE"}
          %img{src: "/images/ai/pl/101/khan--w.png", height: "20px", alt: "Khan Academy"}
          %img{src: "/images/ai/pl/101/iste--w.png", height: "18px", alt: "ISTE"}
          %img{src: "/images/ai/pl/101/ets--w.png", height: "26px", alt: "ets"}
          %img{src: "/images/ai/pl/101/teach-ai--w.png", height: "24px", alt: "ets"}

%section.intro
  .wrapper
    %h2=hoc_s(:ai_pl_webinar_intro_heading)
    %p=hoc_s(:ai_pl_webinar_intro_text)


= view :section_divider_line

%section.newsletter-signup
  .wrapper
    %h2=hoc_s(:ai_pl_webinar_join_email_title)
    %p=hoc_s(:ai_pl_webinar_pro_join_email_desc)
    %iframe.col-50{:allowtransparency => "true", :frameborder => "0", :height => "500", :scrolling => "yes", :src => "http://go.pardot.com/l/153401/2023-01-10/pqf2sr", :style => "overflow:hidden; float: none", :type => "text/html", :width => "100%"}

= view :visit_partial_event_logger

= view :section_divider_line

%section
  .wrapper
    %h2=hoc_s(:ai_pl_webinar_explore_title)
    %p=hoc_s(:ai_pl_webinar_explore_desc)
    .action-block__wrapper.action-block__wrapper--two-col
      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline=hoc_s(:ai_pl_webinar_explore_tag_1)
          %h3=hoc_s(:ai_pl_webinar_explore_title_1)
          %img{src: "/images/ai/pl/101/explore_1.jpg", alt: ""}
          %p=hoc_s(:ai_pl_webinar_explore__desc_1)
        .content-footer
          %a{href: "https://blog.khanacademy.org/teacher-khanmigo/?utm_source=code.org&utm_medium=referral&utm_campaign=code.orgwebinar", class: "link-button",  rel: "noopener noreferrer", aria:{label: hoc_s(:ai_resources_btn_aria_label_01)}}
            =hoc_s(:ai_pl_webinar_explore__button_1)

      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline=hoc_s(:ai_pl_webinar_explore_tag_2)
          %h3=hoc_s(:ai_pl_webinar_explore_title_2)
          %img{src: "/images/ai/pl/101/explore_2.jpg", alt: ""}
          %p=hoc_s(:ai_pl_webinar_explore__desc_2)
        .content-footer
          %a{href: "https://www.iste.org/professional-development/iste-u/artificial-intelligence ", class: "link-button",  rel: "noopener noreferrer", aria:{label: hoc_s(:ai_resources_btn_aria_label_01)}}
            =hoc_s(:ai_pl_webinar_explore__button_2)

      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline=hoc_s(:ai_pl_webinar_explore_tag_3)
          %h3=hoc_s(:ai_pl_webinar_explore_title_3)
          %img{src: "/images/ai/ai-curriclum-machine-learning.png", alt: ""}
          %p=hoc_s(:ai_pl_webinar_explore__desc_3)
        .content-footer
          %a{href: "https://code.org/ai#ai-curricula", class: "link-button",  rel: "noopener noreferrer", aria:{label: hoc_s(:ai_resources_btn_aria_label_01)}}
            =hoc_s(:ai_pl_webinar_explore__button_3)

      .action-block.action-block--one-col.flex-space-between
        .content-wrapper
          %p.overline=hoc_s(:ai_pl_webinar_explore_tag_4)
          %h3=hoc_s(:ai_pl_webinar_explore_title_4)
          %img{src: "/images/ai/pl/101/explore_4.jpg", alt: ""}
          %p=hoc_s(:ai_pl_webinar_explore__desc_4)
        .content-footer
          %a{href: "https://www.ets.org/research/ai-labs.html", class: "link-button",  rel: "noopener noreferrer", aria:{label: hoc_s(:ai_resources_btn_aria_label_01)}}
            =hoc_s(:ai_pl_webinar_explore__button_4)

%section.partnership-ai
  .wrapper
    .partnership-ai__content
      %p=hoc_s(:ai_pl_webinar_partnership_between)
      .partnership-ai__list
        %img{src: "/images/ai/pl/101/code.svg", height: "50px", alt: "CODE"}
        %img{src: "/images/ai/pl/101/khan.svg", height: "50px", alt: "Khan Academy"}
        %img{src: "/images/ai/pl/101/iste.png", height: "38px", alt: "ISTE"}
        %img{src: "/images/ai/pl/101/ets.png", height: "55px", alt: "ETS"}
        %img{src: "/images/ai/pl/101/teach-ai.png", height: "50px", alt: "ETS"}
