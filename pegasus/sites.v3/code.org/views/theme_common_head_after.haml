=::NewRelic::Agent.browser_timing_header rescue ''
=view :old_ie
=view :jquery

- cookie_key = '_user_type' + (rack_env == :production ? '' : "_#{rack_env.to_s}")
- user_type = request.cookies[cookie_key]

%script=view 'google_analytics.js', user_type: user_type

%script{:defer => "defer", src: minifiable_asset_path("js/code.org/views/theme_common_head_after.js")}

- if header['chart']
  %script{src:'https://www.google.com/jsapi'}

- if @header['video_player']
  :javascript
    $(window).load(function() {
      loadVideos(window.location.search.indexOf("force_youtube_fallback") !== -1)
    });
