- nav_index = 7

%nav.how-to-nav
  %ul
    %li
      %button.menu-button
        = nav_current
      %ul.menu-list{style: "display: none"}
        - nav_index.times do |index|
          %li
            %a{href: resolve_url(nav_url[index])}
              = nav_label[index]

%script{src: "/js/how-to-nav.js"}
