    var c_name = 'language_';
    var exdays = 5000;
    var date = new Date();
    date.setTime(date.getTime()+(exdays*60*60*24*1000));
    var c_value=escape(value) + ((exdays==null) ? "" : "; expires=" + date.toGMTString());
