Host *.ec2.internal *.cdn-code.org
  User ubuntu
  StrictHostKeyChecking no
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/server_access_key

<% @hosts.each do |node| %>
Host <%= node.name %>
  HostName <%= node['cloud'] ? node['cloud']['local_hostname'] : node['fqdn'] || 'fqdn_missing' %>
<% end %>
