{
  "name": "react-selectize",
  "version": "2.1.0",
  "homepage": "https://github.com/furqanZafar/react-selectize",
  "authors": [
    "Furqan Zafar (Mac Pro) <fruzi89@gmail.com>"
  ],
  "main": [
    "dist/index.min.js",
    "dist/index.min.css"
  ],
