<xml id="toolbox" style="display: none;">
  <block type="maze_moveForward"></block>
  <block type="maze_turn"><title name="DIR">turnLeft</title></block>
  <block type="maze_turn"><title name="DIR">turnRight</title></block>
  <block type="maze_dig"></block>
  <% if (level > 1) { -%>
    <block type="maze_fill"></block>
    <% if (level > 2) { -%>
      <block type="controls_repeat">
        <title name="TIMES">5</title>
      </block>
      <% if (level > 9) { -%>
        <block type="karel_if"></block>
      <% } -%>
    <% } -%>
    <% if (level == 5 || level == 10 || level == 11) { -%>
      <block type="maze_untilBlocked"></block>
    <% } -%>
    <% if (level > 5 && level < 8) { -%>
      <block type="maze_untilBlockedOrNotClear"></block>
    <% } -%>
    <% if (level == 8 || level == 9) { -%>
      <block type="maze_untilBlockedOrNotClear">
        <title name="DIR">isPathForward</title>
      </block>
    <% } -%>
  <% } -%>
</xml>
