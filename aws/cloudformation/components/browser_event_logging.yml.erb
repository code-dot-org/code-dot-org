  # Browser Event Logs Log Group and Log Stream
  BrowserEventLogs:
    Type: AWS::Logs::LogGroup
    Properties:
      LogGroupName: !Sub "<%=environment%>-browser-events-${DashboardSubdomainName}.${BaseDomainName}"

  BrowserEventLogsStream:
    Type: AWS::Logs::LogStream
    Properties:
      LogGroupName: !Ref BrowserEventLogs
      LogStreamName: !Sub "${DashboardSubdomainName}.${BaseDomainName}"
