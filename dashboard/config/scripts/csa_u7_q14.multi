// Precondition: b > 0
public static int surprise(int b) {
  if ((b % 2) == 0) {
    if (b < 10)
      return b;
    else
      return ((b % 10) + surprise(b / 10));
   } else {
     if (b < 10)
       return 0;
     else
       return surprise(b / 10);
   }
 }
```

