name 'AP_practice_Qs_rqb_99_CSP_4_1_1'
right '<pre>
xPos &lt;- 3
yPos &lt;- 3
REPEAT 3 TIMES
{
    drawCircle(xPos, yPos, 3)
    xPos &lt;- xPos + 6
}
</pre>'
wrong '<pre>
xPos &lt;- 3
yPos &lt;- 3
REPEAT 3 TIMES
{
    drawCircle(xPos, yPos, 3)
    yPos &lt;- yPos + 6
}
</pre>'
wrong '<pre>
xPos &lt;- 3
yPos &lt;- 3
REPEAT 3 TIMES
{
    drawCircle(xPos, yPos, 3)
    xPos &lt;- xPos + 6
    yPos &lt;- yPos + 6
}
</pre>'
wrong '<pre>
xPos &lt;- 3
yPos &lt;- 3
REPEAT 3 TIMES
{
    drawCircle(xPos, yPos, 3)
}  
xPos &lt;- xPos - 6
