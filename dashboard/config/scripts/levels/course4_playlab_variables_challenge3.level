  <blocks>
    <start_blocks>
      <xml>
        <block type="procedures_defnoreturn" editable="false" usercreated="true">
          <mutation>
            <description>The game asks the player for a question, then tells them to click on a fortune teller.</description>
          </mutation>
          <title name="NAME">Ask a question</title>
          <statement name="STACK">
            <block type="studio_ask">
              <title name="TEXT">Ask a yes/no question to the fortune tellers:</title>
              <title name="VAR">question</title>
              <next>
                <block type="studio_showTitleScreenParams" inline="false">
                  <value name="TITLE">
                    <block type="variables_get">
                      <title name="VAR">question</title>
                    </block>
                  </value>
                  <value name="TEXT">
                    <block type="text">
                      <title name="TEXT">Click on the witch to find out!</title>
                    </block>
                  </value>
                </block>
              </next>
            </block>
          </statement>
        </block>
        <block type="when_run">
          <next>
            <block type="procedures_callnoreturn">
              <mutation name="Ask a question"/>
            </block>
          </next>
        </block>
        <block type="studio_whenSpriteClicked">
          <title name="SPRITE">0</title>
          <next>
            <block type="studio_ifActorHasEmotionParams" inline="true">
              <title name="EMOTION">1</title>
              <value name="SPRITE">
                <block type="math_number">
                  <title name="NUM">1</title>
                </block>
              </value>
              <statement name="DO">
                <block type="variables_set" inline="false">
                  <title name="VAR">fortune</title>
                  <value name="VALUE">
                    <block type="text" movable="false">
                      <title name="TEXT">Yes!</title>
                    </block>
                  </value>
                </block>
              </statement>
              <next>
                <block type="studio_ifActorHasEmotionParams" inline="true">
                  <title name="EMOTION">3</title>
                  <value name="SPRITE">
                    <block type="math_number">
                      <title name="NUM">1</title>
                    </block>
                  </value>
                  <statement name="DO">
                    <block type="variables_set" inline="false">
                      <title name="VAR">fortune</title>
                      <value name="VALUE">
                        <block type="text" movable="false">
                          <title name="TEXT">No!</title>
                        </block>
                      </value>
                    </block>
                  </statement>
                  <next>
                    <block type="studio_saySpriteParamsTime" inline="true">
                      <value name="SPRITE">
                        <block type="math_number">
                          <title name="NUM">1</title>
                        </block>
                      </value>
                      <value name="TEXT">
                        <block type="variables_get">
                          <title name="VAR">fortune</title>
                        </block>
                      </value>
                      <value name="TIME">
                        <block type="math_number">
                          <title name="NUM">2</title>
                        </block>
                      </value>
                    </block>
                  </next>
                </block>
              </next>
            </block>
          </next>
        </block>
        <block type="studio_repeatForever">
          <statement name="DO">
            <block type="studio_wait">
              <title name="VALUE">500</title>
              <next>
                <block type="studio_setSpriteEmotionParams" inline="true">
                  <title name="VALUE">1</title>
                  <value name="SPRITE">
                    <block type="math_number">
                      <title name="NUM">1</title>
                    </block>
                  </value>
                  <next>
                    <block type="studio_wait">
                      <title name="VALUE">500</title>
                      <next>
                        <block type="studio_setSpriteEmotionParams" inline="true">
                          <title name="VALUE">3</title>
                          <value name="SPRITE">
                            <block type="math_number">
                              <title name="NUM">1</title>
                            </block>
                          </value>
                        </block>
                      </next>
                    </block>
                  </next>
                </block>
              </next>
            </block>
          </statement>
        </block>
      </xml>
    </start_blocks>
