  <blocks>
    <toolbox_blocks>
      <xml>
        <block type="storylab_setHeading">
          <value name="HEADING">
            <block type="text">
              <field name="TEXT"/>
            </block>
          </value>
        </block>
        <block type="storylab_setSubheading">
          <value name="SUBHEADING">
            <block type="text">
              <field name="TEXT"/>
            </block>
          </value>
        </block>
        <block type="gamelab_setBackgroundImageAs">
          <field name="IMG">"front_of_house"</field>
        </block>
        <block type="gamelab_makeNewSpriteAnon">
          <field name="ANIMATION_NAME">"green_shirt_wave2"</field>
          <value name="LOCATION">
            <block type="gamelab_location_picker" can_disconnect_from_parent="false">
              <field name="LOCATION">{"x":200,"y":200}</field>
            </block>
          </value>
        </block>
        <block type="gamelab_spriteSay">
          <field name="SPEECH">Hello, world!</field>
          <value name="SPRITE">
            <block type="gamelab_allSpritesWithAnimation" can_disconnect_from_parent="false">
              <field name="ANIMATION">"green_shirt_wave2"</field>
            </block>
          </value>
        </block>
        <block type="gamelab_setProp">
          <field name="PROPERTY">"scale"</field>
          <value name="SPRITE">
            <block type="gamelab_allSpritesWithAnimation" can_disconnect_from_parent="false">
              <field name="ANIMATION">"green_shirt_wave2"</field>
            </block>
          </value>
          <value name="VAL">
            <block type="math_number" can_disconnect_from_parent="false">
              <field name="NUM">50</field>
            </block>
          </value>
        </block>
        <block type="gamelab_spriteClicked">
          <field name="CONDITION">"when"</field>
          <value name="SPRITE">
            <block type="gamelab_allSpritesWithAnimation" can_disconnect_from_parent="false">
              <field name="ANIMATION">"green_shirt_wave2"</field>
            </block>
          </value>
        </block>
        <block type="gamelab_setPromptWith2Choices">
          <value name="QUESTION">
            <block type="text">
              <field name="TEXT">Would you read this book?</field>
            </block>
          </value>
          <value name="VAR">
            <block type="variables_get">
              <field name="VAR">answer</field>
            </block>
          </value>
          <value name="A">
            <block type="text">
              <field name="TEXT">Yes</field>
            </block>
          </value>
          <value name="B">
            <block type="text">
              <field name="TEXT">No</field>
            </block>
          </value>
        </block>
        <block type="gamelab_whenAllPromptsAnswered"/>
        <block type="gamelab_printText">
          <value name="TEXT">
            <block type="text">
              <field name="TEXT">Thanks for answering!</field>
            </block>
          </value>
        </block>
      </xml>
    </toolbox_blocks>
