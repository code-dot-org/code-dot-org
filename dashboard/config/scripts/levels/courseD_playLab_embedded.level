  <blocks>
    <start_blocks>
      <xml>
        <block type="when_run">
          <next>
            <block type="studio_setSpriteSpeedParams" inline="true">
              <value name="SPRITE">
                <block type="math_number">
                  <title name="NUM">1</title>
                </block>
              </value>
              <value name="VALUE">
                <block type="math_number">
                  <title name="NUM">5</title>
                </block>
              </value>
              <next>
                <block type="studio_setSpriteBehavior" inline="true">
                  <title name="VALUE">"chase"</title>
                  <value name="SPRITE">
                    <block type="math_number">
                      <title name="NUM">3</title>
                    </block>
                  </value>
                  <value name="TARGETSPRITE">
                    <block type="math_number">
                      <title name="NUM">1</title>
                    </block>
                  </value>
                </block>
              </next>
            </block>
          </next>
        </block>
        <block type="studio_whenArrow">
          <title name="VALUE">up</title>
          <next>
            <block type="studio_move">
              <title name="SPRITE">0</title>
              <title name="DIR">1</title>
            </block>
          </next>
        </block>
        <block type="studio_whenArrow">
          <title name="VALUE">left</title>
          <next>
            <block type="studio_move">
              <title name="SPRITE">0</title>
              <title name="DIR">8</title>
            </block>
          </next>
        </block>
        <block type="studio_repeatForever">
          <statement name="DO">
            <block type="studio_setSpriteEmotion">
              <title name="SPRITE">1</title>
              <title name="VALUE">random</title>
              <next>
                <block type="studio_wait">
                  <title name="VALUE">1000</title>
                </block>
              </next>
            </block>
          </statement>
        </block>
        <block type="studio_whenArrow">
          <title name="VALUE">down</title>
          <next>
            <block type="studio_move">
              <title name="SPRITE">0</title>
              <title name="DIR">4</title>
            </block>
          </next>
        </block>
        <block type="studio_repeatForever">
          <statement name="DO">
            <block type="controls_if" inline="false">
              <value name="IF0">
                <block type="logic_compare" inline="true">
                  <title name="OP">GTE</title>
                  <value name="A">
                    <block type="studio_getScore"/>
                  </value>
                  <value name="B">
                    <block type="math_number">
                      <title name="NUM">5</title>
                    </block>
                  </value>
                </block>
              </value>
              <statement name="DO0">
                <block type="studio_endGame">
                  <title name="VALUE">win</title>
                </block>
              </statement>
            </block>
          </statement>
        </block>
        <block type="studio_repeatForever">
          <statement name="DO">
            <block type="studio_wait">
              <title name="VALUE">1000</title>
              <next>
                <block type="studio_ifActorPositionElse" inline="true">
                  <title name="SPRITE">0</title>
                  <title name="POSITION">y</title>
                  <title name="OPERATOR">GT</title>
                  <value name="COMPARED_VALUE">
                    <block type="math_number">
                      <title name="NUM">225</title>
                    </block>
                  </value>
                  <statement name="DO">
                    <block type="studio_changeScore">
                      <title name="VALUE">-1</title>
                      <next>
                        <block type="studio_setSpritesFlee" inline="true">
                          <title name="VALUE">"ninja"</title>
                          <value name="SPRITE">
                            <block type="math_number">
                              <title name="NUM">1</title>
                            </block>
                          </value>
                        </block>
                      </next>
                    </block>
                  </statement>
                  <statement name="ELSE">
                    <block type="studio_setSpritesChase" inline="true">
                      <title name="VALUE">"ninja"</title>
                      <value name="SPRITE">
                        <block type="math_number">
                          <title name="NUM">1</title>
                        </block>
                      </value>
                    </block>
                  </statement>
                </block>
              </next>
            </block>
          </statement>
        </block>
        <block type="studio_whenSpriteCollided">
          <title name="SPRITE1">0</title>
          <title name="SPRITE2">2</title>
          <next>
            <block type="studio_endGame">
              <title name="VALUE">lose</title>
            </block>
          </next>
        </block>
        <block type="studio_whenSpriteCollided">
          <title name="SPRITE1">0</title>
          <title name="SPRITE2">1</title>
          <next>
            <block type="studio_ifActorHasEmotionElse">
              <title name="SPRITE">1</title>
              <title name="EMOTION">1</title>
              <statement name="DO">
                <block type="studio_changeScore">
                  <title name="VALUE">1</title>
                  <next>
                    <block type="studio_changeScore">
                      <title name="VALUE">1</title>
                    </block>
                  </next>
                </block>
              </statement>
              <statement name="ELSE">
                <block type="studio_changeScore">
                  <title name="VALUE">1</title>
                </block>
              </statement>
            </block>
          </next>
        </block>
        <block type="studio_whenArrow">
          <title name="VALUE">right</title>
          <next>
            <block type="studio_move">
              <title name="SPRITE">0</title>
              <title name="DIR">2</title>
            </block>
          </next>
        </block>
      </xml>
