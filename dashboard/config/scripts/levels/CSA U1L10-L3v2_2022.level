    "start_sources": {
      "MyNeighborhood.java": {
        "text": "import org.code.neighborhood.Painter;\n\npublic class MyNeighborhood {\n  public static void main(String[] args) {\n\n    PainterPlus myPainterPlus = new PainterPlus();\n\n    myPainterPlus.moveTakePaint();\n    myPainterPlus.paintEastTurnSouth(\"white\");\n    myPainterPlus.paintSouthTurnEast(\"purple\");\n    myPainterPlus.paintEastTurnNorth(\"green\");\n    myPainterPlus.paintNorthTurnWest(\"blue\");\n    myPainterPlus.paintWestTurnNorth(\"red\");\n    \n  }\n}",
        "isVisible": true
      },
      "PainterPlus.java": {
        "text": "import org.code.neighborhood.Painter;\n\n/*\n * Creates a PainterPlus\n * PainterPlus is a subclass of Painter.\n */\npublic class PainterPlus extends Painter {\n\n  /*\n   * Constructor to create a PainterPlus object\n   */\n  public PainterPlus() {\n    super();  // Calls the Painter constructor\n  }\n\n  /*\n   * Turns a PainterPlus object to the right\n   * by turning left three times\n   */\n  public void turnRight() {\n    turnLeft();\n    turnLeft();\n    turnLeft();\n  }\n\n  /*\n   * Moves forward in the direction it is facing\n   * and takes paint if on a paint bucket.\n   */\n  public void moveTakePaint() {\n    if (canMove()) {\n      move();\n\n      if (isOnBucket()) {\n        takeAllPaint();\n      }\n    }\n  }\n\n  /*\n   * Moves and paints while facing east as long\n   * as it cannot move south. If able to move\n   * south, turns right and moves forward.\n   */\n  public void paintEastTurnSouth(String color) {\n    while (facingEast()) {\n      if (!canMove(\"south\")) {\n        move();\n        paint(color);\n      }\n      else {\n        turnRight();\n        move();\n      }\n    }\n  }\n\n  /*\n   * Moves and paints while facing east as long\n   * as it cannot move north. If able to move\n   * north, turns left and moves forward.\n   */\n  public void paintEastTurnNorth(String color) {\n    while (facingEast()) {\n      if (!canMove(\"north\")) {\n        paint(color);\n        move();\n      }\n      else {\n        turnLeft();\n        move();\n      }\n    }\n  }\n\n  /*\n   * Moves and paints while facing south as long\n   * as it is able to move. If not able to move, turns\n   * left and moves forward.\n   */\n  public void paintSouthTurnEast(String color) {\n    while (facingSouth()) {\n      if (!canMove()) {\n        turnLeft();\n        move();\n      }\n      else {\n        paint(color);\n        move();\n      }\n    }\n  }\n\n  /*\n   * Moves and paints while facing north as long\n   * as it cannot move west. If able to move west,\n   * turns left.\n   */\n  public void paintNorthTurnWest(String color) {\n    while (facingNorth()) {\n      if (!canMove(\"west\")) {\n        paint(color);\n        move();\n      }\n      else {\n        turnLeft();\n      }\n    }\n  }\n\n  /*\n   * Moves and paints while facing west as long as\n   * it is not on paint. If is on paint, turns right.\n   */\n  public void paintWestTurnNorth(String color) {\n    while (facingWest()) {\n      if (!isOnPaint()) {\n        paint(color);\n        move();\n      }\n      else {\n        turnRight();\n      }\n    }\n  }\n\n  /*\n   * Unit 1 Lesson 9 - Choice A\n   */\n  public void takeAllPaint() {\n    while (isOnBucket()) {\n      takePaint();\n    }\n  }\n  \n}",
        "isVisible": true
      }
    },
