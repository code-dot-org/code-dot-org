                      <title name="COSTUME">"carrot"</title>
                      <value name="LOCATION">
                        <block type="gamelab_randomLocation"/>
                      </value>
                    </block>
                  </statement>
                  <next>
                    <block type="gamelab_comment">
                      <title name="COMMENT"># Collect one cupcake to remove them all.</title>
                      <next>
                        <block type="controls_repeat_ext">
                          <value name="TIMES">
                            <block type="math_number">
                              <title name="NUM">10</title>
                            </block>
                          </value>
                          <statement name="DO">
                            <block type="aalab_createNewSpriteAdvanced">
                              <title name="NAME">cupcake</title>
                              <title name="COSTUME">"cupcake"</title>
                              <value name="LOCATION">
                                <block type="gamelab_locationAt">
                                  <value name="X">
                                    <block type="math_random_int">
                                      <value name="FROM">
