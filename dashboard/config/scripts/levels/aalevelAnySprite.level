                                          <title name="NUM">10</title>
                                        </block>
                                      </value>
                                      <value name="TO">
                                        <block type="math_number">
                                          <title name="NUM">390</title>
                                        </block>
                                      </value>
                                    </block>
                                  </value>
                                  <value name="Y">
                                    <block type="math_random_int">
                                      <value name="FROM">
                                        <block type="math_number">
                                          <title name="NUM">10</title>
                                        </block>
                                      </value>
                                      <value name="TO">
                                        <block type="math_number">
                                          <title name="NUM">300</title>
                                        </block>
                                      </value>
                                    </block>
                                  </value>
                                </block>
                              </value>
                            </block>
                          </statement>
                          <next>
                            <block type="aalab_changeGroupPropBy">
                              <title name="PROPERTY">"scale"</title>
                              <value name="GROUP">
                                <block type="aalab_getSpritesByName">
                                  <title name="NAME">cupcake</title>
                                </block>
                              </value>
                              <value name="VAL">
                                <block type="math_number">
                                  <title name="NUM">-50</title>
                                </block>
                              </value>
                              <next>
                                <block type="aalab_createNewSpriteAdvanced">
                                  <title name="NAME">bunny</title>
                                  <title name="COSTUME">"brown bunny"</title>
                                  <value name="LOCATION">
                                    <block type="gamelab_location_picker">
                                      <title name="LOCATION">{"x":4,"y":6}</title>
                                    </block>
                                  </value>
                                  <next>
                                    <block type="aalab_createNewSpriteAdvanced">
                                      <title name="NAME">bear</title>
                                      <title name="COSTUME">"bear"</title>
                                      <value name="LOCATION">
                                        <block type="gamelab_location_picker">
                                          <title name="LOCATION">{"x":324,"y":316}</title>
                                        </block>
                                      </value>
                                      <next>
                                        <block type="gamelab_showTitleScreen">
                                          <value name="TITLE">
                                            <block type="text">
                                              <title name="TEXT">Click bear.</title>
                                            </block>
                                          </value>
                                          <value name="SUBTITLE">
