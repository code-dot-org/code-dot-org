      <xml>
        <block type="when_run">
          <next>
            <block type="gamelab_comment">
              <title name="COMMENT"># Collect carrots one-by-one.</title>
              <next>
                <block type="controls_repeat_ext">
                  <value name="TIMES">
                    <block type="math_number">
                      <title name="NUM">25</title>
