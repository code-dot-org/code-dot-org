    "palette_category_at_start": "Circuit",
    "start_blocks": "onBoardEvent(buttonL, \"down\", function() {\n  setText(\"thisLabel\", \"The toggle switch is open\");\n});\nonBoardEvent(buttonL, \"up\", function() {\n  setText(\"thisLabel\", \"The right button is down\");\n});\nonBoardEvent(buttonR, \"down\", function(event) {\n  setText(\"thisLabel\", \"The right button is up\");\n});\nonBoardEvent(buttonR, \"up\", function(event) {\n  setText(\"thisLabel\", \"The left button is up\");\n});\nonBoardEvent(toggleSwitch, \"open\", function(event) {\n  setText(\"thisLabel\", \"The left button is down\");\n});\nonBoardEvent(toggleSwitch, \"close\", function(event) {\n  setText(\"thisLabel\", \"The toggle switch is close\");\n});\n",
