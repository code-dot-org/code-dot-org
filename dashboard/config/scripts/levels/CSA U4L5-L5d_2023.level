      "DonutScene.java": {
        "text": "import org.code.theater.*;\n\n/*\n * Represents a scene that visualizes donuts\n */\npublic class DonutScene extends Scene {\n  /*\n   * Generates a random number of donuts\n   */\n  public String getRandomDonuts() {\n    /* ----------------------------------- TO DO -----------------------------------\n     * ‚úÖ Generate a random number of donuts between 1 and 24. Draw the number of\n     * donuts in the scene. If the number of donuts is greater than 12, return a\n     * String containing the text \"You're eligible for a 20% discount if you order\n     * {numDonuts} number of donuts!\". Else, return a String containing the text\n     * \"Sorry, no discount is available for {numDonuts} number of donuts!\".\n     * -----------------------------------------------------------------------------\n     */\n\n    int donuts = (int)(Math.random() * 23)  + 1;\n\n    /* for (int yPos = 20; yPos < getHeight(); yPos += 100) {\n        for (int xPos = 20; xPos < getWidth(); xPos += 100) {\n          drawImage(images[index], xPos, yPos, 50);\n          pause(0.2);\n        }\n      } */\n\n    if (donuts >= 12) {\n      return \"You're eligible for a 20% discount if you order \" + donuts +\n        \" number of donuts!\";\n      // drawImage(\"snowflake.png\", 0, 0, getWidth());\n    } else if (donuts < 12) {\n      return \"Sorry, no discount is available for \" + donuts +\n        \" number of donuts!\";\n      // drawImage(\"sun.png\", 0, 0, getWidth());\n    }\n\n    return \"\";\n  }\n  \n}",
