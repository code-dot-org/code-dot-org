    "validation_code": "//validation\r\n\r\n//Make sure [CSC Module Helper Library] is included as a library in this level\r\nif (World.frameCount == 1) {\r\n  //Everything in this if-statement will just run once, during the first frame of your level\r\n  //README() //uncomment this to see the README for the helper library in the console of this level\r\n  //turnOnDebugging() //uncomment this to use a DEBUG global variable to control console.log statements in your code\r\n  \r\n  setFailTime(150); // Frames to wait before failing student\r\n  setDelayTime(90); // Frames to wait after success before stopping program\r\n  setupPrevious(); //Defines the validationProps.previous{} object. To use it, call updatePrevious() at the end of this box\r\n  \r\n  //use addCriteria to setup the validation logic.\r\n  //You can use criterion commands as helper functions: \r\n  //https://github.com/code-dot-org/code-dot-org/blob/staging/apps/src/p5lab/spritelab/commands/criterionCommands.js\r\n  \r\n  //These are snowballing validation functions. They are defined in the helper library\r\n  //They can be called on future levels to carry forward validation\r\n  //To STOP validating a particular criteria, set the boolean value of the function to false\r\n  //for example: level2Validation(false, false, false) won't actually validate any of the criteria from level 2\r\n  \r\n  //Level 1 Validation Functions\r\n  //check if the background changed\r\n  addCriteria(function() {\r\n    return checkSetBackground();\r\n  }, \"spritelabFeedbackChangeBackgroundColor\");  // include i18n feedback string\r\n\t\r\n  //Level 2 Validation Functions\r\n  //Checks whether 1 sprite exists\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 1;\r\n  }, \"createNewSprite\");  // include i18n feedback string\r\n\r\n  //check for a new location on 1st sprite\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 1 && checkMovedSpritePin(spriteIds[0]);\r\n  }, \"changeLocation\");  // include i18n feedback string\r\n  \r\n  //Check for a new size property on 1st sprite\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 1 && checkNotDefaultSize(spriteIds[0]);\r\n  }, \"useSetpropBlock\");  // include i18n feedback string\r\n  \r\n  //Level 3 Validation Functions\r\n  //Checks if the first sprite says something\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 1 && checkSpriteSpeech(spriteIds[0]);\r\n  }, \"noSpeech\");  // include i18n feedback string\r\n  checkSpriteSpeech();\r\n  \r\n  //Level 4 Validation Functions\r\n  //Checks whether 2 sprites exist\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 2;\r\n  }, \"cscLandmarkAddAnotherSprite\");  // include i18n feedback string\r\n  \r\n  //Checks whether the second sprite is not the default costume\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 2 && checkNotDefaultCostume(spriteIds[1], \"red_shirt_wave2\");\r\n  }, \"cscLandmarkChangeCostume\");  // include i18n feedback string\r\n  \r\n  //Checks whether the second sprite has moved locations\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 2 && checkMovedSpritePin(spriteIds[1]);\r\n  }, \"changeLocation\");  // include i18n feedback string\r\n  \r\n  //checks that 2nd sprite changed sizes\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 2 && checkNotDefaultSize(spriteIds[1]);\r\n  }, \"useSetpropBlock\");  // include i18n feedback string\r\n  \r\n  //Checks that a movement behavior has been added\r\n  addCriteria(function() {\r\n    return interactiveBehaviorFound();\r\n  }, \"cscLandmarkBehavior\");  // include i18n feedback string\r\n  \r\n  /*\r\n  //Level 5 Validation Functions\r\n  //Checks for 1 touch event\r\n  addCriteria(function() {\r\n    return checkUniqueTouchEvents(1);\r\n  }, \"cscLandmarkTouchAllSprites\");\r\n  \r\n  //Check that the sprite said something after touching the landmark\r\n  addCriteria(function() {\r\n    if(checkUniqueTouchEvents(1)) {\r\n      var sprites = getSpritesThatTouched();\r\n      if(strictSpriteSpeechRenderedThisFrame(sprites[0])) {\r\n         return true;\r\n      }\r\n    }\r\n    return false;\r\n  }, \"cscLandmarkTouchButNoSpeech\");\r\n  \r\n  */\r\n  \r\n  //Level 6 Validation Functions\r\n  //Checks for 3 sprites\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 3;\r\n  }, \"cscLandmarkAddAnotherSprite\");  // include i18n feedback string\r\n  \r\n  //Checks whether the 3rd sprite is not the default costume\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 3 && checkNotDefaultCostume(spriteIds[2], \"red_shirt_wave2\");\r\n  }, \"cscLandmarkChangeCostume\");  // include i18n feedback string\r\n  \r\n  //Checks whether the 3rd sprite has moved locations\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 3 && checkMovedSpritePin(spriteIds[2]);\r\n  }, \"changeLocation\");  // include i18n feedback string\r\n  \r\n  //checks that 3rd sprite changed sizes\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 3 && checkNotDefaultSize(spriteIds[2]);\r\n  }, \"useSetpropBlock\");  // include i18n feedback string\r\n  \r\n  /*\r\n  //Level 7 Validation Functions\r\n  //Check that we touched the most recent landmark\r\n  addCriteria(function() {\r\n    if(checkUniqueTouchEvents(1) && spriteIds.length >= 3) {\r\n      var sprites = getSpritesThatTouched();\r\n      //Check that the sprite we touched (sprites[1]) is the 3rd sprite (spriteIds[2])\r\n      if(sprites[1] == spriteIds[2]) {\r\n        return true;\r\n      }\r\n    }\r\n    return false;\r\n  }, \"cscLandmarkMostRecent\");\r\n  \r\n  //Check that the sprite said something after touching the landmark\r\n  addCriteria(function() {\r\n    if(checkUniqueTouchEvents(1) && spriteIds.length >= 3) {\r\n      var sprites = getSpritesThatTouched();\r\n      //Check that the sprite we touched (sprites[1]) is the 3rd sprite (spriteIds[2])\r\n      if(sprites[1] == spriteIds[2]) {\r\n        //Check that the sprite said something this frame\r\n        if(strictSpriteSpeechRenderedThisFrame(sprites[0])) {\r\n          return true;\r\n        }\r\n      }\r\n    }\r\n    return false;\r\n  }, \"cscLandmarkTouchButNoSpeech\");\r\n  */\r\n  \r\n  //Level 8 Validation Functions\r\n  //Checks for 4 sprites\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 4;\r\n  }, \"cscLandmarkAddAnotherSprite\");  // include i18n feedback string\r\n  \r\n  //Checks whether the 4th sprite is not the default costume\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 4 && checkNotDefaultCostume(spriteIds[3], \"red_shirt_wave2\");\r\n  }, \"cscLandmarkChangeCostume\");  // include i18n feedback string\r\n  \r\n  //Checks whether the 4th sprite has moved locations\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 4 && checkMovedSpritePin(spriteIds[3]);\r\n  }, \"changeLocation\");  // include i18n feedback string\r\n  \r\n  //checks that 4th sprite changed sizes\r\n  addCriteria(function() {\r\n    return spriteIds.length >= 4 && checkNotDefaultSize(spriteIds[3]);\r\n  }, \"useSetpropBlock\");  // include i18n feedback string\r\n  \r\n}\r\n//Everything after this will run during each 'tick' of the draw loop in your level\r\n//Be careful about putting console.log statements here\r\n\r\n//Always have this first to get most up-to-date helper variables\r\ngetHelperVars();\r\n\r\n//This is what calls updateValidation in \r\n//https://github.com/code-dot-org/code-dot-org/blob/staging/apps/src/p5lab/spritelab/commands/validationCommands.js\r\n//To check the validation criteria\r\ncheck();\r\n\r\n//Always have this at end to store previous state in validationProps.previous\r\nupdatePrevious();",
