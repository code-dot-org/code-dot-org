    "start_blocks": "//-------------------\n//-------DAYS--------\n//-------------------\n\n//Sets how the days go by and which screens to go to\n\nvar DAYS = [\"start\", \"buy\", \"news_3\", \"news_3\", \"sell\", \"buy\", \"news_3\", \"news_3\", \"final\"];\nvar DAYSINDEX = 0;\nvar NEWSSTORY = -1;\n\nfunction nextDay() {\n  DAYSINDEX++;\n  //console.log(\"DAYSINDEX: \" + DAYSINDEX);\n  var curDay = DAYS[DAYSINDEX];\n  if(curDay == \"start\") {\n    setScreen(\"startScreen\");\n  } else if(curDay == \"buy\") {\n    setupBuyScreen();\n  } else if(curDay == \"sell\") {\n    setupSellScreen();\n  } else if(curDay == \"final\") {\n    ASSETS[\"ConnectUSD\"][\"value\"] = (ASSETS[\"ConnectUSD\"][\"value\"] * 1.05).toFixed(2);\n    ASSETS[\"Sol\"][\"value\"] = (ASSETS[\"Sol\"][\"value\"] * 1.2).toFixed(2);\n    updateAllAssetsValue();\n    setScreen(\"finalScreen\");\n  } else {\n    //process of elimination - must be a news day\n    setupNewsScreen();\n  }\n}\n\nfunction setupNewsScreen() {\n  NEWSSTORY = -1;\n  setProperty(\"assetsForeground\", \"height\", 450);\n  setProperty(\"newsForeground\", \"height\", 450);\n  showElement(\"assetsDescriptionLabel\");\n  showElement(\"newsNumStories\");\n  showElement(\"newsNext\");\n  showElement(\"flitterTitle\");\n  showElement(\"newsForeground\");\n  showElement(\"severalMonthsLabel\");\n  hideElement(\"resultsExplanationLabel\");\n  hideElement(\"newsPrev\");\n  hideElement(\"newsScreenstory\");\n  hideElement(\"newsScreenimage\");\n  hideElement(\"newsScreenheadline\");\n  hideElement(\"checkAssetsButton\");\n  hideElement(\"notificationLabel\");\n  hideElement(\"notificationIcon\");\n  var numStories = DAYS[DAYSINDEX].split(\"_\")[1];\n  setText(\"newsNumStories\", \"Today there are \" + numStories + \" news headlines relevant to your interests!\");\n  setScreen(\"newsScreen\");\n  newsScreenAnimation();\n  updateAssetImages();\n}\n\nvar severalMonthsLoop = 0;\nfunction newsScreenAnimation() {\n  severalMonthsLoop = 0;\n  timedLoop(400, function() {\n    if(severalMonthsLoop < 8) {\n      var monthsLabel = getText(\"severalMonthsLabel\");\n      setText(\"severalMonthsLabel\", monthsLabel + \".\");\n      severalMonthsLoop += 1;\n    } else {\n      stopTimedLoop();\n      hideElement(\"severalMonthsLabel\");\n      scrollForeground();\n    }\n  });\n}\n\nfunction scrollForeground() {\n  hideElement(\"newsNext\");\n  timedLoop(5, function() {\n    var h = getProperty(\"newsForeground\", \"height\");\n    h = h - 2;\n    setProperty(\"newsForeground\", \"height\", h);\n    if(h < 10) {\n      hideElement(\"newsForeground\");\n      showElement(\"newsNext\");\n      stopTimedLoop();\n    }\n  });\n}\n\nfunction initialTransactScreen() {\n  setProperty(\"transactForeground\", \"height\", 450);\n  showElement(\"transactForeground\");\n  showElement(\"assetExplanationLabel\");\n  showElement(\"arrowLabel\");\n  showElement(\"transactContinueButton\");\n}\n\nonEvent(\"transactContinueButton\", \"click\", function() {\n  hideElement(\"assetExplanationLabel\");\n  hideElement(\"arrowLabel\");\n  hideElement(\"transactContinueButton\");\n  scrollTransact();\n});\n\nfunction scrollTransact() {\n  hideElement(\"transactScreenNext\");\n  timedLoop(5, function() {\n    var h = getProperty(\"transactForeground\", \"height\");\n    h = h - 2;\n    setProperty(\"transactForeground\", \"height\", h);\n    if(h < 10) {\n      hideElement(\"transactForeground\");\n      showElement(\"transactScreenNext\");\n      stopTimedLoop();\n    }\n  });\n}\n\nfunction setupBuyScreen() {\n  setText(\"transactScreentitle\", \"CryptoBase is an exchange that lets you buy and sell NFTs and Cryptocurrencies. On this screen, you can buy new NFTs from the marketplace. The app keeps track of how many NFTs are available in the entire market.\");\n  showElement(\"transactScreentitleBuy\");\n  hideElement(\"transactScreentitleSell\");\n  showElement(\"BOOPLOBuy\");\n\tshowElement(\"high5Buy\");\n\tshowElement(\"cryptoGerbilsBuy\");\n\tshowElement(\"lazyOcelotsBuy\");\n\thideElement(\"lazyOcelotsSell\");\n\thideElement(\"high5Sell\");\n\thideElement(\"BOOPLOSell\");\n\thideElement(\"cryptoGerbilsSell\");\n\tinitialTransactScreen();\n  setScreen(\"transactScreen\");\n}\n\nfunction setupSellScreen() {\n  setText(\"transactScreentitle\", \"CryptoBase is an exchange that lets you buy and sell NFTs and Cryptocurrencies. On this screen, you can sell NFTs back to the marketplace. The app keeps track of how many NFTs are available in the entire market.\");\n  showElement(\"transactScreentitleSell\");\n  hideElement(\"transactScreentitleBuy\");\n  hideElement(\"BOOPLOBuy\");\n\thideElement(\"high5Buy\");\n\thideElement(\"cryptoGerbilsBuy\");\n\thideElement(\"lazyOcelotsBuy\");\n\tshowElement(\"lazyOcelotsSell\");\n\tshowElement(\"high5Sell\");\n\tshowElement(\"BOOPLOSell\");\n\tshowElement(\"cryptoGerbilsSell\");\n\tinitialTransactScreen();\n  setScreen(\"transactScreen\");\n}\n\nfunction daysInitialize() {\n  setText(\"daySettings\", DAYS + \"\");\n}\n\nfunction daysSettingsUpdate() {\n  var text = getText(\"daySettings\");\n  DAYS = text.split(\",\");\n  for(var i = 0; i < DAYS.length; i++) {\n    DAYS[i] = DAYS[i].trim();\n  }\n}\n\n//-------------------\n//---ASSETS----\n//-------------------\n\nvar ASSETS = {};\n/*\nASSETS = {\n  â€œnameâ€: {\n    â€œvalueâ€: num,\n    â€œquantityâ€: num,\n    \"max\", num --> maximum amount of NFT in the market. Used for buy/sell logic\n    \"valueIDs\": [text] --> array of IDs of design elements tied to value model\n    \"quantityIDs\": [text] --> array of IDs of design elements tied to quantity model\n  }\n}\n\n-1 quantity -> infinite amount (like tokens or cryptocurrency)\n\n*/\n\nfunction addAsset(name, value, quantity, img) {\n  \n  ASSETS[name] = {\n    \"value\": value,\n    \"quantity\": quantity,\n    \"max\": quantity,\n    \"valueIDs\": [],\n    \"quantityIDs\": [],\n    \"image\": img\n  };\n}\n\nfunction makeAssetIDs() {\n  var screenNames = [\"transactScreen\", \"newsScreen\", \"finalScreen\"];\n  for(var key in ASSETS) {\n     for(var i = 0; i < screenNames.length; i++) {\n       var screen = screenNames[i];\n       var screenKeyCombo = screen + key;\n       var noSpaces = screenKeyCombo.replace(/\\s+/g, '');\n       ASSETS[key][\"valueIDs\"].push(noSpaces + \"value\");\n       if(screen == \"transactScreen\") {\n        ASSETS[key][\"quantityIDs\"].push(noSpaces + \"quantity\");\n       }\n     }\n  }\n}\n\nfunction updateAssetView(id, property) {\n  var elements = ASSETS[id][property + \"IDs\"];\n  var value = ASSETS[id][property];\n  for(var i = 0; i < elements.length; i++) {\n    setText(elements[i], value);\n  }\n}\n\nfunction updateAllAssetsValue() {\n  for(var id in ASSETS) {\n    updateAssetView(id, \"value\");\n  }\n}\n\nfunction updateAllAssetsQuantity() {\n  for(var id in ASSETS) {\n    updateAssetView(id, \"quantity\");\n  }\n}\n\nfunction updateAllAssets() {\n  for(var id in ASSETS) {\n    updateAssetView(id, \"value\");\n    updateAssetView(id, \"quantity\");\n  }\n}\n\nfunction setAsset(id, property, value) {\n  ASSETS[id][property] = value;\n}\n\nfunction decreaseAssetQuantity(id) {\n  var asset = ASSETS[id];\n  if(asset[\"quantity\"] > 0) {\n    asset[\"quantity\"] -= 1;\n  }\n}\n\nfunction increaseAssetQuantity(id) {\n  var asset = ASSETS[id];\n  if(asset[\"quantity\"] < asset[\"max\"]) {\n    asset[\"quantity\"] += 1;\n  }\n}\n\nfunction assetDebug(extended) {\n  console.log(\"ASSETS - \");\n  for(var key in ASSETS) {\n    console.log(\"\\t\" + key);\n    console.log(\"\\t\\t\" + \"value: \" + ASSETS[key][\"value\"]);\n    console.log(\"\\t\\t\" + \"quantity: \" + ASSETS[key][\"quantity\"]);\n    if(extended) {\n      console.log(\"\\t\\t\" + \"valueIDs: [\" + ASSETS[key][\"valueIDs\"] + \"]\");\n      console.log(\"\\t\\t\" + \"quantityIDs: [\" + ASSETS[key][\"quantityIDs\"] + \"]\");\n    }\n    \n  }\n}\n\nvar ASSETIMAGES = {};\nASSETIMAGES[\"Lazy Ocelots\"] = [\n  \"//images.code.org/9d406b1f99f30b54b86c1029665743f1-ocelot1B.png\",\n  \"//images.code.org/c73455291eb7b69c9d0d48d19eaace56-ocelot2D.png\",\n  \"//images.code.org/682ac134ce297ffc5991ad65d5b4ea2f-ocelot3B.png\",\n  \"//images.code.org/a7711902b633382a5ba182a5d011fcff-ocelot4E.png\"\n  ];\nASSETIMAGES[\"High 5\"] = [\n  \"//images.code.org/ab39104739d3d80c561ea3f68145f97f-high-five-emoji.png\"\n  ];\nASSETIMAGES[\"BOOPLO\"] = [\n  \"//images.code.org/e24862ce2c0c86c1b5e73c04e830f8aa-booplo1.png\",\n  \"//images.code.org/b0e87ca127baadbca78067bce1f6d452-booplo3.png\",\n  \"//images.code.org/0e62d1319b2f8990571b98e3ecd876cb-booplo4.png\"\n  ];\nASSETIMAGES[\"Crypto Gerbils\"] = [\n  \"//images.code.org/6ba524cdf2c3fec69278999dd9a0ecb7-gerbil1B.png\",\n  \"//images.code.org/7c4ee8c46ffbd77f762c80f8d763c3e8-gerbil2C.png\",\n  \"//images.code.org/0bfe07b3b2f70481f7720dcdb85c7cdc-gerbil3E.png\"\n  ];\nASSETIMAGES[\"ByteCoin\"] = [\n  \"//images.code.org/f098855015124c44a6febcd51b4d2f50-bytecoin.png\"\n  ];\nASSETIMAGES[\"IntCoin\"] = [\n  \"//images.code.org/78c6046e4bc2f7743228cb5ff129cc6d-intcoin.png\"\n  ];\nASSETIMAGES[\"ParrotCoin\"] = [\n  \"//images.code.org/dde43c06c771baf71bbf3884ffca0a03-parrotcoin.png\"\n  ];\nASSETIMAGES[\"ConnectUSD\"] = [\n  \"//images.code.org/305a567d3e20163e7d51e5041eec5feb-connectusd.png\"\n  ];\nASSETIMAGES[\"Sol\"] = [\n  \"//images.code.org/d23df197978a53faac69aa32152fa57c-sol.png\"\n  ];\n\nfunction assetInitialize(debug) {\n  addAsset(\"Lazy Ocelots\", 100, 20, ASSETIMAGES[\"Lazy Ocelots\"][0]);\n  addAsset(\"High 5\", 800, 1, ASSETIMAGES[\"High 5\"][0]);\n  addAsset(\"BOOPLO\", 500, 5, ASSETIMAGES[\"BOOPLO\"][0]);\n  addAsset(\"Crypto Gerbils\", 35, 15, ASSETIMAGES[\"Crypto Gerbils\"][0]);\n  //addAsset(\"CBrown Catt\", 50, 10, ASSETIMAGES[\"Lazy Ocelots\"][0]);\n  addAsset(\"ByteCoin\", 256, -1, ASSETIMAGES[\"ByteCoin\"][0]);\n  addAsset(\"IntCoin\", 125, -1, ASSETIMAGES[\"IntCoin\"][0]);\n  addAsset(\"ParrotCoin\", 10, -1, ASSETIMAGES[\"ParrotCoin\"][0]);\n  addAsset(\"ConnectUSD\", 1, -1, ASSETIMAGES[\"ConnectUSD\"][0]);\n  addAsset(\"Sol\", 1, -1, ASSETIMAGES[\"Sol\"][0]);\n  makeAssetIDs();\n  if(debug) {\n    assetDebug(true);\n  }\n}\n\n//-------------------\n//---ALLNEWS----\n//-------------------\n\nvar ALLNEWS = {};\n/*\nALLNEWS = \n  {\n    â€œidâ€: {\n      â€œheadline:â€ text,\n      â€œstoryâ€: text,\n      â€œresultâ€: text,\n      â€œassetâ€: text,\n      â€œvalueMultâ€: number,\n      â€œrippleâ€: [\n          â€œidâ€: text\n        ],\n      â€œvalidâ€: boolean  \n    }\n  }\n\n- asset and valueMult are how the change in asset is calculated in the ASSETS model behind the scenes. You use the asset key to find that asset in the model, and then multiply that value by valueMult\n- ripple is an array of strings that correspond to ids. As the headlines are being chosen, the controller iterates through the ripple array and finds the corresponding id in the ALLNEWS array. When it does, it sets the boolean value of valid to false. This means that story can no longer be chosen.\n  - For example, if the news story about markets freezing certain assets gets selected, then no other stories about those assets should be generated since the markets already froze those assets\n*/\n\nvar newsIDs = [];\n\nfunction allNewsInitialize(debug) {\n  ALLNEWS[\"ocelots1\"] = {\n      \"headline\": \"Private Daytrip with NFT Owners\",\n      \"story\": \"The Lazy Ocelot NFT group has organized an exclusive daytrip for owners of a Lazy Ocelot NFT. Several celebrities have already indicated they will attend. Other NFT owners will have the opportunity to meet each other, as well as hang out with these famous people.\",\n      \"result\": \"Lazy Ocelots NFTs increase by 20%\",\n      \"asset\": \"Lazy Ocelots\",\n      \"valueMult\": 1.2,\n      \"ripple\": [],\n      \"valid\": true\n    };\n\n  ALLNEWS[\"ocelots2\"] = {\n      \"headline\": \"Exclusive Lazy Ocelot Art Raffle\",\n      \"story\": \"Lazy Ocelot NFT owners have been entered into a raffle to receive exclusive art designed by prominent artists in the crypto space. Lazy Ocelot owners can receive exclusive artwork from this raffle\",\n      \"result\": \"Lazy Ocelots NFTs increase by 15%\",\n      \"asset\": \"Lazy Ocelots\",\n      \"valueMult\": 1.15,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"ocelots3\"] = {\n      \"headline\": \"New Lazy Ocelot Owners\",\n      \"story\": \"The Lazy Ocelot NFT project recently created 20 new NFTs, 5 of which were purchased by the cast of a popular young adult vampire franchise. The new owners have been promoting the NFTs on social media and encouraging folks to buy the remaining NFTs\",\n      \"result\": \"Lazy Ocelots NFTs increase by 25%\",\n      \"asset\": \"Lazy Ocelots\",\n      \"valueMult\": 1.25,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"ocelots4\"] = {\n      \"headline\": \"Lazy Ocelots Hacked!\",\n      \"story\": \"The Lazy Ocelot community was hacked and several NFTs were stolen from high-profile members and celebrities. These celebrities took to social media to complain about the theft and are hoping to reclaim their NFTs.\",\n      \"result\": \"Lazy Ocelots NFTs decrease by 20%\",\n      \"asset\": \"Lazy Ocelots\",\n      \"valueMult\": 0.8,\n      \"ripple\": [],\n      \"valid\": true\n    };\n  \n    ALLNEWS[\"ocelots5\"] = {\n      \"headline\": \"Lazy Ocelots Artwork Plagiarized\",\n      \"story\": \"It was recently revealed that a prominent artist of the Lazy Ocelot project plagiarized their artwork from several prominent sources, including tracing their artwork rather than creating it themselves. This led many people to sell their NFTs that had been created by this artist.\",\n      \"result\": \"Lazy Ocelots NFTs decrease by 15%\",\n      \"asset\": \"Lazy Ocelots\",\n      \"valueMult\": 0.85,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"highfive1\"] = {\n      \"headline\": \"Chippr Banned in China\",\n      \"story\": \"The social media app Chippr was recently banned in China for censorship reasons. As a form of protest, activists from around the world are attempting to preserve and document social expression on the app by converting them to NFTs, increasing the value of any NFTs associated with Chippr.\",\n      \"result\": \"The First-Ever High 5 NFT increased by 80%\",\n      \"asset\": \"High 5\",\n      \"valueMult\": 1.8,\n      \"ripple\": [\"highfive2\"],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"highfive2\"] = {\n      \"headline\": \"First Fist-Bump NFT sells at a loss\",\n      \"story\": \"The website â€œInstafamâ€ recently minted an NFT of the first fist-bump ever given on the platform and attempted to sell it, similar to the first high-five on Chippr. However, this NFT sold for significantly less than asking price, causing similar NFTs to also decline in value.\",\n      \"result\": \"The First-Ever High 5 NFT decreased by 80%\",\n      \"asset\": \"High 5\",\n      \"valueMult\": 0.2,\n      \"ripple\": [\"highfive1\"],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"booplo1\"] = {\n      \"headline\": \"BOOPLO Enters the Metaverse\",\n      \"story\": \"The artist BOOPLO signed an exclusive deal where owners of their NFTs can display their artwork in a virtual Metaverse environment. People can create virtual art galleries that friends can visit to admire their collection.\",\n      \"result\": \"The BOOPLO NFTs increased by 40%\",\n      \"asset\": \"BOOPLO\",\n      \"valueMult\": 1.4,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"booplo2\"] = {\n      \"headline\": \"BOOPLO Passes Away\",\n      \"story\": \"BOOPLO, the NFT artist, passed away in a natural disaster. They were revered for their artwork, but never created any physical designs. Instead, all of their artwork were digital NFTs and are one of the only remaining pieces of their legacy.\",\n      \"result\": \"The BOOPLO NFTs increased by 70%\",\n      \"asset\": \"BOOPLO\",\n      \"valueMult\": 1.7,\n      \"ripple\": [\"booplo1\"],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"gerbils1\"] = {\n      \"headline\": \"Crypto Gerbil Evolution!\",\n      \"story\": \"The popular Crypto Gerbilâ€™s game has a new promotion that allows older NFTs to evolve and morph into unique, updated NFTs with special abilities within the game. This has increased the value of older NFTs capable of evolution.\",\n      \"result\": \"The Crypto Gerbils NFTs increased by 15%\",\n      \"asset\": \"Crypto Gerbils\",\n      \"valueMult\": 1.15,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"gerbils2\"] = {\n      \"headline\": \"Crypto Gerbil Museum\",\n      \"story\": \"An avid Crypto Gerbils fan is getting ready to open a museum within the Metaverse, allowing users to visit their vast collection of crypto gerbils. In preparation, this super fan is buying up as many crypto gerbil NFTs as they can find\",\n      \"result\": \"The Crypto Gerbils NFTs increased by 25%\",\n      \"asset\": \"Crypto Gerbils\",\n      \"valueMult\": 1.25,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"gerbils3\"] = {\n      \"headline\": \"Crypto Gerbils 2.0\",\n      \"story\": \"The Crypto Gerbil game company has announced they will be releasing a version 2.0 of the Crypto Gerbil game with new features and gameplay option. However, the new game will not be backwards compatible, and all existing Crypto Gerbils will be for display only. To participate in the new game, fans will need to buy all new Crypto Gerbils\",\n      \"result\": \"The Crypto Gerbils NFTs decreased by 30%\",\n      \"asset\": \"Crypto Gerbils\",\n      \"valueMult\": 1.25,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"bytecoin1\"] = {\n      \"headline\": \"Bytecoin exchange exploited\",\n      \"story\": \"A security vulnerability was discovered in a popular Bytecoin exchange, causing hackers to steal several million dollars worth of Bytecoin. While most individuals werenâ€™t affected, the collective result as for the value of Bytecoin to drop.\",\n      \"result\": \"This event has decreased the value of Bytecoin by 10%\",\n      \"asset\": \"ByteCoin\",\n      \"valueMult\": 0.9,\n      \"ripple\": [\"bytecoin2\"],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"bytecoin2\"] = {\n      \"headline\": \"Rising International Inflation\",\n      \"story\": \"A recent quarterly economic report indicated that inflation is rising in many international economies, causing the value of individual currencies to decrease. However, sine Bytecoin is not tied to a physical currency, it is resistant to problems caused by inflation. As a result, more people are looking to invest in Bytecoin.\",\n      \"result\": \"This event has increased the value of Bytecoin by 10%\",\n      \"asset\": \"ByteCoin\",\n      \"valueMult\": 1.1,\n      \"ripple\": [\"bytecoin1\"],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"intcoin1\"] = {\n      \"headline\": \"Access to IntCoin Revoked\",\n      \"story\": \"A recent law made it illegal to access the online exchanges where IntCoin was primarily traded. This made it impossible to access any IntCoin funds stored in those accounts - the coins are frozen until either the law is changed or people can move to another country to access the coins.\",\n      \"result\": \"This event has frozen all IntCoins and they cannot be withdrawn\",\n      \"asset\": \"IntCoin\",\n      \"valueMult\": 0,\n      \"ripple\": [\"intcoin2\", \"intcoin3\"],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"intcoin2\"] = {\n      \"headline\": \"International Crypto Regulation\",\n      \"story\": \"Several countries have begun regulating cryptocurrencies making them harder to trade and adding more steps to the trading process. IntCoin is especially affected since itâ€™s used in so many international markets\",\n      \"result\": \"This event has decreased the value of IntCoin by 30%\",\n      \"asset\": \"IntCoin\",\n      \"valueMult\": 0.7,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"intcoin3\"] = {\n      \"headline\": \"IntCoin becomes Currency of Columbia\",\n      \"story\": \"Columbia announced that it will use IntCoin as an official form of currency to help stabilize its economy. Over the course of the next year, it can be used for government transactions (like paying taxes) and businesses will be required to accept IntCoin as a possible currency.\",\n      \"result\": \"This event has increased the value of IntCoin by 30%\",\n      \"asset\": \"IntCoin\",\n      \"valueMult\": 1.3,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"parrot1\"] = {\n      \"headline\": \"ParrotCoin Hype\",\n      \"story\": \"An eccentric tech billionaire recently tweeted that they invested some money into ParrotCoin, which was previously thought to be a joke currency created by a few friends. But - because of the attention from the tech billionaire - the value of ParrotCoin has skyrocketed.\",\n      \"result\": \"This event has increased the value of ParrotCoin by 500%\",\n      \"asset\": \"ParrotCoin\",\n      \"valueMult\": 5,\n      \"ripple\": [],\n      \"valid\": true\n    };\n    \n    ALLNEWS[\"parrot2\"] = {\n      \"headline\": \"ParrotCoin Rug Pull\",\n      \"story\": \"The ParrotCoin organizers executed a rug pull, which is when they take coins that were invested into their program and sell them off. This led more people to sell their ParrotCoin and caused the value to decrease significantly until it eventually bottomed out.\",\n      \"result\": \"This event has decreased the value of ParrotCoin by 99%\",\n      \"asset\": \"ParrotCoin\",\n      \"valueMult\": 0.01,\n      \"ripple\": [\"parrot1\"],\n      \"valid\": true\n    };\n  \n  ALLNEWS[\"sol1a\"] = {\n      \"headline\": \"Sol Loses its Peg to the US Dollar\",\n      \"story\": \"Several large account holders sold their tokens and flooded the market with Sol. Since Sol is regulated via an algorithm, this large unexpected change in the market wasnâ€™t handled correctly and caused the value of Sol tokens to sell for less than $1. Once investors realized their tokens were worth less than they promised, people began selling their tokens at a rapid pace, plunging the value of sol\",\n      \"result\": \"Sol token decreases by 99%\",\n      \"asset\": \"Sol\",\n      \"valueMult\": 0.01,\n      \"ripple\": [\"sol1a\", \"sol1b\", \"sol2a\", \"sol2b\"],\n      \"valid\": true\n    };\n    \n  ALLNEWS[\"sol1b\"] = {\n      \"headline\": \"Sol Loses its Peg to the US Dollar\",\n      \"story\": \"Several large account holders sold their tokens and flooded the market with Sol. Since Sol is regulated via an algorithm, this large unexpected change in the market wasnâ€™t handled correctly and caused the value of Sol tokens to sell for less than $1. Once investors realized their tokens were worth less than they promised, people began selling their tokens at a rapid pace, plunging the value of sol\",\n      \"result\": \"Sol token decreases by 99%\",\n      \"asset\": \"Sol\",\n      \"valueMult\": 0.01,\n      \"ripple\": [\"sol1a\", \"sol1b\", \"sol2a\", \"sol2b\"],\n      \"valid\": true\n    };\n    \n  ALLNEWS[\"sol2a\"] = {\n      \"headline\": \"Sol Exchange / Investment Firm Files for Bankruptcy\",\n      \"story\": \"One of the central exchanges for Sol filed for bankruptcy. As one of its final acts before bankruptcy, it froze all withdrawals on its platform. As a result, many crypto assets are â€œlockedâ€ in the exchange and are unable to be retrieved.\",\n      \"result\": \"This event has frozen all Sol tokens and they cannot be withdrawn\",\n      \"asset\": \"Sol\",\n      \"valueMult\": 0,\n      \"ripple\": [\"sol1a\", \"sol1b\", \"sol2a\", \"sol2b\"],\n      \"valid\": true\n    };\n    \n  ALLNEWS[\"sol2b\"] = {\n      \"headline\": \"Sol Exchange / Investment Firm Files for Bankruptcy\",\n      \"story\": \"One of the central exchanges for Sol filed for bankruptcy. As one of its final acts before bankruptcy, it froze all withdrawals on its platform. As a result, many crypto assets are â€œlockedâ€ in the exchange and are unable to be retrieved.\",\n      \"result\": \"This event has frozen all Sol tokens and they cannot be withdrawn\",\n      \"asset\": \"Sol\",\n      \"valueMult\": 0,\n      \"ripple\": [\"sol1a\", \"sol1b\", \"sol2a\", \"sol2b\"],\n      \"valid\": true\n    };\n    \n    //This is a special news story that helps the widget fail gracefully if\n    //there aren't enough news stories for the days generated\n    ALLNEWS[\"nonews\"] = {\n      \"headline\": \"No More Crypto Headlines!\",\n      \"story\": \"There aren't any more crypto headlines\",\n      \"result\": \"Values remain the same\",\n      \"asset\": \"Sol\",\n      \"valueMult\": 1,\n      \"ripple\": [\"sol1\"],\n      \"valid\": true\n    };\n    \n    //This part removes the \"result\" string based on a decision I made - If I want to undo that and keep\n    //those strings as part of the UI, just remove this part below\n    \n    for(var entry in ALLNEWS) {\n      ALLNEWS[entry][\"result\"] = \"\";\n    }\n    \n    generateNewsIDs();\n    \n    if(debug) {\n      allNewsDebug();\n      newsIDsDebug();\n    }\n}\n\nfunction allNewsDebug() {\n  console.log(\"ALLNEWS - \");\n  for(var key in ALLNEWS) {\n    var newsStory = ALLNEWS[key];\n    console.log(\"\\tid: \" + key);\n    console.log(\"\\t\\theadline: \" + newsStory[\"headline\"]);\n    console.log(\"\\t\\tstory: \" + newsStory[\"story\"]);\n    console.log(\"\\t\\tresult: \" + newsStory[\"result\"]);\n    console.log(\"\\t\\tasset: \" + newsStory[\"asset\"]);\n    console.log(\"\\t\\tvalueMult: \" + newsStory[\"valueMult\"]);\n    console.log(\"\\t\\tripple: [\" + newsStory[\"ripple\"] + \"]\");\n    console.log(\"\\t\\tvalid: \" + newsStory[\"valid\"]);\n  }\n}\n\nfunction generateNewsIDs() {\n  for(var key in ALLNEWS) {\n    if(key != \"nonews\") {\n      newsIDs.push(key); \n    }\n  }\n}\n\nfunction newsIDsDebug() {\n  console.log(\"newsIds: [\" + newsIDs + \"]\");\n}\n\n//-------------------\n//---currentNews----\n//-------------------\n\nvar currentNews = [];\nvar currentNewsIndex = 0;\nvar currentNewsScreen = \"newsScreen\";\nvar visitedNews = [];\n\nfunction currentNewsDebug() {\n  console.log(\"currentNews: [\" + currentNews + \"]\");\n}\n\nfunction randomNews() {\n  //num: number - number of news stories to randomly grab\n  //Resets currentNews to empty\n  //Picks a random news story from newsIDs array\n  //Checks the 'valid' property of that ID in allnews (is this necessary anymore?)\n  //Adds that news story to currentNews array\n  //Removes that news story from newsIDs array\n  \n  var newsStr = DAYS[DAYSINDEX].replace(\"news_\", \"\");\n  var num = parseInt(newsStr);\n  \n  currentNews = [];\n  visitedNews = [];\n  currentNewsIndex = 0;\n  for(var i = 0; i < num; i++) {\n    var randomNewsIndex = Math.floor(newsIDs.length * Math.random());\n    var randomNewsID = newsIDs[randomNewsIndex];\n    //If newsIDs is empty, then there are no more valid news stories\n    //In this case: manually set the story to special \"nonews\" story\n    if(newsIDs.length == 0) {\n      randomNewsID = \"nonews\";\n    }\n    if(ALLNEWS[randomNewsID][\"valid\"]) {\n      //Story is valid - add it to news array\n      currentNews.push(randomNewsID);\n      //Will keep track of when we've visited all news stories\n      visitedNews.push(0);\n    } else {\n      //Story is invalid - need to make a different choice\n      //So subtract 1 to counter variable as a \"Redo\" to pick another story\n      i--;\n    }\n    newsIDs.splice(randomNewsIndex, 1); //deletes the story from the array\n    ALLNEWS[randomNewsID][\"valid\"] = false; //sets the valid bit to false\n    //Check the case where we ran out of news stories. In this case,\n    //reset the valid bit back to true so can re-use if needed\n    if(randomNewsID == \"nonews\") {\n      ALLNEWS[\"nonews\"][\"valid\"] = true;\n    }\n  }\n  invalidate();\n}\n\n//Who's in charge of keeping track whether or not a story is still valid?\n//Answer: ALLNEWS valid bit on each story\nfunction invalidate() {\n  for(var i = 0; i < currentNews.length; i++) {\n    var currentNewsId = currentNews[i];\n    var currentNewsRipple = ALLNEWS[currentNewsId][\"ripple\"];\n    for(var j = 0; j < currentNewsRipple.length; j++) {\n      var rippleId = currentNewsRipple[j];\n      ALLNEWS[rippleId][\"valid\"] = false;\n    }\n  }\n}\n\n//Updates the model with the new values based on news stories\n//This is an ASYNCHRONOUS action from when we generate / view the news stories\n//The idea is to see and digest the stories, then  see their impact on the markets\nfunction applyNews() {\n  for(var i = 0; i < currentNews.length; i++) {\n    var currentNewsId = currentNews[i];\n    var currentNewsAsset = ALLNEWS[currentNewsId][\"asset\"];\n    var currentNewsValueMult = ALLNEWS[currentNewsId][\"valueMult\"];\n    ASSETS[currentNewsAsset][\"value\"] = (ASSETS[currentNewsAsset][\"value\"] * currentNewsValueMult).toFixed(2);\n  }\n}\n\nfunction arrayChoice(arr) {\n  return arr[Math.floor((Math.random() * arr.length))];\n}\n\nfunction updateAssetImages() {\n  var assets = [\"Lazy Ocelots\", \"BOOPLO\", \"Crypto Gerbils\"];\n  for(var i = 0; i < assets.length; i++) {\n    var asset = assets[i];\n    ASSETS[asset][\"image\"] = arrayChoice(ASSETIMAGES[asset]);\n  }\n}\n\nfunction updateNewsView() {\n  var newsStory = ALLNEWS[currentNews[currentNewsIndex]];\n  var properties = [\"headline\", \"story\", \"result\"];\n  for(var i = 0; i < properties.length; i++) {\n    var property = properties[i];\n    setText(currentNewsScreen + property, newsStory[property]);\n  }\n  var img = ASSETS[newsStory[\"asset\"]][\"image\"];\n  setImageURL(currentNewsScreen + \"image\", img);\n  \n  if(currentNewsIndex >= 1) {\n    showElement(\"newsPrev\");\n  } else {\n    hideElement(\"newsPrev\");\n  }\n  \n  if(currentNewsIndex <= currentNews.length - 2) {\n    showElement(\"newsNext\");\n  } else {\n    hideElement(\"newsNext\");\n  }\n  \n  checkVisitedAllNews();\n  \n}\n\nfunction checkVisitedAllNews() {\n  visitedNews[currentNewsIndex] = 1;\n  var sum = 0;\n  for(var i = 0; i < visitedNews.length; i++) {\n    sum += visitedNews[i];\n  }\n  if(sum == visitedNews.length) {\n    showElement(\"notificationIcon\");\n    showElement(\"notificationLabel\");\n    showElement(\"checkAssetsButton\");\n    playSound(\"sound://category_alerts/vibrant_game_courious_alert_3_winner.mp3\", false);\n  }\n}\n\nfunction nextStory() {\n  currentNewsIndex += 1;\n  updateNewsView();\n  \n}\n\nfunction prevStory() {\n  currentNewsIndex -= 1;\n  updateNewsView();\n}\n\n//-------------------\n//------TESTS--------\n//-------------------\n\n\nfunction newsTest1() {\n  //Checks randomly generating 3 news stories in sequence\n  newsIDsDebug();\n  currentNewsDebug();\n  //randomNews(1);\n  applyNews();\n  newsIDsDebug();\n  currentNewsDebug();\n  //randomNews(1);\n  applyNews();\n  newsIDsDebug();\n  currentNewsDebug();\n  //randomNews(1);\n  applyNews();\n  newsIDsDebug();\n  currentNewsDebug();\n  assetDebug();\n}\n\nfunction newsTest2() {\n  //randomNews(2);\n  updateNewsView();\n}\n\n//--------------------------\n//------RUNNING CODE--------\n//--------------------------\n\nassetInitialize();\nallNewsInitialize();\ndaysInitialize();\nupdateAllAssetsValue();\nupdateAllAssetsQuantity();\n\n//----------------------\n//--------EVENTS--------\n//----------------------\n\n\n//newsScreen Events\n\nonEvent(\"checkAssetsButton\", \"click\", function( ) {\n\thideElement(\"newsScreenheadline\");\n\thideElement(\"newsScreenstory\");\n\thideElement(\"newsScreenresult\");\n\thideElement(\"newsScreenimage\");\n\thideElement(\"checkAssetsButton\");\n\thideElement(\"newsNext\");\n\thideElement(\"newsPrev\");\n\t//hideElement(\"assetsForeground\"); //this actually SHOWS the values by hiding the foreground label\n\thideElement(\"notificationIcon\");\n\thideElement(\"notificationLabel\");\n\thideElement(\"flitterTitle\");\n\t//showElement(\"assetsDescriptionLabel\");\n\tshowElement(\"newsScreenNext\");\n\tshowElement(\"resultsExplanationLabel\");\n\t\n\tapplyNews();\n\tupdateAllAssetsValue();\n\trevealAssets();\n});\n\nfunction revealAssets() {\n  hideElement(\"newsScreenNext\");\n  timedLoop(5, function() {\n    var h = getProperty(\"assetsForeground\", \"height\");\n    h = h - 2;\n    setProperty(\"assetsForeground\", \"height\", h);\n    if(h < 10) {\n      showElement(\"newsScreenNext\");\n      hideElement(\"assetsForeground\");\n      stopTimedLoop();\n    }\n  });\n}\n\nonEvent(\"newsPrev\", \"click\", function( ) {\n  prevStory();\n});\nonEvent(\"newsNext\", \"click\", function( ) {\n  if(NEWSSTORY < 0) {\n    setupNewsScreen2();\n  } else {\n\t  nextStory();\n  }\n});\n\nfunction setupNewsScreen2() {\n  NEWSSTORY = 0;\n  randomNews();\n\tupdateNewsView();\n  hideElement(\"assetsDescriptionLabel\");\n  hideElement(\"newsNumStories\");\n  showElement(\"newsScreenstory\");\n  showElement(\"newsScreenimage\");\n  showElement(\"newsScreenheadline\");\n}\n\nonEvent(\"newsScreenNext\", \"click\", function( ) {\n\tshowElement(\"assetsForeground\"); //this actually HIDES the values by showing the foreground label\n\thideElement(\"assetsDescriptionLabel\");\n\thideElement(\"newsScreenNext\");\n\t\n\tnextDay();\n});\n\n//startScreen Events\nonEvent(\"startScreenNext\", \"click\", function( ) {\n  daysSettingsUpdate();\n\tnextDay();\n});\n\n//transactScreen Events\nonEvent(\"transactScreenNext\", \"click\", function( ) {\n\tnextDay();\n});\nonEvent(\"lazyOcelotsBuy\", \"click\", function( ) {\n\tdecreaseAssetQuantity(\"Lazy Ocelots\");\n\tupdateAssetView(\"Lazy Ocelots\", \"quantity\");\n});\nonEvent(\"high5Buy\", \"click\", function( ) {\n\tdecreaseAssetQuantity(\"High 5\");\n\tupdateAssetView(\"High 5\", \"quantity\");\n});\nonEvent(\"BOOPLOBuy\", \"click\", function( ) {\n\tdecreaseAssetQuantity(\"BOOPLO\");\n\tupdateAssetView(\"BOOPLO\", \"quantity\");\n});\nonEvent(\"cryptoGerbilsBuy\", \"click\", function( ) {\n\tdecreaseAssetQuantity(\"Crypto Gerbils\");\n\tupdateAssetView(\"Crypto Gerbils\", \"quantity\");\n});\nonEvent(\"lazyOcelotsSell\", \"click\", function( ) {\n\tincreaseAssetQuantity(\"Lazy Ocelots\");\n\tupdateAssetView(\"Lazy Ocelots\", \"quantity\");\n});\nonEvent(\"high5Sell\", \"click\", function( ) {\n\tincreaseAssetQuantity(\"High 5\");\n\tupdateAssetView(\"High 5\", \"quantity\");\n});\nonEvent(\"BOOPLOSell\", \"click\", function( ) {\n\tincreaseAssetQuantity(\"BOOPLO\");\n\tupdateAssetView(\"BOOPLO\", \"quantity\");\n});\nonEvent(\"cryptoGerbilsSell\", \"click\", function( ) {\n\tincreaseAssetQuantity(\"Crypto Gerbils\");\n\tupdateAssetView(\"Crypto Gerbils\", \"quantity\");\n});\nonEvent(\"oopsButton\", \"click\", function( ) {\n\tshowElement(\"BOOPLOBuy\");\n\tshowElement(\"BOOPLOSell\");\n\tshowElement(\"cryptoGerbilsBuy\");\n\tshowElement(\"cryptoGerbilsSell\");\n\tshowElement(\"lazyOcelotsBuy\");\n\tshowElement(\"lazyOcelotsSell\");\n\tshowElement(\"high5Buy\");\n\tshowElement(\"high5Sell\");\n});\nonEvent(\"label2\", \"click\", function( ) {\n\tshowElement(\"daySettings\");\n});\n\nsetStyle(\"transactScreentitle\", \"padding: 2px\");\n//START THE DAY, ASSUMING WE BEGIN WITH A \"start\" STRING!\n//daysSettingsUpdate();\nnextDay();",
