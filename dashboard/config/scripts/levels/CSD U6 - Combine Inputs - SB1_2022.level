    "start_blocks": "setProperty(\"instructions\", \"hidden\", false);\r\n\r\nonBoardEvent(buttonL, \"press\", function() {\r\n  var LEDtype = getProperty(\"LED_input\", \"value\");\r\n  if (LEDtype == \"blink\") {\r\n    led.blink(200);\r\n  }\r\n  if (LEDtype == \"pulse\") {\r\n    led.pulse(200);\r\n  }\r\n});\r\nonBoardEvent(buttonR, \"press\", function() {\r\n  led.off();\r\n});\r\n\r\n",
