  <blocks>
    <start_blocks>
      <xml>
        <block type="when_run">
          <next>
            <block type="gamelab_setPrompt">
              <value name="QUESTION">
                <block type="Poetry_stringValue" can_disconnect_from_parent="false">
                  <title name="TEXT">Name an animal:</title>
                </block>
              </value>
              <value name="VAR">
                <block type="variables_get" can_disconnect_from_parent="false">
                  <title name="VAR">animal1</title>
                </block>
              </value>
              <next>
                <block type="gamelab_setPrompt">
                  <value name="QUESTION">
                    <block type="Poetry_stringValue" can_disconnect_from_parent="false">
                      <title name="TEXT">Name another animal:</title>
                    </block>
                  </value>
                  <value name="VAR">
                    <block type="variables_get" can_disconnect_from_parent="false">
                      <title name="VAR">animal2</title>
                    </block>
                  </value>
                </block>
              </next>
            </block>
          </next>
        </block>
        <block type="gamelab_whenAllPromptsAnswered" deletable="false">
          <next>
            <block type="Poetry_setTitle">
              <value name="TITLE">
                <block type="gamelab_textJoin">
                  <title name="TEXT1">The </title>
                  <value name="TEXT2">
                    <block type="Poetry_textVariableJoin">
                      <title name="VAR">animal1</title>
                      <value name="TEXT2">
                        <block type="gamelab_textJoin">
                          <title name="TEXT1"> and the </title>
                          <value name="TEXT2">
                            <block type="gamelab_textVariableJoin">
                              <title name="VAR">animal2</title>
                            </block>
                          </value>
                        </block>
                      </value>
                    </block>
                  </value>
                </block>
              </value>
              <next>
                <block type="Poetry_setAuthor">
                  <value name="AUTHOR">
                    <block type="gamelab_textJoin">
                      <title name="TEXT1">(your name here)</title>
                    </block>
                  </value>
                  <next>
                    <block type="Poetry_addLine"/>
                  </next>
                </block>
              </next>
            </block>
          </next>
        </block>
        <block type="Poetry_animateText" deletable="false"/>
      </xml>
    </start_blocks>
    <toolbox_blocks>
      <xml>
        <category name="Events">
          <block type="gamelab_whenAllPromptsAnswered"/>
        </category>
        <category name="Text">
          <block type="Poetry_setTitle">
            <value name="TITLE">
              <block type="gamelab_textJoin">
                <title name="TEXT1"/>
              </block>
            </value>
          </block>
          <block type="Poetry_setAuthor">
            <value name="AUTHOR">
              <block type="gamelab_textJoin">
                <title name="TEXT1"/>
              </block>
            </value>
          </block>
          <block type="Poetry_addLine">
            <value name="LINE">
              <block type="gamelab_textJoin">
                <title name="TEXT1"/>
              </block>
            </value>
          </block>
          <block type="Poetry_animateText"/>
          <block type="gamelab_textJoin">
            <title name="TEXT1"/>
          </block>
        </category>
        <category name="Variables">
          <block type="gamelab_setPrompt">
            <value name="QUESTION">
              <block type="text" can_disconnect_from_parent="false">
                <title name="TEXT"/>
              </block>
            </value>
            <value name="VAR">
              <block type="variables_get" can_disconnect_from_parent="false">
                <title name="VAR">???</title>
              </block>
            </value>
          </block>
          <block type="gamelab_textVariableJoin">
            <title name="VAR">???</title>
          </block>
        </category>
      </xml>
    </toolbox_blocks>
