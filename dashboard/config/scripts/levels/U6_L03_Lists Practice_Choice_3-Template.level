    "start_blocks": "  // 1️⃣\r\n// Lists of images from the DATA tab that draw the emoji features\r\nvar eyeList;\r\nvar mouthList;\r\nvar extraList;\r\n\r\n  // 2️⃣\r\n// An index value of which emoji feature to display on the screen\r\nvar mouthIndex;\r\nvar eyeIndex;\r\nvar extraIndex;\r\n\r\n// Lists that store a user's favorite emojis\r\nvar favoriteEyes = [];\r\nvar favoriteMouth = [];\r\nvar favoriteExtras = [];\r\nvar favoriteName = [];\r\nvar favoriteDescription = [];\r\nvar favoriteRed = [];\r\nvar favoriteGreen = [];\r\nvar favoriteBlue = [];\r\nvar favoriteIndex = 0;\r\n\r\nvar currentScreen;\r\nsetup();\r\n\r\n  // 3️⃣\r\nfunction updateEmoji() {   \r\n  // Use setImageURL() to set the 'eyes', 'mouth', and 'extras' images (3x)\r\n  // The images are stored in eyeList, mouthList, and extraList\r\n\r\n\r\n  // 🐛 Bug Alert! \r\n  //    What happens if the index variable is greater than the number of elements in a list?\r\n}\r\n\r\n  // 4️⃣\r\nonEvent(\"eyeButton\", \"click\", function( ) {\r\n  // When the eye button is pressed, increment the appropriate index value\r\n  // and update the Emoji being displayed in Create Mode.\r\n\r\n});\r\n\r\nonEvent(\"mouthButton\", \"click\", function( ) {\r\n  // When the mouth button is pressed, increment the appropriate index value\r\n  // and update the Emoji being displayed in Create Mode.\r\n\r\n});\r\n\r\nonEvent(\"extraButton\", \"click\", function( ) {\r\n  // When the extras button is pressed, increment the appropriate index value\r\n  // and update the Emoji being displayed in Create Mode.\r\n  \r\n});\r\n\r\n  // 5️⃣  \r\nfunction randomizeValues(){\r\n  // This function sets our color values and the values of \r\n  // 'eyeIndex', 'mouthIndex', and 'extraIndex' to random numbers\r\n  \r\n  // Use setNumber to change the value of 'colorSliderR', 'colorSliderG'\r\n  // and 'colorSliderB' to a number between 0 and 255.\r\n  setNumber(\"colorSliderR\", randomNumber(0, 255));\r\n\r\n  // Set each of our three index variables to a random value\r\n  // Implement the \"Random List Access Pattern\" -->\r\n  // https://levelbuilder-studio.code.org/s/csp6-2023/lessons/2/levels/5\r\n\r\n}\r\n\r\n  // 6️⃣  \r\nfunction addToFavorites(){\r\n  // This function will save the current data into each favorite list\r\n  /* Use appendItem() to add the following data into your favorite lists:\r\n    eyeIndex                  --> favoriteEyes\r\n    mouthIndex                --> favoriteMouth\r\n    extraIndex                --> favoriteExtras\r\n    getNumber(\"colorSliderR\") --> favoriteRed\r\n    getNumber(\"colorSliderG\") --> favoriteGreen\r\n    getNumber(\"colorSliderB\") --> favoriteBlue\r\n    getText(\"nameText\")       --> favoriteName\r\n    getText(\"descText\")       --> favoriteDescription   */\r\n}\r\n\r\nfunction removeFromFavorites(){\r\n  // This function will use removeItem() to remove an emoji from all\r\n  // favorite lists\r\n  \r\n\r\n}\r\n\r\n  // 7️⃣\r\nfunction displayGalleryMode(){\r\n  if (favoriteName.length > 0){  // Only display data if something is favorited\r\n  \r\n    // Based on the value of our favoriteIndex (controlled by the arrows)\r\n    // we will set 'eyeIndex', 'mouthIndex', and 'extraIndex' to the values\r\n    // contained in our favorite lists\r\n    \r\n\r\n    // Below we are setting the color, name, and description\r\n    // and updating the emoji display\r\n    setNumber(\"colorSliderR\", favoriteRed[favoriteIndex]);\r\n    setNumber(\"colorSliderG\", favoriteGreen[favoriteIndex]);\r\n    setNumber(\"colorSliderB\", favoriteBlue[favoriteIndex]);\r\n    setText(\"displayTextArea\", displayText);\r\n    updateEmoji();\r\n    setColor();\r\n  }\r\n}\r\n\r\nfunction displayCreateMode(){\r\n  randomizeValues();\r\n  setColor();\r\n  updateEmoji();\r\n}\r\n\r\n\r\n  // 8️⃣\r\nonEvent(\"leftArrowButton\", \"click\", function( ) {\r\n  // When the left button is clicked in gallery mode, change the\r\n  // value of favoriteIndex and redraw the emoji, name, and description\r\n\r\n  // Reference the \"List Scrolling Pattern\" -->\r\n  // https://levelbuilder-studio.code.org/s/csp6-2023/lessons/2/levels/6\r\n  \r\n  \r\n});\r\n\r\nonEvent(\"rightArrowButton\", \"click\", function( ) {\r\n  // When the left button is clicked in gallery mode, change the\r\n  // value of favoriteIndex and redraw the emoji, name, and description\r\n\r\n  // Reference the \"List Scrolling Pattern\" -->\r\n  // https://levelbuilder-studio.code.org/s/csp6-2023/lessons/2/levels/5  \r\n  \r\n  \r\n});\r\n\r\n\r\n /*******************************************************************************\r\n * All of the Events and Functions below have been created to assist with the   *\r\n * EmojiMaker. Feel free to explore, to modify, and to learn from them!         *\r\n *******************************************************************************/\r\n\r\n\r\nonEvent(\"favoriteButton\", \"click\", function( ) {\r\n  if (currentScreen == \"Create\"){\r\n    setProperty(\"selectionHighlight\", \"x\", 214);\r\n    addToFavorites();\r\n    setText(\"descText\", \"\");\r\n    setText(\"nameText\", \"\");\r\n  }\r\n  showElement(\"galleryButton\");\r\n  showElement(\"galleryMode\");\r\n});\r\n\r\nonEvent(\"galleryButton\", \"click\", function( ) {\r\n  if (favoriteName.length > 0){\r\n    setProperty(\"selectionHighlight\", \"x\", 107);  \r\n    hideCreateTools();\r\n    setProperty(\"CreateScreen\", \"image\", \"https://studio.code.org/v3/assets/hNTBPyz3t1HMoh8KXTV3dl8jmxaa7JfmdCcHB-VzoSQ/galleryBG.png\");\r\n    displayGalleryMode();\r\n    currentScreen = \"Gallery\";\r\n  }\r\n});\r\n\r\nonEvent(\"createButton\", \"click\", function( ) {\r\n  setProperty(\"selectionHighlight\", \"x\", 0);\r\n  setProperty(\"CreateScreen\", \"image\", \"https://studio.code.org/v3/assets/hNTBPyz3t1HMoh8KXTV3dl8jmxaa7JfmdCcHB-VzoSQ/createBG.png\");\r\n  showCreateTools();\r\n  currentScreen = \"Create\";\r\n  displayCreateMode();\r\n});\r\n\r\nonEvent(\"colorSliderR\", \"change\", function( ) {\r\n  setColor();\r\n});\r\n\r\nonEvent(\"colorSliderG\", \"change\", function( ) {\r\n  setColor();\r\n});\r\n\r\nonEvent(\"colorSliderB\", \"change\", function( ) {\r\n  setColor();\r\n});\r\n\r\n\r\nfunction setColor() {\r\n  setActiveCanvas(\"emojiColor\");\r\n  setFillColor(rgb(getNumber(\"colorSliderR\"), getNumber(\"colorSliderG\"), getNumber(\"colorSliderB\")));\r\n  circle(100, 150, 250);\r\n}\r\n\r\nfunction hideCreateTools(){\r\n  hideElement(\"colorSliderR\");\r\n  hideElement(\"colorSliderG\");\r\n  hideElement(\"colorSliderB\");\r\n  hideElement(\"extraButton\");\r\n  hideElement(\"mouthButton\");\r\n  hideElement(\"eyeButton\");\r\n  hideElement(\"palette\");\r\n  hideElement(\"nameText\");\r\n  hideElement(\"descText\");\r\n  hideElement(\"favoriteButton\");\r\n  hideElement(\"favoriteIcon\");\r\n  hideElement(\"removeFavorite\");\r\n  showElement(\"rightArrow\");\r\n  showElement(\"leftArrow\");\r\n  showElement(\"rightArrowButton\");\r\n  showElement(\"leftArrowButton\");\r\n  showElement(\"displayTextArea\");\r\n  setProperty(\"emojiOverlay\", \"x\", -50);\r\n  setProperty(\"emojiColor\", \"x\", -20);\r\n  setProperty(\"eyes\", \"x\", 133);\r\n  setProperty(\"mouth\", \"x\", 122);\r\n  setProperty(\"extras\", \"x\", 67);\r\n  \r\n}\r\n\r\nfunction showCreateTools(){\r\n  showElement(\"colorSliderR\");\r\n  showElement(\"colorSliderG\");\r\n  showElement(\"colorSliderB\");\r\n  showElement(\"extraButton\");\r\n  showElement(\"mouthButton\");\r\n  showElement(\"eyeButton\");\r\n  showElement(\"palette\");  \r\n  showElement(\"nameText\");\r\n  showElement(\"descText\");\r\n  showElement(\"favoriteButton\");\r\n  showElement(\"favoriteIcon\");\r\n  hideElement(\"rightArrow\");\r\n  hideElement(\"leftArrow\");\r\n  hideElement(\"rightArrowButton\");\r\n  hideElement(\"leftArrowButton\");\r\n  hideElement(\"displayTextArea\");\r\n  hideElement(\"removeFavorite\");\r\n  setProperty(\"emojiOverlay\", \"x\", 0);\r\n  setProperty(\"emojiColor\", \"x\", 82);\r\n  setProperty(\"eyes\", \"x\", 183);\r\n  setProperty(\"mouth\", \"x\", 172);\r\n  setProperty(\"extras\", \"x\", 117);  \r\n}\r\n\r\nfunction setup(){\r\n  randomizeValues();\r\n  setColor();\r\n  updateEmoji();\r\n  showCreateTools();\r\n  hideElement(\"galleryButton\");\r\n  hideElement(\"galleryMode\");\r\n  currentScreen = \"Create\";\r\n}\r\n",
