    "data_tables": "{\r\n\"CSP_Vocabulary_Bingo_4\": [\r\n  {\r\n    \"id\": 1,\r\n    \"Term\": \"Expression\",\r\n    \"Clue\": \"A combination of operators and values that evaluates to a single value\"\r\n  },\r\n  {\r\n    \"id\": 2,\r\n    \"Term\": \"Assignment Operator\",\r\n    \"Clue\": \"Allows a program to change the value represented by a variable\"\r\n  },\r\n  {\r\n    \"id\": 3,\r\n    \"Term\": \"Variable\",\r\n    \"Clue\": \"Holds one value at a time\"\r\n  },\r\n  {\r\n    \"id\": 4,\r\n    \"Term\": \"String\",\r\n    \"Clue\": \"A variable type created from any characters and contained inside double quotes\"\r\n  },\r\n  {\r\n    \"id\": 5,\r\n    \"Term\": \"Boolean Value\",\r\n    \"Clue\": \"A data type that is either true or false\"\r\n  },\r\n  {\r\n    \"id\": 6,\r\n    \"Term\": \"Comparison Operators\",\r\n    \"Clue\": \"The < (less than), > (greater than), <= (less than or equal), and >= (greater than or equal) comparisons\"\r\n  },\r\n  {\r\n    \"id\": 7,\r\n    \"Term\": \"Logical operator\",\r\n    \"Clue\": \"A symbol or word used to connect two or more expressions\"\r\n  },\r\n  {\r\n    \"id\": 8,\r\n    \"Term\": \"Conditional Statement\",\r\n    \"Clue\": \"A statement joining two events together often written in the form of 'If something, then something'\"\r\n  },\r\n  {\r\n    \"id\": 9,\r\n    \"Term\": \"Function\",\r\n    \"Clue\": \"A named group of programming instructions. Also referred to as a procedure.\"\r\n  },\r\n  {\r\n    \"id\": 10,\r\n    \"Term\": \"Function Call\",\r\n    \"Clue\": \"A command that executes the code within a function\"\r\n  }\r\n]\r\n}",
