    "start_html": "<div id=\"designModeViz\" class=\"appModern clip-content\" tabindex=\"1\" data-radium=\"true\" style=\"width: 320px; height: 450px; display: none;\"><div class=\"screen\" tabindex=\"1\" id=\"birthdayScreen\" style=\"display: block; height: 450px; width: 320px; left: 0px; top: 0px; position: absolute; z-index: 0; background-color: rgb(255, 255, 255);\" data-theme=\"classic\"><label style=\"margin: 0px; padding: 2px; line-height: 1; font-size: 27px; overflow: hidden; overflow-wrap: break-word; color: rgb(51, 51, 51); max-width: 320px; width: 320px; height: 95px; position: absolute; left: 0px; top: 0px; text-align: center; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; font-family: Arial, Helvetica, sans-serif; background-color: rgba(0, 0, 0, 0); text-rendering: optimizespeed;\" id=\"titleLabel\">Blow On The Circuit Playground to Blow Out the Candles!</label><img src=\"//levelbuilder-studio.code.org/media?u=http%3A%2F%2Fimages.all-free-download.com%2Fimages%2Fgraphiclarge%2Fbirthday_candles_312999.jpg\" data-canonical-image-url=\"http://images.all-free-download.com/images/graphiclarge/birthday_candles_312999.jpg\" data-object-fit=\"fill\" id=\"candleImage\" style=\"height: 210px; width: 275px; position: absolute; left: 25px; top: 200px; margin: 0px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px;\" data-image-type=\"url\"><button id=\"resetBtn\" style=\"padding: 0px; margin: 0px; height: 45px; width: 135px; font-size: 22px; color: rgb(255, 255, 255); background-color: rgb(26, 188, 156); position: absolute; left: 90px; top: 95px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; font-family: Arial, Helvetica, sans-serif;\" class=\"design-mode-hidden\">Reset</button></div></div>",
    "start_blocks": "//Add your sensor event here!\r\n\r\nonEvent(\"resetBtn\", \"click\", function() {\r\n  setProperty(\"candleImage\", \"hidden\", false);\r\n  setProperty(\"resetBtn\", \"hidden\", true);\r\n  setText(\"titleLabel\", \"Blow On The Circuit Playground to Blow Out the Candles!\");\r\n});\r\n",
