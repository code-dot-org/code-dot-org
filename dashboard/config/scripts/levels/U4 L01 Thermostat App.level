    "start_blocks": "var tempF = 70;\r\nvar tempC = Math.round((tempF - 32)*(5/9));\r\nvar tempDisplayF = tempF + \" F\";\r\nvar tempDisplayC = tempC + \" C\";\r\nvar userName;\r\nvar password;\r\n\r\nsetText(\"temperatureF\", tempDisplayF);\r\nsetText(\"temperatureC\", tempDisplayC);\r\n\r\nonEvent(\"downButton\", \"click\", function() {\r\n  if(tempF > 60){\r\n    tempF = tempF - 1;\r\n    tempC = Math.round((tempF - 32)*(5/9));\r\n  }\r\n  if(tempF >= 80){\r\n    setProperty(\"temperatureF\",\"text-color\",\"red\");\r\n    setProperty(\"temperatureC\",\"text-color\",\"red\");\r\n  } else if (tempF >= 70) {\r\n    setProperty(\"temperatureF\",\"text-color\",\"lightBlue\");\r\n    setProperty(\"temperatureC\",\"text-color\",\"lightBlue\");\r\n  } else {\r\n    setProperty(\"temperatureF\",\"text-color\",\"blue\");\r\n    setProperty(\"temperatureC\",\"text-color\",\"blue\");\r\n  }\r\n  tempDisplayF = tempF + \" F\";\r\n  tempDisplayC = tempC + \" C\";\r\n  setText(\"temperatureF\", tempDisplayF);\r\n  setText(\"temperatureC\", tempDisplayC);\r\n  playSound(\"sound://category_objects/sharp_switch.mp3\");\r\n});\r\n\r\nonEvent(\"upButton\", \"click\", function(){\r\n  if(tempF < 90){\r\n    tempF = tempF + 1;\r\n    tempC = Math.round((tempF - 32)*(5/9));\r\n  }\r\n  if(tempF >= 80){\r\n    setProperty(\"temperatureF\",\"text-color\",\"red\");\r\n    setProperty(\"temperatureC\",\"text-color\",\"red\");\r\n  } else if (tempF >= 70) {\r\n    setProperty(\"temperatureF\",\"text-color\",\"lightBlue\");\r\n    setProperty(\"temperatureC\",\"text-color\",\"lightBlue\");\r\n  } else {\r\n    setProperty(\"temperatureF\",\"text-color\",\"blue\");\r\n    setProperty(\"temperatureC\",\"text-color\",\"blue\");\r\n  }\r\n  tempDisplayF = tempF + \" F\";\r\n  tempDisplayC = tempC + \" C\";\r\n  setText(\"temperatureF\", tempDisplayF);\r\n  setText(\"temperatureC\", tempDisplayC);\r\n  playSound(\"sound://category_objects/sharp_switch.mp3\");\r\n});\r\n\r\nonEvent(\"loginButton\", \"click\", function() {\r\n  userName = getText(\"nameInput\");\r\n  password = getText(\"passwordInput\");\r\n  if((userName == \"raysHouse\") && (password == \"hotandcold\")){\r\n    setScreen(\"homeScreen\");\r\n  } else if ((userName == \"jillsHouse\") && (password == \"hotandcold\")){\r\n    setScreen(\"homeScreen\");\r\n  } else {\r\n    playSound(\"sound://category_instrumental/marimba_downscale_1.mp3\");\r\n  }\r\n});\r\n",
