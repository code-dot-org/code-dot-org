    "validation_code": "\r\n\r\nif (World.frameCount == 1) {\r\n    setFailTime(400); // Frames to wait before failing student\r\n  setDelayTime(90); // Frames to wait after success before stopping program\r\n  setupPrevious(); //Defines the validationProps.previous{} object. To use it, call updatePrevious() at the end of this box\r\n\r\n  addCriteria(function() {\r\n    return minimumSprites(1); // Check whether or not the student created a sprite.\r\n  }, \"noSprites\");  // Failure message: \"You need to make a sprite.\"\r\n  // Additional calls to addCriteria(), in order of precedence\r\n\r\n  \r\n  //check the first three fish, and fail if size > 30\r\n  addCriteria(function() {\r\n    for (var i = 0; i < spriteIds.length && i < 3; ++i) {\r\n      if (getProp({id: spriteIds[i]}, \"scale\") > 30)\r\n        return false;\r\n    }\r\n    return true;\r\n  }, \"cscAdaptationsSetSpriteSize\");  // Updated\r\n\r\n  //check the first three fish, fail if none are swimming in a group\r\n  addCriteria(function() {\r\n    for (var i = 0; i < spriteIds.length && i < 3; ++i) {\r\n      //shrinking = swimming in a group actually\r\n      if (checkSpriteHasBehavior(spriteIds[i], \"shrinking\"))\r\n        return true;\r\n    }\r\n    return false;\r\n  }, \"cscAdaptationsSetBehavior\");\r\n\r\n  //fail if arrow keys not pressed\r\n  addCriteria(function() {\r\n    return isKeyPressed(\"up\") || isKeyPressed(\"down\") || isKeyPressed(\"left\") || isKeyPressed(\"right\");\r\n  }, \"didntPressKey\");\r\n\r\n\r\n\r\n}\r\n\r\ngetHelperVars();\r\ncheck();\r\nupdatePrevious();\r\n",
