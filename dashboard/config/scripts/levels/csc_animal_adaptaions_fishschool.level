  <blocks>
    <start_blocks>
      <xml>
        <block type="when_run" movable="false">
          <next>
            <block type="procedures_callnoreturn" can_disconnect_from_parent="false">
              <mutation name="oceanSetup"/>
            </block>
          </next>
        </block>
        <block type="gamelab_everyInterval" uservisible="false">
          <field name="UNIT">"frames"</field>
          <value name="N">
            <block type="math_number" uservisible="false">
              <field name="NUM">1</field>
            </block>
          </value>
          <next>
            <block type="gamelab_makeNumSpritesNear" uservisible="false" can_disconnect_from_parent="false">
              <field name="ANIMATION_NAME">"cuteanimals_tuna"</field>
              <value name="NUM">
                <block type="math_number" uservisible="false">
                  <field name="NUM">3</field>
                </block>
              </value>
              <value name="LOCATION">
                <block type="gamelab_locationOf" uservisible="false">
                  <value name="SPRITE">
                    <block type="sprite_variables_get" uservisible="false">
                      <field name="VAR">schoolOfFish</field>
                    </block>
                  </value>
                </block>
              </value>
            </block>
          </next>
        </block>
        <block type="gamelab_whenSpriteCreated" deletable="false">
          <value name="SPRITE">
            <block type="gamelab_allSpritesWithAnimation">
              <field name="ANIMATION">"cuteanimals_tuna"</field>
            </block>
          </value>
        </block>
        <block type="procedures_defnoreturn" editable="false" usercreated="true">
          <mutation/>
          <field name="NAME" id="oceanSetup">oceanSetup</field>
          <statement name="STACK">
            <block type="gamelab_setBackgroundImageAs" deletable="false" can_disconnect_from_parent="false">
              <field name="IMG">"background_underwater_17"</field>
              <next>
                <block type="gamelab_printText" can_disconnect_from_parent="false">
                  <value name="TEXT">
                    <block type="text" can_disconnect_from_parent="false">
                      <field name="TEXT">Use your arrow keys to move the school.</field>
                    </block>
                  </value>
                  <next>
                    <block type="gamelab_createNewSprite" can_disconnect_from_parent="false">
                      <field name="NAME">schoolOfFish</field>
                      <field name="COSTUME">"cuteanimals_tuna"</field>
                      <value name="LOCATION">
                        <block type="gamelab_location_picker" can_disconnect_from_parent="false">
                          <field name="LOCATION">{"x":200,"y":200}</field>
                        </block>
                      </value>
                      <next>
                        <block type="gamelab_setProp" can_disconnect_from_parent="false">
                          <field name="PROPERTY">"scale"</field>
                          <value name="SPRITE">
                            <block type="gamelab_allSpritesWithAnimation" can_disconnect_from_parent="false">
                              <field name="ANIMATION">"cuteanimals_tuna"</field>
                            </block>
                          </value>
                          <value name="VAL">
                            <block type="math_number" can_disconnect_from_parent="false">
                              <field name="NUM">30</field>
                            </block>
                          </value>
                          <next>
                            <block type="adaptations_toggleBehaviorSimple" can_disconnect_from_parent="false">
                              <field name="TOGGLE">"begins"</field>
                              <value name="SPRITE">
                                <block type="gamelab_allSpritesWithAnimation" can_disconnect_from_parent="false">
                                  <field name="ANIMATION">"cuteanimals_tuna"</field>
                                </block>
                              </value>
                              <value name="BEHAVIOR">
                                <block type="gamelab_behavior_get" can_disconnect_from_parent="false">
                                  <mutation/>
                                  <field name="VAR" id="moving with arrow keys">moving with arrow keys</field>
                                </block>
                              </value>
                            </block>
                          </next>
                        </block>
                      </next>
                    </block>
                  </next>
                </block>
              </next>
            </block>
          </statement>
        </block>
        <block type="behavior_definition" editable="false" uservisible="false">
          <mutation>
            <arg name="this sprite" type="Sprite"/>
            <description>change the size of a sprite</description>
          </mutation>
          <field name="NAME" id="shrinking">swimming in a group</field>
          <statement name="STACK">
            <block type="gamelab_changePropBy" uservisible="false">
              <field name="PROPERTY">"scale"</field>
              <value name="SPRITE">
                <block type="sprite_parameter_get" uservisible="false">
                  <field name="VAR">this sprite</field>
                </block>
              </value>
              <value name="VAL">
                <block type="math_number" uservisible="false">
                  <field name="NUM">-1</field>
                </block>
