    "authored_hints": "[{\"hint_class\":\"content\",\"hint_markdown\":\"Replace each prompt in your program with a new prompt that includes choices: \\n\\n<div style=\\\"width: 100%;height:280px;\\\">\\n        <div style=\\\"width: 40%; float: left;\\\">\\n<xml>\\n  <block type=\\\"gamelab_setPrompt\\\">\\n    <value name=\\\"QUESTION\\\">\\n      <block type=\\\"text\\\">\\n        <title name=\\\"TEXT\\\">Name an animal:</title>\\n      </block>\\n    </value>\\n    <value name=\\\"VAR\\\">\\n      <block type=\\\"variables_get\\\" can_disconnect_from_parent=\\\"false\\\">\\n        <title name=\\\"VAR\\\">animal1</title>\\n      </block>\\n    </value>\\n    </block></xml>&emsp;&emsp;&emsp;&ensp; ↑↓<xml>\\n  <block type=\\\"gamelab_setPromptWithChoices\\\" x=\\\"300\\\">\\n      <value name=\\\"QUESTION\\\">\\n        <block type=\\\"text\\\">\\n          <title name=\\\"TEXT\\\">Name an animal:</title>\\n        </block>\\n      </value>\\n      <value name=\\\"VAR\\\">\\n        <block type=\\\"variables_get\\\" can_disconnect_from_parent=\\\"false\\\">\\n          <title name=\\\"VAR\\\">animal1</title>\\n        </block>\\n      </value>\\n      <value name=\\\"A\\\">\\n        <block type=\\\"Poetry_stringValue\\\">\\n          <title name=\\\"TEXT\\\">octopus</title>\\n        </block>\\n      </value>\\n      <value name=\\\"B\\\">\\n        <block type=\\\"Poetry_stringValue\\\">\\n          <title name=\\\"TEXT\\\">bird</title>\\n        </block>\\n      </value>\\n      <value name=\\\"C\\\">\\n        <block type=\\\"Poetry_stringValue\\\">\\n          <title name=\\\"TEXT\\\">cat</title>\\n        </block>\\n      </value>\\n    </block>\\n  </xml>\\n  </div>\\n</div>\\n\",\"hint_id\":\"poetry_module_choices_hint\",\"hint_type\":\"general\",\"hint_path\":\"\",\"hint_video\":\"\",\"tts_url\":\"https://tts.code.org/sharon22k/180/100/88ac71c213c6b80cb87155c2ba61e2f8/poetry_module_choice_prompts.mp3\"}]",
