    "start_blocks": "//FUTURE ITERATION: Don't include hashing - just include to / from / signature\r\n//FUTURE ITERATION: Generate message text boxes activate live-updates - don't need to press generate message button\r\n\r\n\r\n//**CONSTANTS**\r\nvar SHAREDPRIME = SHAHashLibrary.sharedPrime();\r\nvar PRIVATENUM;\r\nvar PUBLICNUM;\r\nvar MESSAGE;\r\nvar HASH;\r\nvar ENCRYPTEDHASH;\r\n\r\n//**HELPER FUNCTIONS**\r\nfunction xgcd(a,b) {\r\n    if (b == 0) {\r\n      return [1, 0, a];\r\n    } else {\r\n        var temp = xgcd(b, a % b);\r\n        var x = temp[0];\r\n        var y = temp[1];\r\n        var d = temp[2];\r\n        return [y, x-y*Math.floor(a/b), d];\r\n    }\r\n}\r\n\r\nfunction createSignature(hash, prikey) {\r\n  //console.log(\"HASH: \" + hash);\r\n  var tempStr = \"\";\r\n  for(var i = 0; i < hash.length; i += 4) {\r\n    var subStr = hash.substr(i, 4);\r\n    //console.log(subStr);\r\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\r\n    //console.log(\"toDec: \" + toDec);\r\n    var encrypted = (toDec * prikey) % SHAREDPRIME;\r\n    //console.log(\"encrypted: \" + encrypted);\r\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\r\n    //console.log(\"toHex: \" + toHex);\r\n    while(toHex.length < 4) {\r\n      toHex = \"0\" + toHex;\r\n    }\r\n    tempStr += toHex;\r\n  }\r\n  //console.log(\"SIGNED HASH: \" + tempStr);\r\n  return tempStr;\r\n}\r\n\r\nfunction decryptSignature(encryptedHash, pubKey) {\r\n  var tempStr = \"\";\r\n  for(var i = 0; i < encryptedHash.length; i += 4) {\r\n    var subStr = encryptedHash.substr(i, 4);\r\n    //console.log(subStr);\r\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\r\n    //console.log(\"toDec: \" + toDec);\r\n    var encrypted = (toDec * pubKey) % SHAREDPRIME;\r\n    //console.log(\"encrypted: \" + encrypted);\r\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\r\n    //console.log(\"toHex: \" + toHex);\r\n    \r\n    while(toHex.length < 4) {\r\n      toHex = \"0\" + toHex;\r\n    }\r\n    \r\n    tempStr += toHex;\r\n  }\r\n  return tempStr;\r\n}\r\n\r\nfunction messageFormat(from, to, amount) {\r\n  return \"<From: \" + from + \" | To: \" + to + \" | Amount: \" + amount + \">\";\r\n}\r\n\r\n//**SCREEN 1 EVENTS**\r\nonEvent(\"priKeyInput\", \"input\", function( ) {\r\n  var val = getNumber(\"priKeyInput\");\r\n  if(val < SHAREDPRIME) {\r\n    showElement(\"generatePublicButton\");\r\n    hideElement(\"errorLabel\");\r\n  } else {\r\n    showElement(\"errorLabel\");\r\n    hideElement(\"generatePublicButton\");\r\n  }\r\n});\r\n\r\nonEvent(\"generatePublicButton\", \"click\", function() {\r\n  PRIVATENUM = getNumber(\"priKeyInput\");\r\n  PUBLICNUM = xgcd(PRIVATENUM, SHAREDPRIME)[0];\r\n  PUBLICNUM = (PUBLICNUM < 0 ? (PUBLICNUM + SHAREDPRIME) : PUBLICNUM); //make positive\r\n  showElement(\"outputText\");\r\n  var output = \"Private Key: \" + PRIVATENUM + \"\\nPublic Key: \" + PUBLICNUM;\r\n  setText(\"outputText\", output);\r\n  showElement(\"screen1NextButton\");\r\n  showElement(\"copyLabel2\");\r\n});\r\n\r\nonEvent(\"screen1NextButton\", \"click\", function() {\r\n  setScreen(\"screen2\");\r\n});\r\n\r\n\r\n//**SCREEN 2 EVENTS**\r\nonEvent(\"screen2BackButton\", \"click\", function() {\r\n  setScreen(\"screen1\");\r\n});\r\n\r\nonEvent(\"messageButton\", \"click\", function () {\r\n  var name = getText(\"nameInput\");\r\n  var toName = getText(\"toInput\");\r\n  var amount = getText(\"amountInput\");\r\n  MESSAGE = messageFormat(name, toName, amount);\r\n  setText(\"messageTextBox\", MESSAGE);\r\n  showElement(\"messageTextBox\");\r\n  showElement(\"hashButton\");\r\n});\r\n\r\nonEvent(\"hashButton\", \"click\", function() {\r\n  HASH = SHAHashLibrary.md5(MESSAGE);\r\n  setText(\"hashTextBox\", HASH);\r\n  showElement(\"hashTextBox\");\r\n  showElement(\"screen2NextButton\");\r\n  showElement(\"copyLabel\");\r\n});\r\n\r\nonEvent(\"screen2NextButton\", \"click\", function() {\r\n  setScreen(\"screen4\");\r\n});\r\n\r\n//**SCREEN 3 EVENTS**\r\nonEvent(\"screen3BackButton\", \"click\", function() {\r\n  setScreen(\"screen4\");\r\n});\r\n\r\nonEvent(\"verifyButton\", \"click\", function() {\r\n  var hash = getText(\"hashTextInput\");\r\n  var sig = getText(\"signatureTextInput\");\r\n  var pubKey = getText(\"publicKeyTextInput\");\r\n  var decryptedHash = decryptSignature(sig, pubKey);\r\n  setText(\"decryptedTextBox\", decryptedHash);\r\n  showElement(\"decryptedTextBox\");\r\n  if(hash == decryptedHash) {\r\n    setText(\"verifyLabel\", \"SUCCESS - the original hash and the verified hash match!\");\r\n    setProperty(\"verifyLabel\", \"background-color\", \"green\");\r\n  } else {\r\n    setText(\"verifyLabel\", \"ERROR - the original hash and the verified hash do NOT match!\");\r\n    setProperty(\"verifyLabel\", \"background-color\", \"red\");\r\n  }\r\n});\r\n\r\n//**SCREEN 4 EVENTS**\r\n\r\nonEvent(\"screen4BackButton\", \"click\", function() {\r\n  setScreen(\"screen2\");\r\n});\r\n\r\nonEvent(\"screen4NextButton\", \"click\", function() {\r\n  setScreen(\"screen3\");\r\n});\r\n\r\nonEvent(\"screen4MessageButton\", \"click\", function() {\r\n  var name = getText(\"screen4FromInput\");\r\n  var toName = getText(\"screen4ToInput\");\r\n  var amount = getText(\"screen4AmountInput\");\r\n  MESSAGE = messageFormat(name, toName, amount);\r\n  setText(\"screen4MessageTextBox\", MESSAGE);\r\n  HASH = SHAHashLibrary.md5(MESSAGE);\r\n  setText(\"screen4HashTextBox\", HASH);\r\n  showElement(\"screen4HashLabel\");\r\n  showElement(\"screen4MessageTextBox\");\r\n  showElement(\"screen4HashTextBox\");\r\n  showElement(\"screen4PrivateLabel\");\r\n  showElement(\"screen4PrivateInput\");\r\n  showElement(\"screen4SignatureButton\");\r\n  showElement(\"hashButton\");\r\n});\r\n\r\nonEvent(\"screen4SignatureButton\", \"click\", function() {\r\n  var privateNum = getNumber(\"screen4PrivateInput\");\r\n  ENCRYPTEDHASH = createSignature(HASH, privateNum);\r\n  setText(\"screen4SignatureTextBox\", ENCRYPTEDHASH);\r\n  showElement(\"screen4SignatureTextBox\");\r\n  showElement(\"screen4NextButton\");\r\n});\r\n\r\n\r\n",
