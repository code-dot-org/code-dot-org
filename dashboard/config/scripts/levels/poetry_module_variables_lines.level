  <blocks>
    <start_blocks>
      <xml>
        <block type="when_run" movable="false"/>
      </xml>
    </start_blocks>
    <toolbox_blocks>
      <xml>
        <block type="gamelab_setPrompt">
          <value name="QUESTION">
            <block type="Poetry_stringValue">
              <title name="TEXT">Enter a word:</title>
            </block>
          </value>
        </block>
        <block type="variables_get">
          <title name="VAR">word1</title>
        </block>
        <block type="gamelab_whenAllPromptsAnswered"/>
        <block type="Poetry_addLine">
          <value name="LINE">
            <block type="Poetry_stringValue">
              <title name="TEXT">Sometimes</title>
            </block>
          </value>
          <next>
            <block type="Poetry_addLine">
              <value name="LINE">
                <block type="Poetry_stringValue">
                  <title name="TEXT">the short poems</title>
                </block>
              </value>
              <next>
                <block type="Poetry_addLine">
                  <value name="LINE">
                    <block type="Poetry_stringValue">
                      <title name="TEXT">are the hardest to write</title>
                    </block>
                  </value>
                  <next>
                    <block type="Poetry_addLine">
                      <value name="LINE">
                        <block type="Poetry_stringValue">
                          <title name="TEXT">change one word</title>
                        </block>
                      </value>
                      <next>
                        <block type="Poetry_addLine">
                          <value name="LINE">
                            <block type="Poetry_stringValue">
                              <title name="TEXT">and the whole poem</title>
                            </block>
                          </value>
                        </block>
                      </next>
                    </block>
                  </next>
                </block>
              </next>
            </block>
          </next>
        </block>
        <block type="Poetry_addLine"/>
        <block type="variables_get">
          <title name="VAR">word1</title>
        </block>
        <block type="Poetry_animateText"/>
      </xml>
    </toolbox_blocks>
  </blocks>
