    "start_sources": {
      "MyNeighborhood.java": {
        "text": "import org.code.neighborhood.Painter;\nimport java.util.Scanner;\n\npublic class MyNeighborhood {\n  public static void main(String[] args) {\n    // Create a Scanner object\n    Scanner input = new Scanner(System.in);\n\n    // Prompt user for x-coordinate and take input from the console as an integer\n    System.out.print(\"Enter the initial x-coordinate: \");\n    int initialX = input.nextInt();\n\n    // Prompt the user for y-coordinate and take input from the console as an integer\n    System.out.print(\"Enter the initial y-coordinate: \");\n    int initialY = input.nextInt();\n\n    // Prompt the user for direction and takes input from the console as a String\n    System.out.print(\"Enter the initial direction: \");\n    String direction = input.next();\n\n    // Prompt the user for paint level and takes input from the keyboard as an integer\n\tSystem.out.print(\"Enter the initial paint level: \");\n    int paintLevel = input.nextInt();\n\n    // Creates a new Painter object and passes initialX, initialY, direction, and paintLevel to the constructor\n    Painter dali = new Painter(initialX, initialY, direction, paintLevel);\n\n    // Closes the Scanner object\n    input.close();\n  }\n}",
