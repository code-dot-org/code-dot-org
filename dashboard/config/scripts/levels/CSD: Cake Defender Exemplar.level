    "start_blocks": "var score = 0;\r\n\r\nvar hero = createSprite(300,200);\r\nhero.setAnimation(\"alienWalkRight\");\r\n\r\nvar cake = createSprite(350,200);\r\ncake.setAnimation(\"cake\");\r\n\r\nvar enemy1 = createSprite();\r\nenemy1.setAnimation(\"ladybug\");\r\nsetEnemy1();\r\n\r\nvar enemy2 = createSprite();\r\nenemy2.setAnimation(\"ladybug\");\r\nsetEnemy2();\r\n\r\nfunction draw() {\r\n  gameBackground();\r\n  moveHero();\r\n  spriteInteractions();\r\n  showScore();\r\n  drawSprites();\r\n}\r\n\r\nfunction gameBackground(){\r\n  noStroke();\r\n  background(color(0,100,255));\r\n  fill(color(100,100,100));\r\n  rect(0,150,400,100);\r\n  fill(color(80,80,80));\r\n  rect(0,140,400,10);\r\n  rect(0,250,400,10);\r\n}\r\n\r\nfunction moveHero(){\r\n  if(keyDown(\"left\")){\r\n    hero.x = hero.x - 3;\r\n    hero.setAnimation(\"alienWalkLeft\");\r\n  }\r\n  if(keyDown(\"right\")){\r\n    hero.x = hero.x + 3;\r\n    hero.setAnimation(\"alienWalkRight\");\r\n  }\r\n  if(keyDown(\"up\")){\r\n    hero.y = hero.y - 3;\r\n  }\r\n  if(keyDown(\"down\")){\r\n    hero.y = hero.y + 3;\r\n  }\r\n}\r\n\r\nfunction spriteInteractions(){\r\n  \r\n  enemy1.velocityX = 2;\r\n  enemy2.velocityX = 2;\r\n  \r\n  hero.displace(enemy1);\r\n  hero.displace(enemy2);\r\n  \r\n  if(enemy1.isTouching(cake)){\r\n    score = score - 5;\r\n    setEnemy1();\r\n  }\r\n  if(enemy2.isTouching(cake)){\r\n    score = score - 5;\r\n    setEnemy2();\r\n  }\r\n  if(enemy1.x > 420 || enemy1.y < 140 || enemy1.y > 260){\r\n    setEnemy1();\r\n    score = score + 1;\r\n  }\r\n  if(enemy2.x > 420 || enemy2.y < 140 || enemy2.y > 260){\r\n    setEnemy2();\r\n    score = score + 1;\r\n  }\r\n}\r\n\r\nfunction showScore(){\r\n  textSize(20);\r\n  fill(\"white\");\r\n  text(\"Score\", 20, 20, 100, 100);\r\n  text(score, 20, 40, 200, 100);\r\n}\r\n\r\nfunction setEnemy1(){\r\n  enemy1.x = 0;\r\n  enemy1.y = randomNumber(150,250);\r\n}\r\n\r\nfunction setEnemy2(){\r\n  enemy2.x = 0;\r\n  enemy2.y = randomNumber(150,250);\r\n}",
