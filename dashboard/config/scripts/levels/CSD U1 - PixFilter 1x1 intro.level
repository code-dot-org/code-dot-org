    "start_blocks": "var colors = {\r\n  \"red\": false,\r\n  \"green\": false,\r\n  \"blue\": false,\r\n}\r\n\r\nvar iconUrl = \"//curriculum.code.org/images/icons/\";\r\n\r\nonEvent(\"screen1\", \"click\", function(event) {\r\n  var target = event.targetId;\r\n  if (target == \"red\" || target == \"green\" || target == \"blue\") {\r\n    colors[target] = !colors[target];\r\n    if (colors[target]) {\r\n      setImageURL(target, iconUrl + target + \".png\");\r\n      setText(target + \"_bin\", 1);\r\n    } else {\r\n      setImageURL(target, iconUrl + \"black.png\");\r\n      setText(target + \"_bin\", 0);\r\n    }\r\n    setPixel();\r\n  }\r\n});\r\n\r\nfunction setPixel() {\r\n  setProperty(\"pixel\", \"background-color\", \"rgb(\" + (colors.red * 255) +\r\n                  \",\" + (colors.green * 255) +\r\n                  \",\" + (colors.blue * 255) + \")\");\r\n}\r\n",
