    "start_animations": "{\r\n    \"orderedKeys\": [\r\n        \"balloon\",\r\n        \"tack\",\r\n        \"popped\"\r\n    ],\r\n    \"propsByKey\": {\r\n        \"balloon\": {\r\n            \"name\": \"balloon\",\r\n            \"sourceUrl\": \"https://images.code.org/7afe11fccea99cd7f31497710cce5ac3-image-1471190004988.png\",\r\n            \"frameSize\": {\r\n                \"x\": 100,\r\n                \"y\": 135\r\n            },\r\n            \"frameCount\": 1,\r\n            \"frameDelay\": 2,\r\n            \"looping\": true\r\n        },\r\n        \"tack\": {\r\n            \"name\": \"tack\",\r\n            \"sourceUrl\": \"https://images.code.org/0b6e98b829379d88b9952ee013cf70bd-image-1471190009596.png\",\r\n            \"frameSize\": {\r\n                \"x\": 50,\r\n                \"y\": 66\r\n            },\r\n            \"frameCount\": 1,\r\n            \"frameDelay\": 2,\r\n            \"looping\": true\r\n        },\r\n        \"popped\": {\r\n            \"name\": \"popped\",\r\n            \"sourceUrl\": \"https://images.code.org/49c7a3b6fe2eff7e51d3f40581b1dcbb-image-1471190256258.png\",\r\n            \"frameSize\": {\r\n                \"x\": 173,\r\n                \"y\": 135\r\n            },\r\n            \"frameCount\": 1,\r\n            \"frameDelay\": 2,\r\n            \"looping\": true\r\n        }\r\n    }\r\n}",
    "start_blocks": "var balloon = createSprite(200,200);\r\nballoon.setAnimation(\"balloon\");\r\n\r\nvar tack = createSprite(300, 100);\r\ntack.setAnimation(\"tack\");\r\n\r\nfunction draw() {\r\n  // check if tack is touching balloon\r\n  if(((tack.x > balloon.x) && ((tack.x - balloon.x) < (tack.width/2 + balloon.width/2))) || \r\n    ((tack.x < balloon.x) && ((tack.x - balloon.x) > -(tack.width/2 + balloon.width/2)))){\r\n      \r\n    if(((tack.y > balloon.y) && ((tack.y - balloon.y) < (tack.height/2 + balloon.height/2))) || \r\n      ((tack.y < balloon.y) && ((tack.y - balloon.y) > -(tack.height/2 + balloon.height/2)))){\r\n      // Pop the balloon\r\n      balloon.setAnimation(\"popped\");\r\n    } \r\n  }\r\n  \r\n  // make tack move with arrow keys\r\n  if (keyDown(\"up\")) {\r\n    tack.y = tack.y - 1;\r\n  }\r\n  if (keyDown(\"down\")) {\r\n    tack.y = tack.y + 1;\r\n  }\r\n  if (keyDown(\"left\")) {\r\n    tack.x = tack.x - 1;\r\n  }\r\n  if (keyDown(\"right\")) {\r\n    tack.x = tack.x + 1;\r\n  }\r\n  \r\n  background(\"gray\");\r\n  drawSprites();\r\n}",
