    "start_blocks": "var pitch;\r\nvar ball_x;\r\nvar player_y;\r\nonBoardEvent(accelerometer, \"data\", function() {\r\n  pitch = accelerometer.getOrientation(\"pitch\");\r\n  ball_x = getProperty(\"ball\", \"x\");\r\n  player_y = getProperty(\"player\", \"y\");\r\n  setProperty(\"player\", \"y\", player_y + pitch);\r\n});\r\n",
