    "start_blocks": "// define the variables\r\n\r\nvar balance = 0;\r\nvar feed = \"\";\r\nvar paymentAmount = 0;\r\n\r\n\r\n// (purpose) When clicked decreases the balance by the amount from the dropdown\r\n// also adds a line to the feed about who the payment was to and for what reason\r\n\r\nonEvent(\"payButton\", \"click\", function() {\r\n  paymentAmount = getNumber(\"amountDropdown\");\r\n  balance = balance - paymentAmount;\r\n  setText(\"balanceOutput\",balance);\r\n  playSound(\"sound://category_objects/sharp_switch.mp3\");\r\n  feed = \"You paid \" + getText(\"whoInput\") + \" for \" + getText(\"commentInput\") + \"\\n\\n\" + feed;\r\n  setText(\"feedOutput\", feed);\r\n});\r\n\r\n// (purpose) When clicked increases the balance by the amount from the dropdown\r\n// also adds a line to the feed about who the payment was from and for what reason\r\n\r\nonEvent(\"depositButton\", \"click\", function() {\r\n  paymentAmount = getNumber(\"amountDropdown\");\r\n  balance = balance + paymentAmount;\r\n  setText(\"balanceOutput\",balance);\r\n  playSound(\"sound://category_objects/sharp_switch.mp3\");\r\n  feed = getText(\"whoInput\") + \" paid you for \" + getText(\"commentInput\") + \"\\n\\n\" + feed;\r\n  setText(\"feedOutput\", feed);\r\n});\r\n\r\n\r\n\r\n\r\n\r\n\r\n",
