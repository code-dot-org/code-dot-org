    "start_blocks": "hideElement(\"startImage\");\r\nvar bits = [];\r\nbits[\"a\"] = [];\r\nbits[\"b\"] = [];\r\nbits[\"c\"] = [];\r\n\r\ntextLabel(\"instructions\", \"Click on the bits to turn them off and on and change the value of your numbers.\");\r\nsetPosition(\"instructions\", 20, 330, 280, 100);\r\nsetProperty(\"instructions\", \"font-size\", 20);\r\nsetProperty(\"instructions\", \"text-align\", \"center\");\r\n\r\ncreateBits(\"a\", 4, 70);\r\ncreateBits(\"b\", 5, 170);\r\ncreateBits(\"c\", 6, 270);\r\n\r\nfunction createBits(id, length, yPos) {\r\ncreateCanvas(\"background\" + id);\r\nsetActiveCanvas(\"background\" + id);\r\nsetPosition(\"background\" + id, 20, yPos, 285, 50);\r\nsetFillColor(\"gray\");\r\nrect(0, 0, 320, 400);\r\nfor (var i = 0; i<length ; i++) {\r\n  bits[id][i] = [];\r\n  bits[id][i].on = 0;\r\n  bits[id][i].value = Math.pow(2, i);\r\n  textLabel(\"bit\" + id + i, bits[id][i].value);\r\n  setProperty(\"bit\" + id + i, \"font-size\", 20);\r\n  setProperty(\"bit\" + id + i, \"text-color\", \"#303030\");\r\n  setProperty(\"bit\" + id + i, \"text-align\", \"center\");\r\n  setPosition(\"bit\" + id + i, 270 - 35 * i, yPos + 10, 30, 20);\r\n  createCanvas(\"canvas\" + id + i);\r\n  setPosition(\"canvas\" + id + i, 270 - 35 * i, yPos + 5, 30,40);\r\n  setActiveCanvas(\"canvas\" + id + i);\r\n  setFillColor(\"black\");\r\n  rect(0,0, 350, 400);\r\n  onEvent(\"bit\" + id + i, \"click\", function(event) {\r\n    toggleBit(event.targetId.substring(4), id);\r\n  })\r\n  onEvent(\"canvas\" + id + i, \"click\", function(event) {\r\n    toggleBit(event.targetId.substring(7), id);\r\n  })\r\n}\r\nfor (var i = length; i<8 ; i++) {\r\n  bits[id][i] = [];\r\n  bits[id][i].on = 0;\r\n  bits[id][i].value = 0;\r\n}\r\n\r\ntextLabel(\"number\" + id, \"0\");\r\nsetProperty(\"number\" + id, \"text-color\", \"#303030\");\r\nsetProperty(\"number\" + id, \"font-size\", 50);\r\nsetProperty(\"number\" + id, \"text-align\", \"center\");\r\nsetPosition(\"number\" + id, 100, yPos - 55, 120, 20);\r\n}\r\n\r\n\r\n\r\nfunction toggleBit(value, id) {\r\n  //toggle 1 or 0 in array\r\n  bits[id][value].on = 1 - bits[id][value].on;\r\n\r\n  //show or hide bit\r\n  setActiveCanvas(\"canvas\" + id + value);\r\n  if (bits[id][value].on == 1) {\r\n    setFillColor(\"white\");\r\n  } else {\r\n    setFillColor(\"black\");\r\n  }\r\n  rect(0,0, 350,400);\r\n  //recalculate number\r\n  recalculate(id);\r\n}\r\n\r\nfunction recalculate(id) {\r\n  var value = 0;\r\n  for (var i=0; i<8; i++) {\r\n    value += bits[id][i].value * bits[id][i].on;\r\n  }\r\n  setText(\"number\" + id, value);\r\n}\r\n",
