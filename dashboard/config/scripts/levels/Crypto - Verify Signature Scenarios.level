    "start_blocks": "//FUTURE ITERATION: Don't include hashing - just include to / from / signature\r\n//FUTURE ITERATION: Generate message text boxes activate live-updates - don't need to press generate message button\r\n\r\nhideElement(\"foregroundLabel\");\r\n\r\n//**CONSTANTS**\r\nvar SHAREDPRIME = SHAHashLibrary.sharedPrime();\r\nvar PRIVATENUM;\r\nvar PUBLICNUM;\r\nvar MESSAGE;\r\nvar HASH;\r\nvar ENCRYPTEDHASH;\r\n\r\nvar MESSAGES = [\r\n    {\r\n      \"message\": \"Meet by the basketball courts after school\",\r\n      \"signature\": \"5aa912791fe5ba904edb5518f1bde2c7\"\r\n    },\r\n    {\r\n      \"message\": \"Letâ€™s hang out this weekend\",\r\n      \"signature\": \"f30389613965941c937c3f6d5ca5f03e\"\r\n    },\r\n    {\r\n      \"message\": \"I brought some empanadas to share at lunch\",\r\n      \"signature\": \"02fb5739fa2b42458f93515ff065c092\"\r\n    },\r\n    {\r\n      \"message\": \"Someone told me Zoey is the coolest person in school\",\r\n      \"signature\": \"bf6625a37f515e3cb886bfaea9360b40\"\r\n    }\r\n  ];\r\n  \r\n  var MESSAGEINDEX = 0;\r\n\r\n//**HELPER FUNCTIONS**\r\nfunction xgcd(a,b) {\r\n    if (b == 0) {\r\n      return [1, 0, a];\r\n    } else {\r\n        var temp = xgcd(b, a % b);\r\n        var x = temp[0];\r\n        var y = temp[1];\r\n        var d = temp[2];\r\n        return [y, x-y*Math.floor(a/b), d];\r\n    }\r\n}\r\n\r\nfunction createSignature(hash, prikey) {\r\n  //console.log(\"HASH: \" + hash);\r\n  var tempStr = \"\";\r\n  for(var i = 0; i < hash.length; i += 4) {\r\n    var subStr = hash.substr(i, 4);\r\n    //console.log(subStr);\r\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\r\n    //console.log(\"toDec: \" + toDec);\r\n    var encrypted = (toDec * prikey) % SHAREDPRIME;\r\n    //console.log(\"encrypted: \" + encrypted);\r\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\r\n    //console.log(\"toHex: \" + toHex);\r\n    while(toHex.length < 4) {\r\n      toHex = \"0\" + toHex;\r\n    }\r\n    tempStr += toHex;\r\n  }\r\n  //console.log(\"SIGNED HASH: \" + tempStr);\r\n  return tempStr;\r\n}\r\n\r\nfunction decryptSignature(encryptedHash, pubKey) {\r\n  var tempStr = \"\";\r\n  for(var i = 0; i < encryptedHash.length; i += 4) {\r\n    var subStr = encryptedHash.substr(i, 4);\r\n    //console.log(subStr);\r\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\r\n    //console.log(\"toDec: \" + toDec);\r\n    var encrypted = (toDec * pubKey) % SHAREDPRIME;\r\n    //console.log(\"encrypted: \" + encrypted);\r\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\r\n    //console.log(\"toHex: \" + toHex);\r\n    \r\n    while(toHex.length < 4) {\r\n      toHex = \"0\" + toHex;\r\n    }\r\n    \r\n    tempStr += toHex;\r\n  }\r\n  return tempStr;\r\n}\r\n\r\nfunction messageFormat(from, to, amount) {\r\n  return \"<From: \" + from + \" | To: \" + to + \" | Amount: \" + amount + \">\";\r\n}\r\n\r\n//**SCREEN 3 EVENTS**\r\n\r\nvar decryptedHash = \"\";\r\n\r\nonEvent(\"verifyButton\", \"click\", function() {\r\n  if(currentRunning == \"none\") {\r\n    var hash = getText(\"hashTextBox\");\r\n    var sig = getText(\"signatureTextInput\");\r\n    var pubKey = getText(\"publicKeyTextInput\");\r\n    decryptedHash = decryptSignature(sig, pubKey);\r\n    setText(\"decryptedTextBox\", decryptedHash);\r\n    showElement(\"decryptedTextBox\");\r\n    showElement(\"decryptedMessageLabel\");\r\n    typewriterSignature();\r\n  }\r\n});\r\n\r\nfunction generateMessage() {\r\n  MESSAGE = MESSAGES[MESSAGEINDEX];\r\n  setText(\"messageTextBox\", MESSAGE[\"message\"]);\r\n  HASH = SHAHashLibrary.md5(MESSAGE[\"message\"]);\r\n  //setText(\"hashTextBox\", HASH);\r\n  if(currentRunning == \"none\") {\r\n    typewriterHash();  \r\n  }\r\n  setText(\"signatureTextInput\", MESSAGE[\"signature\"]);\r\n  showElement(\"hashLabel\");\r\n  showElement(\"hashTextBox\");\r\n  showElement(\"signatureLabel\");\r\n  showElement(\"signatureTextInput\");\r\n  showElement(\"publicLabel\");\r\n  showElement(\"publicKeyTextInput\");\r\n  showElement(\"verifyButton\");\r\n}\r\n\r\nvar TIMERINDEX = 0;\r\nvar currentRunning = \"none\"\r\nfunction typewriterHash() {\r\n  if(currentRunning == \"none\" || currentRunning == \"hash\") {\r\n    hideElement(\"verifyButton\");\r\n    currentRunning = \"hash\";\r\n    setText(\"hashTextBox\", HASH.substring(0, TIMERINDEX+1));\r\n    TIMERINDEX += 1;\r\n    if(TIMERINDEX < HASH.length) {\r\n      setTimeout(typewriterHash, 100);\r\n    } else {\r\n      TIMERINDEX = 0;\r\n      currentRunning = \"none\";\r\n      showElement(\"verifyButton\");\r\n    }  \r\n  }\r\n}\r\nfunction typewriterSignature() {\r\n  if(currentRunning == \"none\" || currentRunning == \"sig\") {\r\n    currentRunning = \"sig\";\r\n    setText(\"decryptedTextBox\", decryptedHash.substring(0, TIMERINDEX+1));\r\n    TIMERINDEX += 1;\r\n    if(TIMERINDEX < decryptedHash.length) {\r\n      setTimeout(typewriterSignature, 100);\r\n    } else {\r\n      TIMERINDEX = 0;\r\n      currentRunning = \"none\";\r\n      showElement(\"verifyLabel\");\r\n      if(HASH == decryptedHash) {\r\n        setText(\"verifyLabel\", \"SUCCESS - the original message hash and the decrypted signature match!\");\r\n        setProperty(\"verifyLabel\", \"background-color\", \"lightgreen\");\r\n        setProperty(\"verifyLabel\", \"text-color\", \"black\");\r\n      } else {\r\n        setText(\"verifyLabel\", \"ERROR - the original message hash and the decrypted signature do NOT match!\");\r\n        setProperty(\"verifyLabel\", \"background-color\", \"red\");\r\n        setProperty(\"verifyLabel\", \"text-color\", \"black\");\r\n      }\r\n    }  \r\n  }\r\n}\r\n\r\nfunction clearScreen() {\r\n  hideElement(\"hashLabel\");\r\n  hideElement(\"hashTextBox\");\r\n  hideElement(\"signatureLabel\");\r\n  hideElement(\"signatureTextInput\");\r\n  hideElement(\"publicLabel\");\r\n  hideElement(\"publicKeyTextInput\");\r\n  hideElement(\"verifyButton\");\r\n  hideElement(\"decryptedTextBox\");\r\n  hideElement(\"decryptedMessageLabel\");\r\n  hideElement(\"verifyLabel\");\r\n  setText(\"signatureTextInput\", \"\");\r\n  setText(\"publicKeyTextInput\", \"\");\r\n}\r\n\r\nonEvent(\"signatureTextInput\", \"input\", function() {\r\n  hideElement(\"decryptedTextBox\");\r\n  hideElement(\"decryptedMessageLabel\");\r\n  hideElement(\"verifyLabel\");\r\n});\r\n\r\nonEvent(\"publicKeyTextInput\", \"input\", function() {\r\n  hideElement(\"decryptedTextBox\");\r\n  hideElement(\"decryptedMessageLabel\");\r\n  hideElement(\"verifyLabel\");\r\n});\r\n\r\nonEvent(\"leftButton\", \"click\", function( ) {\r\n\thideElement(\"decryptedTextBox\");\r\n  hideElement(\"decryptedMessageLabel\");\r\n  hideElement(\"verifyLabel\");\r\n  setText(\"publicKeyTextInput\", \"\");\r\n\tMESSAGEINDEX -= 1;\r\n\tif(MESSAGEINDEX < 0) {\r\n\t  MESSAGEINDEX = MESSAGES.length - 1;\r\n\t}\r\n\tgenerateMessage();\r\n});\r\nonEvent(\"rightButton\", \"click\", function( ) {\r\n  hideElement(\"decryptedTextBox\");\r\n  hideElement(\"decryptedMessageLabel\");\r\n  hideElement(\"verifyLabel\");\r\n  setText(\"publicKeyTextInput\", \"\");\r\n\tMESSAGEINDEX += 1;\r\n\tif(MESSAGEINDEX >= MESSAGES.length) {\r\n\t  MESSAGEINDEX = 0;\r\n\t}\r\n\tgenerateMessage();\r\n});\r\n\r\ngenerateMessage();\r\n",
