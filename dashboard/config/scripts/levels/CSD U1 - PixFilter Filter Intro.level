    "start_blocks": "var ICONURL = \"//curriculum.code.org/images/icons/\";\r\nvar colors = [\"red\", \"green\", \"blue\"];\r\ncreateCanvas(\"diagram\", 320, 450);\r\nvar diagram = {\r\n                \"in\": 202,\r\n                \"out\": 280,\r\n                \"red\": 34,\r\n                \"green\": 100,\r\n                \"blue\": 164,\r\n                \"red_clicked\": false,\r\n                \"green_clicked\": false,\r\n                \"blue_clicked\": false,\r\n                \"red_hex\": \"#ff0000\",\r\n                \"green_hex\": \"#00ff00\",\r\n                \"blue_hex\": \"#0000ff\",\r\n                \"black_hex\": \"#000000\"\r\n              };\r\nvar bin_in = {\"red\":\"0\", \"green\": \"0\", \"blue\": \"0\"};\r\nvar bin_out = {\"red\":\"0\", \"green\": \"0\", \"blue\": \"0\"};\r\nsetStyle(\"bin_in\", \"font-family:monospace\");\r\nsetStyle(\"bin_out\", \"font-family:monospace\");\r\nsetProperty(\"green_source\", \"options\", colors);\r\nsetProperty(\"blue_source\", \"options\", colors);\r\nsetText(\"green_source\", \"green\");\r\nsetText(\"blue_source\", \"blue\");\r\n\r\n\r\ndrawDiagram();\r\n\r\nfunction drawDiagram() {\r\n  clearCanvas();\r\n  bin_in = {\"red\":\"0\", \"green\": \"0\", \"blue\": \"0\"};\r\n  bin_out = {\"red\":\"0\", \"green\": \"0\", \"blue\": \"0\"};\r\n  for (var i=0; i<colors.length; i++) {\r\n    var out_color = colors[i];\r\n    var in_color = getText(colors[i] + \"_source\");\r\n    if (diagram[in_color + \"_clicked\"] || in_color == 1) {\r\n      setStrokeColor(\"#000\");\r\n      setImageURL(out_color + \"_out\", ICONURL + out_color + \".png\");\r\n      bin_out[out_color] = \"1\";\r\n    } else {\r\n      setStrokeColor(\"#CCC\");\r\n      setImageURL(out_color + \"_out\", ICONURL + \"black.png\");\r\n    }\r\n    if (diagram[out_color + \"_clicked\"]) bin_in[out_color] = \"1\";\r\n    line(diagram[in_color], diagram.in,\r\n       diagram[out_color], diagram.out);\r\n  }\r\n  setPixel(\"pixel_in\", bin_in);\r\n  setPixel(\"pixel_out\", bin_out);\r\n  setText(\"bin_in\", bin_in.red + bin_in.green + bin_in.blue);\r\n  setText(\"bin_out\", bin_out.red + bin_out.green + bin_out.blue);\r\n}\r\n\r\nfunction setPixel(pixel, source) {\r\n  setProperty(pixel, \"background-color\", \"rgb(\" + (source.red * 255) +\r\n                  \",\" + (source.green * 255) +\r\n                  \",\" + (source.blue * 255) + \")\");\r\n}\r\n\r\nonEvent(\"red_in\", \"click\", function(event) {\r\n  diagram.red_clicked = !diagram.red_clicked;\r\n  if (diagram.red_clicked) {\r\n    setImageURL(\"red_in\", ICONURL + \"red.png\");\r\n  } else {\r\n    setImageURL(\"red_in\", ICONURL + \"black.png\");\r\n  }\r\n  drawDiagram();\r\n});\r\n\r\nonEvent(\"green_in\", \"click\", function(event) {\r\n  diagram.green_clicked = !diagram.green_clicked;\r\n  if (diagram.green_clicked) {\r\n    setImageURL(\"green_in\", ICONURL + \"green.png\");\r\n  } else {\r\n    setImageURL(\"green_in\", ICONURL + \"black.png\");\r\n  }\r\n  drawDiagram();\r\n});\r\n\r\nonEvent(\"blue_in\", \"click\", function(event) {\r\n  diagram.blue_clicked = !diagram.blue_clicked;\r\n  if (diagram.blue_clicked) {\r\n    setImageURL(\"blue_in\", ICONURL + \"blue.png\");\r\n  } else {\r\n    setImageURL(\"blue_in\", ICONURL + \"black.png\");\r\n  }\r\n  drawDiagram();\r\n});\r\n\r\nonEvent(\"red_source\", \"change\", drawDiagram);\r\nonEvent(\"green_source\", \"change\", drawDiagram);\r\nonEvent(\"blue_source\", \"change\", drawDiagram);\r\n",
