              </value>
              <value name="TARGET">
                <block type="gamelab_locationOf">
                  <value name="SPRITE">
                    <block type="gamelab_allSpritesWithAnimation">
                      <title name="ANIMATION">"carrot"</title>
                    </block>
                  </value>
                </block>
              </value>
            </block>
          </statement>
        </block>
