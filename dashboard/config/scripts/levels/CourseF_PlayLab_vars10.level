  <blocks>
    <start_blocks>
      <xml>
        <block type="when_run">
          <next>
            <block type="studio_showTitleScreen">
              <title name="TITLE">Food Fight</title>
              <title name="TEXT">Press right arrow to throw pie!</title>
              <next>
                <block type="variables_set" inline="false">
                  <title name="VAR">points</title>
                  <value name="VALUE">
                    <block type="math_number">
                      <title name="NUM">0</title>
                    </block>
                  </value>
                </block>
              </next>
            </block>
          </next>
        </block>
        <block type="studio_whenSpriteCollided">
          <title name="SPRITE1">0</title>
          <title name="SPRITE2">sandwich</title>
          <next>
            <block type="studio_playSound">
              <title name="SOUND">hit</title>
            </block>
          </next>
        </block>
        <block type="studio_whenSpriteCollided">
          <title name="SPRITE1">1</title>
          <title name="SPRITE2">pie</title>
          <next>
            <block type="studio_playSound">
              <title name="SOUND">hit</title>
            </block>
          </next>
        </block>
        <block type="studio_whenArrow">
          <title name="VALUE">up</title>
          <next>
            <block type="studio_move">
              <title name="SPRITE">0</title>
              <title name="DIR">1</title>
            </block>
          </next>
        </block>
        <block type="studio_whenArrow">
          <title name="VALUE">down</title>
          <next>
            <block type="studio_move">
              <title name="SPRITE">0</title>
              <title name="DIR">4</title>
            </block>
          </next>
        </block>
        <block type="studio_whenArrow">
          <title name="VALUE">right</title>
          <next>
            <block type="studio_playSound">
              <title name="SOUND">wood</title>
              <next>
                <block type="studio_throw">
                  <title name="SPRITE">0</title>
                  <title name="VALUE">"pie"</title>
                  <title name="DIR">2</title>
                </block>
              </next>
            </block>
          </next>
        </block>
        <block type="studio_repeatForever">
          <statement name="DO">
            <block type="studio_setScoreText" inline="true">
              <value name="TEXT">
                <block type="variables_get">
                  <title name="VAR">points</title>
                </block>
              </value>
            </block>
          </statement>
        </block>
        <block type="studio_repeatForever">
          <statement name="DO">
            <block type="studio_playSound">
              <title name="SOUND">slap</title>
              <next>
                <block type="studio_throw">
                  <title name="SPRITE">1</title>
                  <title name="VALUE">"sandwich"</title>
                  <title name="DIR">8</title>
                  <next>
                    <block type="studio_waitParams" inline="true">
                      <value name="VALUE">
                        <block type="math_number">
                          <title name="NUM">1</title>
                        </block>
                      </value>
                    </block>
                  </next>
                </block>
              </next>
            </block>
          </statement>
        </block>
        <block type="studio_repeatForever">
          <statement name="DO">
            <block type="studio_moveDistanceParams" inline="true">
              <title name="SPRITE">1</title>
              <title name="DIR">1</title>
              <value name="DISTANCE">
                <block type="math_random_int" inline="true">
                  <value name="FROM">
                    <block type="math_number">
                      <title name="NUM">50</title>
                    </block>
                  </value>
                  <value name="TO">
                    <block type="math_number">
                      <title name="NUM">400</title>
                    </block>
                  </value>
                </block>
              </value>
              <next>
                <block type="studio_moveDistanceParams" inline="true">
                  <title name="SPRITE">1</title>
                  <title name="DIR">4</title>
                  <value name="DISTANCE">
                    <block type="math_random_int" inline="true">
                      <value name="FROM">
                        <block type="math_number">
                          <title name="NUM">50</title>
                        </block>
                      </value>
                      <value name="TO">
                        <block type="math_number">
                          <title name="NUM">400</title>
                        </block>
                      </value>
                    </block>
                  </value>
                </block>
              </next>
            </block>
          </statement>
        </block>
        <block type="studio_repeatForever" uservisible="false">
          <statement name="DO">
            <block type="studio_setScoreText" inline="true" uservisible="false">
              <value name="TEXT">
                <block type="variables_get" uservisible="false">
                  <title name="VAR">points</title>
                </block>
              </value>
            </block>
          </statement>
        </block>
