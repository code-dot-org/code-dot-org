      "PainterPlus.java": {
        "text": "import org.code.neighborhood.Painter;\n\npublic class PainterPlus extends Painter {\n  public PainterPlus(int x, int y, String direction, int paint) {\n    super(x, y, direction, paint);\n  }\n\n  public void turnRight() {\n    turnLeft();\n    turnLeft();\n    turnLeft();\n  }\n\n  public void turnAround() {\n    turnLeft();\n    turnLeft();\n  }\n\n  public void moveFast() {\n    move();\n    move();\n  }\n\n  public void paintLineSegment(String color) {\n    paint(color);\n    move();\n    paint(color);\n    move();\n    paint(color);\n  }\n\n  public void paintFullLine(String color) {\n    while (canMove()) {\n      paint(color);\n      move();\n    }\n    paint(color);\n  }\n\n  public void paintDiagonalSegment(String color) {\n    paint(color);\n    if (canMove()) {\n      move();\n    }\n    turnLeft();\n    if (canMove()) {\n      move();\n    }\n    paint(color);\n    turnRight();\n  }\n\n  public void paintFullDiagonal(String color) {\n    while (canMove()) {\n      paintDiagonalSegment(color);\n    }\n  }\n}",
