      "MyFoodTruck.java": {
        "text": "import java.util.Scanner;\n\npublic class MyFoodTruck {\n  public static void main(String[] args) {\n\tDessert cake = new Dessert(\"chocolate\", 5, 1.99);\n    cake.printInfo();\n\n    // Level 1\n    Dessert redVelvetPack = new Dessert();\n    redVelvetPack.printInfo();\n\n    // Level 3\n    Dessert customDozen = new Dessert(\"vanilla\", 2.50);\n    customDozen.printInfo();\n\n    // Level 4\n    String flavor;\n    int quantity;\n    Scanner input = new Scanner(System.in);\n\n    System.out.print(\"Enter the flavor: \");\n    flavor = input.next();\n\n    System.out.print(\"Enter the quantity available: \");\n    quantity = input.nextInt();\n    \n    Dessert cupcake = new Dessert(flavor, quantity);\n    \n    cupcake.printInfo();\n  }\n}",
        "isVisible": true
      },
      "Dessert.java": {
        "text": "public class Dessert {\n  private String flavor;\n  private int quantity;\n  private double price;\n\n  public Dessert(String newFlavor, int newQty, double newPrice) {\n    flavor = newFlavor;\n    quantity = newQty;\n    price = newPrice;\n  }\n\n  // Level 1\n  public Dessert() {\n    flavor = \"red velvet\";\n    quantity = 6;\n    price = newPrice;\n  }\n\n  // Level 3\n  public Dessert(String newFlavor, int newQty) {\n    flavor = newFlavor;\n    quantity = newQty;\n    price = 1.50;\n  }\n\n  public void printInfo() {\n    System.out.println(\"Flavor = \" + flavor + \" Quantity = \" + quantity + \" Price = \" + price);\n  }\n}",
        "isVisible": true
      }
