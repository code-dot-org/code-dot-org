    "start_html": "<div id=\"designModeViz\" class=\"appModern withCrosshair clip-content\" data-radium=\"true\" style=\"width: 320px; height: 450px; display: none;\"><div class=\"screen\" tabindex=\"1\" id=\"screen1\" style=\"display: block; height: 450px; width: 320px; left: 0px; top: 0px; position: absolute; z-index: 0;\"><button id=\"c4\" style=\"padding: 0px; margin: 0px; height: 150px; font-size: 26px; color: rgb(255, 255, 255); background-color: rgb(135, 181, 174); position: absolute; left: 15px; top: 100px; width: 30px;\">C</button><button id=\"d4\" style=\"padding: 0px; margin: 0px; height: 150px; font-size: 26px; color: rgb(255, 255, 255); background-color: rgb(135, 181, 174); position: absolute; left: 55px; width: 30px; top: 100px;\">D</button><button id=\"e4\" style=\"padding: 0px; margin: 0px; height: 150px; font-size: 26px; color: rgb(255, 255, 255); background-color: rgb(135, 181, 174); position: absolute; width: 30px; top: 100px; left: 95px;\">E</button><button id=\"f4\" style=\"padding: 0px; margin: 0px; height: 150px; font-size: 26px; color: rgb(255, 255, 255); background-color: rgb(135, 181, 174); position: absolute; left: 125px; top: 100px; width: 30px;\">F</button><button id=\"g4\" style=\"padding: 0px; margin: 0px; height: 150px; font-size: 26px; color: rgb(255, 255, 255); background-color: rgb(135, 181, 174); position: absolute; width: 30px; top: 100px; left: 163px;\">G</button><button id=\"a4\" style=\"padding: 0px; margin: 0px; height: 150px; font-size: 26px; color: rgb(255, 255, 255); background-color: rgb(135, 181, 174); position: absolute; width: 30px; left: 205px; top: 100px;\">A</button><button id=\"b4\" style=\"padding: 0px; margin: 0px; height: 150px; font-size: 26px; color: rgb(255, 255, 255); background-color: rgb(135, 181, 174); position: absolute; width: 30px; left: 245px; top: 100px;\">B</button><button id=\"c5\" style=\"padding: 0px; margin: 0px; height: 150px; font-size: 26px; color: rgb(255, 255, 255); background-color: rgb(13