,false);\n    setText(\"level_label\", \"Level 3: Rock-a-Walk-a-Rainbow EVOLUTION COMPLETE\");\n  } else if (clicks > 49){\n    setProperty(\"rock1\",\"hidden\",false);\n    setProperty(\"rock2\",\"hidden\",false);\n    setProperty(\"rock3\",\"hidden\",true);\n    setText(\"level_label\", \"Level 2: Rock-a-bunny\");\n  } else {\n    setProperty(\"rock1\",\"hidden\",false);\n    setProperty(\"rock2\",\"hidden\",true);\n    setProperty(\"rock3\",\"hidden\",true);\n    setText(\"level_label\", \"Level 1: Rock-a-bye\");\n  }\n  \n  setText(\"clicks_number_label\", clicks);\n});\n\nonEvent(\"plus_twenty\", \"click\", function() {\n  if(clicks > 99){\n    clicks = clicks - 100;\n    setText(\"upgrade\", \"+20\");\n    hideElement(\"plus_two\");\n    hideElement(\"plus_twenty\");\n    amount = 20;\n  }\n  \n  if(clicks > 499){\n    setProperty(\"rock1\",\"hidden\",false);\n    setProperty(\"rock2\",\"hidden\",true);\n    setProperty(\"rock3\",\"hidden\",false);\n    setText(\"level_label\", \"Level 3: Rock-a-Walk-a-Rainbow EVOLUTION COMPLETE\");\n  } else if (clicks > 49){\n    setProperty(\"rock1\",\"hidden\",false);\n    setProperty(\"rock2\",\"hidden\",false);\n    setProperty(\"rock3\",\"hidden\",true);\n    setText(\"level_label\", \"Level 2: Rock-a-bunny\");\n  } else {\n    setProperty(\"rock1\",\"hidden\",false);\n    setProperty(\"rock2\",\"hidden\",true);\n    setProperty(\"rock3\",\"hidden\",true);\n    setText(\"level_label\", \"Level 1: Rock-a-bye\");\n  }\n  \n  setText(\"clicks_number_label\", clicks);\n});",
