    "validation_code": "var validationInfo = getValidationInfo();\r\nvar spriteIds = getSpriteIdsInUse();\r\nvar hasLineEvent = validationInfo.lineEvents.length > 0;\r\n\r\nif (!validationProps.previous) {\r\n  validationProps.previous = {\r\n    usedEvent: false\r\n  };\r\n}\r\n\r\n// Keep track of sprites' current sizes\r\nif(!validationProps.spritesSizes) {\r\n  validationProps.spritesSizes = {};\r\n}\r\n\r\nfor (var spriteId in spriteIds) {\r\n  var spriteSize = getProp({id: spriteId}, \"scale\");\r\n  if (World.frameCount == 1) {\r\n    if (spriteSize !== 100) {\r\n      setSuccessFrame();\r\n    }\r\n    validationProps.spritesSizes[spriteId] = spriteSize;\r\n  } else {\r\n    if (spriteSize !== validationProps.spritesSizes[spriteId] && hasLineEvent) {\r\n      validationProps.previous.usedEvent = true;\r\n      setSuccessFrame();\r\n      break;\r\n    }\r\n  }\r\n}\r\n\r\n\r\ndrawProgressBar();\r\nif (World.frameCount == validationInfo.endTime) {\r\n  if (validationInfo.successFrame) {\r\n    if (validationProps.previous.usedEvent) {\r\n      levelFailure(0, 'genericBonusSuccess');\r\n    } else {\r\n      levelFailure(0, 'poemBotSuccessLevel5');\r\n    }\r\n  } else if (spriteIds.length == 0) {\r\n    levelFailure(3, 'poemBotRemovedSprite');\r\n  } else {\r\n    levelFailure(3, 'poemBotSetSize');\r\n  }\r\n}",
