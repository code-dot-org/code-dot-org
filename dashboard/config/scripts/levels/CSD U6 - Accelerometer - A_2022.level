    "start_libraries": "[{\"name\":\"PioneerApp\",\"description\":\"Library for Pioneers of Electronics Accessibility app\",\"functions\":[\"isClicked\"],\"dropletConfig\":[{\"func\":\"PioneerApp.isClicked\",\"category\":\"Functions\",\"comment\":\"Checks if the cursor is over the element on the screen\",\"type\":\"either\",\"params\":[\"e\"],\"paletteParams\":[\"e\"]}],\"source\":\"//Checks if the cursor is over the element on the screen\\nfunction isClicked(e) {\\n  var pX = getXPosition(\\\"pointer\\\");\\n  var pY = getYPosition(\\\"pointer\\\");\\n  var eX = getXPosition(e);\\n  var eY = getYPosition(e);\\n  var bound = 70;\\n  if (pX < eX + bound && pX + bound > eX && pY < eY + bound && pY + bound > eY) {\\n    return true;\\n  }\\n}\\n\",\"originalName\":\"PioneerApp\",\"versionId\":\"KPvwoi2rbnANtEJiCmBZD.yZ0BXGi3Vv\",\"channelId\":\"F0TjjgNMs_CxxpcLULhjPQ\",\"fromLevelbuilder\":true}]",
