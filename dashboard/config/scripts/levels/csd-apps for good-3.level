    "long_instructions": "## Four Square\r\n\r\nWhat social issue do you think this app was designed to address?\r\n\r\nHow does it accomplish this?",
    "mini_rubric": "false",
    "start_blocks": "onEvent(\"playersButton\", \"click\", function() {\r\n  setScreen(\"playersScreen\");\r\n});\r\nonEvent(\"instructionsButton\", \"click\", function() {\r\n  setScreen(\"instructionsScreen\");\r\n});\r\nonEvent(\"startAgainButton\", \"click\", function() {\r\n  setScreen(\"main\");\r\n});\r\nonEvent(\"onePlayerButton\", \"click\", function() {\r\n  setText(\"answerLabel\", \"Not enough\");\r\n  playSound(\"sound://category_digital/error_1.mp3\", false);\r\n});\r\nonEvent(\"twoPlayerButton\", \"click\", function() {\r\n  setText(\"answerLabel\", \"Not enough\");\r\n  playSound(\"sound://category_digital/error_2.mp3\", false);\r\n});\r\nonEvent(\"threePlayerButton\", \"click\", function() {\r\n  setText(\"answerLabel\", \"Not enough\");\r\n  playSound(\"sound://category_digital/error_3.mp3\", false);\r\n});\r\nonEvent(\"fourPlayerButton\", \"click\", function() {\r\n  setText(\"answerLabel\", \"Right! You need four players.\");\r\n  playSound(\"sound://category_digital/win.mp3\", false);\r\n});\r\n",
