      "MyFoodTruck.java": {
        "text": "public class MyFoodTruck {\n  public static void main(String[] args) {\n    Dessert donut = new Dessert(\"donut\", 30, 1.0);\n    donut.printInfo();\n  }\n}",
        "isVisible": true
      },
      "Dessert.java": {
        "text": "public class Dessert {\n  private String flavor;\n  private int quantity;\n  private double price;\n\n  public Dessert(String newFlavor, int newQty, double newPrice) {\n    boolean discount = (newQty * newPrice >= 25);\n\n    flavor = newFlavor;\n    quantity = newQty;\n    price = newPrice;\n\n    if (discount) {\n      price = price * 0.9;\n    }\n  }\n  \n  public Dessert() {\n    flavor = \"mint chocolate\";\n    quantity = 5;\n    price = 2.75;\n  }\n\n  public Dessert(int quantity) {\n    boolean isValid = (quantity >= 0);\n\n    if (!isValid) {\n      this.quantity = 1;\n    }\n    else {\n      this.quantity = quantity;\n    }\n\n    flavor = \"coconut\";\n    price = 1.50;\n  }\n\n  public void printInfo() {\n    System.out.println(\"Flavor = \" + flavor + \" Quantity = \" + quantity + \" Price = \" + price);\n  }\n}",
        "isVisible": true
      }
