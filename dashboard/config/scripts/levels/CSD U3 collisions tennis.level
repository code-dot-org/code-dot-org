    "start_animations": "{\r\n    \"orderedKeys\": [\r\n        \"racket\",\r\n        \"ball\"\r\n    ],\r\n    \"propsByKey\": {\r\n        \"racket\": {\r\n            \"name\": \"racket\",\r\n            \"sourceUrl\": \"https://images.code.org/8d8cf039f50114c07a21e73bbc1c8dfe-image-1470418244037.png\",\r\n            \"frameSize\": {\r\n                \"x\": 17,\r\n                \"y\": 75\r\n            },\r\n            \"frameCount\": 1,\r\n            \"frameDelay\": 2,\r\n            \"looping\": true\r\n        },\r\n        \"ball\": {\r\n            \"name\": \"ball\",\r\n            \"sourceUrl\": \"https://images.code.org/d05be653ae7e4717353dffd08af9c656-image-1470418244039.png\",\r\n            \"frameSize\": {\r\n                \"x\": 30,\r\n                \"y\": 30\r\n            },\r\n            \"frameCount\": 1,\r\n            \"frameDelay\": 2,\r\n            \"looping\": true\r\n        }\r\n    }\r\n}",
    "start_blocks": "var racket = createSprite(390, 200);\r\nracket.setAnimation(\"racket\");\r\n\r\nvar ball = createSprite(200,200);\r\nball.setAnimation(\"ball\");\r\nball.setSpeedAndDirection(4, randomNumber(110, 250));\r\n\r\nfunction draw() {\r\n  // reset ball to middle of screen if it goes off screen\r\n  if (ball.x > 500) {\r\n    ball.x = 200;\r\n    ball.y = 200;\r\n    ball.setSpeedAndDirection(4, randomNumber(110, 250));\r\n  }\r\n  // bounce ball off top wall\r\n  if (ball.y < 15) {\r\n    ball.velocityY *= -1;\r\n  }\r\n  // bounce ball off left wall\r\n    if (ball.x < 15) {\r\n    ball.velocityX *= -1;\r\n  }\r\n  // bounce ball off bottom wall\r\n  if (ball.y > 385) {\r\n    ball.velocityY *= -1;\r\n  }\r\n  //keep racket from going off screen\r\n  if (racket.y < 37) {\r\n    racket.y = 37;\r\n  }\r\n  if (racket.y > 363) {\r\n    racket.y = 363;\r\n  }\r\n  background(\"green\");\r\n  // move racket according to keyboard commands\r\n  if (keyDown(\"up\")) {\r\n    racket.y -= 3;\r\n  }\r\n  if (keyDown(\"down\")) {\r\n    racket.y += 3;\r\n  }\r\n  drawSprites();\r\n}\r\n",
