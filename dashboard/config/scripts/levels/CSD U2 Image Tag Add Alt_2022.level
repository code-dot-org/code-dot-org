    "start_sources": "{\r\n  \"files\": [\r\n    {\r\n      \"name\": \"index.html\",\r\n      \"data\": \"<!DOCTYPE html>\\r\\n<html>\\r\\n  <head>\\r\\n  <\\/head>\\r\\n  <body>\\r\\n    <h1>My Travels<\\/h1>\\r\\n    <h2>Kyoto, Japan<\\/h2>\\r\\n      <img src=\\\"Lanterns.jpg\\\">\\r\\n    <p>Photo by Nicholas Dench from FreeImages<\\/p>\\r\\n    <br>\\r\\n    <h2>Paris, France<\\/h2>\\r\\n      <img src=\\\"Eiffel.jpg\\\">\\r\\n    <p>Photo by Benjamin Thorn from FreeImages<\\/p>\\r\\n    <br>\\r\\n    <h2>Rio de Janeiro, Brazil<\\/h2>\\r\\n      <img src=\\\"Brazil.jpg\\\">\\r\\n    <p>Photo by Joe Carey from FreeImages<\\/p>\\r\\n  <\\/body>\\r\\n<\\/html>\"\r\n    },\r\n{\r\n      \"name\": \"Brazil.jpg\",\r\n      \"url\": \"https://images.code.org/4020e82b4090e00b008c46b5cc4ffabe-image-1564422752477.jpg\"\r\n    },\r\n{\r\n      \"name\": \"Lanterns.jpg\",\r\n      \"url\": \"https://images.code.org/6f226811128f1387bdd7360c3eb868b4-image-1564422768687.jpg\"\r\n    },\r\n{\r\n      \"name\": \"Eiffel.jpg\",\r\n      \"url\": \"https://images.code.org/616cf53cc191c3e8112755b3e726cc85-image-1564422791222.jpg\"\r\n    }\r\n  ]\r\n}",
