      "PopBalloonsGame.java": {
        "text": "import org.code.media.*;\nimport org.code.playground.*;\n\npublic final class PopBalloonsGame {\n\n  private static Dice diceLeft;\n  private static Dice diceRight;\n  private static Balloon[] balloons;\n  private static Image[] popped;\n  private static RollButton rollButton;\n  \n  private static int score = 0;\n  private static int remaining = 0;\n\n  public static void play() {\n    createDice();\n    createRollButton(\"rollButton.png\");\n    \n    createBalloons();\t// add in Lesson 5\n    createPopped();\t\t// add in Lesson 5\n    \n    showIntro();\n\n    Playground.run();\n  }\n\n  // STARTER CODE\n  public static void showIntro() {\n    System.out.println(\"Pop the Balloons!\");\n    System.out.println(\"---------------------\");\n    System.out.println(\"This game involves balloons that are numbered 1 through 10.\");\n    System.out.println(\"Your goal is to pop all of them!\");\n    System.out.println(\"You may pop any combination of unpopped ballons to add up to the sum on the dice.\\n\");\n  }\n\n  // STARTER CODE\n  public static void showMessage(String text, Color textColor) {\n    Playground.drawText(text, 5, 10, textColor, \"Arial\", 10, 0);   // text, x, y, color, font, height, rotation\n  }\n\n  // STARTER CODE\n  public static void createDice() {\n    String[] diceLeftFiles = {\"dice1.png\", \"dice2.png\", \"dice3.png\", \"dice4.png\", \"dice5.png\", \"dice6.png\"};\n    diceLeft = new Dice(diceLeftFiles, 100, 100);\n\n    String[] diceRightFiles = {\"dice1.png\", \"dice2.png\", \"dice3.png\", \"dice4.png\", \"dice5.png\", \"dice6.png\"};\n    diceRight = new Dice(diceRightFiles, 200, 100);\n    \n    diceLeft.displayDice(1);\n    diceRight.displayDice(1);\n  }\n\n  // STARTER CODE\n  public static void createRollButton(String filename) {\n    rollButton = new RollButton(filename);\n    Playground.addClickable(rollButton, 300, 300, 100, 50, 0); // image, x, y, width, height, rotation\n  }\n\n  // STARTER CODE\n  public static void createBalloons() {\n    balloons = new Balloon[6];\n    int balloonValue = 1;\n    int xPos = 20;\n    int yPos = 20;\n\n    for (int i = 0; i < balloons.length; i++) {\n      String balloonFile = \"balloon\" + balloonValue + \".png\";\n      balloons[i] = new Balloon(balloonFile, balloonValue, xPos, yPos);\n      Playground.addClickable(balloons[i], xPos, yPos, 100, 100, 0);  // image, x, y, width, height, rotation\n\n      xPos += 40;\n      if (i == 4) {\n        yPos += 40;\n        xPos = 20;\n      }\n      \n      balloonValue++;\n    }\n  }\n\n  // STARTER CODE\n  public static void createPopped() {\n    popped = new Image[6];\n    \n    for (int i = 0; i < popped.length; i++) {\n      if (i == 0 || i == 5) {\n        popped[i] = new Image(\"poppedTeal.png\");\n      }\n      else if (i == 1 || i == 6) {\n        popped[i] = new Image(\"poppedPurple.png\");\n      }\n      else if (i == 2 || i == 7) {\n        popped[i] = new Image(\"poppedOrange.png\");\n      }\n      else if (i == 3 || i == 8) {\n        popped[i] = new Image(\"poppedGreen.png\");\n      }\n      else {\n        popped[i] = new Image(\"poppedBlue.png\");\n      }\n    }\n  }\n\n  // LESSON 4\n  public void updateDice(int value, int diceRoll) {\n\tif (value == 1) {\n      Playground.drawImage(diceLeft.displayDice(diceRoll), diceLeft.getX(), diceLeft.getY, 50, 50, 0);\n    }\n    \n    if (value == 2) {\n      Playground.drawImage(diceRight.displayDice(diceRoll), diceRight.getX(), diceRight.getY, 50, 50, 0);\n    }\n  }\n\n  // LESSON 5\n  public static void calculateScore() {\n    score = 0;\n    remaining = 0;\n\n    Color[] textColors = {Color.RED, Color.PURPLE, Color.ORANGE, Color.BLACK};\n    int randomIndex = (int)(Math.random() * textColors.length);\n\n    for (int i = 0; i < balloons.length; i++) {\n      if (!balloons[i].isPopped()) {\n        score += balloons[i].getValue();\n        remaining++;\n      }\n    }\n\n    showMessage(\"Your score is now \" + score + \".\", textColors[randomIndex]);\n  }\n\n  // LESSON 5\n  public static void popBalloon(Balloon balloon, int value) {\n    String[] popSounds = {\"pop1.wav\", \"pop2.wav\", \"pop3.wav\", \"pop4.wav\", \"pop5.wav\"};\n\tint randomIndex = (int)(Math.random() * popSounds.length);\n    \n    for (int i = 0; i < popped.length; i++) {\n      if (value - 1 == i) {\n        Playground.playSound(popSounds[randomIndex]);\n        balloon.setImage(popped[i]);\n      }\n    }\n  }\n\n  // LESSON 6 and 9\n  public static boolean movesLeft() {\n    boolean canContinue = false;\n    int currentTotalRoll = rollButton.getTotalRoll();\n\n    for (int i = 0; i < balloons.length; i++) {\n      Balloon currentBalloon = balloons[i];\n      \n      for (int j = 0; j < balloons.length; j++) {\n        Balloon nextBalloon = balloons[j];\n\n        if (currentBalloon.equals(nextBalloon)) {\n          // In Lesson 6, just have canContinue = true.\n\n          // Add in Lesson 9\n          int totalBalloonValue = currentBalloon.getValue() + nextBalloon.getValue()\n          if (currentBalloon.getValue() == currentTotalRoll || totalBalloonValue == currentTotalRoll) {\n            canContinue = true;\n          }\n        }\n      }\n    }\n\n    // Add in Lesson 8\n    if (!canContinue) {\n      gameOver();\n    }\n\n    return canContinue;\n  }\n\n  // LESSON 7\n  public static boolean isValidMove(int value) {\n    boolean isValid = false;\n\n    if (value > rollButton.getTotalRoll()) {\n      showMessage(\"That doesn't add up to \" + totalRoll + \".\", Color.BLACK);\n    }\n    else {\n      rollButton.setTotalRoll(rollButton.getTotalRoll() -= value);\n      isValid = true;\n    }\n\n    return isValid;\n  }\n\n  // LESSON 8\n  public static void gameOver() {\n    if (remaining == 0) {\n      Playground.playSound(\"win.wav\");\n      showMessage(\"You popped all of the balloons -- YOU WIN!\", Color.BLACK);\n    }\n    else if (score <= 2) {\n      Playground.playSound(\"gameOver.wav\");\n      showMessage(\"Game over! Nice try. Your score was \" + score + \".\");\n    }\n    else {\n      Playground.playSound(\"gameOver.wav\");\n      showMessage(\"Game over! Your score was \" + score + \".\");\n    }\n\n    Playground.stop();\n  }\n\n  // LESSON 9\n  public static boolean canRollBoth() {\n    boolean status = true;\n    \n    if (balloons[6].isPopped() && balloons[7].isPopped() && balloons[8].isPopped()) {\n      showMessage(\"Balloons 7, 8, and 9 are popped, so you can only roll once dice.\", Color.BLACK);\n      diceRight.hideDice();\n      status = false;\n    }\n\n    return status;\n  }\n}",
        "isVisible": true
      },
      "RollButton.java": {
        "text": "import org.code.media.*;\nimport org.code.playground.*;\n\n// LESSON 1\npublic class RollButton extends ClickableImage {\n\n  private int diceRollLeft;\n  private int diceRollRight;\n  private int totalRoll;\n  \n  public RollButton(String filename) {\n    super(filename);\n    this.diceRollLeft = 1;\n    this.diceRollRight = 1;\n    this.totalRoll = 0;\n  }\n\n  public int getDiceRollLeft() {\n    return diceRollLeft;\n  }\n\n  public int getDiceRollRight() {\n    return diceRollRight;\n  }\n\n  public int getTotalRoll() {\n    return totalRoll;\n  }\n\n  public void setTotalRoll(int newValue) {\n    totalRoll -= newValue;\n  }\n\n  public void onClick() {\n    Playground.playSound(\"diceRoll.wav\");\n\n    // In Lesson 1, have diceRollLeft and diceRollRight\n    // assigned specific values.\n    \n    diceRollLeft = (int)(Math.random() * 5 + 1);\n    PopBalloonsGame.updateDice(1, diceRollLeft);\n\n    \n    // Add in Lesson 9\n    if (PopBalloonsGame.canRollBoth()) {\n      diceRollRight = (int)(Math.random() * 5 + 1);\n      PopBalloonsGame.updateDice(2, diceRollRight);\n    }\n\n    totalRoll = diceRollLeft + diceRollRight;\n    PopBalloonsGame.calculateScore();\n\n    if (PopBalloonsGame.movesLeft()) {\n      PopBalloonsGame.showMessage(\"Pop balloons to total \" + totalRoll + \"!\", Color.BLACK);\n    }\n  }\n}",
        "isVisible": true
      },
      "Balloon.java": {
        "text": "import org.code.media.*;\nimport org.code.playground.*;\n\n// LESSON 6\npublic class Balloon extends ClickableImage {\n\n  private boolean popped;\n  private int value;\n  private int xPos;\n  private int yPos;\n\n  public Balloon(String filename, int value) {\n    super(filename);\n    this.value = value;\n    this.popped = false;\n  }\n\n  public int getValue() {\n    return value;\n  }\n\n  public boolean isPopped() {\n    return popped;\n  }\n\n  public int getX() {\n    return xPos;\n  }\n\n  public int getY() {\n    return yPos;\n  }\n\n  public boolean equals(Balloon otherBalloon) {\n    boolean status = false;\n    \n\tif (this.isPopped() == otherBalloon.isPopped()) {\n      status = true;\n    }\n\n    if (this.getValue() != otherBalloon.getValue()) {\n      status = false;\n    }\n\n    return status;\n  }\n\n  public void onClick() {\n    // Add check for valid move in Lesson 7\n    if (PopBalloonsGame.isValidMove(value)) {\n      this.popped = true;\n      PopBalloonsGame.popBalloon(this, value);\n      Playground.removeClickable(this);\n    }\n  }\n}",
        "isVisible": true
      },
      "Dice.java": {
        "text": "import org.code.media.*;\nimport org.code.playground.*;\n\n// LESSON 1\npublic class Dice extends Image {\n\n  private Image[] diceImages;\n  private int xPos;\n  private int yPos;\n\n  public Dice(String[] diceFiles, int xPos, int yPos) {\n    diceImages = new Image[6];\n    \n\tfor (int i = 0; i < diceFiles.length; i++) {\n      diceImages[i] = new Image(diceFiles[i]);\n    }\n\n    this.xPos = xPos;\n    this.yPos = yPos;\n  }\n\n  public void displayDice(int value) {\n    Playground.drawImage(diceImages[value - 1], xPos, yPos, 50, 50, 0);\n  }\n\n  public void hideDice() {\n    Playground.removeImage(this);\n  }\n}",
        "isVisible": true
      }
    },
