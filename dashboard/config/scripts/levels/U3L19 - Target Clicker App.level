    "start_blocks": "image(\"bkg\", \"http://www.newgeography.com/files/imagecache/Chart_Story_Inset/bigstock-Chicago-Skyline-162456.jpg\");\r\nsetPosition(\"bkg\", 0, 35, 320, 480);\r\nimage(\"sprite\", \"https://code.org/images/logo.png\");\r\nsetPosition(\"sprite\", 150, 200, 50, 50);\r\nvar clicks=0;\r\n\r\ntextLabel(\"clickLbl\", \"Clicks: 0\");\r\nsetPosition(\"clickLbl\", 0, 5);\r\nbutton(\"resetBtn\", \"Reset\");\r\nsetStyle(\"resetBtn\",\"background-color: #00ABDC; color: white; font-size: 12px; padding: 2px\");\r\nsetPosition('resetBtn', 240, 0, 70, 30);\r\n\r\n\r\nonEvent(\"sprite\", \"click\", function() {\r\n  clicks=clicks+1;\r\n  setPosition(\"sprite\", randomNumber(0, 300), randomNumber(0, 450), 50, 50);\r\n  updateClicksLabel();\r\n});\r\nonEvent(\"bkg\", \"click\", function() {\r\n  clicks=clicks-1;\r\n  updateClicksLabel();\r\n});\r\nonEvent(\"resetBtn\", \"click\", function() {\r\n  clicks=0;\r\n  updateClicksLabel();\r\n});\r\nfunction updateClicksLabel() {\r\n    setText(\"clickLbl\", \"Clicks: \"+clicks);\r\n}\r\n",
