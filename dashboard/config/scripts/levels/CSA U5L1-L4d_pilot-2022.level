      "CerealScene.java": {
        "text": "import org.code.theater.*;\n\n/*\n * Creates a scene to show the nutritional\n * information about cereals\n */\npublic class CerealScene extends Scene {\n\n  private int[][] nutrition;   // The 2D array of nutritional values\n\n  /*\n   * Constructor to create an CerealScene\n   * with the specified 2D array of nutritional values\n   */\n  public CerealScene(int[][] nutrition) {\n    this.nutrition = nutrition;\n  }\n\n  /*\n   * Creates a scene by drawing text with the grams\n   * of sugar value and a colored circle based on the amount\n   */\n  public void createScene(int sugarValue) {\n    drawText(\"Grams of Sugar: \" + sugarValue, 50, 50);\n\n    if (sugarValue > 12) {\n      setFillColor(\"red\");\n    }\n    else if (sugarValue > 5) {\n      setFillColor(\"orange\");\n    }\n    else {\n      setFillColor(\"green\");\n    }\n\n    pause(0.3);\n    drawEllipse(50, 100, 200, 200);\n  }\n  \n}",
        "isVisible": true
