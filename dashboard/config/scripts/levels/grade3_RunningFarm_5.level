                          </statement>
                        </block>
                      </statement>
                      <statement name="ELSE">
                        <block type="harvester_ifHasCropElse">
                          <title name="LOC">Pumpkin</title>
                          <statement name="DO">
                            <block type="harvester_whileHasCrop">
                              <title name="LOC">Pumpkin</title>
                              <statement name="DO">
