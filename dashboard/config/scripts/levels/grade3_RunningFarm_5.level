                                  <title name="LOC">Lettuce</title>
                                  <statement name="DO">
                                    <block type="harvester_lettuce"/>
                                  </statement>
                                </block>
                              </statement>
                            </block>
                          </statement>
                        </block>
                      </statement>
                    </block>
                  </next>
                </block>
              </statement>
            </block>
          </next>
        </block>
