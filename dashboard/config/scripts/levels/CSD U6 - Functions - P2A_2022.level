    "start_blocks": "soundSensor.threshold = 50;\r\nonBoardEvent(soundSensor, \"change\", function() {\r\n  moveCloud0();\r\n  loopCloud(\"cloud0\");\r\n  loopCloud(\"cloud1\");\r\n  loopCloud(\"cloud2\");\r\n  loopCloud(\"cloud3\");\r\n  loopCloud(\"cloud4\");\r\n});\r\nfunction loopCloud(name) {\r\n  if (getProperty(name, \"x\") < -100) {\r\n    setProperty(name, \"x\", 325);\r\n  }\r\n}\r\nfunction moveCloud0() {\r\n  var cloudX = getProperty(\"cloud0\", \"x\");\r\n  setProperty(\"cloud0\", \"x\", cloudX - randomNumber(4,12));\r\n}",
