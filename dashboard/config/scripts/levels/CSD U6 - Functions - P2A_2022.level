    "start_blocks": "soundSensor.threshold = 50;\r\n//Events\r\nonBoardEvent(soundSensor, \"change\", function() {\r\n  loopCloud(\"cloud0\");\r\n  loopCloud(\"cloud1\");\r\n  loopCloud(\"cloud2\");\r\n  loopCloud(\"cloud3\");\r\n  loopCloud(\"cloud4\");\r\n});\r\n//Functions\r\nfunction loopCloud(cloudID) {\r\n  //TODO: Update the function to use cloudID instead of \"cloud0\"\r\n  var cloudX = getProperty(\"cloud0\", \"x\");\r\n  setProperty(\"cloud0\", \"x\", cloudX - randomNumber(4,12));\r\n  if (getProperty(\"cloud0\", \"x\") < -100) {\r\n    setProperty(\"cloud0\", \"x\", 325);\r\n  }\r\n}\r\n",
