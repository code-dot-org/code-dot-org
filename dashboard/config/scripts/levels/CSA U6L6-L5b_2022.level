        "text": "import java.util.ArrayList;\n\npublic class MyConsole {\n  public static void main(String[] args) {\n\n    ArrayList<String> stopWords = FileReader.getStringData(\"stopwords.txt\");\n\n    TextProcessor myText = new TextProcessor(\"sentence.txt\");\n    System.out.println(myText);\n\n    // TO DO #2: Call the removeStopWords() method.\n\n\n    // TO DO #3: Print the updated TextProcessor object.\n\n    \n  }\n}",
