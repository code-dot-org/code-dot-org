    "start_blocks": "// define the variables\r\n\r\nvar likes = 0;\r\nvar comment = \"OMG soooo cute!\";\r\n\r\n\r\n// (purpose) When clicked decreases the likes by one\r\n// (function) Uses the Counter Pattern with Event to subtract one from the likes variable\r\n// sets the text on the screen to display the new amount of likes\r\n// plays a sound when clicked\r\nonEvent(\"downButton\", \"click\", function(event) {\r\n  likes = likes - 1;\r\n  setText(\"likeCounterOutput\",\"Likes: \" + likes);\r\n  playSound(\"sound://category_objects/sharp_switch.mp3\");\r\n});\r\n\r\n// (purpose) When clicked increases the likes by one\r\n// (function) Uses the Counter Pattern with Event to add one to the likes variable\r\n// sets the text on the screen to display the new amount of likes\r\n// plays a sound when clicked\r\nonEvent(\"upButton\", \"click\", function(event) {\r\n  likes = likes + 1;\r\n  setText(\"likeCounterOutput\", \"Likes: \" + likes);\r\n  playSound(\"sound://category_objects/sharp_switch.mp3\");\r\n});\r\n\r\n// (purpose) When clicked updates the comments on the screen\r\n// (function) Uses the Variable with String Concatenation pattern to add a new comment to previous comments\r\n// sets the text on the screen to display all comments\r\nonEvent(\"commentButton\", \"click\", function(event) {\r\n  comment = comment + \"\\n\" + getText(\"newCommentInput\");\r\n  setText(\"allCommentsOutput\", comment);\r\n});\r\n\r\n\r\n\r\n\r\n",
    "start_html": "<div id=\"designModeViz\" class=\"appModern clip-content\" tabindex=\"1\" data-radium=\"true\" style=\"width: 320px; height: 450px; display: block;\"><div class=\"screen\" tabindex=\"1\" id=\"homeScreen\" style=\"display: block; height: 450px; width: 320px; left: 0px; top: 0px; position: absolute; z-index: 0; background-color: rgb(254, 219, 208);\" data-theme=\"millennial\"><button id=\"downButton\" style=\"padding: 0px; margin: 0px; height: 30px; color: rgb(255, 255, 255); position: absolute; left: 10px; top: 320px; font-size: 40px; background-color: pink; background-size: contain; background-position: 50% 50%; background-repeat: no-repeat; border-style: solid; border-width: 0px; border-color: rgb(255, 255, 255); border-radius: 20px; font-family: Verdana, Geneva, sans-serif; width: 30px;\" data-canonical-image-url=\"icon://fa-thumbs-o-down\" data-icon-color=\"white\" data-mod-backgroundcolor=\"1\" data-mod-fontsize=\"1\"></button><label style=\"margin: 0px; padding: 2px 15px; line-height: 1; overflow: hidden; overflow-wrap: break-word; color: rgb(68, 44, 46); max-width: 320px; width: 320px; height: 40px; position: absolute; left: 0px; top: 0px; text-align: left; background-color: rgb(254, 234, 230); border-style: solid; border-width: 0px; border-color: rgb(255, 255, 255); border-radius: 4px; font-family: Arial, Helvetica, sans-serif; font-size: 35px;\" id=\"appName\">Photo Liker</label><button id=\"upButton\" style=\"padding: 0px; margin: 0px; color: rgb(255, 255, 255); position: absolute; left: 50px; top: 320px; font-size: 40px; background-color: lightgreen; background-size: contain; background-position: 50% 50%; background-repeat: no-repeat; height: 30px; border-style: solid; border-width: 0px; border-color: rgb(255, 255, 255); border-radius: 20px; font-family: Verdana, Geneva, sans-serif; width: 30px;\" data-canonical-image-url=\"icon://fa-thumbs-o-up\" data-icon-color=\"white\" data-mod-backgroundcolor=\"1\" data-mod-fontsize=\"1\"></button><img data-canonical-image-url=\"icon://fa-camera-retro\" data-object-fit=\"contain\" id=\"settingsIcon\" style=\"height: 40px; position: absolute; left: 280px; top: 0px; margin: 0px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; width: 40px;\" data-icon-color=\"black\"><img data-object-fit=\"cover\" id=\"catImage\" style=\"position: absolute; top: 50px; margin: 0px; left: 10px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); width: 300px; border-radius: 5px; height: 260px;\" data-canonical-image-url=\"https://images.code.org/af41e7d1f0a7c1990cc9db2d061b7f46-image-1567805031839.jpg\" src=\"//levelbuilder-studio.code.org/media?u=https%3A%2F%2Fimages.code.org%2Faf41e7d1f0a7c1990cc9db2d061b7f46-image-1567805031839.jpg\"><label style=\"margin: 0px; padding: 2px 15px; line-height: 1; overflow: hidden; overflow-wrap: break-word; color: rgb(68, 44, 46); max-width: 320px; width: 220px; height: 30px; position: absolute; left: 90px; top: 320px; text-align: center; background-color: rgb(254, 234, 230); border-style: solid; border-width: 0px; border-color: rgb(255, 255, 255); border-radius: 4px; font-family: Arial, Helvetica, sans-serif; font-size: 25px;\" id=\"likeCounterOutput\">Likes: 0</label><div contenteditable=\"false\" class=\"textArea\" id=\"allCommentsOutput\" style=\"width: 170px; color: rgb(68, 44, 46); background-color: rgb(254, 234, 230); position: absolute; left: 10px; top: 360px; margin: 0px; font-size: 12px; border-style: solid; border-width: 0px; border-color: rgb(255, 255, 255); border-radius: 8px; font-family: Arial, Helvetica, sans-serif; padding: 5px 15px; height: 80px;\">OMG soooo cute!</div><button id=\"commentButton\" style=\"padding: 0px; margin: 0px; font-size: 15px; color: rgb(255, 255, 255); background-color: lightblue; position: absolute; left: 190px; top: 410px; background-size: contain; background-position: 50% 50%; background-repeat: no-repeat; border-style: solid; border-width: 0px; border-color: rgb(255, 255, 255); border-radius: 20px; font-family: Verdana, Geneva, sans-serif; width: 120px; height: 30px;\" data-canonical-image-url=\"icon://fa-comment-o\" data-mod-backgroundcolor=\"1\"></button><input id=\"newCommentInput\" style=\"margin: 0px; width: 120px; color: rgb(68, 44, 46); position: absolute; left: 190px; top: 360px; font-size: 12px; border-style: solid; border-width: 2px; border-color: rgb(255, 255, 255); border-radius: 4px; font-family: Arial, Helvetica, sans-serif; padding: 5px 15px; background-color: rgb(254, 219, 208); height: 40px;\" placeholder=\"New Comment\"></div></div>",
