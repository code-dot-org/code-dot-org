    "authored_hints": "[{\"hint_class\":\"content\",\"hint_markdown\":\"Variables are helpful because you can add to them each time through a loop. \",\"hint_id\":\"courseF_artist_variables_challenge2_a\",\"hint_type\":\"general\",\"tts_url\":\"https://tts.code.org/sharon22k/180/100/a73bcd3a2f9b7e1f462cfcd91749b71b/course4_artist_variables_challenge2.mp3\"},{\"hint_class\":\"content\",\"hint_markdown\":\"This puzzle has one loop that creates the polygon, one loop that repeats the polygon `sides` times, then another loop that does it all again after adding 5 to `sides`.\",\"hint_id\":\"courseF_artist_variables_challenge2_b\",\"hint_type\":\"general\",\"tts_url\":\"https://tts.code.org/sharon22k/180/100/eee696cbae5f23af492a1ee0120e773b/course4_artist_variables_challenge2.mp3\"},{\"hint_class\":\"content\",\"hint_markdown\":\"Using a variable for `sides` and one for `length` makes the code cleaner because you can loop the same code multiple times, instead of creating a new copy for each set of numbers.\",\"hint_id\":\"courseF_artist_variables_challenge2_c\",\"hint_type\":\"general\",\"tts_url\":\"https://tts.code.org/sharon22k/180/100/e70e472dc3c9dc29746db06e08f142d6/course4_artist_variables_challenge2.mp3\"},{\"hint_class\":\"bottom-out\",\"hint_markdown\":\"**Solution:**\\n\\n![Solution](https://images.code.org/311b4a946cb8f3b4144b5075be6460a3-image-1469641878222.png)\",\"hint_id\":\"courseF_artist_variables_challenge2_d\",\"hint_type\":\"general\",\"tts_url\":\"https://tts.code.org/sharon22k/180/100/58958c365db37efc69a4c0b4439a4989/course4_artist_variables_challenge2.mp3\"}]",
