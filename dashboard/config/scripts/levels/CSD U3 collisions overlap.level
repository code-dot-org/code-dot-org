    "start_animations": "{\n    \"orderedKeys\": [\n        \"rainbow\",\n        \"horse\",\n        \"unicorn\"\n    ],\n    \"propsByKey\": {\n        \"rainbow\": {\n            \"name\": \"rainbow\",\n            \"sourceUrl\": \"https://images.code.org/1d41d2e5b3b55037911b8f73bdc2ff48-image-1470257277875.png\",\n            \"frameSize\": {\n                \"x\": 66,\n                \"y\": 100\n            },\n            \"frameCount\": 1,\n            \"frameDelay\": 2,\n            \"looping\": true\n        },\n        \"horse\": {\n            \"name\": \"horse\",\n            \"sourceUrl\": \"https://images.code.org/4d1d0e36e574117140ecc377d406c1de-image-1470256869920.png\",\n            \"frameSize\": {\n                \"x\": 100,\n                \"y\": 72\n            },\n            \"frameCount\": 1,\n            \"frameDelay\": 2,\n            \"looping\": true\n        },\n        \"unicorn\": {\n            \"name\": \"unicorn\",\n            \"sourceUrl\": \"https://images.code.org/558b42903bf40cc26410e651a85de922-image-1470256869923.png\",\n            \"frameSize\": {\n                \"x\": 100,\n                \"y\": 72\n            },\n            \"frameCount\": 1,\n            \"frameDelay\": 2,\n            \"looping\": true\n        }\n    }\n}",
    "start_blocks": "var horse = createSprite(200, 50);\r\nhorse.setAnimation(\"horse\");\r\nhorse.setSpeed(5, 315);\r\n\r\nvar rainbow = createSprite(200, 370);\r\nrainbow.setAnimation(\"rainbow\");\r\nrainbow.rotation = 270;\r\n\r\nvar isTouchingX;\r\nvar isTouchingY;\r\nvar isTouching;\r\n\r\nfunction draw() {\r\n  background(\"skyblue\");\r\n  setOverlapXY();\r\n\r\n  \r\n  if (keyDown(\"right\") && rainbow.rotation < 350 ) {\r\n    rainbow.rotation += 1;\r\n  }\r\n  if (keyDown(\"left\") && rainbow.rotation > 190 ) {\r\n    rainbow.rotation -= 1;\r\n  }\r\n  \r\n  if (keyDown(\"space\") && rainbow.y == 370) {\r\n    rainbow.setSpeed(10, rainbow.rotation);\r\n  }\r\n  \r\n\r\n  moveHorse();\r\n  \r\n  if (overlap) {\r\n    horse.setAnimation(\"unicorn\");\r\n  }\r\n  drawSprites();\r\n}\r\n\r\nfunction moveHorse() {\r\n  horse.setSpeed(5, horse.getDirection() + randomNumber(-5, 5));\r\n  if (horse.x < 50 || horse.x > 350) {\r\n    horse.velocityX *= -1; \r\n  }\r\n  if (horse.y < 35 || horse.y > 170) {\r\n    horse.velocityY *= -1; \r\n  }\r\n}\r\n\r\nfunction setOverlapXY() {\r\n  overlapX = (abs(rainbow.x - horse.x) < (horse.width + rainbow.width)/ 2);\r\n  overlapY = (abs(rainbow.y - horse.y) < (horse.height + rainbow.height)/ 2);\r\n}\r\n",
