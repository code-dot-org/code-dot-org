    "start_blocks": "var eyeList = getColumn(\"Emoji Data\", \"Eyes\");\r\nvar eyeIndex = 0;\r\nvar mouthList = getColumn(\"Emoji Data\", \"Mouth\");\r\nvar mouthIndex = 0;\r\nvar extraList = getColumn(\"Emoji Data\", \"Extras\");\r\nvar extraIndex = 0;\r\n\r\nvar favoriteEyes = [];\r\nvar favoriteMouth = [];\r\nvar favoriteExtras = [];\r\nvar favoriteName = [];\r\nvar favoriteDescription = [];\r\nvar favoriteRed = [];\r\nvar favoriteGreen = [];\r\nvar favoriteBlue = [];\r\nvar favoriteIndex = 0;\r\n\r\nvar currentScreen = \"Create\";\r\n\r\nhideElement(\"galleryButton\");\r\nhideElement(\"galleryMode\");\r\n\r\nsetup();\r\n\r\n// This function will already be created and will not need to be coded up by the student  üîé\r\nfunction setColor() {\r\n  setActiveCanvas(\"emojiColor\");\r\n  setFillColor(rgb(getNumber(\"colorSliderR\"), getNumber(\"colorSliderG\"), getNumber(\"colorSliderB\")));\r\n  circle(100, 150, 250);\r\n}\r\n\r\nfunction updateEmoji() {\r\n  setImageURL(\"eyes\", eyeList[eyeIndex % eyeList.length]);\r\n  setImageURL(\"mouth\", mouthList[mouthIndex % mouthList.length]);\r\n  setImageURL(\"extras\", extraList[extraIndex % extraList.length]);\r\n}\r\n\r\nfunction captureData(){\r\n  var emojiName = getText(\"nameText\").substring(0,18);\r\n  var emojiDesc = getText(\"descText\").substring(0,70);\r\n  \r\n  appendItem(favoriteEyes, eyeIndex);\r\n  appendItem(favoriteMouth, mouthIndex);\r\n  appendItem(favoriteExtras, extraIndex);\r\n  appendItem(favoriteRed, getNumber(\"colorSliderR\"));\r\n  appendItem(favoriteGreen, getNumber(\"colorSliderG\"));  \r\n  appendItem(favoriteBlue, getNumber(\"colorSliderB\"));\r\n  appendItem(favoriteName, emojiName);\r\n  appendItem(favoriteDescription, emojiDesc);\r\n}\r\n\r\nfunction removeData(){\r\n  if (favoriteName.length > 0){\r\n    removeItem(favoriteEyes, favoriteIndex);\r\n    removeItem(favoriteMouth, favoriteIndex);\r\n    removeItem(favoriteExtras, favoriteIndex);\r\n    removeItem(favoriteRed, favoriteIndex);\r\n    removeItem(favoriteGreen, favoriteIndex);  \r\n    removeItem(favoriteBlue, favoriteIndex);\r\n    removeItem(favoriteName, favoriteIndex);\r\n    removeItem(favoriteDescription, favoriteIndex);\r\n    if (favoriteIndex > 0){\r\n      favoriteIndex--;\r\n    }\r\n  }\r\n  if (favoriteName.length == 0){\r\n    setProperty(\"selectionHighlight\", \"x\", 0);\r\n    setProperty(\"CreateScreen\", \"image\", \"https://studio.code.org/v3/assets/hNTBPyz3t1HMoh8KXTV3dl8jmxaa7JfmdCcHB-VzoSQ/createBG.png\");\r\n    showCreateTools();\r\n    currentScreen = \"Create\";\r\n    randomizeValues();\r\n    setColor();\r\n    updateEmoji();\r\n    hideElement(\"galleryButton\");\r\n    hideElement(\"galleryMode\");\r\n  }\r\n}\r\n\r\n\r\nfunction displayData(){\r\n  if (favoriteName.length > 0){  \r\n    \r\n    // This is optional -- but it introduces the concept of the newline character and substring...\r\n    var displayText = favoriteName[favoriteIndex] + \"\\n\\n\";\r\n    for (var i = 0; i < favoriteDescription[favoriteIndex].length; i++){\r\n      displayText = displayText + favoriteDescription[favoriteIndex].substring(i, i+1);\r\n      if ((i+1) % 19 == 0){\r\n        displayText = displayText + \"\\n\";\r\n      }\r\n    }\r\n    \r\n    // Read the favorite data and print to the screen in Gallery Mode\r\n    setNumber(\"colorSliderR\", favoriteRed[favoriteIndex]);\r\n    setNumber(\"colorSliderG\", favoriteGreen[favoriteIndex]);\r\n    setNumber(\"colorSliderB\", favoriteBlue[favoriteIndex]);\r\n    eyeIndex = favoriteEyes[favoriteIndex];\r\n    mouthIndex = favoriteMouth[favoriteIndex];\r\n    extraIndex = favoriteExtras[favoriteIndex];\r\n    setText(\"displayTextArea\", displayText);\r\n    updateEmoji();\r\n    setColor();\r\n  }\r\n}\r\n\r\nfunction randomizeValues(){\r\n  setNumber(\"colorSliderR\", randomNumber(0, 255));\r\n  setNumber(\"colorSliderG\", randomNumber(0, 255));\r\n  setNumber(\"colorSliderB\", randomNumber(0, 255));\r\n  eyeIndex = randomNumber(0, eyeList.length-1);\r\n  mouthIndex = randomNumber(0, mouthList.length-1);\r\n  extraIndex = randomNumber(0, extraList.length-1);\r\n}\r\n\r\nonEvent(\"leftArrowButton\", \"click\", function( ) {\r\n  if (favoriteIndex > 0){\r\n    favoriteIndex--;\r\n  }\r\n  displayData();\r\n});\r\n\r\nonEvent(\"rightArrowButton\", \"click\", function( ) {\r\n  if (favoriteIndex < favoriteName.length-1){\r\n    favoriteIndex++;\r\n  }\r\n  displayData();\r\n});\r\n\r\nonEvent(\"removeFavorite\", \"click\", function( ) {\r\n  if (favoriteName.length > 0){\r\n    removeData();\r\n  }\r\n  displayData();\r\n});\r\n\r\nonEvent(\"favoriteButton\", \"click\", function( ) {\r\n  if (currentScreen == \"Create\"){\r\n    setProperty(\"selectionHighlight\", \"x\", 214);\r\n    captureData();\r\n    setText(\"descText\", \"\");\r\n    setText(\"nameText\", \"\");\r\n  }\r\n  showElement(\"galleryButton\");\r\n  showElement(\"galleryMode\");\r\n});\r\n\r\nonEvent(\"galleryButton\", \"click\", function( ) {\r\n  if (favoriteName.length > 0){\r\n    setProperty(\"selectionHighlight\", \"x\", 107);  \r\n    hideCreateTools();\r\n    setProperty(\"CreateScreen\", \"image\", \"https://studio.code.org/v3/assets/hNTBPyz3t1HMoh8KXTV3dl8jmxaa7JfmdCcHB-VzoSQ/galleryBG.png\");\r\n    displayData();\r\n    currentScreen = \"Gallery\";\r\n  }\r\n});\r\n\r\nonEvent(\"createButton\", \"click\", function( ) {\r\n  setProperty(\"selectionHighlight\", \"x\", 0);\r\n  setProperty(\"CreateScreen\", \"image\", \"https://studio.code.org/v3/assets/hNTBPyz3t1HMoh8KXTV3dl8jmxaa7JfmdCcHB-VzoSQ/createBG.png\");\r\n  showCreateTools();\r\n  currentScreen = \"Create\";\r\n  randomizeValues();\r\n  setColor();\r\n  updateEmoji();\r\n});\r\n\r\nonEvent(\"colorSliderR\", \"change\", function( ) {\r\n  setColor();\r\n});\r\nonEvent(\"colorSliderG\", \"change\", function( ) {\r\n  setColor();\r\n});\r\nonEvent(\"colorSliderB\", \"change\", function( ) {\r\n  setColor();\r\n});\r\nonEvent(\"eyeButton\", \"click\", function( ) {\r\n  eyeIndex++;\r\n  updateEmoji();\r\n});\r\nonEvent(\"mouthButton\", \"click\", function( ) {\r\n  mouthIndex++;\r\n  updateEmoji();\r\n});\r\nonEvent(\"extraButton\", \"click\", function( ) {\r\n  extraIndex++;\r\n  updateEmoji();\r\n});\r\n\r\nfunction hideCreateTools(){\r\n  hideElement(\"colorSliderR\");\r\n  hideElement(\"colorSliderG\");\r\n  hideElement(\"colorSliderB\");\r\n  hideElement(\"extraButton\");\r\n  hideElement(\"mouthButton\");\r\n  hideElement(\"eyeButton\");\r\n  hideElement(\"palette\");\r\n  hideElement(\"nameText\");\r\n  hideElement(\"descText\");\r\n  hideElement(\"favoriteButton\");\r\n  hideElement(\"favoriteIcon\");\r\n  showElement(\"rightArrow\");\r\n  showElement(\"leftArrow\");\r\n  showElement(\"rightArrowButton\");\r\n  showElement(\"leftArrowButton\");\r\n  showElement(\"displayTextArea\");\r\n  showElement(\"removeFavorite\");\r\n  setProperty(\"emojiOverlay\", \"x\", -50);\r\n  setProperty(\"emojiColor\", \"x\", -20);\r\n  setProperty(\"eyes\", \"x\", 133);\r\n  setProperty(\"mouth\", \"x\", 122);\r\n  setProperty(\"extras\", \"x\", 67);\r\n  \r\n}\r\n\r\nfunction showCreateTools(){\r\n  showElement(\"colorSliderR\");\r\n  showElement(\"colorSliderG\");\r\n  showElement(\"colorSliderB\");\r\n  showElement(\"extraButton\");\r\n  showElement(\"mouthButton\");\r\n  showElement(\"eyeButton\");\r\n  showElement(\"palette\");  \r\n  showElement(\"nameText\");\r\n  showElement(\"descText\");\r\n  showElement(\"favoriteButton\");\r\n  showElement(\"favoriteIcon\");\r\n  hideElement(\"rightArrow\");\r\n  hideElement(\"leftArrow\");\r\n  hideElement(\"rightArrowButton\");\r\n  hideElement(\"leftArrowButton\");\r\n  hideElement(\"displayTextArea\");\r\n  hideElement(\"removeFavorite\");\r\n  setProperty(\"emojiOverlay\", \"x\", 0);\r\n  setProperty(\"emojiColor\", \"x\", 82);\r\n  setProperty(\"eyes\", \"x\", 183);\r\n  setProperty(\"mouth\", \"x\", 172);\r\n  setProperty(\"extras\", \"x\", 117);  \r\n}\r\n\r\nfunction setup(){\r\n  randomizeValues();\r\n  setColor();\r\n  updateEmoji();\r\n  showCreateTools();\r\n}\r\n",
