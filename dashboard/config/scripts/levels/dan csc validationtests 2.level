unction() {\r\n    var events = validationProps.particlesValidation.events;\r\n    if(events.length > 0) {\r\n      for(var i = 0; i < events.length; i++) {\r\n        event = events[i];\r\n        if(event.behavior == \"liquid\" && event.costume == \"corgi\") {\r\n          return true;\r\n        }\r\n      }\r\n      //If we made it here: never found a liquid beehavior paired with a corgi\r\n      console.log(\"You need the corgis to behave like liquid!\");\r\n      return false;\r\n    }\r\n  }, \"addPrompt\");  // Failure message: \"You need to make a sprite.\"\r\n}\r\ngetHelperVars();\r\n//console.log(animations);\r\n\r\n\r\ncheck();\r\n\r\n\r\nupdatePrevious();\r\n",
