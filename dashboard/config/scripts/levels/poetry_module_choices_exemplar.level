  <blocks>
    <start_blocks>
      <xml>
        <block type="when_run">
          <next>
            <block type="gamelab_setPromptWithChoices">
              <value name="QUESTION">
                <block type="text" can_disconnect_from_parent="false">
                  <title name="TEXT">Enter a food:</title>
                </block>
              </value>
              <value name="VAR">
                <block type="variables_get">
                  <title name="VAR">food</title>
                </block>
              </value>
              <value name="A">
                <block type="Poetry_stringValue" can_disconnect_from_parent="false">
                  <title name="TEXT">sandwich</title>
                </block>
              </value>
              <value name="B">
                <block type="Poetry_stringValue" can_disconnect_from_parent="false">
                  <title name="TEXT">salad</title>
                </block>
              </value>
              <value name="C">
                <block type="Poetry_stringValue" can_disconnect_from_parent="false">
                  <title name="TEXT">cupcake</title>
                </block>
              </value>
            </block>
          </next>
        </block>
        <block type="gamelab_whenAllPromptsAnswered">
          <next>
            <block type="Poetry_setTitle">
              <value name="TITLE">
                <block type="gamelab_textJoin">
                  <title name="TEXT1">An Ode to Food</title>
                </block>
              </value>
              <next>
                <block type="Poetry_addLine">
                  <value name="LINE">
                    <block type="gamelab_textJoin">
                      <title name="TEXT1">Life is too short</title>
                    </block>
                  </value>
                  <next>
                    <block type="Poetry_addLine">
                      <value name="LINE">
                        <block type="gamelab_textJoin">
                          <title name="TEXT1">to eat a bad </title>
                          <value name="TEXT2">
                            <block type="gamelab_textVariableJoin">
                              <title name="VAR">food</title>
                            </block>
                          </value>
                        </block>
                      </value>
                      <next>
                        <block type="Poetry_animateText">
                          <next>
                            <block type="Poetry_setBackgroundEffect">
                              <title name="EFFECT">"squiggles"</title>
                              <title name="PALETTE">"summer"</title>
                            </block>
                          </next>
                        </block>
                      </next>
                    </block>
                  </next>
                </block>
              </next>
            </block>
          </next>
        </block>
      </xml>
