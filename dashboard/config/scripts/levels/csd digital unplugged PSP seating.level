    "start_blocks": "var tables = createSeating();\r\n\r\nvar alphabet = \"ABCDEFGHIJKLMNOPQRSTUVWXYZ\";\r\nfor (var i=0; i<15; i++) {\r\n  textLabel(alphabet.charAt(i), alphabet.charAt(i));\r\n  setPosition(alphabet.charAt(i), i*20 + 10, 10, 20, 20);\r\n  setStyle(alphabet.charAt(i), \"text-align: center; font-size: 20px;\");\r\n}\r\n\r\nvar dragging = false;\r\nvar myElements = [];\r\nloadElements(myElements);\r\n    for (var j=0; j<myElements.length; i++) {\r\n      snapToSeats(myElements[j].name);\r\n    }\r\n\r\nfunction saveState() {\r\n  var newX = Math.min(Math.max(0, getProperty(dragging.name, \"x\")), 280);\r\n  var newY = Math.min(Math.max(0, getProperty(dragging.name, \"y\")), 400);\r\n  updateRecord(\"elements\", {id: dragging.id, name: dragging.name, pageX: newX, pageY: newY }, function(record, success) {\r\n  });\r\n  dragging = false;\r\n}\r\n\r\nfunction loadElements(elements) {\r\n  readRecords(\"elements\", {}, function(records) {\r\n    for (var i =0; i < records.length; i++) {\r\n      elements[i] = {};\r\n      elements[i].id = records[i].id;\r\n      elements[i].name = records[i].name;\r\n      elements[i].dragging = false;\r\n      setProperty(records[i].name, \"x\", records[i].pageX);\r\n      setProperty(records[i].name, \"y\", records[i].pageY);\r\n      setStyle(records[i].name, \"display: block\");\r\n      snapToSeats(elements[i].name);\r\n    }\r\n    createDragEvents();\r\n  });\r\n}\r\n\r\n\r\nfunction createDragEvents() {\r\n  for (var i=0; i<myElements.length; i++) {\r\n    eval(\r\n    \"onEvent(myElements[\"+ i +\"].name, \\\"mousedown\\\", function(event) {\" +\r\n      \"myElements[\" + i + \"].dragging = true;\" +\r\n      \"dragging = myElements[\"+ i +\"];\" +\r\n      \"removeFromSeat(myElements[\"+ i +\"].name);\" +\r\n      \"setProperty(myElements[\" + i + \"].name, \\\"x\\\", getProperty(myElements[\" + i + \"].name, \\\"x\\\"));\" +\r\n      \"setProperty(myElements[\" + i + \"].name, \\\"y\\\", getProperty(myElements[\" + i + \"].name, \\\"y\\\"));\" +\r\n      \"setStyle(myElements[\" + i + \"].name, \\\"z-index: 20;\\\");\" +\r\n      \"myElements[\" + i + \"].offsetX = event.offsetX;\" +\r\n      \"myElements[\" + i + \"].offsetY = event.offsetY;\" +\r\n    \"});\");\r\n  }\r\n}\r\n\r\nonEvent(\"screen1\", \"mousemove\", function(event) {\r\n  for (var i=0; i<myElements.length; i++) {\r\n    if (myElements[i].dragging) {\r\n      setProperty(myElements[i].name, \"x\", Math.min(Math.max(-15, event.pageX - myElements[i].offsetX), 280));\r\n      setProperty(myElements[i].name, \"y\", Math.min(Math.max(-15, event.pageY - myElements[i].offsetY), 410));\r\n    }\r\n  }\r\n});\r\nonEvent(\"screen1\", \"mouseup\", function() {\r\n  for (var i=0; i<myElements.length; i++) {\r\n    myElements[i].dragging = false;\r\n    setStyle(myElements[i].name, \"z-index: 10;\");\r\n  }\r\n  if (dragging) {\r\n    snapToSeats(dragging.name);\r\n    saveState();\r\n  }\r\n});\r\nonEvent(\"screen1\", \"mouseout\", function() {\r\n  for (var i=0; i<myElements.length; i++) {\r\n    myElements[i].dragging = false;\r\n    setStyle(myElements[i].name, \"z-index: 10;\");\r\n  }\r\n  if (dragging) { \r\n    snapToSeats(dragging.name);\r\n    saveState();\r\n  }\r\n});\r\n\r\nfunction createSeating() {\r\n  var localTables = [];\r\n  localTables.push([{}, {}, {}, {}, {}]);\r\n  localTables.push([{}, {}, {}, {}, {}]);\r\n  localTables.push([{}, {}, {}, {}, {}]);\r\n  createCanvas(\"seating\", 320, 450);\r\n  setFillColor(\"gray\");\r\n  circle(90, 150, 40);\r\n  circle(240, 250, 40);\r\n  circle(100, 360, 40);\r\n  setFillColor(\"white\");\r\n  \r\n  circle(295, 215, 20);\r\n  localTables[0][0].x = 295;\r\n  localTables[0][0].y = 215;\r\n  localTables[0][0].letter = false;\r\n  circle(295, 285, 20);\r\n  localTables[0][1].x = 295;\r\n  localTables[0][1].y = 285;\r\n  localTables[0][1].letter = false;\r\n  circle(225, 187, 20);\r\n  localTables[0][2].x = 225;\r\n  localTables[0][2].y = 187;\r\n  localTables[0][2].letter = false;\r\n  circle(225, 313, 20);\r\n  localTables[0][3].x = 225;\r\n  localTables[0][3].y = 313;\r\n  localTables[0][3].letter = false;\r\n  circle(175, 250, 20);\r\n  localTables[0][4].x = 175;\r\n  localTables[0][4].y = 250;\r\n  localTables[0][4].letter = false;\r\n  \r\n  circle(145, 115, 20);\r\n  localTables[1][0].x = 145;\r\n  localTables[1][0].y = 115;\r\n  localTables[1][0].letter = false;\r\n  circle(145, 185, 20);\r\n  localTables[1][1].x = 145;\r\n  localTables[1][1].y = 185;\r\n  localTables[1][1].letter = false;\r\n  circle(75, 87, 20);\r\n  localTables[1][2].x = 75;\r\n  localTables[1][2].y = 87;\r\n  localTables[1][2].letter = false;\r\n  circle(75, 213, 20);\r\n  localTables[1][3].x = 75;\r\n  localTables[1][3].y = 213;\r\n  localTables[1][3].letter = false;\r\n  circle(25, 150, 20);\r\n  localTables[1][4].x = 25;\r\n  localTables[1][4].y = 150;\r\n  localTables[1][4].letter = false;\r\n  \r\n  circle(45, 325, 20);\r\n  localTables[2][0].x = 45;\r\n  localTables[2][0].y = 325;\r\n  localTables[2][0].letter = false;\r\n  circle(45, 395, 20);\r\n  localTables[2][1].x = 45;\r\n  localTables[2][1].y = 395;\r\n  localTables[2][1].letter = false;\r\n  circle(115, 297, 20);\r\n  localTables[2][2].x = 115;\r\n  localTables[2][2].y = 297;\r\n  localTables[2][2].letter = false;\r\n  circle(115, 423, 20);\r\n  localTables[2][3].x = 115;\r\n  localTables[2][3].y = 423;\r\n  localTables[2][3].letter = false;\r\n  circle(165, 360, 20);\r\n  localTables[2][4].x = 165;\r\n  localTables[2][4].y = 360;\r\n  localTables[2][4].letter = false;\r\n  return localTables;\r\n}\r\n\r\nfunction snapToSeats(name) {\r\n  for (var j=0; j<tables.length; j++) {\r\n    for (var i=0; i<tables[j].length; i++) {\r\n      if (snapToSeat(tables[j][i], name)) {\r\n        return;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\nfunction snapToSeat(seat, name) {\r\n  var xDiff = Math.abs(seat.x - 10 - getProperty(name, \"x\"));\r\n  //console.log(seat.x - 10 + \" \" + getProperty(name, \"x\") + \" \" + xDiff);\r\n  var yDiff = Math.abs(seat.y - 10 - getProperty(name, \"y\"));\r\n  //console.log(name + \" \" + (seat.y - 10) + \" \" + getProperty(name, \"y\") + \" \" + yDiff);\r\n  if ((xDiff < 20) && (yDiff < 20)) {\r\n    if (seat.letter == false) {\r\n      setProperty(name, \"x\", seat.x - 10);\r\n      setProperty(name, \"y\", seat.y - 10);\r\n      seat.letter = name;\r\n      return true;\r\n    } else {\r\n      setProperty(name, \"y\", 20);\r\n      return true;\r\n    }\r\n    return false;\r\n  }\r\n}\r\nfunction removeFromSeat(letter) {\r\n  for (var j=0; j<tables.length; j++) {\r\n    for (var i=0; i<tables[j].length; i++) {\r\n      if (tables[j][i].letter == letter) {\r\n        tables[j][i].letter = false;\r\n        return;\r\n      }\r\n    }\r\n  }\r\n}\r\n",
