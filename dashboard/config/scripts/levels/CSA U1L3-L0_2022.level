      "MyNeighborhood.java": {
        "text": "import org.code.neighborhood.Painter;\n\npublic class MyNeighborhood {\n  public static void main(String[] args) {\n\n    Painter myPainter = new Painter();\n\n    myPainter.move();\n\n    myPainter.takePaint();\n    myPainter.takePaint();\n\n    myPainter.turnLeft();\n    myPainter.turnLeft();\n    myPainter.turnLeft();\n\n    myPainter.move();\n    myPainter.paint(\"coral\");\n\n    myPainter.move();\n    myPainter.paint(\"thistle\");\n\n    // TO DO #3: Add the code below this comment.\n\n\n\n    // TO DO #4: Add the code below this comment.\n\n    \n    \n  }\n}",
        "isVisible": true
