    "start_blocks": "\r\nvar fortunes = [];\r\nvar luckyColorCode = [];\r\nvar luckyColorName = [];\r\n\r\nvar fortuneIndex;\r\nvar colorIndex  = 0;\r\n\r\nfortunes = getColumn(\"Fortune Data\", \"Fortunes\");\r\nluckyColorCode = getColumn(\"Fortune Data\", \"Lucky Color\");\r\nluckyColorName = getColumn(\"Fortune Data\", \"Color Name\");\r\n\r\naddFortuneData();\r\n\r\nonEvent(\"fortuneButton\", \"click\", function( ) {   // Random List Access Pattern\r\n  showElement(\"fortuneText\");\r\n  hideElement(\"9NumberImage\");\r\n  \r\n  fortuneIndex = randomNumber(0, fortunes.length-1);\r\n  setText(\"fortuneText\", fortunes[fortuneIndex]);\r\n  \r\n  setLuckyColor();\r\n  setLuckyNumber();\r\n});\r\n\r\nfunction setLuckyColor(){                         // List Scrolling Pattern\r\n\r\n  setText(\"luckyColorTextBox\", luckyColorName[colorIndex]);\r\n  setProperty(\"bgColor\", \"background-color\", luckyColorCode[colorIndex]);\r\n\r\n  if (colorIndex < luckyColorName.length-1){\r\n    colorIndex++;\r\n    } else {\r\n    colorIndex = 0;\r\n  }\r\n}\r\n\r\nfunction setLuckyNumber(){\r\n  var luckyNumber  = randomNumber(0, 100);\r\n  setText(\"luckyNumberTextBox\", luckyNumber);\r\n  \r\n}\r\n\r\nfunction addFortuneData(){\r\n  appendItem(fortunes, \"This is a fortune that is added to the list\");\r\n  appendItem(luckyColorName, \"Burnt Orange\");\r\n  appendItem(luckyColorCode, '#CC5500');\r\n}\r\n",
