    "start_blocks": "// Create lists of all words\r\nvar allWords          = getColumn(\"words\",\"Word\");\r\nvar allPartsOfSpeech  = getColumn(\"words\",\"Part of Speech\");\r\n\r\n// Create lists of adjectives and nouns\r\nvar adjectives        = loadWords(\"adjective\");\r\nvar nouns             = loadWords(\"noun\");\r\n\r\n// When the user clicks the button, update the labels\r\nonEvent(\"newPoemButton\", \"click\", function( ) {\r\n  for(var i = 1; i < 4; i++){\r\n    setProperty(\"line\"+i,\"text\",randomWord(\"noun\") + \"s are \" + randomWord(\"adjective\"));\r\n  }\r\n});\r\n\r\n// Filters the words list based on the part of speech given\r\n// wordType {string} - the part of speech of the words that should be returned\r\n// return {list} - a list of words with the part of speech type given\r\nfunction loadWords(wordType){\r\n  var wordList = [];\r\n  for(var i = 0; i < allWords.length; i++){\r\n    var word = allWords[i];\r\n    var partOfSpeech = allPartsOfSpeech[i];\r\n    if(partOfSpeech == wordType){\r\n      appendItem(wordList,word);\r\n    }\r\n  } \r\n  return wordList;\r\n}\r\n\r\n// Returns a random word of the type provided\r\n// wordType {string} - the type of word, either \"adjective\" or \"noun\" that should be returned\r\n// return {string} - a random word from the type of list given\r\nfunction randomWord(wordType){\r\n  var word = \"\";\r\n  if(wordType == \"adjective\"){\r\n    word = randomListPick(adjectives);\r\n  } else if (wordType == \"noun\"){\r\n    word = randomListPick(nouns);\r\n  }\r\n  return word;\r\n}\r\n\r\n// Returns a random element from the list\r\n// list {list} - the list to randomly pick an element from\r\n// return {unknown} - returns a random element in the list\r\nfunction randomListPick(list){\r\n  return list[randomNumber(0,list.length-1)];\r\n}",
