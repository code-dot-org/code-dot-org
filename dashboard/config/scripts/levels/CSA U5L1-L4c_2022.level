      "MyTheater.java": {
        "text": "import org.code.theater.*;\n\npublic class MyTheater {\n  public static void main(String[] args) {\n\n    // TO DO #1: Declare and initialize a 2D array.\n\n\n    // TO DO #2: Create an InstagramScene object with the 2D array.\n\n\n    // TO DO #3: Call the createScene() method with a row from the 2D array.\n\n\n    // TO DO #4: Use Theater.playScenes() to play the scene.\n\n    \n  }\n}",
        "isVisible": true
      },
      "InstagramScene.java": {
        "text": "import org.code.theater.*;\n\n/*\n * Creates a scene to show the an Instagram user\n * and their information\n */\npublic class InstagramScene extends Scene {\n\n  private String[][] users;   // The 2D array of Instagram users\n\n  /*\n   * Constructor to create an InstagramScene\n   * with the specified 2D array of users\n   */\n  public InstagramScene(String[][] users) {\n    this.users = users;\n  }\n\n  /*\n   * Creates a scene by drawing the image of the user\n   * and text containing the information about the user\n   */\n  public void createScene(String[] user) {\n    drawText(user[0], 50, 50);\n    pause(0.3);\n    \n    drawText(\"Followers: \" + user[2], 200, 200);\n    pause(0.3);\n    \n    drawText(user[3], 50, 350);\n    pause(0.3);\n\n    if (user[1].equals(\"Instagram\")) {\n      drawImage(\"instagram.png\", 20, 75, 125);\n    }\n    else if (user[1].equals(\"Cristiano Ronaldo\")) {\n      drawImage(\"cristianoronaldo.jpg\", 20, 75, 125);\n    }\n    else if (user[1].equals(\"Ariana Grande\")) {\n      drawImage(\"arianagrande.png\", 20, 75, 125);\n    }\n    else if (user[1].equals(\"Dwayne Johnson\")) {\n      drawImage(\"dwaynejohnson.jpg\", 20, 75, 125);\n    }\n    else {\n      drawImage(\"kyliejenner.jpg\", 20, 75, 125);\n    }\n  }\n  \n}",
        "isVisible": true
      }
