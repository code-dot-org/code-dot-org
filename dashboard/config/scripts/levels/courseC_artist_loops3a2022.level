    "authored_hints": "[{\"hint_class\":\"content\",\"hint_markdown\":\"You will want to add just one block inside of your loop, after the first sticker.\",\"hint_id\":\"courseC_artist_loops3a_a\",\"hint_type\":\"general\",\"hint_path\":\"\",\"hint_video\":\"\",\"tts_url\":\"https://tts.code.org/sharon22k/180/100/9cb5c092b7c0f1e206eb94c29567725a/courseC_artist_loops3a_2019.mp3\"},{\"hint_class\":\"bottom-out\",\"hint_markdown\":\"Your program should look like this!\\n\\n<xml>\\n  <block type=\\\"when_run\\\" deletable=\\\"false\\\" movable=\\\"false\\\">\\n    <next>\\n      <block type=\\\"controls_repeat\\\">\\n        <title name=\\\"TIMES\\\">???</title>\\n        <statement name=\\\"DO\\\">\\n          <block type=\\\"jump_to\\\">\\n            <title name=\\\"VALUE\\\">13</title>\\n            <next>\\n              <block type=\\\"draw_turn_by_constant_dropdown\\\">\\n                <title name=\\\"DIR\\\">???</title>\\n                <title name=\\\"VALUE\\\" config=\\\"1,2,5,10,12,20,30,45,60,90,120,180\\\">???</title>\\n                <next>\\n                  <block type=\\\"sticker\\\">\\n                    <title name=\\\"VALUE\\\">Cat</title>\\n                    <next>\\n                      <block type=\\\"sticker\\\">\\n                        <title name=\\\"VALUE\\\">Dog</title>\\n                      </block>\\n                    </next>\\n                  </block>\\n                </next>\\n              </block>\\n            </next>\\n          </block>\\n        </statement>\\n      </block>\\n    </next>\\n  </block>\\n</xml>\\n\\nYou can use any stickers you like!\",\"hint_id\":\"courseC_artist_loops3a_b\",\"hint_type\":\"general\",\"hint_path\":\"\",\"hint_video\":\"\",\"tts_url\":\"https://tts.code.org/sharon22k/180/100/ca0778b4c090002c5114a23d5a174091/courseC_artist_loops3a_2019.mp3\"}]",
