    "start_blocks": "var points = 0;\r\nvar screenNum = 0;\r\nhideElement(\"q1check\");\r\nhideElement(\"q2check\");\r\nhideElement(\"q3check\");\r\n\r\nsetScreen(\"startGameScreen\");\r\n\r\nonEvent(\"play\", \"click\", function( ) {\r\n  setScreen(\"question1Screen\");\r\n  screenNum = 1;\r\n  console.log(\"screen: \" + screenNum);\r\n});\r\n\r\nonEvent(\"goToQ2\", \"click\", function(){\r\n  setScreen(\"question2Screen\");\r\n  screenNum = 2;\r\n  console.log(\"screen: \" + screenNum);\r\n});\r\n\r\nonEvent(\"goToQ3\", \"click\", function(){\r\n  setScreen(\"question3Screen\");\r\n  screenNum = 3;\r\n  console.log(\"screen: \" + screenNum);\r\n});\r\n\r\nonEvent(\"goToResults\", \"click\", function(){\r\n  setScreen(\"resultsScreen\");\r\n  if(points > 3){\r\n    setProperty(\"results\", \"font-size\", 24);\r\n    setProperty(\"results\",\"text\",\"You pressed buttons too many times when answering. \\nTry again.\");\r\n  }\r\n  if (points < 3) {\r\n    setProperty(\"results\",\"text\",points);\r\n    \r\n  }\r\n  if(points == 3){\r\n    setProperty(\"allCorrect\", \"text\", \"Great Job\");\r\n    setProperty(\"results\", \"text\", points);\r\n    buzzer.frequency(800, 300);\r\n    led.blink(200);\r\n  }\r\n});\r\n\r\n\r\nonBoardEvent(buttonL, \"down\", function() {\r\n  if(screenNum == 1){\r\n    //points = points - 1;\r\n    console.log(\"Left button clicked on screen 1. Points: \" + points);\r\n    showElement(\"q1check\");\r\n    buzzer.frequency(400, 150);\r\n  }\r\n  if(screenNum == 2){\r\n    //points = points - 1;\r\n    console.log(\"Left button clicked on screen 2. Points: \" + points);\r\n    showElement(\"q2check\");\r\n    buzzer.frequency(400, 150);\r\n  }\r\n  if(screenNum == 3){\r\n    points = points + 1;\r\n    console.log(\"Left button clicked on screen 3. Points: \" + points);\r\n    showElement(\"q3check\");\r\n    buzzer.frequency(400, 150);\r\n  }\r\n  \r\n  \r\n});\r\n\r\nonBoardEvent(buttonR, \"down\", function() {\r\n  if(screenNum == 1){\r\n    points = points + 1;\r\n    console.log(\"Right button clicked on screen 1. Points: \" + points);\r\n    showElement(\"q1check\");\r\n    buzzer.frequency(400, 150);\r\n\r\n  }\r\n  \r\n  if(screenNum == 2){\r\n    points = points + 1;\r\n    console.log(\"Right button clicked on screen 2. Points: \" + points);\r\n    showElement(\"q2check\");\r\n    buzzer.frequency(400, 150);\r\n  }\r\n  \r\n  if(screenNum == 3){\r\n    //points = points - 1;\r\n    console.log(\"Right button clicked on screen 3. Points: \" + points);\r\n    showElement(\"q3check\");\r\n    buzzer.frequency(400, 150);\r\n  }\r\n \r\n  \r\n});\r\n",
