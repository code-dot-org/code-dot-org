    "start_animations": "{\r\n  \"orderedKeys\": [\r\n    \"alien\",\r\n    \"platform\",\r\n    \"star\"\r\n  ],\r\n  \"propsByKey\": {\r\n    \"alien\": {\r\n      \"name\": \"alien\",\r\n      \"sourceUrl\": \"https://studio.code.org/v3/animations/EMUV0XtYgxzQdeTHikOA6Np5_X8Lre3ER2WIAWXiDfM/alien.png?version=oOwHmBWVYrWNSvb3cpbB5Rk5qVdK7rZX\",\r\n      \"frameSize\": {\r\n        \"x\": 64,\r\n        \"y\": 80\r\n      },\r\n      \"frameCount\": 2,\r\n      \"looping\": true,\r\n      \"frameDelay\": 12,\r\n      \"version\": \"oOwHmBWVYrWNSvb3cpbB5Rk5qVdK7rZX\"\r\n    },\r\n    \"platform\": {\r\n      \"name\": \"platform\",\r\n      \"sourceUrl\": \"https://studio.code.org/v3/animations/EMUV0XtYgxzQdeTHikOA6Np5_X8Lre3ER2WIAWXiDfM/platform.png?version=eAofWI0uKuHu6H691vAnGjR6idKRYHhw\",\r\n      \"frameSize\": {\r\n        \"x\": 150,\r\n        \"y\": 20\r\n      },\r\n      \"frameCount\": 1,\r\n      \"looping\": true,\r\n      \"frameDelay\": 12,\r\n      \"version\": \"eAofWI0uKuHu6H691vAnGjR6idKRYHhw\"\r\n    },\r\n    \"star\": {\r\n      \"name\": \"star\",\r\n      \"sourceUrl\": \"https://studio.code.org/v3/animations/EMUV0XtYgxzQdeTHikOA6Np5_X8Lre3ER2WIAWXiDfM/star.png?version=EPDCMarrfXLmBFGCBU87Ue5GSf8EbOcg\",\r\n      \"frameSize\": {\r\n        \"x\": 30,\r\n        \"y\": 30\r\n      },\r\n      \"frameCount\": 4,\r\n      \"looping\": true,\r\n      \"frameDelay\": 12,\r\n      \"version\": \"EPDCMarrfXLmBFGCBU87Ue5GSf8EbOcg\"\r\n    }\r\n  }\r\n}",
