{
  "title": "Teacher Lesson Feedback",
  "pages": [
    {
      "name": "page1",
      "elements": [
        {
          "type": "text",
          "name": "script_name",
          "visible": false,
          "readOnly": true
        },
        {
          "type": "text",
          "name": "lesson_number",
          "visible": false,
          "readOnly": true
        },
        {
          "type": "html",
          "name": "heading_with_unit",
          "html": "<h3>Course: {course_name}<br/>Unit {unit_number}: {unit_name}<br/>Lesson {lesson_number}: {lesson_name}",
          "visibleIf": "!!{unit_name}"
        },
        {
          "type": "html",
          "name": "heading_without_unit",
          "html": "<h3>Course: {course_name}<br/>Lesson {lesson_number}: {lesson_name}",
          "visibleIf": "!{unit_name}"
        },
        {
          "type": "rating",
          "name": "how_enjoyable_nps",
          "title": "How much did you enjoy teaching this lesson?",
          "isRequired": true,
          "rateMin": 0,
          "rateMax": 10,
          "minRateDescription": "Not at all enjoyable",
          "maxRateDescription": "Extremely enjoyable"
        },
        {
          "type": "comment",
          "name": "any_feedback_fr",
          "title": "Do you have any feedback on this lesson for the Code.org curriculum team?",
          "isRequired": false
        }
      ]
    }
  ],
  "published": true
}