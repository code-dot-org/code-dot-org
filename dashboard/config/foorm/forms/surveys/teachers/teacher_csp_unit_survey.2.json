        {
          "type": "html",
          "name": "unit_instructions",
          "html": " <p style='font-size: larger'>Please tell us your overall thoughts about teaching this unit. Your feedback is crucial for us to better understand how to improve this course for teachers.</p>"
        },
        {
          "type": "html",
          "name": "unit_num_name_heading",
          "visibleIf": "{unit_number} notempty and {unit_number} >= 1 and {unit_number} <=10",
          "html": "<h2>Unit {unit_number} - {unit_name}</h2>"
        },
        {
          "type": "rating",
          "name": "interesting_and_engaging",
          "title": "The activities in this unit were interesting and engaging to students.",
          "rateMax": 7,
          "minRateDescription": "Strongly disagree",
          "maxRateDescription": "Strongly agree"
        },
        {
          "type": "rating",
          "name": "develop_problem_solving_skills",
          "title": "The activities in this unit helped students develop problem-solving skills.",
          "rateMax": 7,
          "minRateDescription": "Strongly disagree",
          "maxRateDescription": "Strongly agree"
        },
        {
          "type": "rating",
          "name": "successfully_learned",
          "title": "The supports and materials in this unit allowed ____ of my students to successfully learn and apply the concepts to the activities and project.",
          "rateValues": [
            {
              "value": 4,
              "text": " All "
            },
            {
              "value": 3,
              "text": " Most"
            },
            {
              "value": 2,
              "text": " Some"
            },
            {
              "value": 1,
              "text": " Few, or None"
            }
          ]
        },
        {
          "type": "rating",
          "name": "make_connections",
