                },
                {
                  "value": 3,
                  "text": " Most"
                },
                {
                  "value": 2,
                  "text": " Some"
                },
                {
                  "value": 1,
                  "text": " Few, or None"
                }
              ]
            }
          ],
          "visibleIf": "{unit_number} = 3 or {unit_number} = 4 or {unit_number} = 6 or {unit_number} = 7"
