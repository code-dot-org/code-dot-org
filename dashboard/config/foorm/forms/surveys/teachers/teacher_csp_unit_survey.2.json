              "defaultValueExpression": "{unit_number}",
              "choices": [
                {
                  "value": 1,
                  "text": "Digital Information"
                },
                {
                  "value": 2,
                  "text": "The Internet"
                },
                {
                  "value": 3,
                  "text": "Intro to App Design"
