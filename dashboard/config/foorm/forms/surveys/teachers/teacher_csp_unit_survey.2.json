                {
                  "value": "most",
                  "text": " Most levels"
                },
                {
                  "value": "some",
                  "text": " Some levels"
                },
                {
                  "value": "few",
                  "text": " Few / No Levels"
                }
              ]
            },
            {
              "type": "radiogroup",
