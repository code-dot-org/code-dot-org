          "rateValues": [
            {
              "value": 4,
              "text": " All "
            },
            {
              "value": 3,
              "text": " Most"
            },
            {
              "value": 2,
              "text": " Some"
            },
            {
              "value": 1,
              "text": " Few, or None"
            }
          ]
        },
        {
          "type": "radiogroup",
          "name": "time_on_project",
          "title": "Approximately, how much time did your students (class) spend on the end-of-unit project?",
          "choices": [
            {
              "value": "more_than_allotted",
              "text": " We spent more than the allotted number of lessons on the project."
            },
            {
              "value": "allotted",
              "text": " We spent the allotted number of lessons on the project."
            },
            {
              "value": "less_than_allotted",
              "text": " We spent less than the allotted number of lessons on the project."
            },
            {
              "value": "did_not_do",
              "text": " We did not do the end-of-unit project."
            }
          ]
        },
