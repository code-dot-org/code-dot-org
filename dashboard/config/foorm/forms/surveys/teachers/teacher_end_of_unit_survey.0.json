          "visibleIf": "{unit_number} notempty"
        },
        {
          "type": "rating",
          "name": "interesting_and_engaging",
          "title": "The activities in this unit were interesting and engaging to students.",
          "rateMax": 7,
          "minRateDescription": "Strongly disagree",
          "maxRateDescription": "Strongly agree"
        },
        {
          "type": "rating",
          "name": "develop_problem_solving_skills",
          "title": "The activities in this unit helped students develop problem-solving skills.",
          "rateMax": 7,
          "minRateDescription": "Strongly disagree",
          "maxRateDescription": "Strongly agree"
        },
        {
          "type": "rating",
          "name": "successfully_learned",
          "title": "The supports and materials in this unit allowed ____ of my students to successfully learn and apply the concepts to the activities and project.",
          "rateValues": [
            {
              "value": 4,
              "text": " All "
            },
            {
              "value": 3,
              "text": " Most"
            },
            {
              "value": 2,
              "text": " Some"
            },
            {
              "value": 1,
              "text": " Few, or None"
            }
          ]
        },
        {
          "type": "rating",
          "name": "make_connections",
          "title": "I saw ____ of my students make connections to their personal lives and interests in this unit.",
          "rateValues": [
            {
              "value": 4,
              "text": " All "
            },
            {
              "value": 3,
              "text": " Most"
            },
            {
              "value": 2,
              "text": " Some"
            },
            {
              "value": 1,
              "text": " Few, or None"
            }
          ]
        },
        {
          "type": "radiogroup",
          "name": "used_extra_practice",
          "title": "Did you use the Extra Practice resources in this unit?",
          "choices": [
            {
              "value": "yes_effective",
              "text": "Yes, and they were effective!"
            },
            {
              "value": "yes_not_effective",
              "text": "Yes, but they were not as effective as I would have liked."
            },
            {
              "value": "no",
              "text": "No."
            },
            {
              "value": "unaware",
              "text": "I didn't know they existed."
            }
          ]
        },
        {
          "type": "radiogroup",
          "name": "how_used_extra_practice",
          "visibleIf": "{used_extra_practice} = 'yes_effective' or {used_extra_practice} = 'yes_not_effective'",
          "title": "How did you use the Extra Practice resources?",
          "description": "If you used them multiple ways, please select the option you use most frequently.",
          "choices": [
            {
              "value": "bell_ringer",
              "text": "I used them as bell ringers at the beginning of the lesson."
            },
            {
              "value": "wrap_up",
              "text": "I used them as wrap-up activities at the end of the lesson."
            },
            {
              "value": "homework",
              "text": "I assigned them for homework."
            },
            {
              "value": "extra_credit",
              "text": "I assigned them for extra credit."
            },
            {
              "value": "didnt_use",
              "text": "I didn't use these, or I didn't know they existed."
            },
            {
              "value": "other",
              "text": "I used them in some other way. (please describe)"
            }
          ]
        },
        {
          "type": "comment",
          "name": "extra_practice_other_comment",
          "visibleIf": "{how_used_extra_practice} = 'other'",
          "title": "Please describe the other way(s) you used the Extra Practice resources."
        },
        {
          "type": "radiogroup",
          "name": "did_code_reviews",
          "title": "Did students conduct code reviews in this unit?",
