{
  "title": "Code.org CSA Teacher End-of-Unit Survey",
  "logoPosition": "right",
  "pages": [
    {
      "name": "page1",
      "elements": [
        {
          "type": "panel",
          "name": "variable_panel",
          "elements": [
            {
              "type": "radiogroup",
              "name": "unit_number",
              "startWithNewLine": false,
              "choices": [
                1,
                2,
                3,
                4,
                5,
                6,
                7,
                8
              ]
            },
            {
              "type": "radiogroup",
              "name": "unit_name",
              "startWithNewLine": false,
              "defaultValueExpression": "{unit_number}",
              "readOnly": true,
              "choices": [
                {
                  "value": 1,
                  "text": "Object-Oriented Programming"
                },
                {
                  "value": 2,
                  "text": "Class Structure and Design"
                },
                {
                  "value": 3,
                  "text": "Arrays and Algorithms"
                },
                {
                  "value": 4,
                  "text": "Conditions and Logic"
                },
                {
                  "value": 5,
                  "text": "Two-Dimensional Arrays"
                },
                {
                  "value": 6,
                  "text": "ArrayLists and String Methods"
                },
                {
                  "value": 7,
                  "text": "Method Decomposition and Recursion"
                },
                {
                  "value": 8,
                  "text": "Searching and Sorting"
                }
              ]
            }
          ],
          "title": "Panel to hold variables for foorm. HIDE AFTER TESTING",
          "startWithNewLine": false,
          "innerIndent": 3,
          "indent": 3,
          "visible": "false"
        },
        {
          "type": "html",
          "name": "unit_instructions",
          "html": " <h3>Please tell us your overall thoughts about teaching this unit.</h3>"
        },
