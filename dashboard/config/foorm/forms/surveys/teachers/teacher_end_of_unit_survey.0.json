            {
              "value": "every_lesson",
              "text": "Yes, in every lesson (even if not prompted to)"
            },
            {
              "value": "yes_with_lesson_plan",
              "text": "Yes, according to the lesson plan."
            },
            {
              "value": "sometimes",
              "text": "Some of the time"
            },
            {
              "value": "no",
              "text": "No, not at all"
            }
          ]
        },
        {
          "type": "comment",
          "name": "benefits_drawbacks_code_review",
          "description": "(Optional)",
          "rows": 2,
          "title": "What do you see as the benefits and/or drawbacks for students doing code reviews?"
        },
        {
          "type": "radiogroup",
          "name": "complete_end_of_unit_project",
          "title": "Did students complete the end-of-unit project?",
          "choices": [
            {
              "value": "yes_more_than_3",
              "text": "Yes, we spent more than three days on the project"
            },
            {
              "value": "yes_3_day",
              "text": "Yes, the three-day version"
            },
            {
              "value": "yes_1_day",
              "text": "Yes, the one-day version"
            },
            {
              "value": "no",
              "text": "No, we did not complete the project"
            }
          ]
        },
