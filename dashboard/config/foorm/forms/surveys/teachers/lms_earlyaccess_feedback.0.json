            },
            {
              "value": "rosterSync",
              "text": "Roster Sync"
            }
          ]
        },
        {
          "type": "radiogroup",
          "name": "easyToInstall",
          "isRequired": true,
          "title": "How easy was it to install and set-up the Code.org Integration?",
          "choices": [
            {
              "value": "veryEasy",
              "text": "Very Easy"
            },
            {
              "value": "somewhatEasy",
              "text": "Somewhat Easy"
            },
            {
              "value": "neither",
              "text": "Neither Easy nor Difficult"
            },
            {
              "value": "somewhatDifficult",
              "text": "Somewhat Difficult"
