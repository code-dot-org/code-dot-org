      }
    },
    {
      "key": "e659d8a5-512e-423f-8819-65df3768a163",
      "position": 2,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Do This:** Direct students to the Unit 5 Assessment on Code Studio.",
        "progression_name": "Unit 5 Assessment"
      },
      "seeding_key": {
        "activity_section.key": "e659d8a5-512e-423f-8819-65df3768a163",
