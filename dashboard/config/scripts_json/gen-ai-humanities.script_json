      }
    },
    {
      "key": "50fee6c1-86e2-4025-a6ca-db7b2e3d7291",
      "position": 40,
      "properties": {
      },
      "seeding_key": {
        "activity_section.key": "50fee6c1-86e2-4025-a6ca-db7b2e3d7291",
        "lesson_activity.key": "3042ea20-9a72-4615-bdb7-430ec112dad2"
