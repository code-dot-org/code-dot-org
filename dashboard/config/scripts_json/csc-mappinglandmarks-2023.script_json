  "objectives": [
    {
      "key": "1a27b887-2aa2-4f1b-8137-85395c9d1db8",
      "properties": {
        "description": "Reflect on the role of a map maker to develop a deeper appreciation for the purpose and value of maps as tools for understanding the world around them."
      },
      "seeding_key": {
        "lesson.key": "Local Landmarks - Unplugged Project Planning",
        "objective.key": "1a27b887-2aa2-4f1b-8137-85395c9d1db8"
      }
    },
    {
      "key": "2a8ffef0-4b21-4dad-99b2-2cd1e48dbcbb",
      "properties": {
        "description": "Identify ways in which maps can vary based on the mapmaker's decisions."
      },
      "seeding_key": {
        "lesson.key": "Local Landmarks - Coding Your Project",
        "objective.key": "2a8ffef0-4b21-4dad-99b2-2cd1e48dbcbb"
      }
    },
    {
      "key": "46412626-979f-4311-a180-f4e43809b4db",
      "properties": {
        "description": "Demonstrate geographic knowledge, coding skills and creativity by coding an interactive map with personally relevant and important landmarks. "
      },
      "seeding_key": {
        "lesson.key": "Local Landmarks - Coding Your Project",
        "objective.key": "46412626-979f-4311-a180-f4e43809b4db"
      }
    },
    {
      "key": "80a85b7e-ae26-4786-98ed-9632de96ea24",
      "properties": {
        "description": "Given a series of maps showing different components, students will analyze and compare the different purposes of each map. "
      },
      "seeding_key": {
        "lesson.key": "US Landmarks",
        "objective.key": "80a85b7e-ae26-4786-98ed-9632de96ea24"
      }
    },
    {
      "key": "829ee795-fac0-4509-8b25-cd11f443e52e",
      "properties": {
        "description": "Demonstrate an understanding of the power of using code to make a map interactive, engaging users and enhance geographic understanding. "
      },
      "seeding_key": {
        "lesson.key": "Project Share Out",
        "objective.key": "829ee795-fac0-4509-8b25-cd11f443e52e"
      }
    },
    {
      "key": "84fb7001-86ee-4932-8d58-b6122037e146",
      "properties": {
        "description": "Learn the basics of Sprite Lab by creating multiple sprites, changing their location, setting their size property, making the sprite “speak,” assigning a behavior, and using the when touches event to make their program interactive."
      },
      "seeding_key": {
        "lesson.key": "US Landmarks",
        "objective.key": "84fb7001-86ee-4932-8d58-b6122037e146"
      }
    },
    {
      "key": "98520886-dfc7-4343-a7ba-c32e0a8b043f",
      "properties": {
        "description": "Students will develop a written plan outlining the steps they will take to code their digital mapping landmarks project, including identifying key features, planning the layout, and describing the purpose of their map."
      },
      "seeding_key": {
        "lesson.key": "Local Landmarks - Unplugged Project Planning",
        "objective.key": "98520886-dfc7-4343-a7ba-c32e0a8b043f"
      }
    },
    {
      "key": "ac701f5c-93be-48e3-b614-d5be26d5b11d",
      "properties": {
        "description": "Engage in a personal reflection on the coding process of creating a digital map, highlighting successes, challenges, and lessons learned. "
      },
      "seeding_key": {
        "lesson.key": "Project Share Out",
        "objective.key": "ac701f5c-93be-48e3-b614-d5be26d5b11d"
      }
    },
    {
      "key": "ade6631d-16d1-43dd-a955-09eba7039e81",
      "properties": {
        "description": "Write thoughtful observations and provide constructive evaluations of peer projects, offering feedback and suggestions for improvement. "
      },
      "seeding_key": {
        "lesson.key": "Project Share Out",
        "objective.key": "ade6631d-16d1-43dd-a955-09eba7039e81"
      }
    },
    {
      "key": "f5a42772-0ecb-42e3-825f-81dc236b9151",
      "properties": {
        "description": "Practice effective presentation strategies when showcasing the interactive map to peers, including clear communication of implemented features, the purpose, and the significance of the project. "
      },
      "seeding_key": {
        "lesson.key": "Project Share Out",
        "objective.key": "f5a42772-0ecb-42e3-825f-81dc236b9151"
      }
    },
    {
      "key": "f625be86-4b66-4421-8a7c-f724fd230eb5",
      "properties": {
        "description": "Utilize coding skills to incorporate text explanations within the interactive map to communicate the significance of landmarks."
      },
