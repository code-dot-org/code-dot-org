    {
      "key": "cb6f50e7-196f-4f13-b051-cae1bf20541d",
      "position": 1,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display:** Show \"Tour Guide Practice\" Slide",
        "name": "Tour Guide Practice"
      },
      "seeding_key": {
        "activity_section.key": "cb6f50e7-196f-4f13-b051-cae1bf20541d",
        "lesson_activity.key": "8e70c6b9-5326-4d17-a491-ece94bcbd540"
      }
