    {
      "key": "3bf87e35-9289-4b54-83c0-1b67ab5584d4",
      "position": 6,
      "properties": {
        "progression_name": "Skill Building"
      },
      "seeding_key": {
        "activity_section.key": "3bf87e35-9289-4b54-83c0-1b67ab5584d4",
