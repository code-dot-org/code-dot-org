    {
      "key": "05475623-0e75-4a94-b2c0-7eaac79bcb01",
      "position": 1,
      "properties": {
        "name": "Pair Pseudocode",
        "remarks": true,
        "description": "Today we’re going to continue expanding the class `PainterPlus`, but this time we’re going to add methods that allow the `Painter` to paint as they move in The Neighborhood.\n\nTo get started, we will plan out our methods using a technique called “pseudocode.”"
      },
      "seeding_key": {
        "activity_section.key": "05475623-0e75-4a94-b2c0-7eaac79bcb01",
        "lesson_activity.key": "7b0d38ba-abf4-4ad5-b274-5f3b7b5d0151"
      }
