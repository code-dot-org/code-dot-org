      "key": "1c664172-cfa9-40b8-8dca-e6f70822122e",
      "position": 13,
      "properties": {
        "remarks": true,
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> Great job, everyone! Here’s what the start of our code could look like."
      },
      "seeding_key": {
        "activity_section.key": "1c664172-cfa9-40b8-8dca-e6f70822122e",
