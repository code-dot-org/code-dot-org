    {
      "key": "106b304f-f337-4906-a992-e24ef3549fe0",
      "position": 10,
      "properties": {
        "description": "Thanks, everyone, for sharing out! In the future, we will learn the skill and tools necessary to implement scenarios one and two. For now, we will focus on scenario three.",
        "duration": "7",
        "name": "Update the Code",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "106b304f-f337-4906-a992-e24ef3549fe0",
