      "position": 1,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> Display: Upon entering the classroom, students should see the inspirational quote. Read the quote to your students.\n",
        "name": "Software Engineering Discussion"
      },
      "seeding_key": {
        "activity_section.key": "a0ab2dad-7ce5-42fa-8dfb-a5f40e522756",
