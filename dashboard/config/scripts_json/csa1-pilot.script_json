    {
      "key": "55b81395-0ba8-4be7-b7bd-4e8635fb24f2",
      "position": 2,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display:** Key Vocabulary\n\n---"
      },
      "seeding_key": {
        "activity_section.key": "55b81395-0ba8-4be7-b7bd-4e8635fb24f2",
