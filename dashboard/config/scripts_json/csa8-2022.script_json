    },
    {
      "key": "66c23a6c-ff86-48f6-af17-d31fef9075a0",
      "position": 2,
      "properties": {
        "description": "Group: Place students in pairs.\n\n<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Do This:** Have students brainstorm potential solutions for sorting the list."
      },
      "seeding_key": {
        "activity_section.key": "66c23a6c-ff86-48f6-af17-d31fef9075a0",
        "lesson_activity.key": "f73908e2-03cf-4e8f-b2c8-1c93d3d315a3"
