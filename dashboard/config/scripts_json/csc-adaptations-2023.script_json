      }
    },
    {
      "key": "6c95601d-7633-48e7-ade2-eed3f5512588",
      "position": 5,
      "properties": {
        "progression_name": "Skill Building"
      },
      "seeding_key": {
        "activity_section.key": "6c95601d-7633-48e7-ade2-eed3f5512588",
