      "position": 3,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display**: Show “Welcome to Sprite Lab” video",
        "progression_name": "Video: Hello World: Welcome to Sprite Lab"
      },
      "seeding_key": {
        "activity_section.key": "d8ad2443-d4db-413f-8b6e-deeb10d2c3b2",
