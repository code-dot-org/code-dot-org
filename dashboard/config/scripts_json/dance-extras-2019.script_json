    {
      "key": "16f14f63-53a6-4f79-b6cf-8aa9f3b22e69",
      "position": 3,
      "properties": {
        "description": "What's your favorite way to be creative?",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "16f14f63-53a6-4f79-b6cf-8aa9f3b22e69",
