    {
      "key": "90986348-020e-4b00-b92f-ba4160018feb",
      "position": 1,
      "properties": {
        "name": "Digital Sharing",
        "progression_name": "Digital Sharing - Unplugged Activity"
      },
      "seeding_key": {
        "activity_section.key": "90986348-020e-4b00-b92f-ba4160018feb",
