      "key": "a0f0bda0-512f-4e9c-8c28-1f749a2a7431",
      "position": 2,
      "properties": {
        "tips": [
          {
            "type": "teachingTip",
            "markdown": "Save 5 minutes or so at the end of the day to have students trade their project guide to look at each other’s work.  This will help make sure that nothing is omitted or overlooked.\r\n"
          }
        ]
      },
      "seeding_key": {
