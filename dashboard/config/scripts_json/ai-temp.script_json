    {
      "key": "d92e52fc-a258-4a74-bea6-15fd25fe9511",
      "position": 16,
      "properties": {
        "description": "<i class=\"fa fa-desktop\" aria-hidden=\"true\"></i>  **Code Studio:** Have students go to the next level in Code Studio.",
        "progression_name": "WeTube Video Recommendations (2.0)"
      },
      "seeding_key": {
        "activity_section.key": "d92e52fc-a258-4a74-bea6-15fd25fe9511",
