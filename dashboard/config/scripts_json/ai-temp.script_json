    {
      "key": "a8384314-5f97-47ca-b9b7-4fd43dcd8def",
      "position": 15,
      "properties": {
        "description": "<i class=\"fa fa-check-square-o\" aria-hidden=\"true\"></i> **Do This:** Have students work in pairs to construct their network. The rules for the network are displayed on the slides."
      },
      "seeding_key": {
        "activity_section.key": "a8384314-5f97-47ca-b9b7-4fd43dcd8def",
