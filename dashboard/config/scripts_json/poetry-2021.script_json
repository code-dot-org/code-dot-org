    {
      "key": "84fd2c9b-b3c7-4be6-ac46-dc6d947bb5d5",
      "position": 1,
      "properties": {
        "description": "Amazing work, coders! \n\nNext time, we’ll share the apps we developed with each other. \n",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "84fd2c9b-b3c7-4be6-ac46-dc6d947bb5d5",
        "lesson_activity.key": "e865c776-4bdf-43d3-adc7-f3255fb464ef"
