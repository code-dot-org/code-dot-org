      "position": 1,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display**: Show “Level 1 - Free Play” slide\n",
        "name": "Code Your Own Time Capsule",
        "progression_name": "Free Play "
      },
      "seeding_key": {
