        "lesson_activity.key": "1ebe210d-17f1-4c61-9ebb-4bed4ba0ec4a"
      }
    },
    {
      "key": "2e04ca81-f474-45f4-8ed1-6420d3ec6d46",
      "position": 4,
      "properties": {
        "description": "####Work Time - 20 minutes"
      },
      "seeding_key": {
        "activity_section.key": "2e04ca81-f474-45f4-8ed1-6420d3ec6d46",
