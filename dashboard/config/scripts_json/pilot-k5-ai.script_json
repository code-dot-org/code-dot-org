      }
    },
    {
      "key": "6d472705-ea7f-44b7-a266-7a1c264f0e10",
      "position": 2,
      "properties": {
        "tips": [
          {
            "type": "teachingTip",
            "markdown": "**Professional Learning Opportunity**\n\nAre you interested in teaching your students more about AI? Consider engaging in our self-paced professional learning module, [Preparing to Teach AI and Machine Learning](https://studio.code.org/courses/self-paced-pl-aiml-2023)! This module will give you all the tools, skills, and knowledge you need to teach your students our AI and Machine Learning unit.\n"
          }
        ]
      },
      "seeding_key": {
        "activity_section.key": "6d472705-ea7f-44b7-a266-7a1c264f0e10",
