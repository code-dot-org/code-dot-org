  "lessons": [
    {
      "key": "Parameters and Return Explore",
      "name": "Parameters and Return Explore",
      "absolute_position": 1,
      "lockable": false,
      "has_lesson_plan": true,
      "relative_position": 1,
      "properties": {
        "creative_commons_license": "Creative Commons BY-NC-SA",
        "overview": "Students work with envelopes and paper to model functions with parameters and return values. Students create their own physical function envelope for drawing a house that takes in different parameters, and then build another function to calculate and return the cost of building that house.  ",
        "preparation": "* Collect various sizes of sticky notes. Small sticky notes are particularly useful for this activity. \n* Two envelopes per group\n* Pencils/pens\n* One baggy for a teacher demonstration (optional)\n* Print out copies of the Activity Guide for students\n* Review the Unit 7, Lesson 1 slides prior to the class\n- Check the [\"Teacher's Lounge\"](https://forum.code.org/t/resource-hub-parameters-and-return-values-lessons-1-4/36484) forum for verified teachers to find additional strategies or resources shared by fellow teachers\n- If you are teaching virtually, consider checking our [Virtual Lesson Modifications](https://docs.google.com/document/d/104BvGOZDwzVgA2Kxv1sKOg-8DkGDHAp6rTPGTozhvm4/edit?usp=sharing)",
        "student_overview": "Develop a mental model for how functions can be generalized using parameters and return values.",
        "unplugged": true
      },
      "seeding_key": {
        "lesson.key": "Parameters and Return Explore",
        "lesson_group.key": "csp_parameters_return_values",
        "script.name": "csp7-2023"
      }
    },
    {
