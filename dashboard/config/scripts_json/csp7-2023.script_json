    {
      "key": "90deef45-b3da-4981-9b18-1da0bb7348e1",
      "position": 2,
      "properties": {
        "description": "Today we are going to learn a new way to share code.",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "90deef45-b3da-4981-9b18-1da0bb7348e1",
