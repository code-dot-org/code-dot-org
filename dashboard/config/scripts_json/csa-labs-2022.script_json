    {
      "key": "51fd9eef-c950-464a-ac90-f6a1e8bba02d",
      "position": 1,
      "properties": {
        "tips": [
          {
            "type": "teachingTip",
            "markdown": "This section can take anywhere from 1 to 2 hours, depending on how well students can read the starter code, their knowledge of the `String` class, and the depth in which they want to explore possible additional ways to enrich the Magpie chatbot."
          }
        ]
      },
      "seeding_key": {
        "activity_section.key": "51fd9eef-c950-464a-ac90-f6a1e8bba02d",
