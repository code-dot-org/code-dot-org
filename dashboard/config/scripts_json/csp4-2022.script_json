    {
      "key": "dd8900c3-7c2c-4c29-83dc-f16298949232",
      "position": 2,
      "properties": {
        "description": "Have you ever been stuck trying to make a decision? What movie should we watch? Where should we go for lunch? How many chocolate bars can I buy? For the next three days you will build an app to help people make a decision.",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "dd8900c3-7c2c-4c29-83dc-f16298949232",
        "lesson_activity.key": "2f0aa596-12c0-44f9-b99f-76de635f2dca"
