    {
      "key": "3595c543-ce81-417d-a2f3-3f2e0313de0a",
      "position": 1,
      "properties": {
        "description": "**Review:** Review the problem-solving process framework: Define, Prepare, Try, Reflect",
        "name": "Problem-Solving Process Review\r"
      },
      "seeding_key": {
        "activity_section.key": "3595c543-ce81-417d-a2f3-3f2e0313de0a",
