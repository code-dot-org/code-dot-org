    },
    {
      "key": "aab7c516-a05f-4296-a1fb-55c500d57c64",
      "position": 5,
      "properties": {
        "description": "**Share:** Have students share their content ideas with a neighbor."
      },
      "seeding_key": {
        "activity_section.key": "aab7c516-a05f-4296-a1fb-55c500d57c64",
        "lesson_activity.key": "2a61bb87-38d7-4772-b57a-23949dce5cef"
