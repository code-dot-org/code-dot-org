    {
      "key": "979f4d5e-5b36-42a7-9ca4-dccebb1bc076",
      "position": 2,
      "properties": {
        "name": "Getting Loopy"
      },
      "seeding_key": {
        "activity_section.key": "979f4d5e-5b36-42a7-9ca4-dccebb1bc076",
