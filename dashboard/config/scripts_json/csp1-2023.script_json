    {
      "key": "527c496d-e333-44cf-b4e8-b368a4573bda",
      "position": 1,
      "properties": {
        "description": "This list secretly represents four things we experience every year. This list is not complete - there are others that we could add to it.",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "527c496d-e333-44cf-b4e8-b368a4573bda",
