    {
      "key": "2613a0c9-920e-401e-a65c-c7ad13b6ed8d",
      "position": 1,
      "properties": {
        "description": "Tomorrow we will finish our position posters.",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "2613a0c9-920e-401e-a65c-c7ad13b6ed8d",
        "lesson_activity.key": "14dc2ad8-b574-4575-825b-1cf38edb6f53"
