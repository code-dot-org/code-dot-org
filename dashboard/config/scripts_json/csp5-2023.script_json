      "position": 2,
      "properties": {
        "description": "Now let's build this app. The screen has been set up for you - it's your job to add the code!",
        "name": "Make the Random Forecaster App",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "956d6fc3-8503-4844-960a-c220a854e94f",
