    {
      "key": "617bb2db-25b8-4e5a-acf3-cdefab92bb55",
      "position": 1,
      "properties": {
        "description": "Look at the following code segments and tell me what is missing. You are looking at the syntax, not the functionality.",
        "name": "What's Missing?",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "617bb2db-25b8-4e5a-acf3-cdefab92bb55",
