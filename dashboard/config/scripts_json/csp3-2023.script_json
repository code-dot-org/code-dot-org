    {
      "key": "ac870811-5823-4776-afb3-b698ed6c9c26",
      "position": 5,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i>  **Code Studio:** Direct students to continue working on their app. When they are finished, they should hit \"Submit\" to turn in their app.",
        "progression_name": "Submit Your App"
      },
      "seeding_key": {
        "activity_section.key": "ac870811-5823-4776-afb3-b698ed6c9c26",
