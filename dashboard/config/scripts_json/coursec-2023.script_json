    {
      "key": "9a4a0cca-932f-4afa-9abc-1756bf1c47e6",
      "position": 2,
      "properties": {
        "description": "<img alt=\"Robot Symbols\" src=\"https://curriculum.code.org/media/uploads/Screenshot-2018-03-13-19.49.03.png\" style=\"float: left; max-width: 200px; margin: 10px;\">"
