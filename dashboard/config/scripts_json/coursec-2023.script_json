    {
      "key": "a7bc4075-a961-45ae-b854-221fac83cd1b",
      "position": 1,
      "properties": {
        "name": "The Big Event",
        "progression_name": "The Big Event - Unplugged Activity"
      },
      "seeding_key": {
        "activity_section.key": "a7bc4075-a961-45ae-b854-221fac83cd1b",
