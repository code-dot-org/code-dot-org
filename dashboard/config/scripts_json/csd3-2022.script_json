    {
      "key": "3d65a24a-c99b-400e-a632-249258bbe645",
      "position": 8,
      "properties": {
        "progression_name": "Review Your Game"
      },
      "seeding_key": {
        "activity_section.key": "3d65a24a-c99b-400e-a632-249258bbe645",
