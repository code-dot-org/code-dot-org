      "position": 1,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display:** Show the Agenda slide.",
        "name": "Fill-in-the-Blank Game"
      },
      "seeding_key": {
        "activity_section.key": "f3859f37-8de1-4f73-bfba-ef839cceb2be",
