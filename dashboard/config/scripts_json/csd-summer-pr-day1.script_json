    {
      "key": "3b715853-04ee-4532-b620-c1472a6d1cea",
      "position": 1,
      "properties": {
        "description": "**Group:** Place students in groups of 3 or 4",
        "name": "Computer or Not?\r"
      },
      "seeding_key": {
        "activity_section.key": "3b715853-04ee-4532-b620-c1472a6d1cea",
