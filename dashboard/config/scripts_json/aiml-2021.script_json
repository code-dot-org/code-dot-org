      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Question of the Day:**  What are different types of machine learning?"
      },
      "seeding_key": {
        "activity_section.key": "cc19a0fd-68ee-444b-b6ed-45ed116bcaa6",
        "lesson_activity.key": "dc28d486-0bc1-4918-a015-b8d71e42b86f"
