      "position": 1,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display:** Show the description for Hawa’s app from yesterday, the Club Recommender.",
        "name": "Journal"
      },
      "seeding_key": {
        "activity_section.key": "5a42b0c0-0dde-4ea0-bf7d-fb675fd69253",
        "lesson_activity.key": "f7b5e19b-8e99-4ca3-a9b8-bacbc648954a"
