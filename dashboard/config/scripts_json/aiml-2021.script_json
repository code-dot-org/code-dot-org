    {
      "key": "90bf3acf-cd19-469d-b6ec-0d5b3df4cffa",
      "position": 2,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Do This:** Students should analyze the results of their survey and record their answers in their project guide.",
        "name": "Step 5 - Data Reflection"
      },
      "seeding_key": {
        "activity_section.key": "90bf3acf-cd19-469d-b6ec-0d5b3df4cffa",
