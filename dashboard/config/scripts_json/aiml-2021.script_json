    },
    {
      "key": "e6cfe656-09a6-47d0-8fce-aeb2b9af0c80",
      "position": 23,
      "properties": {
        "description": "Because we decided to reject our last app, we’re now looking to buy a new one. This is the model card for the new app - let’s look over the details and see if we can recommend using this app in our hospital.",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "e6cfe656-09a6-47d0-8fce-aeb2b9af0c80",
        "lesson_activity.key": "29223996-2e3c-443c-aaf8-2cb97474ee10"
      }
    },
