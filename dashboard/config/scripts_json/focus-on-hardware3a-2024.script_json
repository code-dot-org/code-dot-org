    {
      "key": "083c0312-4ade-4b61-84d8-c67eb84c43a0",
      "position": 2,
      "properties": {
        "description": "**Share Out:** Have students share with a partner, then share as a full group"
      },
      "seeding_key": {
        "activity_section.key": "083c0312-4ade-4b61-84d8-c67eb84c43a0",
        "lesson_activity.key": "5707951c-9c5d-4a37-9024-7627c4418298"
