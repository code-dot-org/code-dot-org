    {
      "key": "e42f0b2d-41d8-4cd9-becd-860f3b1cb8ad",
      "position": 2,
      "properties": {
        "description": "**Groups**: Place students in groups of three or four.",
        "name": "Research and Reflect\r"
      },
      "seeding_key": {
        "activity_section.key": "e42f0b2d-41d8-4cd9-becd-860f3b1cb8ad",
