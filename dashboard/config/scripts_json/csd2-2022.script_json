    {
      "key": "f36ad856-4c75-4c2c-a1c1-a618b90319ee",
      "position": 7,
      "properties": {
        "description": "Before students review each other's work, each student should fill out the top of the guide with what they are most interested in feedback on.",
        "name": "Pre-Review\r"
      },
      "seeding_key": {
        "activity_section.key": "f36ad856-4c75-4c2c-a1c1-a618b90319ee",
