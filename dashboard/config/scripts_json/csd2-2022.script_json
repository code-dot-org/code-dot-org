      }
    },
    {
      "key": "9f41899b-b6c3-4bac-8af2-1a55ec025c8a",
      "position": 9,
      "properties": {
        "description": "Students then use the \"I like, I wish, What if\" structure to provide more open-ended feedback to each other. Again, some classes may need specific support in providing constructive feedback.",
        "name": "Free Response Feedback\r"
      },
      "seeding_key": {
        "activity_section.key": "9f41899b-b6c3-4bac-8af2-1a55ec025c8a",
