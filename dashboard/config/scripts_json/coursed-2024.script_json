    {
      "key": "98a4c619-a04f-40c9-9553-c37bcf6e9f64",
      "position": 1,
      "properties": {
        "name": "Online Puzzles",
        "tips": [
          {
            "type": "teachingTip",
            "markdown": "Show the students the right way to help classmates:\r\n\r\n - Donâ€™t sit in the classmateâ€™s chair\r\n - Donâ€™t use the classmateâ€™s keyboard\r\n - Donâ€™t touch the classmateâ€™s mouse\r\n - Make sure the classmate can describe the solution to you out loud before you walk away\r\n\r\n"
          }
        ]
      },
      "seeding_key": {
        "activity_section.key": "98a4c619-a04f-40c9-9553-c37bcf6e9f64",
