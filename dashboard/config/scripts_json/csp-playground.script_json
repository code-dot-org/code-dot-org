      "properties": {
        "tips": [
          {
            "type": "teachingTip",
            "markdown": "**Short Intro:** The Warm Up today is short and light. Students should spend the maximum amount of time working on their projects. \n"
          }
        ]
      },
      "seeding_key": {
        "activity_section.key": "0ff4e75a-f8f6-4086-8537-4ddd14a711a1",
