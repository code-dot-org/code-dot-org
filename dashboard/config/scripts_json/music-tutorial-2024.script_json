  "lessons": [
    {
      "key": "lesson-1",
      "name": "Music Lab Intro Tutorial ",
      "absolute_position": 1,
      "lockable": false,
      "has_lesson_plan": true,
      "relative_position": 1,
      "properties": {
        "overview": "In this interactive lesson, students will use Music Lab, a block-based IDE, to learn about events, functions, and loops while creating unique sounds, composing songs, and performing live music. They will utilize sound generation, loops, functions, and events to craft and share a unique composition, developing foundational coding skills and an appreciation for the connection between music and technology."
      },
      "seeding_key": {
        "lesson.key": "lesson-1",
        "lesson_group.key": "",
        "script.name": "music-tutorial-2024"
      }
    }
  ],
