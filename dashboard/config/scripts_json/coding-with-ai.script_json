    {
      "key": "37998809-f639-4c4a-8e2d-0468a3db3591",
      "position": 1,
      "properties": {
        "description": "Every app or software we use can be improved. Sometimes, the best ideas for new features come from regular users like you!",
        "name": "What's Missing?",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "37998809-f639-4c4a-8e2d-0468a3db3591",
