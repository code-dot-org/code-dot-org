        "lesson_activity.key": "63c9e4fc-de4f-4068-a017-5e259380d2f1"
      }
    },
    {
      "key": "01101455-24b3-4a6b-87df-dabcd6060126",
      "position": 8,
      "properties": {
        "description": "<i class=\"fa fa-check-square-o\" aria-hidden=\"true\"></i> **Do This:** Introduce the Question of the Day."
      },
      "seeding_key": {
        "activity_section.key": "01101455-24b3-4a6b-87df-dabcd6060126",
        "lesson_activity.key": "63c9e4fc-de4f-4068-a017-5e259380d2f1"
      }
