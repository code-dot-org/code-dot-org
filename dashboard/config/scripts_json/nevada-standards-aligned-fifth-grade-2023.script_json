    {
      "key": "4e30df0e-f902-451d-bf8d-2befd3726dcc",
      "position": 2,
      "properties": {
        "description": "One way to store things is with labeled containers.",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "4e30df0e-f902-451d-bf8d-2befd3726dcc",
