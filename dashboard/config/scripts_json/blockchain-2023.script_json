    },
    {
      "key": "11b1d5c5-7b81-4a4e-bd16-9598ca92c299",
      "position": 6,
      "properties": {
        "description": "<i class=\"fa fa-file-text-o\" aria-hidden=\"true\"></i> **Distribute:** Give each group a set of\n[r asset_cards/blockchain/2023]."
      },
      "seeding_key": {
        "activity_section.key": "11b1d5c5-7b81-4a4e-bd16-9598ca92c299",
