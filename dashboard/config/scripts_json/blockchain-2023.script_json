      "position": 2,
      "properties": {
        "description": "<i class=\"fa fa-video-camera\" aria-hidden=\"true\"></i> **Video:**  Show students the [r how_blockchain_works_marketplace_price/blockchain/2023] video"
      },
      "seeding_key": {
        "activity_section.key": "098281b0-6bb1-438e-b077-5ee372e1090a",
