    {
      "key": "77d7cfad-7b3f-421b-9bb1-a5d7e95e1b6f",
      "position": 39,
      "properties": {
        "description": "We’ve reached the halfway point of our simulation. Let’s pause and reflect on how things are going.",
        "duration": "5",
        "name": "Pause for Reflection",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "77d7cfad-7b3f-421b-9bb1-a5d7e95e1b6f",
