      "key": "099593d7-8030-40b9-893d-365e9eb7c12d",
      "position": 5,
      "properties": {
        "description": "Text segmentation is a technique used in natural language processing to identify individual words in a sentence and break up paragraphs into individual sentences.",
        "duration": "15",
        "name": "NLP Techniques",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "099593d7-8030-40b9-893d-365e9eb7c12d",
