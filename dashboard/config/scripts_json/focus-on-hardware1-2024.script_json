    {
      "key": "dd33ac28-7aa4-4ada-a5da-aa8e04f24960",
      "position": 1,
      "properties": {
        "description": "**Group**: Put students in groups of 2 or 3.",
        "name": "Building a Newspaper Table\r"
      },
      "seeding_key": {
        "activity_section.key": "dd33ac28-7aa4-4ada-a5da-aa8e04f24960",
