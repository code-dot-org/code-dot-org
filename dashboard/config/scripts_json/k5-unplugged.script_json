      }
    },
    {
      "key": "b5923773-82e5-4713-a610-81db9bce7201",
      "position": 2,
      "properties": {
        "progression_name": "The Internet - Student Resources"
      },
      "seeding_key": {
        "activity_section.key": "b5923773-82e5-4713-a610-81db9bce7201",
