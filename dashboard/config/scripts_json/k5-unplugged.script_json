        "lesson_activity.key": "9956810b-4d43-4b89-bf82-ef4c4b596714"
      }
    },
    {
      "key": "1c1ea39e-fb0f-426e-8c63-6eb0418b3db3",
      "position": 3,
      "properties": {
        "tips": [
