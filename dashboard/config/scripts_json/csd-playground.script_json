    {
      "key": "66004f4a-ccb5-4ae1-bd04-c491c7be428e",
      "position": 2,
      "properties": {
        "progression_name": "Predict"
      },
      "seeding_key": {
        "activity_section.key": "66004f4a-ccb5-4ae1-bd04-c491c7be428e",
