      }
    },
    {
      "key": "7b1a9caf-8cc2-4287-b479-f23f02b7f0bf",
      "position": 2,
      "properties": {
        "progression_name": "Predict"
      },
      "seeding_key": {
        "activity_section.key": "7b1a9caf-8cc2-4287-b479-f23f02b7f0bf",
