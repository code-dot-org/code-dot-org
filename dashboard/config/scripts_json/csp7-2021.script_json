    {
      "key": "b1e4ffb3-ffc0-4254-afc7-ee92f94f50be",
      "position": 2,
      "properties": {
        "description": "Today we are going to learn a new way to share code.",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "b1e4ffb3-ffc0-4254-afc7-ee92f94f50be",
