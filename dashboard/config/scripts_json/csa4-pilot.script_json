    {
      "key": "d49d5c73-f08b-4af3-8f00-8c24ca8e6688",
      "position": 2,
      "properties": {
        "description": "Programming is all about testing and comparing values to make decisions about actions to take. Relational operators help us evaluate values to make these decisions.",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "d49d5c73-f08b-4af3-8f00-8c24ca8e6688",
        "lesson_activity.key": "4b423dad-bc00-4e13-9c54-6b81cf366aa6"
