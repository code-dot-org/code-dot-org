      "position": 5,
      "properties": {
        "description": "Today, our goal is to explore the relationship between reference variables and objects and how Java references and accesses data in a computer. ",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "1d548755-2e08-46eb-ba70-fa3fd91d70f8",
        "lesson_activity.key": "a64e75c8-d839-4ba0-afb3-7edbe024305a"
