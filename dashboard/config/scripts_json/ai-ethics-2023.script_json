      "properties": {
        "description": "<i class=\"fa fa-refresh\" aria-hidden=\"true\"></i> **Circulate:** Monitor students as they have their discussions and come up with their recommendations."
      },
      "seeding_key": {
        "activity_section.key": "5417593d-f1ca-4856-b6fd-40244bb8d194",
