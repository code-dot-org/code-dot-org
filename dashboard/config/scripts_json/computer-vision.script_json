    {
      "key": "cd20d5bf-cd24-4d04-9be1-5b7f720c7967",
      "position": 3,
      "properties": {
        "description": "It is now your turn to train your classmates! When you get your object card, make sure you do NOT show anyone! Keep this a secret!",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "cd20d5bf-cd24-4d04-9be1-5b7f720c7967",
        "lesson_activity.key": "7bb3709f-a535-4b3a-96a9-a0e981cb9979"
      }
