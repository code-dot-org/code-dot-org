      "key": "78aa2079-ec2a-438a-90cd-fc04c7fd1a89",
      "position": 8,
      "properties": {
        "tips": [
          {
            "key": "tip",
            "type": "teachingTip",
            "markdown": "Show the students the *right* way to help classmates by:\r\n\r\n - Don't sit in their chair\r\n - Don't use their keyboard\r\n - Don't touch their mouse\r\n - Make sure the classmate can describe the solution before you walk away\r\n"
          }
        ]
      },
      "seeding_key": {
        "activity_section.key": "78aa2079-ec2a-438a-90cd-fc04c7fd1a89",
