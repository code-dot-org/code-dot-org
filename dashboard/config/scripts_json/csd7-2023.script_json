    },
    {
      "key": "9543c55c-399d-4753-988b-cf83615b0024",
      "position": 1,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Code Studio:** Have students log into Code Studio. The second level contains an app that they will use for the first part of this lesson.",
        "duration": "15",
        "name": "Medical Priority App"
      },
      "seeding_key": {
        "activity_section.key": "9543c55c-399d-4753-988b-cf83615b0024",
        "lesson_activity.key": "d405fe8d-f5de-4678-af4e-974a96c56543"
      }
    },
    {
      "key": "7e3379a4-1acb-46f4-86f8-982b5569cd50",
      "position": 2,
      "properties": {
        "progression_name": "Medical Priority App"
      },
      "seeding_key": {
        "activity_section.key": "7e3379a4-1acb-46f4-86f8-982b5569cd50",
        "lesson_activity.key": "d405fe8d-f5de-4678-af4e-974a96c56543"
