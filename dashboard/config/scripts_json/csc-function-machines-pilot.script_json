    {
      "key": "799267b4-5398-46c3-a1eb-b4bda4e11632",
      "position": 1,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display**: Show “Caught!” slide\n",
        "name": "Reflect"
      },
      "seeding_key": {
        "activity_section.key": "799267b4-5398-46c3-a1eb-b4bda4e11632",
