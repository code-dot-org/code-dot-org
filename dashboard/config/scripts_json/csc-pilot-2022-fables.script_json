      "properties": {
        "tips": [
          {
            "type": "teachingTip",
            "markdown": "In the next level, students will begin to program their own fables. Point out to students that each of these examples features characters saying one of the \"big ideas\"."
          }
        ]
      },
      "seeding_key": {
        "activity_section.key": "10b09bde-7204-440f-b04b-5943116e33ee",
