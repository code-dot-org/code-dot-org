    {
      "key": "565aadde-fc4e-4baf-a8db-55f1ad511827",
      "position": 4,
      "properties": {
        "description": "Now, that you have a better understanding of the content of your article, you're going to re-read the article to determine if our world is in a better or worse place.",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "565aadde-fc4e-4baf-a8db-55f1ad511827",
        "lesson_activity.key": "fdab9505-ed27-4b09-8ea2-85e2f3b3378f"
      }
