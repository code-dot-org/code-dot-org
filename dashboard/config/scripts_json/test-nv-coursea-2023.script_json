    {
      "key": "8eb80fb2-ad09-4ece-a9ce-bdba14dc87c8",
      "position": 1,
      "properties": {
        "name": "Happy Loops",
        "progression_name": "Happy Loops Unplugged Activity"
      },
      "seeding_key": {
        "activity_section.key": "8eb80fb2-ad09-4ece-a9ce-bdba14dc87c8",
