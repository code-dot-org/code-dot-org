    {
      "key": "3e9efa37-f2c4-4dc6-b02f-e476df38465f",
      "position": 4,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display**: Show “Planning” slide"
      },
      "seeding_key": {
        "activity_section.key": "3e9efa37-f2c4-4dc6-b02f-e476df38465f",
