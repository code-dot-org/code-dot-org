    {
      "key": "d806a931-3b88-45bb-8c64-c5aff16c5826",
      "position": 2,
      "properties": {
        "progression_name": "Wrap Up"
      },
      "seeding_key": {
        "activity_section.key": "d806a931-3b88-45bb-8c64-c5aff16c5826",
        "lesson_activity.key": "f7ec8244-a0ad-4d0b-8574-2ca7cdf3a160"
      }
