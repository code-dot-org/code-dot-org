  "lessons": [
    {
      "key": "lesson-1",
      "name": "Reviews and Sentiment Value",
      "absolute_position": 1,
      "lockable": false,
      "has_lesson_plan": true,
      "relative_position": 1,
      "properties": {
        "overview": "This activity introduces students to the sentiment value of words. A word can have a positive or negative connotation based on the context with which it is often used. Before students start working with the `Review` class, it is helpful for them to read a few reviews.",
        "student_overview": "The persuasive power of words can be seen in a variety of places such as entertainment, news, social media, and even reviews and comments. This activity focuses on the sentiment value of individual words, and you will start by reading a few online reviews of your choice."
      },
      "seeding_key": {
        "lesson.key": "lesson-1",
        "lesson_group.key": "",
        "script.name": "csa-consumer-review-lab"
      }
    },
    {
      "key": "lesson-2",
      "name": "Sentiment Value and Star Ratings",
      "absolute_position": 2,
      "lockable": false,
      "has_lesson_plan": true,
      "relative_position": 2,
      "properties": {
        "overview": "Now that students have read several reviews and started exploring the `sentimentVal()` method, students will use this method to determine the overall sentiment of an entire review.",
        "student_overview": "Now that you have read several reviews and started exploring the `sentimentVal()` method, you will use this method to determine the overall sentiment of an entire review."
      },
      "seeding_key": {
        "lesson.key": "lesson-2",
        "lesson_group.key": "",
        "script.name": "csa-consumer-review-lab"
      }
    },
    {
      "key": "lesson-3",
      "name": "Autogenerated Review",
      "absolute_position": 3,
      "lockable": false,
      "has_lesson_plan": true,
      "relative_position": 3,
      "properties": {
        "overview": "In this activity, students will write and test a method that will autogenerate a review from the review used in the previous lesson. The method will create a new fake review by replacing adjectives in the given review with randomly selected adjectives.",
        "student_overview": "In this activity, you will write and test a method that will autogenerate a review based on the review used in the previous lesson. The method will autogenerate a review by replacing adjectives in the given review with randomly selected adjectives from a list."
      },
      "seeding_key": {
        "lesson.key": "lesson-3",
        "lesson_group.key": "",
        "script.name": "csa-consumer-review-lab"
      }
