    {
      "key": "78c16cd4-9a8a-4712-b921-e76ae3095ab3",
      "position": 4,
      "properties": {
        "description": " <i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display**: Show “Lesson Extras” slide\n**Lesson Extras**",
        "progression_name": "Lesson Extra"
      },
      "seeding_key": {
        "activity_section.key": "78c16cd4-9a8a-4712-b921-e76ae3095ab3",
