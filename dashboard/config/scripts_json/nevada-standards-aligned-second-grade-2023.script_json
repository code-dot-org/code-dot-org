    {
      "key": "aa0c064b-7a82-483e-afe1-b59ceaaddc2d",
      "position": 3,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display**: Show “Level 2-7 - Create Your Project” slide",
        "progression_name": "Project"
      },
      "seeding_key": {
