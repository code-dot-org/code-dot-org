    },
    {
      "key": "3adcc7f4-8fcc-4738-88c5-a621ff09cb08",
      "position": 2,
      "properties": {
        "description": "**Share Out:** Have students share with a partner, then share as a full group"
      },
      "seeding_key": {
        "activity_section.key": "3adcc7f4-8fcc-4738-88c5-a621ff09cb08",
        "lesson_activity.key": "7b73b3f1-50b0-43a0-b986-6ca761f73d6f"
