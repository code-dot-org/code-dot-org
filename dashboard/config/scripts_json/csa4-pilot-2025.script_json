      "key": "bf0977c8-dd72-4f2f-a8d4-38a37f8317e3",
      "position": 5,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Do This:** Introduce `Math.random()`.",
        "duration": "15",
        "name": "Random Numbers"
      },
      "seeding_key": {
        "activity_section.key": "bf0977c8-dd72-4f2f-a8d4-38a37f8317e3",
