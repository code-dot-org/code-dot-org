  "lessons": [
    {
      "key": "The AP CSA Exam",
      "name": "The AP CSA Exam",
      "absolute_position": 1,
      "lockable": false,
      "has_lesson_plan": true,
      "relative_position": 1,
      "properties": {
        "overview": "Students learn about the structure and expectations of the AP CSA Exam. In the process, students identify and answer questions about the exam. Students explore test-taking strategies and study resources that they can use to prepare for the exam.",
        "preparation": "* Print copies of the Course Exam Description (one for each student)\n* Print copies of the Java Quick Reference (one for each student)\n* Print copies of the Exploring Exam Expectations handout (one for each student)\n* Gather three sticky notes for each student\n* Check the [**Teacher's Lounge**](https://forum.code.org/t/csa-unit-9-lesson-1-the-ap-csa-exam/36306/2) for verified teachers on the CSA Forum to find additional strategies or resources shared by fellow teachers",
        "student_overview": "You did it! You have completed the content for the course, and now it is time to prepare for the AP CSA Exam. In the process, you identify and answer questions you may have about the exam. You explore test-testing strategies and study resources that you can use to prepare for the exam."
      },
      "seeding_key": {
        "lesson.key": "The AP CSA Exam",
        "lesson_group.key": "lessonGroup-2",
        "script.name": "csa9-2025"
      }
    },
    {
      "key": "MCQ Pre-Assessment",
      "name": "MCQ Pre-Assessment",
      "absolute_position": 2,
      "lockable": false,
      "has_lesson_plan": true,
      "relative_position": 2,
      "properties": {
        "overview": "Students complete a Multiple-Choice Pre-Assessment on AP Classroom.",
        "preparation": "* Create a multiple-choice pre-assessment on AP Classroom as indicated on the Creating a Multiple-Choice Pre-Assessment resource\n* Print copies of the Java Quick Reference (one for each student)\n* Check the [**Teacher's Lounge**](https://forum.code.org/t/csa-unit-9-lesson-2-mcq-pre-assessment/36305/2) for verified teachers on the CSA Forum to find additional strategies or resources shared by fellow teachers",
        "student_overview": "To identify your current strengths and weaknesses on the multiple-choice portion of the AP CSA Exam, you take a Multiple-Choice Pre-Assessment on AP Classroom. Best of luck!"
      },
      "seeding_key": {
        "lesson.key": "MCQ Pre-Assessment",
        "lesson_group.key": "lessonGroup-2",
        "script.name": "csa9-2025"
      }
    },
