      "position": 1,
      "properties": {
        "description": "<i class=\"fa fa-list-alt\" aria-hidden=\"true\"></i> **Display**: Show “Level 1 - Sample Program” slide\n",
        "duration": "35",
        "name": "Code Your Fable"
      },
      "seeding_key": {
        "activity_section.key": "e5cd2cb7-0b2b-41b6-8f33-263e71b524a6",
        "lesson_activity.key": "c74598ed-6053-497e-b56e-61c00782aaf5"
      }
