      "position": 3,
      "properties": {
        "description": "**Do This**: Have students move to their computer and sign in. ",
        "progression_name": "Example"
      },
      "seeding_key": {
        "activity_section.key": "48a28d14-eb09-48e9-8fa5-9aed3259b86d",
        "lesson_activity.key": "c74598ed-6053-497e-b56e-61c00782aaf5"
