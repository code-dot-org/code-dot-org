<?xml version="1.0"?>
<block type="behavior_definition" deletable="false" movable="false" editable="false">
  <mutation>
    <arg name="this sprite" type="Sprite"/>
    <description>Maps a sprite's x position to a given sound energy range</description>
  </mutation>
  <title name="NAME">mapping my x position to</title>
  <statement name="STACK">
    <block type="controls_if">
  <mutation elseif="1" else="1"/>
  <value name="IF0">
    <block type="logic_compare">
      <title name="OP">EQ</title>
      <value name="A">
        <block type="parameters_get">
          <mutation output="String"/>
          <title name="VAR">range</title>
        </block>
      </value>
      <value name="B">
        <block type="text">
          <title name="TEXT">low</title>
        </block>
      </value>
    </block>
  </value>
  <statement name="DO0">
    <block type="Dancelab_setProp">
      <title name="SPRITE">this_sprite</title>
      <title name="PROPERTY">"x"</title>
      <value name="VAL">
        <block type="Dancelab_getEnergy">
          <title name="RANGE">"low"</title>
        </block>
      </value>
    </block>
  </statement>
  <value name="IF1">
    <block type="logic_compare">
      <title name="OP">EQ</title>
      <value name="A">
        <block type="parameters_get">
          <mutation output="String"/>
          <title name="VAR">range</title>
        </block>
      </value>
      <value name="B">
        <block type="text">
          <title name="TEXT">med</title>
        </block>
      </value>
    </block>
  </value>
  <statement name="DO1">
    <block type="Dancelab_setProp">
      <title name="SPRITE">this_sprite</title>
      <title name="PROPERTY">"x"</title>
      <value name="VAL">
        <block type="Dancelab_getEnergy">
          <title name="RANGE">"mid"</title>
        </block>
      </value>
    </block>
  </statement>
  <statement name="ELSE">
    <block type="Dancelab_setProp">
      <title name="SPRITE">this_sprite</title>
      <title name="PROPERTY">"x"</title>
      <value name="VAL">
        <block type="Dancelab_getEnergy">
          <title name="RANGE">"high"</title>
        </block>
      </value>
    </block>
  </statement>
</block>
  </statement>
</block>
