    "start_sources": {
      "NeighborhoodRunner.java": {
        "text": "import org.code.neighborhood.*;\n\npublic class NeighborhoodRunner {\n  public static void main(String[] args) {\n\n    // Creates a MuralPainter object\n    MuralPainter backgroundPainter = new MuralPainter();\n\n    // Creates a DuckPainter object\n    DuckPainter duck = new DuckPainter();\n\n    // Paints the background for the mural\n    backgroundPainter.paintBackground(\"LightSkyBlue\", 13);\n\n    // Paints the duck\n    duck.paintDuck(\"yellow\", \"orange\", \"black\");\n    \n  }\n}",
        "isVisible": true,
        "tabOrder": 0
      },
      "PainterPlus.java": {
        "text": "import org.code.neighborhood.*;\n\npublic class PainterPlus extends Painter {\n\n  // Turns the Painter to the right\n  public void turnRight() {\n    turnLeft();\n    turnLeft();\n    turnLeft();\n  }\n\n  // Takes all of the paint from a paint bucket\n  public void takeAllPaint() {\n    while (isOnBucket()) {\n      takePaint();\n    }\n  }\n  \n}",
        "isVisible": true,
        "tabOrder": 1
      },
      "DuckPainter.java": {
        "text": "import org.code.neighborhood.*;\n\npublic class DuckPainter extends MuralPainter {\n\n  /*\n   * Paints a duck\n   */\n  public void paintDuck(String bodyColor, String accentColor, String eyeColor) {\n    getToStartBody();\n    paintBody(bodyColor);\n\n    resetPosition();\n\n    getToStartBeak();\n    paintBeak(accentColor);\n\n    turnAround();\n    resetPosition();\n\n    getToStartEyes();\n    paintEyes(eyeColor);\n\n    resetPosition();\n\n    getToStartFeet();\n    paintBothFeet(accentColor);\n\n    resetPosition();\n\n    getToStartWings();\n    paintBothWings(accentColor);\n\n    moveToCorner();\n  }\n\n  /*\n   * Moves the DuckPainter to the starting position to paint the body\n   */\n  public void getToStartBody() {\n    move();\n    move();\n    move();\n    move();\n    turnRight();\n    move();\n    turnLeft();\n  }\n\n  /*\n   * Paints the duck's body\n   */\n  public void paintBody(String color) {\n    paintLine(color, 5);\n    turnToWest();\n\n    paintLine(color, 7);\n    turnToEast();\n\n    paintLine(color, 9);\n    turnToWest();\n    move();\n\n    paintLine(color, 9);\n    turnToEast();\n    move();\n\n    paintLine(color, 9);\n    turnToWest();\n    move();\n\n    paintLine(color, 9);\n    turnToEast();\n    move();\n\n    paintLine(color, 9);\n    turnToWest();\n    move();\n\n    paintLine(color, 9);\n    turnToEast();\n    move();\n    move();\n\n    paintLine(color, 7);\n  }\n\n  /*\n   * Moves the DuckPainter to the starting position to paint the beak\n   */\n  public void getToStartBeak() {\n    move();\n    move();\n    move();\n    move();\n    turnRight();\n    move();\n    move();\n    move();\n    move();\n    turnLeft();\n  }\n\n  /*\n   * Paint a beak\n   */\n  public void paintBeak(String color) {\n    paintLine(color, 5);\n    turnToWest();\n    move();\n    move();\n\n    paintLine(color, 3);\n  }\n\n  /*\n   * Moves to the start position to the eyes\n   */\n  public void getToStartEyes() {\n    move();\n    move();\n    move();\n    move();\n    turnRight();\n    move();\n    move();\n    move();\n    turnLeft();\n  }\n\n  /*\n   * Paints the eyes\n   */\n  public void paintEyes(String color) {\n    paintLine(color, 1);\n\n    move();\n    move();\n    move();\n\n    paintLine(color, 1);\n  }\n\n  /*\n   * Moves the DuckPainter to the starting position to paint the feet\n   */\n  public void getToStartFeet() {\n    turnRight();\n\n    while (canMove()) {\n      move();\n    }\n\n    turnLeft();\n    move();\n    move();\n    move();\n    turnLeft();\n    move();\n    move();\n    turnLeft();\n    turnLeft();\n  }\n\n  /*\n   * Paints the duck's feet\n   */\n  public void paintBothFeet(String color) {\n    paintFoot(color);\n\n    turnLeft();\n    move();\n    turnRight();\n    move();\n    move();\n    move();\n    move();\n    turnRight();\n\n    paintFoot(color);\n  }\n\n  /*\n   * Paints a foot with color\n   */\n  public void paintFoot(String color) {\n    paintLine(color, 1);\n\n    turnRight();\n    move();\n    turnLeft();\n    turnLeft();\n\n    paintLine(color, 3);\n  }\n\n  /*\n   * Moves the DuckPainter to the starting position to paint the wings\n   */\n  public void getToStartWings() {\n    turnRight();\n    move();\n    move();\n    move();\n    move();\n    move();\n    turnLeft();\n  }\n\n  /*\n   * Moves the DuckPainter to the start position to paint the left wing,\n   * then move to the start position to paint the right wing\n   */\n  public void paintBothWings(String color) {\n    paintLeftWing(color);\n\n    move();\n    move();\n    move();\n    move();\n    move();\n    move();\n    move();\n    turnLeft();\n    move();\n    move();\n    turnRight();\n\n    paintRightWing(color);\n  }\n\n  /*\n   * Paints a wing on the left with color\n   */\n  public void paintLeftWing(String color) {\n    paintLine(color, 3);\n    turnToWest();\n    move();\n\n    paintLine(color, 2);\n    turnToEast();\n    move();\n    move();\n\n    paintLine(color, 1);\n  }\n\n  /*\n   * Paints a wing on the right with color\n   */\n  public void paintRightWing(String color) {\n    paintLine(color, 3);\n    turnToWest();\n    move();\n\n    paintLine(color, 2);\n    turnToEast();\n    move();\n\n    paintLine(color, 1);\n  }\n  \n}",
        "isVisible": true,
        "tabOrder": 3
