      "MyTheater.java": {
        "text": "import org.code.theater.Theater;\nimport org.code.theater.Stage;\nimport org.code.theater.Instrument;\nimport org.code.media.Color;\nimport org.code.media.Image;\nimport org.code.media.Pixel;\nimport java.io.FileNotFoundException;\n\npublic class MyTheater {\n  public static void main(String[] args) throws Exception {\n    // ----- Edit this to add your own image! -----\n  \tcreateMovie(true, \"dog.jpeg\");\n\n  \tTheater.stage.play();\n  }\n\n  private static double wholeNote = 2.856;\n  private static double halfNote = 1.428;\n  private static double quarterNote = 0.714;\n  private static double eigthNote = 0.357;\n  private static double sixteenthNote = 0.1785;\n  private static int g = 67;\n  private static int a = 69;\n  private static int b = 71;\n  private static int c = 72;\n  private static int d = 74;\n  private static int e = 76;\n  private static int f = 77;\n \n  public static void createMovie(boolean wearMedal, String imageName) throws FileNotFoundException {\n    double eigthNoteWithDecay = eigthNote + sixteenthNote;\n  \tStage stage = Theater.stage;\n    stage.removeFillColor();\n    stage.setStrokeWidth(5);\n  \n\t// First sequence and rings\n    stage.pause(eigthNote);\n    stage.playNote(Instrument.PIANO, g, eigthNoteWithDecay);\n    stage.setStrokeColor(Color.BLUE);\n    stage.drawEllipse(40, 50, 100, 100);\n    stage.pause(eigthNote);\n    stage.playNote(Instrument.PIANO, c, eigthNoteWithDecay);\n    stage.setStrokeColor(Color.BLACK);\n    stage.drawEllipse(150, 50, 100, 100);\n    stage.pause(eigthNote);\n    stage.playNote(Instrument.PIANO, e, eigthNoteWithDecay);\n    stage.setStrokeColor(Color.RED);\n    stage.drawEllipse(260, 50, 100, 100);\n    stage.pause(eigthNote);\n    stage.playNote(Instrument.PIANO, a, wholeNote+eigthNote);\n    stage.setStrokeColor(Color.GOLD);\n    stage.drawEllipse(94, 100, 100, 100);\n    stage.pause(eigthNote);\n    stage.setStrokeColor(Color.GREEN);\n    stage.drawEllipse(206, 100, 100, 100);\n    stage.pause(wholeNote);\n\n    // Second Note Sequence and face\n  \tImage im = new Image(imageName);\n  \tstage.drawImage(im, 110, 210, 180, 180, 0);\n  \tstage.playNote(Instrument.PIANO, a, eigthNoteWithDecay);\n    stage.pause(eigthNote);\n  \tstage.playNote(Instrument.PIANO, b, eigthNoteWithDecay);\n    stage.pause(eigthNote);\n  \tstage.playNote(Instrument.PIANO, c, eigthNoteWithDecay);\n    stage.pause(eigthNote);\n  \tstage.playNote(Instrument.PIANO, f, eigthNoteWithDecay);\n    stage.pause(eigthNote);\n\n    // Draw medal\n    if (wearMedal) {\n      stage.setStrokeWidth(2);\n      stage.setStrokeColor(Color.GRAY);\n      stage.setFillColor(Color.NAVY);\n      stage.drawShape(new int[]{154,310,194,360,206,360,166,310},true);\n      stage.drawShape(new int[]{234,310,194,360,206,360,246,310},true);\n\t  stage.setStrokeWidth(2);\n      stage.setStrokeColor(Color.GRAY);\n      stage.setFillColor(Color.GOLD);\n      stage.drawEllipse(186, 360, 28, 28);\n    } else {\n      stage.setStrokeWidth(2);\n      stage.setStrokeColor(Color.GRAY);\n      stage.setFillColor(Color.NAVY);\n      stage.drawShape(new int[]{124,226,124,270,136,270,136,226},true);\n\t  stage.setStrokeWidth(2);\n      stage.setStrokeColor(Color.GRAY);\n      stage.setFillColor(Color.GOLD);\n      stage.drawEllipse(116, 260, 28, 28); \n    }\n    \n  \tstage.playNote(Instrument.PIANO, e, sixteenthNote);\n    stage.pause(sixteenthNote);\n  \tstage.playNote(Instrument.PIANO, d, sixteenthNote);\n    stage.pause(sixteenthNote);\n  \tstage.playNote(Instrument.PIANO, e, eigthNoteWithDecay);\n    stage.pause(eigthNote);\n  \tstage.playNoteAndPause(Instrument.PIANO, c, eigthNote+wholeNote+halfNote+eigthNote);\n  }\n}\n",
