      "MyConsole.java": {
        "text": "import java.util.ArrayList;\n\npublic class MyConsole {\n  public static void main(String[] args) {\n\n    ArrayList<Book> bookList = new ArrayList<Book>();\n    ArrayList<Patron> patronList = new ArrayList<Patron>();\n\n    Book gatsby = new Book(\"The Great Gatsby\", \"F. Scott Fitzgerald\");\n    Book solitude = new Book(\"One Hundred Years of Solitude\", \"Gabriel García Márquez\");\n    Book braveNewWorld = new Book(\"Brave New World\", \"Aldous Huxley\");\n    Book jane = new Book(\"Jane Eyre\", \"Charlotte Bronte\");\n    Book crime = new Book(\"Crime and Punishment\", \"Fyodor Dostoevsky\");\n\n    bookList.add(gatsby);\n    bookList.add(solitude);\n    bookList.add(braveNewWorld);\n    bookList.add(jane);\n    bookList.add(crime);\n\n    Patron liz = new Patron(\"Liz Stanley\", \"145 Rhodes Shaw Ave\", solitude);\n    Patron joelle = new Patron(\"Joelle McRae\", \"1224 S 5th St\", jane);\n    Patron adam = new Patron(\"Adam Mateus\", \"6436 Hatchery Rd\", crime);\n    Patron sophie = new Patron(\"Sophie Martel\", \"8145 Timber Lane Trl\", gatsby);\n    Patron charles = new Patron(\"Charles Henryson\", \"49 S 2nd Ave\", braveNewWorld);\n\n    patronList.add(liz);\n    patronList.add(joelle);\n    patronList.add(adam);\n    patronList.add(sophie);\n    patronList.add(charles);\n\n    Librarian adele = new Librarian(\"Adele Goldberg\", bookList, patronList);\n    \n\n    Book newBook = adele.addNewBook();\n    System.out.println(\"\");\n    Patron newPatron = adele.addNewPatron(newBook);\n    System.out.print(\"\\nCurrent Status (all Patrons):\\n\");\n    System.out.println(adele);\n    \n  }\n}",
        "isVisible": true
      },
      "Book.java": {
        "text": "public class Book {\n\n  /**\n   * the title of the book\n   */\n  private String title;\n\n  /**\n   * the author of the book\n   */\n  private String author;\n\n  /**\n   * whether or not the book is checked out\n   */\n  private boolean isCheckedOut;\n\n  /**\n   * Creates a Book\n   * @param title the title of the book\n   * @param author the author of the book\n   */\n  public Book(String title, String author) {\n    this.title = title;\n    this.author = author;\n    isCheckedOut = false;\n  }\n\n  /**\n   * Gets the title of the book\n   * @return the title of the book\n   */\n  public String getTitle() {\n    return title;\n  }\n\n  /**\n   * Gets the book's author\n   * @return the book's author\n   */\n  public String getAuthor() {\n    return author;\n  }\n\n  /**\n   * Gets whether or not the book is checked out\n   * @return true if the book is checked out, otherwise false\n   */\n  public boolean hasBeenChecked() {\n    return isCheckedOut;\n  }\n\n  /**\n   * Changes whether or not the book is checked out\n   * @param newStatus true if the book is checked out, otherwise false\n   */\n  public void setHasBeenChecked(boolean newStatus) {\n    isCheckedOut = newStatus;\n  }\n\n  /**\n   * Gets the book's information\n   * @return a String containing the book's information\n   */\n  public String toString() {\n    return title + \" by \" + author;\n  }\n}",
        "isVisible": true
      },
      "Librarian.java": {
        "text": "import java.util.ArrayList;\nimport java.util.Scanner;\n\npublic class Librarian {\n\n  /**\n   * the first and last name of the librarian\n   */\n  private String name;\n  \n  /**\n   * the list of Books\n   */\n  private ArrayList<Book> bookList;\n\n  /**\n   * the list of Patrons\n   */\n  private ArrayList<Patron> patronList;\n\n  /**\n   * Creates a Librarian\n   * @param name the name of the librarian\n   * @param bookList the list of books\n   * @param patronList the list of patrons\n   */\n  public Librarian(String name, ArrayList<Book> bookList, ArrayList<Patron> patronList) {\n    this.name = name;\n    this.bookList = bookList;\n    this.patronList = patronList;\n  }\n\n  /**\n   * Allows the Lirarian to add a new Book.\n   * Once created, the book is added to the bookList.\n   * @return the newly create Book object.\n   */\n  public Book addNewBook(){\n    Book newBook;\n    Scanner sc = new Scanner(System.in);\n    System.out.println(\"To add a new book, please enter the following information:\");\n    System.out.print(\"Book Title: \");\n    String newTitle = sc.nextLine();\n    System.out.print(\"Book Author: \");\n    String newAuthor = sc.nextLine();\n    newBook = new Book(newTitle, newAuthor);\n    bookList.add(newBook);\n    sc.close();\n    return newBook;\n  }\n\n  /**\n   * Allows the Lirarian to add a new Patron.\n   * Once created, the Patron is added to the Patron List.\n   * @ param book the book to be checked out by new Patron.\n   * @return the newly create Patron object.\n   */\n  public Patron addNewPatron(Book book){\n    Patron newPatron;\n    Scanner sc = new Scanner(System.in);\n    System.out.println(\"To add a new Patron, please enter the following information:\");\n    System.out.print(\"Patron Name: \");\n    String newName = sc.nextLine();\n    System.out.print(\"Patron Address: \");\n    String newAddress = sc.nextLine();\n    newPatron = new Patron(newName, newAddress, book);\n    patronList.add(newPatron);\n    sc.close();\n    return newPatron;\n  }\n\n\n  /**\n   * Gets the information of all patrons in the list\n   * @return a String containing the information of all\n   *         patrons in the list\n   */\n  public String toString() {\n    String allPatrons = \"\";\n    \n    for (int index = 0; index < patronList.size(); index++) {\n      allPatrons = allPatrons + patronList.get(index) + \"\\n\";\n    }\n\n    return allPatrons;\n  }\n}",
        "isVisible": true
      },
      "Patron.java": {
        "text": "public class Patron {\n\n  /**\n   * the first and last name of the patron\n   */\n  private String name;\n\n  /**\n   * the address of the patron\n   */\n  private String address;\n\n  /**\n   * the book the patron has checked out\n   */\n  private Book currentBook;\n\n  /**\n   * Creates a Patron\n   * @param name the first and last name of the patron\n   * @param address the address of the patron\n   */\n  public Patron(String name, String address, Book currentBook) {\n    this.name = name;\n    this.address = address;\n    this.currentBook = currentBook;\n    this.currentBook.setHasBeenChecked(true);\n  }\n\n  /**\n   * Gets the patron's information\n   * @return a String containing the patron's information\n   */\n  public String toString() {\n    return name + \" (\" + address + \"): Current Book = \" + currentBook;\n  }\n}",
        "isVisible": true
      }
