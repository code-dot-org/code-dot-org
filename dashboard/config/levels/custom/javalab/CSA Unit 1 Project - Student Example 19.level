      "PainterPlus.java": {
        "text": "import org.code.neighborhood.Painter;\n\npublic class PainterPlus extends Painter {\n  public PainterPlus (int x, int y, String direction, int paint){\n    super(x, y, direction, paint);\n  }\n\n  // Turns the Painter to the right\n  public void turnRight(){\n    turnLeft();\n    turnLeft();\n    turnLeft();\n  }\n\n  // Turns the Painter around 180 degrees\n  public void turnAround(){\n    turnLeft();\n    turnLeft();\n  }\n\n  // Allows the Painter to take all the paint in a bucket\n  public void takeAllPaint(){\n    while (isOnBucket()){\n      takePaint();\n    }\n  }\n\n  // Moves 2 spaces forward instead of one\n  public void moveFast(){\n    if (canMove()){\n      move();\n    }\n    if (canMove()){\n      move();\n    }\n  }\n\n  // Paints a line segment of a given length & color\n  public void paintLineSegment(String color, int length){\n    for (int i = 0; i < length; i++){\n      paint(color);\n      move();\n    }\n  }\n\n  // Paints a line until Painter hits a wall/obstacle\n  public void paintFullLine(String color){\n    while (canMove()){\n      paint(color);\n      move();\n    }\n    paint(color);\n  }\n\n  // Paints a diagonal segment of a given color in a given direction\n  public void paintDiagonalSegment(String color, String direction){\n    paint(color);\n    move();\n    if (direction == \"left\"){\n      turnLeft();\n    }\n    else if (direction == \"right\"){\n      turnRight();\n    }\n    move();\n    paint(color);\n  }\n\n  // Paints a diagonal segment until the Painter hits a wall/obstacle\n  public void paintFullDiagonal(String color){\n    while (canMove()){\n      paint(color);\n      turnLeft();\n      move();\n      turnRight();\n      move();\n    }\n    paint(color);\n  }\n\n  // Allows the Painter to move backwards\n  public void moveBackwards(){\n    turnAround();\n    move();\n    turnAround();\n  }\n\n  // Moves the Painter in a diagonal shape\n  public void moveDiagonal(String direction){\n    move();\n    if (direction == \"left\"){\n      turnLeft();\n    }\n    else if (direction == \"right\"){\n      turnRight();\n    }\n    move();\n  }\n\n  // Moves the Painter a specified diagonal segment\n  public void moveDiagonalSegment(int length){\n    for (int i = 0; i < length; i++){\n      moveDiagonal(\"left\");\n      moveDiagonal(\"right\");\n    }\n  }\n\n  // Paints a border around the entire neighborhood\n  public void paintBorder(String color){\n    for (int i = 0; i < 4; i++){\n      paintFullLine(color);\n      turnRight();\n    }\n  }\n\n  // Paints a rectangle with two given colors and two given lengths of each pair of sides\n  public void paintRectangle(String color1, String color2, int length1, int length2){\n    for (int i = 0; i < 2; i++){\n      paintLineSegment(color1, length1);\n      turnRight();\n      paintLineSegment(color2, length2);\n      turnRight();\n    }\n    paint(color1);\n  }\n\n  // Moves the Painter a given distance\n  public void moveSegment(int length){\n    for (int i = 0; i < length; i++){\n      move();\n    }\n  }\n\n  // Paints a diagonal segment of a given lenght & color\n  public void paintDiagonalPart(String color, int length){\n    for (int i = 0; i < length; i++){\n      paintDiagonalSegment(color, \"left\");\n      paintDiagonalSegment(color, \"right\");\n    }\n  }\n\n  // Moves the Painter to the specified corner of the Neighborhood\n  public void goToCorner(String corner){\n    if (corner == \"upper left\"){\n      while (!facingNorth()){\n        turnLeft();\n      }\n      while (canMove()){\n        move();\n      }\n      turnLeft();\n      while (canMove()){\n        move();\n      }\n      turnAround();\n    }\n    else if (corner == \"upper right\"){\n      while (!facingNorth()){\n        turnLeft();\n      }\n      while (canMove()){\n        move();\n      }\n      turnRight();\n      while (canMove()){\n        move();\n      }\n      turnAround();\n    }\n    else if (corner == \"bottom left\"){\n      while (!facingSouth()){\n        turnLeft();\n      }\n      while (canMove()){\n        move();\n      }\n      turnRight();\n      while (canMove()){\n        move();\n      }\n      turnAround();\n    }\n    else if (corner == \"bottom right\"){\n      while (!facingSouth()){\n        turnLeft();\n      }\n      while (canMove()){\n        move();\n      }\n      turnLeft();\n      while (canMove()){\n        move();\n      }\n      turnAround();\n    }\n  }\n\n  // Clears the entire Neighborhood of paint\n  public void clearBoard(){\n    goToCorner(\"upper left\");\n    while (canMove()){\n      while (canMove()){\n        move();\n        if (isOnPaint()){\n          scrapePaint();\n        }\n      }\n      turnRight();\n      move();\n      turnRight();\n      while (canMove()){\n        move();\n        if (isOnPaint()){\n          scrapePaint();\n        }\n      }\n      if (canMove(\"south\")){\n        turnLeft();\n        move();\n        turnLeft();\n      }\n      else {\n        break;\n      }\n    }\n  }\n\n  // Moves the Painter to a certain position on the grid\n  public void goTo(int desired_x, int desired_y){\n    int current_x = 0;\n    int current_y = 0;\n    goToCorner(\"upper left\");\n\n    while (current_x != desired_x){\n      move();\n      current_x += 1;\n    }\n\n    turnRight();\n\n    while (current_y != desired_y){\n      move();\n      current_y += 1;\n    }\n\n    turnLeft();\n  }\n}",
        "isVisible": true
      },
      "BackgroundPainter.java": {
        "text": "import org.code.neighborhood.Painter;\n\npublic class BackgroundPainter extends PainterPlus{\n  public BackgroundPainter(int x, int y, String direction, int paint){\n    super(x, y, direction, paint);\n  }\n\n  // Paints a background onto the Neighborhood\n  public void paintBackground(String color, int boardLength){\n    goToCorner(\"upper left\");\n    for (int i = 0; i < boardLength; i++){\n      while (canMove()){\n        paint(color);\n        move();\n      }\n      paint(color);\n      turnRight();\n      move();\n      turnRight();\n      while (canMove()){\n        paint(color);\n        move();\n      }\n      paint(color);\n      if (canMove(\"south\")){\n        turnLeft();\n        move();\n        turnLeft();\n      }\n      else {\n        break;\n      }\n    }\n  }\n}",
