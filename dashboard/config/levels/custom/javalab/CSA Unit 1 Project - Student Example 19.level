      "MyNeighborhood.java": {
        "text": "import org.code.neighborhood.Painter;\n\npublic class MyNeighborhood {\n  public static void main(String[] args) {\n\n    // Intializing the Painters\n    StampPainter spongebob = new StampPainter(2, 2, \"east\", 1000);\n    BackgroundPainter patrick = new BackgroundPainter(0, 0, \"east\", 1024);\n\n    // Paints background\n    patrick.paintBackground(\"black\", 32);\n    \n    // Paints moon\n    spongebob.paintSquare(\"#9a9fa6\", 4, false, true);\n    spongebob.move();\n    spongebob.turnLeft();\n    spongebob.moveSegment(2);\n    spongebob.turnRight();\n    spongebob.paintSquare(\"#d3d9e3\", 1, true, false);\n\n    // Paints stars\n    spongebob.goTo(20, 30);\n    spongebob.paintOctagon(\"#f0db24\");\n    spongebob.moveDiagonalSegment(3);\n    spongebob.paintOctagon(\"#f0db24\");\n\n    spongebob.goTo(13, 17);\n    spongebob.paintOctagon(\"#d6b11c\");\n    spongebob.moveDiagonalSegment(2);\n    spongebob.paintOctagon(\"#f7e305\");\n    spongebob.goToCorner(\"upper right\");\n    spongebob.moveDiagonalSegment(2);\n    spongebob.paintOctagon(\"#f7f014\");\n    spongebob.moveSegment(6);\n    spongebob.turnLeft();\n    spongebob.moveSegment(7);\n    spongebob.paintOctagon(\"#d6b11c\");\n\n    spongebob.goTo(5, 13);\n    spongebob.paintOctagon(\"#f7f014\");\n    spongebob.turnRight();\n    spongebob.moveSegment(10);\n    spongebob.paintOctagon(\"#d6b11c\");\n    spongebob.moveSegment(2);\n    spongebob.moveDiagonalSegment(2);\n    spongebob.paintOctagon(\"#f7e305\");\n\n    spongebob.goTo(10, 6);\n    spongebob.paintOctagon(\"#edbc4a\");\n    spongebob.goToCorner(\"bottom right\");\n    spongebob.moveSegment(3);\n    spongebob.turnRight();\n    spongebob.moveSegment(3);\n    spongebob.paintOctagon(\"#edbc4a\"); \n\n    spongebob.goTo(20, 20);\n    spongebob.paintOctagon(\"yellow\");\n    spongebob.goToCorner(\"bottom left\");\n    spongebob.moveSegment(3);\n    spongebob.turnLeft();\n    spongebob.moveSegment(2);\n    spongebob.paintOctagon(\"yellow\"); \n\n    spongebob.goTo(18, 3);\n    spongebob.paintOctagon(\"#faee4d\");\n\n    // Moving the Painters\n    spongebob.goToCorner(\"bottom right\");\n    patrick.goToCorner(\"bottom right\");\n  }\n}",
