      "MyPlayground.java": {
        "text": "public class MyPlayground {\n  public static void main(String[] args) {\n\n    SimpleGame.play();\n    \n  }\n}",
        "isVisible": true
      },
      "Button.java": {
        "text": "// TO DO: Import The Playground.\n\nimport java.io.FileNotFoundException;\n\npublic class Button extends ClickableImage {\n\n  private Board myBoard = Playground.board;\n\n  public Button(String filename, int x, int y, int width, int height) throws FileNotFoundException {\n    super(filename, x, y, width, height);\n  }\n\n  public void onClick() {\n    int attack = (int)((Math.random() * 20) + 5);\n    SimpleGame.updateEnemyHealth(attack);\n  }\n}",
        "isVisible": true
      },
      "SimpleGame.java": {
        "text": "// TO DO: Import The Playground.\n\nimport org.code.media.*;\nimport java.io.FileNotFoundException;\n\npublic final class SimpleGame {\n\n  private static Board myBoard = Playground.board;\n  private static TextItem introMessage;\n  private static TextItem healthMessage;\n  private static Character enemy;\n  private static Character hero;\n  private static Button attackButton;\n\n  public static void play() {\n    createImages();\n    createMessages();\n    loadScreen();\n\n    GameControl.startPlayground();\n  }\n\n  public static void createImages() {\n    enemy = GameControl.createCharacter(\"badguy.png\", 250, 250, 100, 100, 100);\n    hero = GameControl.createCharacter(\"wizard.png\", 50, 250, 100, 100, 100);\n    attackButton = GameControl.createButton(\"attackButton.png\", 100, 150, 200, 60);\n  }\n\n  public static void createMessages() {\n    introMessage = new TextItem(\"Attack the bad guy!\", 30, 50, Color.AQUA, Font.SANS, 40, 0);\n    healthMessage = new TextItem(\"Enemy health is \" + enemy.getHealth(), 100, 100, Color.BLACK, Font.SANS, 20, 0);\n  }\n\n  public static void loadScreen() {\n    myBoard.addImageItem(enemy);\n    myBoard.addImageItem(hero);\n    myBoard.addClickableImage(attackButton);\n    myBoard.addTextItem(introMessage);\n    myBoard.addTextItem(healthMessage);\n  }\n\n  public static void updateEnemyHealth(int attack) {\n    enemy.setHealth(enemy.getHealth() - attack);\n    healthMessage.setText(\"Enemy health is now \" + enemy.getHealth());\n\n    if (gameOver(enemy.getHealth())) {\n      introMessage.setText(\"You win!\");\n      GameControl.playSoundEffect(\"winning.wav\");\n      GameControl.stopPlayground();\n    }\n  }\n\n  public static boolean gameOver(int health) {\n    boolean status = false;\n    \n    if (health <= 0) {\n      status = true;\n    }\n    else {\n      status = false;\n    }\n\n    return status;\n  }\n}",
        "isVisible": true
      },
      "Character.java": {
        "text": "// TO DO: Import The Playground.\n\nimport java.io.FileNotFoundException;\n\npublic class Character extends ImageItem {\n\n  private int health;\n\n  public Character(String filename, int x, int y, int width, int height, int health) throws FileNotFoundException {\n    super(filename, x, y, width, height);\n    this.health = health;\n  }\n\n  public void setHealth(int health) {\n    this.health = health;\n  }\n\n  public int getHealth() {\n    return health;\n  }\n}",
        "isVisible": true
      },
      "GameControl.java": {
        "text": "import org.code.playground.*;\nimport java.io.FileNotFoundException;\n\npublic class GameControl {\n\n  private static Board myBoard = Playground.board;\n  private static Character tempCharacter;\n  private static Button tempButton;\n\n  public static void startPlayground() {\n    try {\n      myBoard.start();\n    } catch (PlaygroundException e) {\n      System.out.println(\"Error starting The Playground.\");\n    }\n  }\n\n  public static void stopPlayground() {\n    try {\n      myBoard.end();\n    } catch (PlaygroundException e) {\n      System.out.println(\"Error starting The Playground.\");\n    }\n  }\n\n  public static Character createCharacter(String filename, int x, int y, int width, int height, int health) {\n    try {\n      tempCharacter = new Character(filename, x, y, width, height, health);\n    } catch (FileNotFoundException e) {\n      System.out.println(\"Error creating character: \" + filename);\n    }\n\n    return tempCharacter;\n  }\n\n  public static Button createButton(String filename, int x, int y, int width, int height) {\n    try {\n      tempButton = new Button(filename, x, y, width, height);\n    } catch (FileNotFoundException e) {\n      System.out.println(\"Error creating button: \" + filename);\n    }\n\n    return tempButton;\n  }\n\n  public static void playSoundEffect(String filename) {\n    try {\n      myBoard.playSound(filename);\n    } catch (FileNotFoundException e) {\n      System.out.println(\"Error playing sound: \" + filename);\n    }\n  }\n}",
        "isVisible": true
      }
