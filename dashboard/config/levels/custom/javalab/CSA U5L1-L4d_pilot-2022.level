      "CerealScene.java": {
        "text": "import org.code.theater.*;\n\n/*\n * Creates a scene to show the nutritional\n * information about cereals\n */\npublic class CerealScene extends Scene {\n\n  private int[][] nutrition;   // The 2D array of nutritional values\n\n  /*\n   * Constructor to create an CerealScene\n   * with the specified 2D array of nutritional values\n   */\n  public CerealScene() {\n    this.nutrition = createDataArray();\n  }\n\n  /*\n   * Returns the 2D array containing the nutritional values\n   */\n  public int[][] getNutrition() {\n    return nutrition;\n  }\n\n  /*\n   * Declares, initializes, and returns a 2D array\n   */\n  public int[][] createDataArray() {\n    // TO DO #1: Declare, initialize, and return a 2D int array called\n    // tempData containing the nutritional information for each cereal.\n    \n\n    // TO DO #2: Return the tempData array.\n    return null;\n  }\n\n  /*\n   * Returns the value in the 2D array nutrition for the\n   * specified ceral and nutritional value\n   */\n  public int getCerealNutrition(String cereal, String nutritionalValue) {\n    // TO DO #3: Return the value in the 2D array nutrition for the corresponding\n    // cereal name and nutritional value specified.\n\n\n    return 0;\n  }\n\n  /*\n   * Returns the corresponding row for the specified cereal\n   */\n  public int getCerealRow(String cereal) {\n    int cerealRow = 0;\n\n    if (cereal.equals(\"Apple Jacks\")) {\n      cerealRow = 0;\n    }\n    else if (cereal.equals(\"Cheerios\")) {\n      cerealRow = 1;\n    }\n    else if (cereal.equals(\"Lucky Charms\")) {\n      cerealRow = 2;\n    }\n    else if (cereal.equals(\"Raisin Bran\")) {\n      cerealRow = 3;\n    }\n\n    return cerealRow;\n  }\n\n  /*\n   * Returns the corresponding column for the specified nutritional value\n   */\n  public int getValueCol(String nutritionalValue) {\n    int valueCol = 0;\n\n    if (nutritionalValue.equals(\"calories\")) {\n      valueCol = 0;\n    }\n    else if (nutritionalValue.equals(\"protein\")) {\n      valueCol = 1;\n    }\n    else if (nutritionalValue.equals(\"fat\")) {\n      valueCol = 2;\n    }\n    else if (nutritionalValue.equals(\"sodium\")) {\n      valueCol = 3;\n    }\n    else if (nutritionalValue.equals(\"fiber\")) {\n      valueCol = 4;\n    }\n    else if (nutritionalValue.equals(\"carbs\")) {\n      valueCol = 5;\n    }\n    else if (nutritionalValue.equals(\"sugar\")) {\n      valueCol = 6;\n    }\n\n    return valueCol;\n  }\n\n  /*\n   * Returns a color based on the nutritional value\n   */\n  public String getColor(int nutritionalValue) {\n    String colorToUse = \"\";\n\n    if (nutritionalValue > 12) {\n      colorToUse = \"red\";\n    }\n    else if (nutritionalValue > 5) {\n      colorToUse = \"orange\";\n    }\n    else {\n      colorToUse = \"green\";\n    }\n\n    return colorToUse;\n  }\n\n  /*\n   * Creates a scene by drawing text with the grams\n   * of sugar value and a colored circle based on the amount\n   */\n  public void createScene(String cerealName, String nutritionalValue) {\n    int valueToDisplay = getCerealNutrition(cerealName, nutritionalValue);\n    \n    drawText(\"Grams of Sugar: \" + valueToDisplay, 50, 50);\n\n    String fillColorResult = getColor(valueToDisplay);\n    setFillColor(fillColorResult);\n\n    pause(0.3);\n    drawEllipse(50, 100, 200, 200);\n  }\n  \n}",
