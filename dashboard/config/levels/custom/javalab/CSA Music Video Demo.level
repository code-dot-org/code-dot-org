      "MyConcert.java": {
        "text": "import org.code.theater.*;\nimport org.code.media.*;\nimport java.io.FileNotFoundException;\nimport java.util.Scanner;\n\npublic class MyConcert {\n  public static void main(String[] args) throws FileNotFoundException {\n    Stage myStage = Theater.stage;\n    String mood = \"none\";\n    String song = \"chill_trap.wav\";\n    int numDrinks = 2;\n\n    String[] imageFiles = {\"coffee.png\", \"waterjug.png\"};\n    \n    String background = \"background.png\";\n    Scanner input = new Scanner(System.in);\n\n    TitleSequence concert = new TitleSequence(mood, song, imageFiles, background);\n\n    //double[] audio = concert.createSoundtrack(song);\n    //myStage.playSound(audio);\n    \n    //concert.displayIntroScene(mood);\n\n    //Image testImage = new Image(background);\n    \n    //testImage = concert.changeMood(mood, testImage);\n    //testImage = concert.changeMood(\"happy\", testImage);\n    //testImage = concert.changeMood(\"sad\", testImage);\n    //myStage.drawImage(testImage, 0, 0, myStage.getWidth(), myStage.getHeight(), 0);\n    \n    //concert.playConcert();\n    //concert.playConcert(mood);\n    \n    //concert.displayTransition(Color.RED);\n    //concert.textEffect(testImage, \"Hello World\");\n    //concert.recursiveTransition(Color.ORANGE);\n    \n    //testImage = ImageEffects.recursiveFilter(testImage);\n    //myStage.drawImage(testImage, 0, 0, myStage.getWidth(), myStage.getHeight(), 0);\n    \n    //concert.displayJourneyScene(mood);\n    //concert.displayRacerScene(mood);\n    //concert.displayFinalScene(mood);\n\n    myStage.play();\n  }\n}",
