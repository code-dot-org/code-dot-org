    "start_blocks": "var bc = createSprite(200, 200);\r\nbc.setAnimation(\"court\");\r\nvar tap = createSprite(200, -200);\r\ntap.setAnimation(\"court\");\r\nvar bot = createSprite(200, 600);\r\nbot.setAnimation(\"court\");\r\nvar r = createSprite(-200, 200);\r\nr.setAnimation(\"court\");\r\nvar l = createSprite(600, 200);\r\nl.setAnimation(\"court\");\r\nvar p1 = createSprite(50, 200);\r\np1.setAnimation(\"p1\");\r\np1.scale = 1.2;\r\nvar p2 = createSprite(350, 200);\r\np2.setAnimation(\"p2\");\r\np2.scale = 1.2;\r\nvar ball = createSprite(200, 200);\r\nball.setAnimation(\"ball\");\r\nball.velocityX = -11;\r\ntitle = \"\";\r\ntxt1 = \"\";\r\ntxt2 = \"\";\r\ntxt3 = \"\";\r\nvar score1 = 0;\r\nvar score2 = 0;\r\nvar level = 0;\r\n\r\n\r\nfunction draw() {\r\n  levelfunction();\r\n  ball.bounciness = 1;\r\n  p1.bounciness = 1;\r\n  p2.bounciness = 1;\r\n  p1.setCollider(\"rectangle\");\r\n  p2.setCollider(\"rectangle\");\r\n  ball.setCollider(\"circle\");\r\n  baller();\r\n  player1();\r\n  player2();\r\n  bluewin();\r\n  redwin();\r\n\r\n\r\n  drawSprites();\r\n  scores();\r\n}\r\nfunction levelfunction() {\r\n  if (level>0) {\r\n    bc.setAnimation(\"court\");\r\n    p1.visible = true;\r\n    p2.visible = true;\r\n    ball.visible = true;\r\n    txt1 = \"\";\r\n    txt2 = \"\";\r\n    txt3 = \"\";\r\n    title = \"\";\r\n  } else {\r\n    bc.setAnimation(\"court_screen\");\r\n    p1.visible = false;\r\n    p2.visible = false;\r\n    ball.visible = false;\r\n    p1.x = 50;\r\n    p2.x = 350;\r\n    ball.x = 200;\r\n    ball.y = 200;\r\n    p1.y = 200;\r\n    p2.y = 200;\r\n    txt1 = \"Red uses WASD ----- Blue uses arrows\";\r\n    txt2 = \"There is a small learning curve ;)\";\r\n    txt3 = \"Press space to start\";\r\n    title = \"Alien Soccer\";\r\n    score1 = 0;\r\n    score2 = 0;\r\n    if (keyDown(\"space\")) {\r\n      level = level+1;\r\n    }\r\n  }\r\n}\r\nfunction scores() {\r\n  fill(\"white\");\r\n  textFont(\"Arial\");\r\n  textSize(25);\r\n  text(\"RED:\", 60, 375);\r\n  text(score2, 130, 375);\r\n  text(\"BLUE:\", 250, 375);\r\n  text(score1, 330, 375);\r\n  textSize(20);\r\n  text(txt1, 30, 150);\r\n  text(txt2, 60, 200);\r\n  text(txt3, 110, 250);\r\n  textSize(30);\r\n  text(title, 120, 80);\r\n}\r\nfunction player1() {\r\n  if (p1.isTouching(tap)) {\r\n    p1.bounceOff(tap);\r\n  }\r\n  if (p1.isTouching(bot)) {\r\n    p1.bounceOff(bot);\r\n  }\r\n  if (p1.isTouching(r)) {\r\n    p1.bounceOff(r);\r\n  }\r\n  if (p1.isTouching(l)) {\r\n    p1.bounceOff(l);\r\n  }\r\n  if (keyDown(\"w\")) {\r\n    p1.y = p1.y-8;\r\n  }\r\n  if (keyDown(\"s\")) {\r\n    p1.y = p1.y+8;\r\n  }\r\n  if (keyDown(\"a\")) {\r\n    p1.x = p1.x-8;\r\n  }\r\n  if (keyDown(\"d\")) {\r\n    p1.x = p1.x+8;\r\n  }\r\n  if (p1.x > 195) {\r\n    p1.x = randomNumber(75, 195);\r\n  }\r\n}\r\nfunction player2() {\r\n  if (p2.isTouching(tap)) {\r\n    p2.bounceOff(tap);\r\n  }\r\n  if (p2.isTouching(bot)) {\r\n    p2.bounceOff(bot);\r\n  }\r\n  if (p2.isTouching(r)) {\r\n    p2.bounceOff(r);\r\n  }\r\n  if (p2.isTouching(l)) {\r\n    p2.bounceOff(l);\r\n  }\r\n  if (keyDown(\"up\")) {\r\n    p2.y = p2.y-8;\r\n  }\r\n  if (keyDown(\"down\")) {\r\n    p2.y = p2.y+8;\r\n  }\r\n  if (keyDown(\"left\")) {\r\n    p2.x = p2.x-8;\r\n  }\r\n  if (keyDown(\"right\")) {\r\n    p2.x = p2.x+8;\r\n  }\r\n  if (p2.x < 205) {\r\n    p2.x = randomNumber(205, 350);\r\n  }\r\n}\r\nfunction baller() {\r\n  if (p1.isTouching(ball)) {\r\n    ball.bounceOff(p1);\r\n  }\r\n  if (p1.isTouching(ball)) {\r\n    p1.setAnimation(\"p1_hit\");\r\n  } else {\r\n    p1.setAnimation(\"p1\");\r\n  }\r\n  if (p2.isTouching (ball)) {\r\n    ball.bounceOff(p2);\r\n  }\r\n  if (p2.isTouching(ball)) {\r\n    p2.setAnimation(\"p2_hit\");\r\n  } else {\r\n    p2.setAnimation(\"p2\");\r\n  }\r\n  if (ball.isTouching(tap)) {\r\n    ball.bounceOff(tap);\r\n  }\r\n  if (ball.isTouching(bot)) {\r\n    ball.bounceOff(bot);\r\n  }\r\n  if (ball.isTouching(r)) {\r\n    ball.bounceOff(r);\r\n    score1 = score1+1;\r\n  }\r\n  if (ball.isTouching(l)) {\r\n    ball.bounceOff(l);\r\n    score2 = score2+1;\r\n  }\r\n}\r\nfunction bluewin() {\r\n  if (score1 >= 30 ) {\r\n    bc.setAnimation(\"court_screen\");\r\n    p1.visible = false;\r\n    p2.visible = false;\r\n    ball.visible = false;\r\n    title = \"  Blue wins!\";\r\n    txt1 = \"     Good job! Press space to restart\";\r\n    if (keyDown(\"space\")) {\r\n      level = 0;\r\n    }\r\n  }\r\n}\r\nfunction redwin() {\r\n  if (score2 >= 30 ) {\r\n    bc.setAnimation(\"court_screen\");\r\n    p1.visible = false;\r\n    p2.visible = false;\r\n    ball.visible = false;\r\n    title = \"  Red wins!\";\r\n    txt1 = \"     Good job! Press space to restart\";\r\n    if (keyDown(\"space\")) {\r\n      level = 0;\r\n    }\r\n  }\r\n}\r\n",
