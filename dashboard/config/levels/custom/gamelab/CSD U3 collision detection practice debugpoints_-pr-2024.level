    "start_blocks": "var points = 0;\r\nvar coin = createSprite(200, 100);\r\ncoin.setAnimation(\"coin\");\r\nvar ghost = createSprite(200, 300);\r\nghost.setAnimation(\"ghost\");\r\n\r\nfunction draw() {\r\n  if (ghost.isTouching(coin)) {\r\n    points = points + 1;\r\n  }\r\n  background(\"lightblue\");\r\n  text(\"Points: \" + points, 25, 25);\r\n  if(keyDown(\"up\")) {\r\n    ghost.y = ghost.y - 5;\r\n  }\r\n  if(keyDown(\"down\")) {\r\n    ghost.y = ghost.y + 5;\r\n  }\r\n  if(keyDown(\"left\")) {\r\n    ghost.x = ghost.x - 5;\r\n  }\r\n  if(keyDown(\"right\")) {\r\n    ghost.x = ghost.x + 5;\r\n  }\r\n  drawSprites();\r\n}",
