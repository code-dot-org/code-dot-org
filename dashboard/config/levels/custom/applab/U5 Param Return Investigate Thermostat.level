    "start_blocks": "// Create and assign variables\r\nvar tempF = 70;\r\nvar tempC;\r\nvar tempDisplayF;\r\nvar tempDisplayC;\r\nvar userName;\r\nvar password;\r\n\r\n// Update Screen\r\nupdateTempC();\r\nupdateScreen();\r\n\r\n// Button to decrease the temperature by\r\n// one degree F.\r\nonEvent(\"downButton\", \"click\", function() {\r\n  // Decrease the temperature if it's greater than 60\r\n  // Otherwise play a sound\r\n  if(tempF > 60){\r\n    tempF = tempF - 1;\r\n    updateTempC();\r\n    playSound(\"sound://category_objects/sharp_switch.mp3\");\r\n  } else {\r\n    playSound(\"sound://category_digital/hop.mp3\");\r\n  }\r\n  \r\n  updateScreen();\r\n\r\n});\r\n\r\n// Button to increase the temperature by\r\n// one degree F.\r\nonEvent(\"upButton\", \"click\", function(){\r\n  \r\n  // Increase the temperature if it's less than 90\r\n  // Otherwise play a sound\r\n  if(tempF < 90){\r\n    tempF = tempF + 1;\r\n    updateTempC();\r\n    playSound(\"sound://category_objects/sharp_switch.mp3\");\r\n  } else {\r\n    playSound(\"sound://category_digital/hop.mp3\");\r\n  }\r\n  \r\n  updateScreen();\r\n  \r\n});\r\n\r\n// Button to log in to the app\r\nonEvent(\"loginButton\", \"click\", function() {\r\n  userName = getText(\"nameInput\");\r\n  password = getText(\"passwordInput\");\r\n  setText(\"usernameText\",\"Hi, \" + userName);\r\n  \r\n  // Check username and password match to sign in to the app\r\n  if((userName == \"Dog\") && (password == \"woof\")){\r\n    setScreen(\"homeScreen\");\r\n  } else if ((userName == \"Cat\") && (password == \"meow\")){\r\n    setScreen(\"homeScreen\");\r\n  } else {\r\n    playSound(\"sound://category_instrumental/marimba_downscale_1.mp3\");\r\n  }\r\n});\r\n\r\n// Button to return to the log in screen\r\nonEvent(\"homeButton\", \"click\", function(){\r\n  setScreen(\"loginScreen\");\r\n  setText(\"passwordInput\", \"\");\r\n});\r\n\r\n// Calculates the celsisus temperature\r\nfunction updateTempC(){\r\n  tempC = Math.round((tempF - 32)*(5/9));\r\n}\r\n\r\n// Updates screen based on current temperature\r\nfunction updateScreen(){\r\n  \r\n  // Checks which color to make text based on temperature\r\n  if(tempF >= 80){\r\n    setProperty(\"temperatureF\",\"text-color\",\"red\");\r\n    setProperty(\"temperatureC\",\"text-color\",\"red\");\r\n  } else if (tempF >= 70) {\r\n    setProperty(\"temperatureF\",\"text-color\",\"lightBlue\");\r\n    setProperty(\"temperatureC\",\"text-color\",\"lightBlue\");\r\n  } else {\r\n    setProperty(\"temperatureF\",\"text-color\",\"blue\");\r\n    setProperty(\"temperatureC\",\"text-color\",\"blue\");\r\n  }\r\n  \r\n  // Updates text on the screen\r\n  tempDisplayF = tempF + \" F\";\r\n  tempDisplayC = tempC + \" C\";\r\n  setText(\"temperatureF\", tempDisplayF);\r\n  setText(\"temperatureC\", tempDisplayC);\r\n}",
