    "long_instructions": "# My Pet Rock App\r\n\r\nTry out this app with a friend. Then discuss the new features this app has. Be ready to share your thoughts with the class.\r\n\r\n",
    "start_blocks": "var clicks = 0;\r\nvar amount = 1;\r\n\r\nonEvent(\"rock\", \"click\", function() {\r\n  updateScreen();\r\n});\r\n\r\nonEvent(\"rock2\", \"click\", function() {\r\n  updateScreen();\r\n});\r\n\r\nonEvent(\"plus_two\", \"click\", function() {\r\n  if(clicks > 19){\r\n    clicks = clicks - 20;\r\n    setText(\"upgrade\", \"+2\");\r\n    hideElement(\"plus_two\");\r\n    amount = 2;\r\n  }\r\n  setText(\"clicks_number_label\", clicks);\r\n});\r\n\r\nonEvent(\"plus_twenty\", \"click\", function() {\r\n  if(clicks > 99){\r\n    clicks = clicks - 100;\r\n    setText(\"upgrade\", \"+20\");\r\n    hideElement(\"plus_two\");\r\n    hideElement(\"plus_twenty\");\r\n    amount = 20;\r\n  }\r\n  setText(\"clicks_number_label\", clicks);\r\n});\r\n\r\nfunction updateScreen(){\r\n  if(clicks === 0){\r\n    clicks = amount;\r\n  } else {\r\n    clicks = clicks + amount;\r\n  }\r\n  if(clicks > 49){\r\n    hideElement(\"rock\");\r\n    showElement(\"rock2\");\r\n    setText(\"level_label\", \"Level 2: Rock-a-bunny\");\r\n  }\r\n  if(clicks > 499){\r\n    hideElement(\"rock2\");\r\n    showElement(\"rock3\");\r\n    setText(\"level_label\", \"Level 3: Rock-a-Walk-a-Rainbow EVOLUTION COMPLETE\");\r\n  }\r\n  setText(\"clicks_number_label\", clicks);\r\n\r\n}\r\n",
