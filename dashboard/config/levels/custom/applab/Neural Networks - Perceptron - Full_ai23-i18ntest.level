translate(\"AANYATEXT\");\r\nmodelOptions[\"Joseph\"] = I18n_HAIW23L3.translate(\"JOSEPHTEXT\");\r\nmodelOptions[\"Zara\"] = I18n_HAIW23L3.translate(\"ZARATEXT\");\r\n\r\nvar studentImages = {\r\n  \"Alex\": \"https://images.code.org/a6aaf4a480329835a5b29359f9c8eafb-alex-notext.png\",\r\n  \"Sofia\": \"https://images.code.org/c23930d822ba247750fce3c0800f8552-sofia-new.png\",\r\n  \"Aanya\": \"https://images.code.org/92acfa4177b18edf2ec810bff434efb5-aanya-notext.png\",\r\n  \"Joseph\": \"https://images.code.org/b0240a95181df29dada166e7e96dec0a-luca-notext.png\",\r\n  \"Zara\": \"https://images.code.org/540f59d031dce1b081e888a0043df9e5-zara-notext.png\"\r\n};\r\n\r\n\r\nvar CATEGORIES = Object.keys(recommendationObject);\r\n//console.log(CATEGORIES);\r\nvar tempChoices = Object.keys(recommendationObject[CATEGORIES[0]]);\r\n//console.log(tempChoices);\r\nvar STUDENTS = Object.keys(recommendationObject[CATEGORIES[0]][tempChoices[0]]);\r\n//console.log(STUDENTS);\r\n\r\nvar featuresObject = {};\r\nfor(var i = 0; i < STUDENTS.length; i++) {\r\n  var tempObj = {};\r\n  tempObj.id = STUDENTS[i];\r\n  tempObj.text = STUDENTS[i];\r\n  tempObj.weight = 0;\r\n  featuresObject[STUDENTS[i]] = tempObj;\r\n}\r\n\r\n/*****************\r\n * GLOBAL VARIABLES\r\n * **************/\r\n\r\nvar currentId = Object.keys(featuresObject)[0];\r\nsetActiveCanvas(\"canvas1\");\r\n\r\n\r\nvar BUTTONPROPS = {\r\n  \"width\": 100,\r\n  \"height\":  30,\r\n  \"startY\": 200,\r\n  \"spacing\": 15,\r\n};\r\n\r\nvar CIRCLEPROPS = {\r\n  \"x\": 200,\r\n  \"y\": 225,\r\n  \"radius\": 30\r\n};\r\n\r\n/*****************\r\n * MODELSCREEN - FUNCTIONS\r\n * **************/\r\n\r\nfunction setupButtons() {\r\n  for(var i = 0; i < Object.keys(featuresObject).length; i++) {\r\n    var feature = featuresObject[Object.keys(featuresObject)[i]];\r\n    //console.log(feature);\r\n    button(feature[\"id\"], feature[\"text\"]);\r\n    setPosition(feature[\"id\"], 0, BUTTONPROPS.startY + (BUTTONPROPS.height + BUTTONPROPS.spacing) * i, BUTTONPROPS.width, BUTTONPROPS.height);\r\n    setProperty(feature[\"id\"], \"font-size\", 12);\r\n    setProperty(feature[\"id\"], \"background-color\", \"#3A00FF\");\r\n    setStyle(feature[\"id\"], \"overflow: auto\");\r\n    onEvent(feature[\"id\"], \"click\", function(e) {\r\n      showElement(\"featureLabel\");\r\n      showElement(\"featureSlider\");\r\n      showElement(\"featureValue\");\r\n      setText(\"modelDescriptionLabel\", modelOptions[getText(e.srcElementId)]);\r\n      setImageURL(\"creatorImage\", studentImages[e.srcElementId]);\r\n      currentId = e.srcElementId;\r\n      updateSlider();\r\n    });\r\n  }\r\n}\r\n\r\nfunction drawCanvas() {\r\n  clearCanvas();\r\n  setStrokeWidth(1);\r\n  setStrokeColor(\"black\");\r\n  //calculate circle position so in the middle of buttons\r\n  var numFeatures = Object.keys(featuresObject).length;\r\n  var dist = BUTTONPROPS.height * numFeatures + BUTTONPROPS.spacing * (numFeatures - 1);\r\n  CIRCLEPROPS.y = BUTTONPROPS.startY + dist / 2;\r\n  circle(CIRCLEPROPS.x, CIRCLEPROPS.y, CIRCLEPROPS.radius);\r\n  for(var i = 0; i < Object.keys(featuresObject).length; i++) {\r\n    setStrokeColor(\"black\");\r\n    var feature = featuresObject[Object.keys(featuresObject)[i]];\r\n    var weight = feature[\"weight\"];\r\n    if(weight < 0) {\r\n      setStrokeColor(\"red\");\r\n      weight *= -1;\r\n    }\r\n    if(weight != 0) {\r\n      setStrokeWidth(weight);\r\n      line(BUTTONPROPS.width, (BUTTONPROPS.startY + i*(BUTTONPROPS.height + BUTTONPROPS.spacing)) + BUTTONPROPS.height * 0.5, CIRCLEPROPS.x, CIRCLEPROPS.y);  \r\n    }\r\n  }\r\n  setStrokeWidth(5);\r\n  line(CIRCLEPROPS.x, CIRCLEPROPS.y, 320, CIRCLEPROPS.y);\r\n}\r\n\r\n\r\nfunction updateSlider() {\r\n  setText(\"featureLabel\", featuresObject[currentId][\"text\"]);\r\n  setNumber(\"featureSlider\", featuresObject[currentId][\"weight\"] * 10);  \r\n  setText(\"featureValue\", featuresObject[currentId][\"weight\"]);\r\n}\r\n\r\n/*****************\r\n * MODELSCREEN - EVENTS\r\n * **************/\r\n\r\nonEvent(\"featureSlider\", \"input\", function( ) {\r\n  featuresObject[currentId][\"weight\"] = getNumber(\"featureSlider\") / 10;\r\n  setText(\"featureValue\", featuresObject[currentId][\"weight\"]);\r\n  var students = Object.keys(featuresObject);\r\n  var sum = 0;\r\n  for(var i = 0; i < students.length; i++) {\r\n    sum += featuresObject[students[i]][\"weight\"];\r\n  }\r\n  if(sum == 0) {\r\n    hideElement(\"nextScreen1\");\r\n  } else {\r\n    showElement(\"nextScreen1\");\r\n  }\r\n  drawCanvas();\r\n});\r\n\r\nonEvent(\"canvas1\", \"click\", function() {\r\n  hideElement(\"featureLabel\");\r\n  hideElement(\"featureValue\");\r\n  hideElement(\"featureSlider\");\r\n  setImageURL(\"creatorImage\", \"\");\r\n  setText(\"modelDescriptionLabel\", modelOptions[\"default\"]);\r\n});\r\n\r\n/*****************\r\n * RECOMMENDATIONSCREEN - FUNCTIONS\r\n * **************/\r\n \r\nvar firstTime = true;\r\nvar recommendations = {};\r\nvar recommendationArrays = {};\r\nvar topChoices = [];\r\nvar currentRecommendations = [];\r\nvar choiceIndex = 0;\r\nvar currentCategory = \"\";\r\n\r\n\r\n\r\nfunction calculateRecommendations() {\r\n  recommendations = {};\r\n  recommendationArrays = {};\r\n  recommendations[\"all\"] = {};\r\n  recommendationArrays[\"all\"] = [];\r\n  //For each category: \r\n  //for each choice: \r\n  //calculate sum of weight * student; \r\n  //stack rank them; \r\n  //show the top 3 on the screen\r\n  \r\n  for(var i = 0; i < CATEGORIES.length; i++) {\r\n    var category = CATEGORIES[i];\r\n    recommendations[category] = {};\r\n    var categoryObj = recommendationObject[category];\r\n    //categoryObj has {choice: { student: # } } structure\r\n    var CHOICES = Object.keys(categoryObj);\r\n    for(var j = 0; j < CHOICES.length; j++) {\r\n      var choice = CHOICES[j];\r\n      var choicesObj = categoryObj[choice];\r\n      //choicesObj is {student: #} structure\r\n      //var STUDENTS = Object.keys(choicesObj); <-- made this global obj\r\n      \r\n      var sum = 0;\r\n      for(var k = 0; k < STUDENTS.length; k++) {\r\n        var student = STUDENTS[k];\r\n        //console.log(choicesObj[student]);\r\n        //console.log(featuresObject[student][\"weight\"]);\r\n        sum += choicesObj[student] * featuresObject[student][\"weight\"];\r\n      }\r\n      if(sum > 0) {\r\n        recommendations[category][choice] = sum;\r\n        recommendations[\"all\"][choice] = sum;\r\n      }\r\n    }\r\n    recommendationArrays[category] = calculateCategoryRecommendations(category);\r\n  }\r\n  \r\n  \r\n  \r\n  //recommendations = {};\r\n  //recommendationArrays = {};\r\n  // console.log(recommendations);\r\n  // console.log(recommendationArrays);\r\n  // console.log(\"-----------\");\r\n  // console.log(recommendations[\"all\"]);\r\n  // console.log(recommendationArrays[\"all\"]);\r\n  //console.log(allRecommendationsObj);\r\n  //console.log(Object.keys(allRecommendationsObj).length);\r\n  //console.log(recommendations);\r\n}\r\n\r\nfunction calculateCategoryRecommendations(category) {\r\n  var NUMRECS = 5;\r\n  //console.log(elt);\r\n  var categoryObj = recommendations[category];\r\n  var choicesArray = Object.keys(categoryObj);\r\n  topChoices = [];\r\n  var repeat = 0;\r\n  while(choicesArray.length > 0 && repeat < NUMRECS) {\r\n    var max = categoryObj[choicesArray[0]];\r\n    var maxChoice = choicesArray[0];\r\n    for(var i = 0; i < choicesArray.length; i++) {\r\n      var choice = choicesArray[i];\r\n      var value = categoryObj[choice];\r\n      if(value > max) {\r\n        max = value;\r\n        maxChoice = choice;\r\n      }\r\n    }\r\n    topChoices.push(maxChoice);\r\n    insertIntoAllRecommendations(maxChoice, max);\r\n    var index = choicesArray.indexOf(maxChoice);\r\n    choicesArray.splice(index, 1);\r\n    repeat++;\r\n  }\r\n  return topChoices;\r\n}\r\n\r\nfunction insertIntoAllRecommendations(choice, value) {\r\n  //recommendations = {};\r\n  //recommendationArrays = {};\r\n  for(var i = 0; i < recommendationArrays[\"all\"].length; i++) {\r\n    var curChoice = recommendationArrays[\"all\"][i];\r\n    if (value > recommendations[\"all\"][curChoice]) {\r\n      recommendationArrays[\"all\"].splice(i, 0, choice);\r\n      return;\r\n    }\r\n  }\r\n  recommendationArrays[\"all\"].push(choice);\r\n}\r\n\r\nfunction showRecommendations(category) {\r\n  currentRecommendations = recommendationArrays[category];\r\n  \r\n  setText(\"recommendLabel1\", \"\");\r\n  setText(\"recommendLabel2\", \"\");\r\n  setText(\"recommendLabel3\", \"\");\r\n  \r\n  var i = 0;\r\n  while(choiceIndex + i < currentRecommendations.length && i < 3) {\r\n    setText(\"recommendLabel\" + (i+1), (choiceIndex + i + 1) + \") \" + currentRecommendations[choiceIndex + i]);\r\n    i++;\r\n  }\r\n  \r\n  // var tempStr = \"\";\r\n  // for(var j = 0; j < currentRecommendations.length; j++) {\r\n  //   tempStr += currentRecommendations[j] + \"\\n\";\r\n  // }\r\n  // setText(\"recommendationsTextbox\", tempStr);\r\n}\r\n\r\nvar CATEGORYPROPS = {\r\n  \"width\": 115,\r\n  \"height\": 46,\r\n  \"startY\": 145,\r\n  \"borderWidth\": 4\r\n};\r\n\r\n\r\nfunction drawCategoryButtons() {\r\n  button(\"allbtn\", allButtonText);\r\n  setPosition(\"allbtn\", 0, CATEGORYPROPS.startY, CATEGORYPROPS.width, CATEGORYPROPS.height);\r\n  setProperty(\"allbtn\", \"border-width\", CATEGORYPROPS.borderWidth);\r\n  setProperty(\"allbtn\", \"border-radius\", 0);\r\n  setStyle(\"allbtn\", \"overflow: auto\");\r\n  onEvent(\"allbtn\", \"click\", function() {\r\n    currentCategory = \"all\";\r\n    choiceIndex = 0;\r\n    showRecommendations(currentCategory);\r\n  });\r\n  for(var i = 0; i < CATEGORIES.length; i++) {\r\n    var btnName = CATEGORIES[i] + \"btn\";\r\n    button(btnName, CATEGORIES[i]);\r\n    setPosition(btnName, 0, CATEGORYPROPS.startY + (i+1)*CATEGORYPROPS.height, CATEGORYPROPS.width, CATEGORYPROPS.height);\r\n    setProperty(btnName, \"border-width\", CATEGORYPROPS.borderWidth);\r\n    setProperty(btnName, \"border-radius\", 0);\r\n    setStyle(btnName, \"overflow: auto\");\r\n    onEvent(CATEGORIES[i] + \"btn\", \"click\", function(e) {\r\n      //console.log(e);\r\n      currentCategory = getText(e.srcElementId);\r\n      choiceIndex = 0;\r\n      showRecommendations(currentCategory);\r\n    });\r\n  }\r\n}\r\n\r\n \r\n/*****************\r\n * RECOMMENDATIONSCREEN - EVENTS\r\n * **************/\r\n\r\nonEvent(\"nextScreen1\", \"click\", function() {\r\n  //setText(\"recommendationsTextbox\", \"\");\r\n  setScreen(\"recommendationScreen\");\r\n  calculateRecommendations();\r\n  currentCategory = \"all\";\r\n  showRecommendations(currentCategory);\r\n  //allRecommendations();\r\n  if(firstTime) {\r\n    drawCategoryButtons();\r\n    firstTime = false;\r\n  }\r\n});\r\n\r\n\r\nonEvent(\"recommendScrollDown\", \"click\", function( ) {\r\n  if(choiceIndex + 3 < recommendationArrays[currentCategory].length) {\r\n\t  choiceIndex += 3;\r\n  }\r\n\tshowRecommendations(currentCategory);\r\n});\r\n\r\nonEvent(\"recommendScrollUp\", \"click\", function( ) {\r\n  if(choiceIndex - 3 >= 0) {\r\n\t  choiceIndex -= 3;\r\n  }\r\n\tshowRecommendations(currentCategory);\r\n});\r\n\r\n/*****************\r\n * SIMPLE BUTTON EVENTS\r\n * **************/\r\n\r\nonEvent(\"backButton2\", \"click\", function( ) {\r\n\tsetScreen(\"modelScreen\");\r\n});\r\nonEvent(\"startNextButton\", \"click\", function( ) {\r\n\tsetScreen(\"modelScreen\");\r\n\tsetupButtons();\r\n});\r\n\r\nvar LABELPROPS = {\r\n  \"backgroundColor\": \"#FFFFFF\",\r\n  \"mouseOverColor\": \"#EEEEEE\"\r\n};\r\n\r\nonEvent(\"recommendLabel1\", \"mouseover\", function( ) {\r\n\tsetProperty(\"recommendLabel1\", \"background-color\", LABELPROPS.mouseOverColor);\r\n});\r\nonEvent(\"recommendLabel1\", \"mouseout\", function( ) {\r\n\tsetProperty(\"recommendLabel1\", \"background-color\", LABELPROPS.backgroundColor);\r\n});\r\nonEvent(\"recommendLabel2\", \"mouseover\", function( ) {\r\n\tsetProperty(\"recommendLabel2\", \"background-color\", LABELPROPS.mouseOverColor);\r\n});\r\nonEvent(\"recommendLabel2\", \"mouseout\", function( ) {\r\n\tsetProperty(\"recommendLabel2\", \"background-color\", LABELPROPS.backgroundColor);\r\n});\r\nonEvent(\"recommendLabel3\", \"mouseover\", function( ) {\r\n\tsetProperty(\"recommendLabel3\", \"background-color\", LABELPROPS.mouseOverColor);\r\n});\r\nonEvent(\"recommendLabel3\", \"mouseout\", function( ) {\r\n\tsetProperty(\"recommendLabel3\", \"background-color\", LABELPROPS.backgroundColor);\r\n});\r\n\r\n\r\n/*****************\r\n * STARTER CODE\r\n * **************/\r\n\r\ndrawCanvas();",
