    "start_blocks": "var myName;\r\nvar myTime;\r\nvar myActivityLevel;\r\nvar recommendation;\r\n\r\nmyTime = getNumber(\"timeDropdown\");\r\nmyActivityLevel = getText(\"levelDropdown\").toUpperCase();\r\n\r\n\r\nonEvent(\"nameInput\", \"input\", function() {\r\n  myName = getText(\"nameInput\").toUpperCase();\r\n  updateScreen();\r\n});\r\n\r\nonEvent(\"timeDropdown\", \"change\", function() {\r\n  myTime = getNumber(\"timeDropdown\");\r\n  updateScreen();\r\n});\r\n\r\nonEvent(\"levelDropdown\", \"change\", function() {\r\n  myActivityLevel = getText(\"levelDropdown\").toUpperCase();\r\n  updateScreen();\r\n});\r\n\r\n// Purpose: Update the screen with a recommendation based on user input\r\n// Functionality: A conditional statement is used to create a recommendation based\r\n//    on the activity level which is in a dropdown menu\r\n//    and the time of day which is in another dropdown menu.\r\n//    The output is a message with the user's name concatenated with the recommendation\r\nfunction updateScreen(){\r\n  if(myTime >= 8 && myActivityLevel === \"LIGHT\"){\r\n    recommendation = \" walking around the mall.\";\r\n  } else if (myTime >= 8 && myActivityLevel == \"MODERATE\") {\r\n    recommendation = \" swimming in the indoor pool at Splash Complex.\";\r\n  } else if (myTime >= 8 && myActivityLevel == \"STRENUOUS\") {\r\n    recommendation = \" hiking Elephant Mountain.\";\r\n  } else if (myTime <8 && myActivityLevel === \"LIGHT\"){\r\n    recommendation = \" kite flying at Manson Park.\";\r\n  } else if (myTime <8 && myActivityLevel == \"MODERATE\"){\r\n    recommendation = \" biking on the boardwalk.\";\r\n  } else {\r\n    recommendation = \"rock climbing at Rocco's Gym\";\r\n  }\r\n  \r\n  setText(\"activityResponse\", \"Hi, \" + myName + \". You want to do a \" + myActivityLevel + \" activity at \" + myTime + \" o'clock. You may want to try \" + recommendation);\r\n}\r\n\r\n",
