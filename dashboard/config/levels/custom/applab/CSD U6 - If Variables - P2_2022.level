    "start_blocks": "var yPosition = 200;\r\n\r\nonBoardEvent(buttonL, \"down\", function() {\r\n  yPosition = yPosition - 15;\r\n  setProperty(\"sunImg\", \"y\", yPosition);\r\n  if(yPosition < 315) {\r\n    setProperty(\"screen1\", \"background-color\", \"orange\");\r\n  }\r\n});\r\n\r\nonBoardEvent(buttonR, \"down\", function() {\r\n  yPosition = yPosition + 15;\r\n  setProperty(\"sunImg\", \"y\", yPosition);\r\n  if(yPosition > 315) {\r\n    setProperty(\"screen1\", \"background-color\", \"gray\");\r\n  }\r\n});\r\n\r\n",
    "preload_asset_list": null,
