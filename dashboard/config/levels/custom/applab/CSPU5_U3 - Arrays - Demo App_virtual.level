    "start_blocks": "var faveThings = [\"Saturday\",\"Sunday\",\"Pizza\"];\r\nvar currentWord = 0;\r\n\r\nupdateWord();\r\n\r\nonEvent(\"lastButton\", \"click\", function() {\r\n  prev();\r\n});\r\n\r\nonEvent(\"nextButton\", \"click\", function() {\r\n  next();\r\n});\r\n\r\nfunction prev(){\r\n  currentWord--;\r\n  if(currentWord < 0){\r\n    currentWord = faveThings.length-1;\r\n  }\r\n  updateWord();\r\n}\r\n\r\nfunction next(){\r\n  currentWord++;\r\n  if(currentWord>=faveThings.length){\r\n    currentWord=0;\r\n  }\r\n  updateWord();\r\n}\r\n\r\nonEvent(\"addButton\", \"click\", function() {\r\n  insertItem(faveThings, currentWord, getText(\"urlInput\"));\r\n  setText(\"myWord\",faveThings[currentWord] );\r\n  setText(\"urlInput\",\"\");\r\n  updateWord();\r\n});\r\n\r\nfunction updateWord(){\r\n  setText(\"myWord\",faveThings[currentWord]);\r\n  setText(\"currentIndexLabel\", (currentWord+1)+\" of \"+faveThings.length);\r\n}",
