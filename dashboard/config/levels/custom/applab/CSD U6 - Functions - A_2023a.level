    "start_html": "<div id=\"designModeViz\" class=\"appModern\" tabindex=\"1\" data-radium=\"true\" style=\"display: none; width: 320px; height: 450px;\"><div class=\"screen\" tabindex=\"1\" data-theme=\"default\" id=\"snowScreen\" style=\"display: block; height: 450px; width: 320px; left: 0px; top: 0px; position: absolute; z-index: 0; background-color: rgb(0, 0, 0);\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow1\" style=\"height: 120px; width: 105px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 0px; top: 0px; margin: 0px;\" data-icon-color=\"#ffffff\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow2\" style=\"height: 85px; width: 75px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 50px; top: 370px; margin: 0px;\" data-icon-color=\"#ffffff\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow3\" style=\"height: 85px; width: 75px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 140px; top: 140px; margin: 0px;\" data-icon-color=\"#ffffff\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow4\" style=\"height: 55px; width: 40px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 150px; top: 45px; margin: 0px;\" data-icon-color=\"#ffffff\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow5\" style=\"height: 55px; width: 40px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 240px; top: 200px; margin: 0px;\" data-icon-color=\"#ffffff\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow6\" style=\"height: 55px; width: 40px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 55px; top: 325px; margin: 0px;\" data-icon-color=\"#ffffff\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow7\" style=\"height: 20px; width: 20px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 210px; top: 165px; margin: 0px;\" data-icon-color=\"#ffffff\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow8\" style=\"height: 25px; width: 20px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 135px; top: 35px; margin: 0px;\" data-icon-color=\"#ffffff\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow9\" style=\"height: 25px; width: 20px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 50px; top: 280px; margin: 0px;\" data-icon-color=\"#ffffff\"><img data-canonical-image-url=\"icon://fa-certificate\" data-image-type=\"icon\" data-object-fit=\"contain\" id=\"snow10\" style=\"height: 25px; width: 20px; border-style: solid; border-width: 0px; border-color: rgb(0, 0, 0); border-radius: 0px; position: absolute; left: 270px; top: 385px; margin: 0px;\" data-icon-color=\"#ffffff\"></div></div>",
    "start_blocks": "var snowY;\r\n//Add code to define a new function for moveSnow() that has two parameters\r\n\r\n\r\n\r\n//Edit the code below to call your new moveSnow() function\r\nonBoardEvent(accelerometer, \"change\", function() {\r\n  //Move \"snow1\" by 32 pixels\r\n  snowY = getYPosition(\"snow1\");\r\n  snowY = snowY + 32;\r\n  setProperty(\"snow1\", \"y\", snowY);\r\n  resetToTop(\"snow1\");\r\n  \r\n  //Move \"snow2\" by 16 pixels\r\n  snowY = getYPosition(\"snow2\");\r\n  snowY = snowY + 16;\r\n  setProperty(\"snow2\", \"y\", snowY);\r\n  resetToTop(\"snow2\");\r\n  \r\n  //Move \"snow3\" by 16 pixels\r\n  snowY = getYPosition(\"snow3\");\r\n  snowY = snowY + 16;\r\n  setProperty(\"snow3\", \"y\", snowY);\r\n  resetToTop(\"snow3\");\r\n  \r\n  //Move \"snow4\" by 8 pixels\r\n  snowY = getYPosition(\"snow4\");\r\n  snowY = snowY + 8;\r\n  setProperty(\"snow4\", \"y\", snowY);\r\n  resetToTop(\"snow4\");\r\n  \r\n  //Move \"snow5\" by 8 pixels\r\n  snowY = getYPosition(\"snow5\");\r\n  snowY = snowY + 8;\r\n  setProperty(\"snow5\", \"y\", snowY);\r\n  resetToTop(\"snow5\");\r\n  \r\n  //Move \"snow6\" by 8 pixels\r\n  \r\n  //Move \"snow7\" by 4 pixels\r\n  \r\n  //Move \"snow8\" by 4 pixels\r\n  \r\n  //Move \"snow9\" by 4 pixels\r\n  \r\n  //Move \"snow10\" by 4 pixels\r\n  \r\n});\r\n\r\n\r\nfunction resetToTop(elt) {\r\n  var snowY = getYPosition(elt);\r\n  //Check if the star is off the bottom of the screen\r\n  if(snowY >= 450) {\r\n    //If it is, move it to the top of the screen\r\n    var snowHeight = getProperty(elt, \"height\");\r\n    setProperty(elt, \"y\", -1 * snowHeight);\r\n    //Move it to a new random x position\r\n    setProperty(elt, \"x\", randomNumber(20, 300));\r\n  }\r\n}\r\n\r\n",
