    "start_blocks": "//FUTURE ITERATION: Don't include hashing - just include to / from / signature\r\n//FUTURE ITERATION: Generate message text boxes activate live-updates - don't need to press generate message button\r\n\r\n\r\n//**CONSTANTS**\r\nvar SHAREDPRIME = SHAHashLibrary.sharedPrime();\r\nvar PRIVATENUM;\r\nvar PUBLICNUM;\r\nvar MESSAGE;\r\nvar HASH;\r\nvar ENCRYPTEDHASH;\r\n\r\n//**HELPER FUNCTIONS**\r\nfunction xgcd(a,b) {\r\n    if (b == 0) {\r\n      return [1, 0, a];\r\n    } else {\r\n        var temp = xgcd(b, a % b);\r\n        var x = temp[0];\r\n        var y = temp[1];\r\n        var d = temp[2];\r\n        return [y, x-y*Math.floor(a/b), d];\r\n    }\r\n}\r\n\r\nfunction createSignature(hash, prikey) {\r\n  //console.log(\"HASH: \" + hash);\r\n  var tempStr = \"\";\r\n  for(var i = 0; i < hash.length; i += 4) {\r\n    var subStr = hash.substr(i, 4);\r\n    //console.log(subStr);\r\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\r\n    //console.log(\"toDec: \" + toDec);\r\n    var encrypted = (toDec * prikey) % SHAREDPRIME;\r\n    //console.log(\"encrypted: \" + encrypted);\r\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\r\n    //console.log(\"toHex: \" + toHex);\r\n    while(toHex.length < 4) {\r\n      toHex = \"0\" + toHex;\r\n    }\r\n    tempStr += toHex;\r\n  }\r\n  //console.log(\"SIGNED HASH: \" + tempStr);\r\n  return tempStr;\r\n}\r\n\r\nfunction decryptSignature(encryptedHash, pubKey) {\r\n  var tempStr = \"\";\r\n  for(var i = 0; i < encryptedHash.length; i += 4) {\r\n    var subStr = encryptedHash.substr(i, 4);\r\n    //console.log(subStr);\r\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\r\n    //console.log(\"toDec: \" + toDec);\r\n    var encrypted = (toDec * pubKey) % SHAREDPRIME;\r\n    //console.log(\"encrypted: \" + encrypted);\r\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\r\n    //console.log(\"toHex: \" + toHex);\r\n    \r\n    while(toHex.length < 4) {\r\n      toHex = \"0\" + toHex;\r\n    }\r\n    \r\n    tempStr += toHex;\r\n  }\r\n  return tempStr;\r\n}\r\n\r\nfunction messageFormat(from, to, amount) {\r\n  return \"<From: \" + from + \" | To: \" + to + \" | Amount: \" + amount + \">\";\r\n}\r\n//**SCREEN 4 EVENTS**\r\n\r\nonEvent(\"screen4MessageButton\", \"click\", function() {\r\n  var name = getText(\"screen4FromInput\");\r\n  var toName = getText(\"screen4ToInput\");\r\n  var amount = getText(\"screen4AmountInput\");\r\n  MESSAGE = messageFormat(name, toName, amount);\r\n  setText(\"screen4MessageTextBox\", MESSAGE);\r\n  HASH = SHAHashLibrary.md5(MESSAGE);\r\n  setText(\"screen4HashTextBox\", HASH);\r\n  showElement(\"screen4HashLabel\");\r\n  showElement(\"screen4MessageTextBox\");\r\n  showElement(\"screen4HashTextBox\");\r\n  showElement(\"screen4PrivateLabel\");\r\n  showElement(\"screen4PrivateInput\");\r\n  showElement(\"screen4SignatureButton\");\r\n});\r\n\r\nonEvent(\"screen4SignatureButton\", \"click\", function() {\r\n  var privateNum = getNumber(\"screen4PrivateInput\");\r\n  ENCRYPTEDHASH = createSignature(HASH, privateNum);\r\n  setText(\"screen4SignatureTextBox\", ENCRYPTEDHASH);\r\n  showElement(\"screen4SignatureTextBox\");\r\n});\r\n\r\nfunction clearScreen() {\r\n  hideElement(\"screen4HashLabel\");\r\n  hideElement(\"screen4MessageTextBox\");\r\n  hideElement(\"screen4HashTextBox\");\r\n  hideElement(\"screen4PrivateLabel\");\r\n  hideElement(\"screen4PrivateInput\");\r\n  hideElement(\"screen4SignatureButton\");\r\n  hideElement(\"screen4SignatureTextBox\");\r\n}\r\n\r\nonEvent(\"screen4ToInput\", \"input\", function() {\r\n  clearScreen();\r\n});\r\n\r\nonEvent(\"screen4FromInput\", \"input\", function() {\r\n  clearScreen();\r\n});\r\n\r\nonEvent(\"screen4AmountInput\", \"input\", function() {\r\n  clearScreen();\r\n});\r\n\r\nonEvent(\"screen4PrivateInput\", \"input\", function() {\r\n  hideElement(\"screen4SignatureTextBox\");\r\n});\r\n\r\n",
