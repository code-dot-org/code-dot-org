tScreen(\"question2\");\r\n});\r\n//Q1 Logic\r\nfunction question1(click) {\r\n  if (click == \"bed\") {\r\n    increment([0, 3, 0, 1, 0, 3]);\r\n  } else if (click == \"indoors\") {\r\n    increment([5, 2, 1, 3, 6, 2]);\r\n  } else if (click == \"city\") {\r\n    increment([4, 2, 5, 3, 2, 2]);\r\n  } else if (click == \"nature\") {\r\n    increment([1, 3, 5, 3, 2, 3]);\r\n  }\r\n  showPoints();\r\n}\r\n\r\n\r\n//Q2 Events\r\nonEvent(\"q2sunny\", \"click\", function(event) {\r\n\tquestion2(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question3\");\r\n});\r\nonEvent(\"q2rainy\", \"click\", function(event) {\r\n\tquestion2(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question3\");\r\n});\r\nonEvent(\"q2snowy\", \"click\", function(event) {\r\n\tquestion2(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question3\");\r\n});\r\nonEvent(\"q2windy\", \"click\", function(event) {\r\n\tquestion2(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question3\");\r\n});\r\n//Q2 Logic\r\nfunction question2(click) {\r\n  if (click == \"sunny\") {\r\n    increment([5, 6, 1, 3, 2, 4]);\r\n  } else if (click == \"rainy\") {\r\n    increment([1, 1, 2, 3, 2, 1]);\r\n  } else if (click == \"snowy\") {\r\n    increment([0, 0, 6, 1, 2, 1]);\r\n  } else if (click == \"windy\") {\r\n    increment([4, 3, 1, 3, 2, 3]);\r\n  }\r\n  showPoints();\r\n}\r\n\r\n//Q3 Events\r\nonEvent(\"q3yes\", \"click\", function(event) {\r\n\tquestion3(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question4\");\r\n});\r\nonEvent(\"q3no\", \"click\", function(event) {\r\n\tquestion3(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question4\");\r\n});\r\n//Q3 Logic\r\nfunction question3(click) {\r\n  if (click == \"yes\") {\r\n    increment([8, 1, 10, 5, 8, 4]);\r\n  } else if (click == \"no\") {\r\n    increment([2, 9, 0, 5, 2, 6]);\r\n  }\r\n  showPoints();\r\n}\r\n\r\n\r\n//Q4 Events\r\nonEvent(\"q4casual\", \"click\", function(event) {\r\n\tquestion4(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question5\");\r\n});\r\nonEvent(\"q4professional\", \"click\", function(event) {\r\n\tquestion4(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question5\");\r\n});\r\nonEvent(\"q4work\", \"click\", function(event) {\r\n\tquestion4(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question5\");\r\n});\r\nonEvent(\"q4lazy\", \"click\", function(event) {\r\n\tquestion4(event.currentTargetId.substring(2));\r\n\tsetScreen(\"question5\");\r\n});\r\n//Q4 Logic\r\nfunction question4(click) {\r\n  if (click == \"casual\") {\r\n    increment([5, 4, 1, 4, 1, 3]);\r\n  } else if (click == \"professional\") {\r\n    increment([2, 0, 1, 4, 9, 3]);\r\n  } else if (click == \"work\") {\r\n    increment([1, 2, 8, 1, 0, 1]);\r\n  } else if (click == \"lazy\") {\r\n    increment([2, 4, 0, 1, 0, 3]);\r\n  }\r\n  showPoints();\r\n}\r\n\r\n//Q5 Events\r\nonEvent(\"q5yes\", \"click\", function(event) {\r\n\tquestion5(event.currentTargetId.substring(2));\r\n\tshowResults();\r\n\tsetScreen(\"resultsScreen\");\r\n});\r\nonEvent(\"q5no\", \"click\", function(event) {\r\n\tquestion5(event.currentTargetId.substring(2));\r\n\tshowResults();\r\n\tsetScreen(\"resultsScreen\");\r\n});\r\n//Q5 Logic\r\nfunction question5(click) {\r\n  if (click == \"yes\") {\r\n    increment([8, 5, 5, 10, 5, 5]);\r\n  } else if (click == \"no\") {\r\n    increment([2, 5, 5, 0, 5, 5]);\r\n  }\r\n  showPoints();\r\n}\r\n\r\n\r\nfunction increment(list) {\r\n  for (var i = 0; i < shoes.length; i++) {\r\n    shoes[i] += list[i];\r\n  }\r\n}\r\n\r\nfunction showPoints() {\r\n  var tempStr = \"\";\r\n  for(var i = 0; i < shoes.length; i++) {\r\n    tempStr += shoeNames[i] + \": \" + shoes[i] + \"\\n\";\r\n  }\r\n  setText(\"q1TextArea\", tempStr);\r\n  setText(\"q2TextArea\", tempStr);\r\n  setText(\"q3TextArea\", tempStr);\r\n  setText(\"q4TextArea\", tempStr);\r\n  setText(\"q5TextArea\", tempStr);\r\n  setText(\"resultsTextArea\", tempStr);\r\n}\r\n\r\n\r\nfunction showResults() {\r\n  //calculate max from array\r\n  var maxIndex = 0;\r\n  for (var i = 1; i < shoes.length; i++) {\r\n    if (shoes[i] > shoes[maxIndex]) {\r\n      maxIndex = i;\r\n    }\r\n  }\r\n  //show result based on max\r\n  setText(\"shoeLabel\", shoeNames[maxIndex]);\r\n  setImageURL(\"image2\", shoeImgs[maxIndex]);\r\n}\r\n",
