    "start_blocks": "hideElement(\"startImage\");\r\ncreateCanvas(\"background\");\r\nsetPosition(\"background\", 25, 80, 275, 60);\r\nsetFillColor(\"gray\");\r\nrect(0, 0, 320, 400);\r\nvar bits = [];\r\n\r\ntextLabel(\"instructions\", \"Use the widget to calculate binary numbers.\");\r\nsetPosition(\"instructions\", 30, 300, 240, 100);\r\nsetProperty(\"instructions\", \"font-size\", 20);\r\nsetProperty(\"instructions\", \"text-align\", \"center\");\r\n\r\n\r\nfor (var i = 0; i<6 ; i++) {\r\n  bits[i] = [];\r\n  bits[i].on = 0;\r\n  bits[i].value = Math.pow(2,i);\r\n  textLabel(\"bit\" + i, bits[i].value);\r\n  setProperty(\"bit\" + i, \"font-size\", 30);\r\n  setProperty(\"bit\" + i, \"text-color\", \"#303030\");\r\n  setProperty(\"bit\" + i, \"text-align\", \"center\");\r\n  setPosition(\"bit\" + i, 255 - 45 * i, 90, 40, 20);\r\n  textInput(\"bitEdit\" + i, \"\");\r\n  setProperty(\"bitEdit\" + i, \"font-size\",22);\r\n  setProperty(\"bitEdit\" + i, \"text-align\", \"center\");\r\n  setPosition(\"bitEdit\" + i, 255 - 45 * i, 85, 40, 45);\r\n  hideElement(\"bitEdit\" + i);\r\n  createCanvas(\"canvas\" + i);\r\n  setPosition(\"canvas\" + i, 255 - 45 * i, 85, 40,50);\r\n  setActiveCanvas(\"canvas\" + i);\r\n  setFillColor(\"black\");\r\n  rect(0,0, 350, 400);\r\n  onEvent(\"bit\" + i, \"click\", function(event) {\r\n    toggleBit(event.targetId.substring(3));\r\n  })\r\n  onEvent(\"canvas\" + i, \"click\", function(event) {\r\n    toggleBit(event.targetId.substring(6));\r\n  })\r\n}\r\n\r\n\r\ntextLabel(\"number\", \"0\");\r\nsetProperty(\"number\", \"text-color\", \"#303030\");\r\nsetProperty(\"number\", \"font-size\", 30);\r\nsetProperty(\"number\", \"text-align\", \"center\");\r\nsetPosition(\"number\", 100, 20, 120, 20);\r\n\r\nfunction toggleBit(value) {\r\n  //toggle 1 or 0 in array\r\n  bits[value].on = 1 - bits[value].on;\r\n\r\n  //show or hide bit\r\n  \r\n  setActiveCanvas(\"canvas\" + value);\r\n  if (bits[value].on == 1) {\r\n    setFillColor(\"white\");\r\n  } else {\r\n    setFillColor(\"black\");\r\n  }\r\n  rect(0,0, 350,400);\r\n  \r\n  //recalculate number\r\n  recalculate();\r\n}\r\n\r\n\r\n\r\n\r\nfunction recalculate() {\r\n  var value = 0;\r\n  for (var i=0; i<6; i++) {\r\n    value += bits[i].value * bits[i].on;\r\n  }\r\n  setText(\"number\", value);\r\n}",
