    "start_libraries": "[{\"name\":\"I18n_HAIW23L6\",\"description\":\"i18n strings for How AI Works 2023 Lesson 6\",\"functions\":[\"translate\",\"getTranslations\",\"testTranslatability\"],\"dropletConfig\":[{\"func\":\"I18n_HAIW23L6.translate\",\"category\":\"Functions\",\"comment\":\"Returns the i18n string for a given key\",\"type\":\"either\",\"params\":[\"key\"],\"paletteParams\":[\"key\"]},{\"func\":\"I18n_HAIW23L6.getTranslations\",\"category\":\"Functions\",\"comment\":\"returns the TRANSLATIONTEXT JSON object\",\"type\":\"either\"},{\"func\":\"I18n_HAIW23L6.testTranslatability\",\"category\":\"Functions\",\"comment\":\"This function adds \\\"!!\\\" to the start of every string in TRANSLATIONTEXT.\\nThis function is for DEBUGGING PURPOSES ONLY.\\nWhen debugging: A levelbuilder should call this at the very beginning of the level in order to\\nsee that all of the places where they expect a translation are getting a transaltion\\n(because the !! will show for translated texts)\",\"type\":\"either\"}],\"source\":\"var TRANSLATIONTEXT = {\\n  \\\"label1_1\\\": \\\"Image Select\\\",\\n  \\\"button2_7\\\": \\\"Crop\\\",\\n  \\\"resetBtn_8\\\": \\\"Reset\\\"\\n};\\n\\n//Note to self: check if labels / buttons have empty text and, if so, ignore them\\n\\n//Returns the i18n string for a given key\\nfunction translate(key) {\\n  return TRANSLATIONTEXT[key];\\n}\\n\\n//returns the TRANSLATIONTEXT JSON object\\nfunction getTranslations() {\\n  return TRANSLATIONTEXT;\\n}\\n\\n/*\\nThis function adds \\\"!!\\\" to the start of every string in TRANSLATIONTEXT.\\nThis function is for DEBUGGING PURPOSES ONLY.\\nWhen debugging: A levelbuilder should call this at the very beginning of the level in order to\\nsee that all of the places where they expect a translation are getting a transaltion\\n(because the !! will show for translated texts)\\n*/\\nfunction testTranslatability() {\\n  var keys = Object.keys(TRANSLATIONTEXT);\\n  for(var i = 0; i < keys.length; i++) {\\n    var key = keys[i];\\n    TRANSLATIONTEXT[key] = \\\"!!\\\" + TRANSLATIONTEXT[key];\\n  }\\n}\",\"originalName\":\"I18n_HAIW23L6\",\"versionId\":\"G2nPDpaWT76SaM8BHpoTLuVaxI7j_WVE\",\"channelId\":\"FKeZm8B2BQiojO82SwMB7w\",\"fromLevelbuilder\":true}]",
