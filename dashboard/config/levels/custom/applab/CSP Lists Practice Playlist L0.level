    "start_blocks": "//Step 1\r\nvar songs = getColumn(\"Viral 50 USA\", \"Track Name\");\r\nvar artists = getColumn(\"Viral 50 USA\", \"Artist\");\r\nvar index = 0;\r\nvar favorites = [];\r\n//Step 5\r\nvar notifications = [\"üôå\", \"ü§ò\", \"ü§©\"];\r\n\r\nupdateHomeScreen();\r\n\r\nfunction updateHomeScreen() {\r\n  hideElement(\"notification_label\");\r\n  //Step 2\r\n  setText(\"song_label\", songs[index]);\r\n  setText(\"artist_label\", artists[index]);\r\n}\r\nonEvent(\"right_button\", \"click\", function( ) {\r\n  //Step 3\r\n  if(index < songs.length-1) {\r\n    index = index + 1;\r\n  } \r\n  updateHomeScreen();\r\n});\r\n\r\nonEvent(\"left_button\", \"click\", function( ) {\r\n  //Step 3\r\n  if (index>0) {\r\n    index = index - 1;\r\n  } \r\n  updateHomeScreen();\r\n});\r\n\r\nonEvent(\"favorite_button\", \"click\", function( ) {\r\n  //Step 4\r\n  appendItem(favorites, songs[index]);\r\n  updateFavorites();\r\n});\r\n\r\nfunction updateFavorites() {\r\n  //Step 4\r\n  setText(\"favorites_output\", favorites.join(\"\\n\\n\"));\r\n  if(favorites.length < 5){\r\n    showElement(\"favorite_button\");\r\n  } else {\r\n    hideElement(\"favorite_button\");\r\n  }\r\n  \r\n  updateNotification();\r\n}\r\n\r\nfunction updateNotification() {\r\n  showElement(\"notification_label\");\r\n  //Step 6\r\n  setText(\"notification_label\", notifications[randomNumber(0, notifications.length-1)]);\r\n}\r\n\r\nonEvent(\"insert_button\", \"click\", function( ) {\r\n  //Step 7\r\n  var favoriteIndex = getNumber(\"position_dropdown\") - 1;\r\n  insertItem(favorites, favoriteIndex, songs[index]);\r\n  //Step 8\r\n  if(favorites.length > 5){\r\n    removeItem(favorites, favorites.length-1);\r\n  }\r\n  //Include in starter code\r\n  updateFavorites();\r\n});\r\n",
