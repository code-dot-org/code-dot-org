    "start_blocks": "//FUTURE ITERATION: Don't include hashing - just include to / from / signature\r\n//FUTURE ITERATION: Generate message text boxes activate live-updates - don't need to press generate message button\r\n\r\nhideElement(\"foregroundLabel\");\r\n\r\n//**CONSTANTS**\r\nvar SHAREDPRIME = SHAHashLibrary.sharedPrime();\r\nvar PRIVATENUM;\r\nvar PUBLICNUM;\r\nvar MESSAGE;\r\nvar HASH;\r\nvar ENCRYPTEDHASH;\r\n\r\n//**HELPER FUNCTIONS**\r\nfunction xgcd(a,b) {\r\n    if (b == 0) {\r\n      return [1, 0, a];\r\n    } else {\r\n        var temp = xgcd(b, a % b);\r\n        var x = temp[0];\r\n        var y = temp[1];\r\n        var d = temp[2];\r\n        return [y, x-y*Math.floor(a/b), d];\r\n    }\r\n}\r\n\r\nfunction createSignature(hash, prikey) {\r\n  //console.log(\"HASH: \" + hash);\r\n  var tempStr = \"\";\r\n  for(var i = 0; i < hash.length; i += 4) {\r\n    var subStr = hash.substr(i, 4);\r\n    //console.log(subStr);\r\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\r\n    //console.log(\"toDec: \" + toDec);\r\n    var encrypted = (toDec * prikey) % SHAREDPRIME;\r\n    //console.log(\"encrypted: \" + encrypted);\r\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\r\n    //console.log(\"toHex: \" + toHex);\r\n    while(toHex.length < 4) {\r\n      toHex = \"0\" + toHex;\r\n    }\r\n    tempStr += toHex;\r\n  }\r\n  //console.log(\"SIGNED HASH: \" + tempStr);\r\n  return tempStr;\r\n}\r\n\r\nfunction decryptSignature(encryptedHash, pubKey) {\r\n  var tempStr = \"\";\r\n  for(var i = 0; i < encryptedHash.length; i += 4) {\r\n    var subStr = encryptedHash.substr(i, 4);\r\n    //console.log(subStr);\r\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\r\n    //console.log(\"toDec: \" + toDec);\r\n    var encrypted = (toDec * pubKey) % SHAREDPRIME;\r\n    //console.log(\"encrypted: \" + encrypted);\r\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\r\n    //console.log(\"toHex: \" + toHex);\r\n    \r\n    while(toHex.length < 4) {\r\n      toHex = \"0\" + toHex;\r\n    }\r\n    \r\n    tempStr += toHex;\r\n  }\r\n  return tempStr;\r\n}\r\n\r\nfunction messageFormat(from, to, amount) {\r\n  return \"<From: \" + from + \" | To: \" + to + \" | Amount: \" + amount + \">\";\r\n}\r\n\r\n//**SCREEN 1 EVENTS**\r\nonEvent(\"priKeyInput\", \"input\", function( ) {\r\n  var val = getNumber(\"priKeyInput\");\r\n  if(val < SHAREDPRIME) {\r\n    showElement(\"generatePublicButton\");\r\n    hideElement(\"errorLabel\");\r\n  } else {\r\n    showElement(\"errorLabel\");\r\n    hideElement(\"generatePublicButton\");\r\n  }\r\n});\r\n\r\nonEvent(\"generatePublicButton\", \"click\", function() {\r\n  PRIVATENUM = getNumber(\"priKeyInput\");\r\n  PUBLICNUM = xgcd(PRIVATENUM, SHAREDPRIME)[0];\r\n  PUBLICNUM = (PUBLICNUM < 0 ? (PUBLICNUM + SHAREDPRIME) : PUBLICNUM); //make positive\r\n  showElement(\"outputText\");\r\n  var output = \"Private Key: \" + PRIVATENUM + \"\\nPublic Key: \" + PUBLICNUM;\r\n  setText(\"outputText\", output);\r\n  showElement(\"copyLabel2\");\r\n});\r\n\r\nonEvent(\"priKeyInput\", \"input\", function() {\r\n  hideElement(\"outputText\");\r\n  hideElement(\"copyLabel2\");\r\n});\r\n\r\n\r\n",
