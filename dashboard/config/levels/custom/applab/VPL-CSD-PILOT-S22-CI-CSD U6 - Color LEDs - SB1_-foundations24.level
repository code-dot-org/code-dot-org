    "start_blocks": "onEvent(\"onButton\", \"click\", function() {\r\n  colorLeds[4].on();\r\n  colorLeds[5].on();\r\n  \r\n});\r\nonEvent(\"offButton\", \"click\", function() {\r\n  colorLeds[4].off();\r\n  colorLeds[5].off();\r\n  \r\n});\r\n\r\n",
