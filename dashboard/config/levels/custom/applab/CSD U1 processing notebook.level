    "start_blocks": "var colors = [\"red\", \"orange\", \"yellow\", \"green\", \"blue\", \"purple\"];\r\nvar icons = [\"note\", \"smiley\", \"star\", \"heart\", \"thumb\"];\r\nvar URLs = [\"icon://fa-music\", \"icon://fa-smile-o\" , \"icon://fa-star\", \"icon://fa-heart\", \"icon://fa-thumbs-up\"];\r\nvar currentIcon = 0;\r\nvar currentColor = 0;\r\nsetProperty(\"note\", \"icon-color\", \"red\");\r\nvar stampCounter = 0;\r\n\r\nonEvent(\"note\", \"click\", function() {\r\n  if (currentIcon == 0) {\r\n    currentColor = (currentColor + 1)  % 6;\r\n    setProperty(\"note\", \"icon-color\", colors[currentColor]);\r\n  } else {\r\n    setProperty(icons[currentIcon] , \"icon-color\", \"black\");\r\n    setProperty(\"note\", \"icon-color\", colors[currentColor]);\r\n    currentIcon = 0;\r\n  }\r\n});\r\n\r\nonEvent(\"smiley\", \"click\", function() {\r\n  if (currentIcon == 1) {\r\n    currentColor = (currentColor + 1)  % 6;\r\n    setProperty(\"smiley\", \"icon-color\", colors[currentColor]);\r\n  } else {\r\n    setProperty(icons[currentIcon] , \"icon-color\", \"black\");\r\n    setProperty(\"smiley\", \"icon-color\", colors[currentColor]);\r\n    currentIcon = 1;\r\n  }\r\n});\r\n\r\nonEvent(\"star\", \"click\", function() {\r\n  if (currentIcon == 2) {\r\n    currentColor = (currentColor + 1)  % 6;\r\n    setProperty(\"star\", \"icon-color\", colors[currentColor]);\r\n  } else {\r\n    setProperty(icons[currentIcon] , \"icon-color\", \"black\");\r\n    setProperty(\"star\", \"icon-color\", colors[currentColor]);\r\n    currentIcon = 2;\r\n  }\r\n});\r\n\r\nonEvent(\"heart\", \"click\", function() {\r\n  if (currentIcon == 3) {\r\n    currentColor = (currentColor + 1)  % 6;\r\n    setProperty(\"heart\", \"icon-color\", colors[currentColor]);\r\n  } else {\r\n    setProperty(icons[currentIcon] , \"icon-color\", \"black\");\r\n    setProperty(\"heart\", \"icon-color\", colors[currentColor]);\r\n    currentIcon = 3;\r\n  }\r\n});\r\n\r\nonEvent(\"thumb\", \"click\", function() {\r\n  if (currentIcon == 4) {\r\n    currentColor = (currentColor + 1)  % 6;\r\n    setProperty(\"thumb\", \"icon-color\", colors[currentColor]);\r\n  } else {\r\n    setProperty(icons[currentIcon] , \"icon-color\", \"black\");\r\n    setProperty(\"thumb\", \"icon-color\", colors[currentColor]);\r\n    currentIcon = 4;\r\n  }\r\n});\r\n\r\nonEvent(\"image1\", \"click\", function(event) {\r\n  stampCounter++;\r\n  image(\"stamp\" + stampCounter, URLs[currentIcon]);\r\n  setPosition(\"stamp\" + stampCounter, event.x - 25, event.y - 25, 50, 50);\r\n  setProperty(\"stamp\" + stampCounter , \"icon-color\", colors[currentColor]);\r\n});",
