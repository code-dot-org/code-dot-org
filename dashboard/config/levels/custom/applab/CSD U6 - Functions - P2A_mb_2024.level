    "start_blocks": "//Events\r\nonBoardEvent(tempSensor, \"change\", function() {\r\n  loopCloud(\"cloud0\");\r\n  loopCloud(\"cloud1\");\r\n  loopCloud(\"cloud2\");\r\n  loopCloud(\"cloud3\");\r\n  loopCloud(\"cloud4\");\r\n});\r\n//Functions\r\nfunction loopCloud(cloudID) {\r\n  //1) Update the function to use the `cloudID` parameter\r\n  var cloudX = getProperty(\"cloud0\", \"x\");\r\n  setProperty(\"cloud0\", \"x\", cloudX - randomNumber(4,12));\r\n  if (cloudX < -100) {\r\n    setProperty(\"cloud0\", \"x\", 325);\r\n  }\r\n}\r\n",
