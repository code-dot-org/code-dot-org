    "start_blocks": "//Events\r\n//Check for changes in LED controls\r\nonBoardEvent(toggleSwitch, \"change\", function() {\r\n  updateLED();\r\n});\r\nonEvent(LEDtype, \"change\", function() {\r\n  updateLED();\r\n});\r\nonEvent(LEDspeed, \"change\", function() {\r\n  updateLED();\r\n});\r\n\r\n//Check for changes in buzzer controls\r\nonBoardEvent(buttonL, \"press\", function() {\r\n  playBuzzer();\r\n});\r\nonBoardEvent(buttonR, \"press\", function() {\r\n  buzzer.off();\r\n});\r\nonBoardEvent(\"duration\", \"change\", function() {\r\n  playBuzzer();\r\n});\r\nonEvent(\"buzzerFrequency\", \"change\", function() {\r\n  playBuzzer();\r\n});\r\n\r\n//Functions\r\n//Update LED according to controls\r\nfunction updateLED() {\r\n  if (toggleSwitch.isOpen) {\r\n    var LEDtype = getProperty(\"LEDtype\", \"value\");\r\n    var LEDinterval = getProperty(\"LEDspeed\", \"value\");\r\n    if (LEDtype == \"constant\") {\r\n      LED.on();\r\n    }\r\n    if (LEDtype = \"blink\") {\r\n      led.blink(LEDinterval);\r\n    }\r\n    if (LEDtype == \"pulse\") {\r\n      led.pulse(LEDinterval);\r\n    }\r\n  } else {\r\n    led.off();\r\n  }\r\n}\r\n\r\n//Play buzzer according to controls\r\nfunction playBuzzer() {\r\n  var duration = getProperty(\"duration\", \"value\");\r\n  var frequency = getProperty(\"frequency\", \"value\");\r\n  buzzer.frequency(duration, frequency);\r\n}\r\n",
