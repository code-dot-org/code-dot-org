    "start_blocks": "//FUTURE ITERATION: Don't include hashing - just include to / from / signature\n//FUTURE ITERATION: Generate message text boxes activate live-updates - don't need to press generate message button\n\nhideElement(\"foregroundLabel\");\n\n//**CONSTANTS**\nvar SHAREDPRIME = SHAHashLibrary.sharedPrime();\nvar PRIVATENUM;\nvar PUBLICNUM;\nvar MESSAGE;\nvar HASH;\nvar ENCRYPTEDHASH;\n\n//**HELPER FUNCTIONS**\nfunction xgcd(a,b) {\n    if (b == 0) {\n      return [1, 0, a];\n    } else {\n        var temp = xgcd(b, a % b);\n        var x = temp[0];\n        var y = temp[1];\n        var d = temp[2];\n        return [y, x-y*Math.floor(a/b), d];\n    }\n}\n\nfunction createSignature(hash, prikey) {\n  //console.log(\"HASH: \" + hash);\n  var tempStr = \"\";\n  for(var i = 0; i < hash.length; i += 4) {\n    var subStr = hash.substr(i, 4);\n    //console.log(subStr);\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\n    //console.log(\"toDec: \" + toDec);\n    var encrypted = (toDec * prikey) % SHAREDPRIME;\n    //console.log(\"encrypted: \" + encrypted);\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\n    //console.log(\"toHex: \" + toHex);\n    while(toHex.length < 4) {\n      toHex = \"0\" + toHex;\n    }\n    tempStr += toHex;\n  }\n  //console.log(\"SIGNED HASH: \" + tempStr);\n  return tempStr;\n}\n\nfunction decryptSignature(encryptedHash, pubKey) {\n  var tempStr = \"\";\n  for(var i = 0; i < encryptedHash.length; i += 4) {\n    var subStr = encryptedHash.substr(i, 4);\n    //console.log(subStr);\n    var toDec = SHAHashLibrary.hexStringToDecimal(subStr);\n    //console.log(\"toDec: \" + toDec);\n    var encrypted = (toDec * pubKey) % SHAREDPRIME;\n    //console.log(\"encrypted: \" + encrypted);\n    var toHex = SHAHashLibrary.decimalToHexString(encrypted);\n    //console.log(\"toHex: \" + toHex);\n    \n    while(toHex.length < 4) {\n      toHex = \"0\" + toHex;\n    }\n    \n    tempStr += toHex;\n  }\n  return tempStr;\n}\n\nfunction messageFormat(from, to, amount) {\n  return \"<From: \" + from + \" | To: \" + to + \" | Amount: \" + amount + \">\";\n}\n\n\n//**SCREEN 2 EVENTS**\n\nonEvent(\"messageButton\", \"click\", function () {\n  var name = getText(\"nameInput\");\n  var toName = getText(\"toInput\");\n  var amount = getText(\"amountInput\");\n  MESSAGE = messageFormat(name, toName, amount);\n  setText(\"messageTextBox\", MESSAGE);\n  showElement(\"messageTextBox\");\n  showElement(\"hashButton\");\n});\n\nonEvent(\"hashButton\", \"click\", function() {\n  HASH = SHAHashLibrary.md5(MESSAGE);\n  setText(\"hashTextBox\", HASH);\n  showElement(\"hashTextBox\");\n  showElement(\"copyLabel\");\n});\n\nfunction clearScreen() {\n  hideElement(\"messageTextBox\");\n  hideElement(\"hashButton\");\n  hideElement(\"hashTextBox\");\n  hideElement(\"copyLabel\");\n}\n\nonEvent(\"nameInput\", \"input\", function() {\n  clearScreen();\n});\n\nonEvent(\"toInput\", \"input\", function() {\n  clearScreen();\n});\n\nonEvent(\"amountInput\", \"input\", function() {\n  clearScreen();\n});\n",
