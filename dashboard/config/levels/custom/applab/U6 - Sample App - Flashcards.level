    "start_blocks": "// SAMPLE APP: FLASHCARDS\r\n\r\n// Global Variables\r\nvar isNewCard = false; // Boolean to track whether adding new card or editing existing card\r\n                       //   (used in btn_newCard_submit click handler)\r\nvar allCards = [];     // Store all card records from the database\r\nvar currentCard = -1;  // Tracks the current card displayed in allCards screen\r\nvar practiceCard = -1; // Tracks the current card displayed in practiceCards screen\r\n\r\n// Event handlers for screen_home buttons\r\nonEvent(\"btn_home_practice\", \"click\", function(event) {\r\n  // Get all existing flashcards from the database\r\n  readRecords(\"cards\", {}, function(records) {\r\n    if (records.length == 0) {\r\n      // If there are no cards in the database, go to emptyDeck screen\r\n      setScreen(\"screen_emptyDeck\");\r\n    }\r\n    else {\r\n      // If there are cards in the deck, load the first card and go to practice screen\r\n      allCards = records;\r\n      choosePracticeCard();\r\n      setScreen(\"screen_practiceCards\");\r\n    }\r\n  });\r\n});\r\nonEvent(\"btn_home_allCards\", \"click\", function(event) {\r\n  // Get all existing records\r\n  readRecords(\"cards\", {}, function(records) {\r\n    if (records.length == 0) {\r\n      // If there are no cards in the database, go to emptyDeck screen\r\n      setScreen(\"screen_emptyDeck\");\r\n    }\r\n    else {\r\n      // If there are cards in the deck, load the first card and go to allCards screen\r\n      // *********Load Card Details***********\r\n      allCards = records;\r\n      currentCard = 0;\r\n      loadCardDetails(currentCard);\r\n      setScreen(\"screen_allCards\");\r\n    }\r\n  });\r\n});\r\nonEvent(\"btn_home_newCard\", \"click\", function(event) {\r\n  isNewCard = true;\r\n  setText(\"newCard_titleLabel\", \"Add a New Card\");\r\n  setScreen(\"screen_newCard\");\r\n});\r\n\r\n// Event handlers for screen_practiceCards buttons\r\nonEvent(\"btn_practice_flipCard\", \"click\", function(event) {\r\n  // Display the answer for the current card\r\n  setText(\"cardText\", allCards[practiceCard].answer);\r\n  setProperty(\"btn_practice_flipCard\", \"hidden\", true);\r\n  setProperty(\"btn_practice_nextCard\", \"hidden\", false);\r\n});\r\nonEvent(\"btn_practice_nextCard\", \"click\", function(event) {\r\n  // Choose a random card from the deck\r\n  choosePracticeCard();\r\n  setProperty(\"btn_practice_flipCard\", \"hidden\", false);\r\n  setProperty(\"btn_practice_nextCard\", \"hidden\", true);\r\n});\r\nonEvent(\"btn_practice_back\", \"click\", function(event) {\r\n  setScreen(\"screen_home\");\r\n  // Reset flip and next button visibility\r\n  setProperty(\"btn_practice_flipCard\", \"hidden\", false);\r\n  setProperty(\"btn_practice_nextCard\", \"hidden\", true);\r\n});\r\n\r\n// Event handlers for screen_allCards buttons\r\nonEvent(\"screen_allCards\", \"keydown\", function(event) {\r\n  if (event.key == \"Right\") {\r\n    loadCardDetails(currentCard + 1);\r\n  }\r\n  else if (event.key == \"Left\") {\r\n    loadCardDetails(currentCard - 1);\r\n  }\r\n});\r\nonEvent(\"btn_allCards_next\", \"click\", function(event) {\r\n  loadCardDetails(currentCard + 1);\r\n});\r\nonEvent(\"btn_allCards_prev\", \"click\", function(event) {\r\n  loadCardDetails(currentCard - 1);\r\n});\r\nonEvent(\"btn_allCards_edit\", \"click\", function(event) {\r\n  isNewCard = false;\r\n  setText(\"newCard_titleLabel\", \"Edit Card Details\");\r\n  \r\n  // Pre-load existing question/answer text for the current card\r\n  setText(\"input_newQuestion\", allCards[currentCard].question);\r\n  setText(\"input_newAnswer\", allCards[currentCard].answer);\r\n  setScreen(\"screen_newCard\");\r\n});\r\nonEvent(\"btn_allCards_delete\", \"click\", function(event) {\r\n  // Delete current card from database\r\n  deleteRecord(\"cards\", allCards[currentCard], function(success) {\r\n    if (success) {\r\n      setScreen(\"screen_delete_success\");\r\n    }\r\n    else {\r\n      setScreen(\"screen_error\");\r\n      setText(\"errorLabel\", \"Something went wrong! (Inside deleteRecord)\");\r\n    }\r\n  });\r\n\r\n});\r\nonEvent(\"btn_allCards_back\", \"click\", function(event) {\r\n  setScreen(\"screen_home\");\r\n});\r\n\r\n// Event handlers for screen_newCard buttons\r\nonEvent(\"btn_newCard_submit\", \"click\", function(event) {\r\n  var question = getText(\"input_newQuestion\");\r\n  var answer = getText(\"input_newAnswer\");\r\n\r\n  if (question == \"\" || answer == \"\") {\r\n    // If either question or answer is empty, display an error message.\r\n    setText(\"newCard_messageLabel\", \"Both sides of the card must be filled out. Please try again.\");\r\n  }\r\n  else if (isNewCard) {\r\n    // If adding a new card, use createRecord\r\n    createRecord(\"cards\", {question:question, answer:answer}, function(record) {\r\n      // When finished creating new card, go to success screen\r\n      setScreen(\"screen_newCard_success\");\r\n      clearNewCardScreen();\r\n    });\r\n  }\r\n  else {\r\n    // If editing an existing card, use updateRecord\r\n    \r\n    // Update a local copy of the current card object with the new question and answer\r\n    var cardToUpdate = allCards[currentCard];\r\n    cardToUpdate.question = question;\r\n    cardToUpdate.answer = answer;\r\n    \r\n    // Attempt to update the corresponding card record in the database\r\n    updateRecord(\"cards\", cardToUpdate, function(record, success) {\r\n      // When finished updating a card, go back to allCards screen\r\n      if (success) {\r\n        // Update local copy of card\r\n        allCards[currentCard] = record;\r\n        // Update UI elements and go back to allCards screen\r\n        loadCardDetails(currentCard);\r\n        setScreen(\"screen_allCards\");\r\n        clearNewCardScreen();\r\n      }\r\n      else {\r\n        setScreen(\"screen_error\");\r\n        setText(\"errorLabel\", \"Something went wrong! (Inside updateRecord)\");\r\n      }\r\n    });\r\n  }\r\n});\r\nonEvent(\"btn_newCard_back\", \"click\", function(event) {\r\n  if (isNewCard) {\r\n    // If adding a new card, go back to home screen\r\n    setScreen(\"screen_home\");\r\n  }\r\n  else {\r\n    // If editing an existing card, go back to list of all cards\r\n    setScreen(\"screen_allCards\");\r\n  }\r\n  clearNewCardScreen();\r\n});\r\n\r\n// Event handlers for screen_newCard_success buttons\r\nonEvent(\"btn_success_home\", \"click\", function(event) {\r\n  setScreen(\"screen_home\");\r\n});\r\nonEvent(\"btn_success_addAnother\", \"click\", function(event) {\r\n  setScreen(\"screen_newCard\");\r\n});\r\n\r\n// Event handlers for screen_emptyDeck buttons\r\nonEvent(\"btn_emptyDeck_back\", \"click\", function(event) {\r\n  setScreen(\"screen_home\");\r\n});\r\n\r\n// Event handlers for screen_delete_success buttons\r\nonEvent(\"btn_delete_home\", \"click\", function(event) {\r\n  setScreen(\"screen_home\");\r\n});\r\n\r\n// Event handlers for screen_error buttons\r\nonEvent(\"btn_error_home\", \"click\", function(event) {\r\n  setScreen(\"screen_home\");\r\n});\r\n\r\n// Functions\r\n\r\n// Reset the UI elements on screen_allCards\r\nfunction clearNewCardScreen() {\r\n  setText(\"input_newQuestion\", \"\");\r\n  setText(\"input_newAnswer\", \"\");\r\n}\r\n// Pick a random card from the deck\r\nfunction choosePracticeCard() {\r\n  // Pick a random card from the deck\r\n  practiceCard = randomNumber(0, allCards.length - 1);\r\n  \r\n  // Load UI elements\r\n  setText(\"cardText\", allCards[practiceCard].question);\r\n}\r\n// Populate allCards UI elements with info from the card at allCards[index]\r\nfunction loadCardDetails(index) {\r\n  // Check index, wrap if out of bounds of allCards array\r\n  if (index == -1) {\r\n    // If going off left side of array, wrap index to right end of array\r\n    index = allCards.length - 1;\r\n  }\r\n  else if (index == allCards.length) {\r\n    // If going off the right side of array, wrap index to left end of array\r\n    index = 0;\r\n  }\r\n  \r\n  currentCard = index;\r\n\r\n  // Load the text areas for the current card\r\n  setText(\"allCards_showQuestion\", allCards[currentCard].question);\r\n  setText(\"allCards_showAnswer\", allCards[currentCard].answer);\r\n}",
