    "start_blocks": "var weather = getList(\"weather\", \"City\");\r\nvar weatherDate = getList(\"weather\", \"Date\");\r\nvar icon = getList(\"weather\", \"Icon\");\r\nvar condition = getList(\"weather\", \"Condition\");\r\n\r\nvar filteredWeather = [];\r\nvar filteredWeatherDate = [];\r\nvar filteredIcon = [];\r\nvar filteredCondition = [];\r\n\r\nvar city = getText(\"cityDropdown\");\r\n\r\nvar index = 0;\r\n\r\nfilter();\r\nupdateScreen();\r\n\r\nonEvent(\"cityDropdown\", \"change\", function( ) {\r\n  city = getText(\"cityDropdown\");\r\n  filter();\r\n  updateScreen();\r\n});\r\n\r\nonEvent(\"leftButton\", \"click\", function( ) {\r\n  if(index>0){\r\n    index--;\r\n  } else {\r\n    index = filteredWeather.length-1; \r\n  }\r\n  updateScreen();\r\n});\r\nonEvent(\"rightButton\", \"click\", function( ) {\r\n  if(index <filteredWeather.length-1){\r\n    index++;\r\n  } else {\r\n    index = 0;\r\n  }\r\n  \r\n  updateScreen();\r\n});\r\n\r\n\r\n\r\nfunction filter(){\r\n  filteredWeather = [];\r\n  filteredWeatherDate = [];\r\n  filteredIcon = [];\r\n  filteredCondition = [];\r\n  \r\n  for(var i=0; i<weather.length; i++){\r\n    if(weather[i] == city){\r\n      appendItem(filteredWeather, weather[i]);\r\n      appendItem(filteredWeatherDate, weatherDate[i]);\r\n      appendItem(filteredIcon, icon[i]);\r\n      appendItem(filteredCondition, condition[i]);\r\n    }\r\n  }\r\n}\r\n\r\nfunction updateScreen(){\r\n  setImageURL(\"iconOutput\", filteredIcon[index]);\r\n  setText(\"dateOutput\", filteredWeatherDate[index]);\r\n  setText(\"conditionOutput\", filteredCondition [index]);\r\n}\r\n\r\n",
