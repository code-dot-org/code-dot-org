    "start_blocks": "// pulls the first column of data from the \"words\" table and stores it in a list\r\nvar wordList = getList(\"words\", \"Word\");\r\n\r\nvar letterCount = 2;\r\nvar word;\r\nvar letter;\r\n\r\nvar filteredWordList = [];\r\n\r\nsetUp();\r\n\r\n// When either the 2 or 3 button is clicked, letterCount is updated and the setUp function is called\r\nonEvent(\"twoButton\", \"click\", function( ) {\r\n  letterCount = 2;\r\n  setUp();\r\n});\r\nonEvent(\"threeButton\", \"click\", function( ) {\r\n  letterCount = 3;\r\n  setUp();\r\n});\r\n\r\n// This checks if the guess is correct by comparing it to the random word that was pulled out of the list\r\n// The guess and the actual word are changed to lowercase to account for any capitalization issues\r\nonEvent(\"checkButton\", \"click\", function( ) {\r\n  var guess = getText(\"wordInput\").toLowerCase();\r\n  if(guess == word.toLowerCase()){\r\n    setText(\"feedbackOutput\", \"You win!\");\r\n    setUp();\r\n  } else {\r\n    setText(\"feedbackOutput\", \"Try again!\");\r\n  }\r\n  \r\n  setText(\"wordInput\", \"\");\r\n});\r\n\r\n// creates the filtered list of words\r\nfunction filter(){\r\n  // start with a blank list\r\n  filteredWordList = [];\r\n\r\n  // append items to filteredWordList that have the letterCount length\r\n  for(var i=0; i<wordList.length; i++){\r\n    if(wordList[i].length == letterCount){\r\n      appendItem(filteredWordList, wordList[i]);\r\n    }\r\n  }\r\n}\r\n\r\nfunction setUp(){\r\n\tfilter();\r\n\t \r\n\t // setup the color of the buttons to indicate which one was selected\r\n\tif(letterCount == 2){\r\n\t   setProperty(\"twoButton\", \"background-color\", \"#e0fb25\");\r\n\t   setProperty(\"threeButton\", \"background-color\", \"#effaff\");\r\n\r\n\t } else {\r\n\t   setProperty(\"threeButton\", \"background-color\", \"#e0fb25\");\r\n\t   setProperty(\"twoButton\", \"background-color\", \"#effaff\");\r\n\t }\r\n\t \r\n\t // choose a random word from filteredWordList\r\n\tword = filteredWordList[randomNumber(0, filteredWordList.length-1)];\r\n\t \r\n\t // get the first letter of the word\r\n\tletter = word.substring(0,1).toUpperCase();\r\n\t \r\n\t // display the first letter as a hint\r\n\tsetText(\"letterOutput\", letter);\r\n}\r\n",
