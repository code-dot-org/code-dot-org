    "start_blocks": "var choreList  = [\"Laundry\",\"Dishes\"];\r\nvar peopleList = [\"GT\",\"Hannah\"];\r\n\r\nupdateScreen();\r\n\r\nfunction updateScreen(){\r\n  setProperty(\"choresLabel\",\"text\",choreList.join(\"\\n\"));\r\n  setProperty(\"peopleLabel\",\"text\",peopleList.join(\"\\n\"));\r\n  setProperty(\"choreDropdown\",\"options\",choreList);\r\n  setProperty(\"peopleDropdown\",\"options\",peopleList);\r\n}\r\n\r\nonEvent(\"addChoreButton\",\"click\",function(){\r\n  var newChore = getText(\"choreInput\");\r\n  appendItem(choreList,newChore);\r\n  setProperty(\"choreInput\",\"text\",\"\");\r\n  updateScreen();\r\n});\r\n\r\nonEvent(\"addPersonButton\",\"click\",function(){\r\n  var newPerson = getText(\"personInput\");\r\n  appendItem(peopleList,newPerson);\r\n  setProperty(\"personInput\",\"text\",\"\");\r\n  updateScreen();\r\n});\r\n\r\nonEvent(\"removeChoreButton\",\"click\",function(){\r\n  var choreIndex = getProperty(\"choreDropdown\",\"index\");\r\n  removeItem(choreList,choreIndex);\r\n  updateScreen();\r\n});\r\n\r\nonEvent(\"removePersonButton\",\"click\",function(){\r\n  var personIndex = getProperty(\"personDropdown\",\"index\");\r\n  removeItem(peopleList,personIndex);\r\n  updateScreen();\r\n});\r\n\r\nonEvent(\"randomAssignButton\",\"click\",function(){\r\n  if((peopleList.length > 0) && (choreList.length > 0)){\r\n    var randomPersonIndex = randomNumber(0,peopleList.length-1);\r\n    var randomPerson = peopleList[randomPersonIndex];\r\n    \r\n    var randomChoreIndex = randomNumber(0,choreList.length-1);\r\n    var randomChore = choreList[randomChoreIndex];\r\n    \r\n    setProperty(\"choreAssignment\",\"text\",randomPerson + \": \" + randomChore);\r\n    \r\n    removeItem(choreList, randomChoreIndex);\r\n    removeItem(peopleList, randomPersonIndex);\r\n  } else {\r\n    setProperty(\"choreAssignment\",\"text\",\"Add more chores or people\");\r\n  }\r\n  updateScreen();\r\n});",
