dex=\"1\" data-theme=\"default\" id=\"screen2\" style=\"display: none; height: 450px; width: 320px; left: 0px; top: 0px; position: absolute; z-index: 0; background-color: rgb(255, 255, 255);\"></div></div>",
    "start_blocks": "//HEY DAN - UPDATE THIS EACH TIME YOU ADD A NEW FEATURE!\r\nvar options = [\"Size\", \"Hardness\", \"Sourness\", \"Easy to Eat\", \"Juiciness\", \"Sweetness\", \"Can Smell It\", \"Green-ness\", \"Stackability\"];\r\nsetProperty(\"feature1Dropdown\", \"options\", options);\r\nsetProperty(\"feature2Dropdown\", \"options\", options);\r\nsetProperty(\"feature2Dropdown\", \"index\", 1);\r\nshowElement(\"dropdownCover\");\r\nshowModelElts(false);\r\nhideElement(\"instructionsLabel\");\r\nshowModelResultsElts(false);\r\n\r\n\r\nvar ORANGEURL = \"https://cdn.glitch.com/863b2e2c-c3b7-405e-8ece-1585b4e83e88%2Forange.png?v=1620167468208\";\r\nvar PURPLEURL = \"https://cdn.glitch.com/863b2e2c-c3b7-405e-8ece-1585b4e83e88%2Fpurple.jpg?v=1620167473662\";\r\nvar GRAYURL = \"https://cdn.glitch.com/863b2e2c-c3b7-405e-8ece-1585b4e83e88%2Fgraysquare.png?v=1620182289152\";\r\nvar CANVWIDTH = 300;\r\nvar CANVHEIGHT = CANVWIDTH;\r\nvar OFFSET = 20;\r\nvar GRIDLINES = 10;\r\nvar MULTIPLIER = CANVWIDTH / GRIDLINES;\r\n\r\nvar LESSTHAN = \"veggie\";\r\n\r\nvar foodName = getColumn(\"Foods\", \"Name\");\r\nvar foodLabel = getColumn(\"Foods\", \"Label\");\r\nvar testId;\r\nvar testLabel = [];\r\n\r\nvar foodFeature1;\r\nvar foodFeature2;\r\ngetNewColumns();\r\n\r\nvar clickedOnce = false;\r\nvar startPt = {\"x\": 0, \"y\": 0};\r\nvar endPt = {\"x\": 0, \"y\": 0};\r\ncreateCanvas(\"bg\", CANVWIDTH + OFFSET, CANVHEIGHT + OFFSET);\r\ncreateCanvas(\"grid\", CANVWIDTH + OFFSET, CANVHEIGHT + OFFSET);\r\ncreateCanvas(\"lines\", CANVWIDTH + OFFSET, CANVHEIGHT + OFFSET);\r\ncreateCanvas(\"clicks\", CANVWIDTH + OFFSET, CANVHEIGHT + OFFSET);\r\n\r\nupdateGradient();\r\nhideElement(\"bg\");\r\ndrawGrid();\r\ngenerateFoodLabels();\r\nshowTestElts(false);\r\nhideElement(\"loadingLabel\");\r\ndrawFoods();\r\nsetActiveCanvas(\"clicks\");\r\n\r\n\r\n\r\nonEvent(\"screen1\", \"click\", function(e) {\r\n  //console.log(e);\r\n  //Hack to make sure we're clicking within the canvas and not on UI elements\r\n  if (e.targetId != \"grid\" && e.targetId != \"lines\" && e.targetId != \"clicks\") {\r\n    return;\r\n  }\r\n  if (clickedOnce == false) {\r\n    clearCanvas();\r\n    setStrokeColor(\"blue\");\r\n    setFillColor(\"blue\");\r\n    startPt.x = e.pageX;\r\n    startPt.y = e.pageY;\r\n    circle(startPt.x, startPt.y, 2);\r\n    setActiveCanvas(\"lines\");\r\n    clickedOnce = true;\r\n    showModelElts(false);\r\n  } else {\r\n    clearCanvas();\r\n    setActiveCanvas(\"clicks\");\r\n    endPt.x = e.pageX;\r\n    endPt.y = e.pageY;\r\n    line(startPt.x, startPt.y, e.pageX, e.pageY);\r\n    circle(e.pageX, e.pageY, 2);\r\n    var angle = calcAngle(startPt.x, startPt.y, e.pageX, e.pageY);\r\n    angle = Math.round(angle);\r\n    //console.log(angle);\r\n    showElement(\"bg\");\r\n    //GRADIENT VIA TRANSFORM\r\n    var transformX = Math.round(startPt.x - (CANVWIDTH + OFFSET) / 2);\r\n    var transformY = Math.round(startPt.y - (CANVHEIGHT + OFFSET) / 2);\r\n    //SERIES OF TRANSFORMATIONS:\r\n    //Move the coordinate plane by x and y first to (don't remember how I got the pt above - midpts between starting coordinate and edge of canvas?)\r\n    //Rotate by the angle of the line (using arctan calculation on the slope of the line)\r\n    //scale the canvas so it takes up the entire visual field\r\n    //NOT SEEN: There's a hidden design label with a white background that covers the bottom of the screen\r\n    //So that when the scale of the gradient element is too big, it doesn't show up because it's blocked by the hidden design element\r\n    setStyle(\"bg\", \"transform: translateX(\" + transformX + \"px) translateY(\" + transformY + \"px) rotateZ(\"+ angle +\"deg) scaleX(3) scaleY(3)\");\r\n    checkAccuracy(startPt.x, startPt.y, endPt.x, endPt.y);\r\n    clickedOnce = false;\r\n    showModelElts(true);\r\n  }\r\n});\r\nfunction calcAngle(x1, y1, x2, y2) {\r\n  var DEBUG = false;\r\n  var width = x2 - x1;\r\n  var height = y2 - y1;\r\n  //inverse tan\r\n  var angle = Math.atan(height/width);\r\n  //convert to degrees\r\n  angle = (angle * 180) / Math.PI;\r\n  if(DEBUG) {\r\n    console.log(\"WIDTH: \" + width);\r\n    console.log(\"HEIGHT: \" + height);\r\n    console.log(\"ANGLE: \" + angle);\r\n  }\r\n  return angle;\r\n}\r\n\r\nfunction showModelElts(bool) {\r\n  if(bool) {\r\n    showElement(\"testBtn\");\r\n    showElement(\"flipColorsBtn\");\r\n    showElement(\"fruitAccuracyLabel\");\r\n    showElement(\"veggieAccuracyLabel\");\r\n  } else {\r\n    hideElement(\"testBtn\");\r\n    hideElement(\"flipColorsBtn\");\r\n    hideElement(\"fruitAccuracyLabel\");\r\n    hideElement(\"veggieAccuracyLabel\");\r\n  }\r\n}\r\n\r\nfunction updateGradient() {\r\n    //console.log(\"here\");\r\n    //console.log(LESSTHAN);\r\n  if(LESSTHAN == \"veggie\") {\r\n    setStyle(\"bg\", \"background-image: linear-gradient(180deg, rgba(255, 165, 0, 0.3) 50%, rgba(148,0,211, 0.3) 50%);\");\r\n  } else if (LESSTHAN == \"fruit\") {\r\n    setStyle(\"bg\", \"background-image: linear-gradient(180deg, rgba(148,0,211, 0.3) 50%, rgba(255, 165, 0, 0.3) 50%);\");\r\n  }\r\n  \r\n}\r\n\r\nfunction checkAccuracy(x1, y1, x2, y2) {\r\n  var m = (y2 - y1)/(x2 - x1);\r\n  var veggieCount = 0;\r\n  var fruitCount = 0;\r\n  var veggieTotal = 0;\r\n  var fruitTotal = 0;\r\n  //y = m(x - x1) + y1\r\n  //console.log(\"(x1, y1): (\" + x1 + \",\" + y1 + \")\");\r\n  //console.log(\"(x2, y2): (\" + x2 + \",\" + y2 + \")\");\r\n  //console.log(\"---------------------------------------\");\r\n  //console.log(\"---------------------------------------\");\r\n  //console.log(\"---------------------------------------\");\r\n  for(var i = 0; i < foodName.length; i++) {\r\n    var imgId = \"img\"+i;\r\n    //setPosition(imgId, foodFeature1[i]*MULTIPLER- 5, CANVHEIGHT - foodFeature2[i]*MULTIPLIER - 5, 10, 10);\r\n    var imgX = getXPosition(imgId);\r\n    var imgY = getYPosition(imgId);\r\n    //console.log(\"(imgX, imgY): (\" + imgX + \",\" + imgY + \")\");\r\n    var imgLabel = foodLabel[i];\r\n    //DAN DISCOVERY! Because coordinates are flipped in computer graphics,\r\n    //UNDER the graph is actually >, even though in a regular coordinate plane it would be <\r\n    var lessThanBool = (imgY >= m*(imgX - x1)+y1);\r\n    //console.log(\"lessThanBool: \" + lessThanBool + \"; food: \" + foodName[i]);\r\n    if(imgLabel == \"Veggie\") {\r\n      //currently looking at a veggie image\r\n      veggieTotal++;\r\n      if(lessThanBool && LESSTHAN == \"veggie\") {\r\n        //the veggie image is under the line AND under the line is veggie\r\n        veggieCount++;\r\n      } else if (!lessThanBool && LESSTHAN != \"veggie\") {\r\n        //veggie image is NOT under line and under the line is NOT veggie\r\n        //This implies: image is ABOVE the line, and above the line IS veggie\r\n        veggieCount++;\r\n      }\r\n    } else if (imgLabel == \"Fruit\") {\r\n      //currently looking at a fruit image\r\n      fruitTotal++;\r\n      if(lessThanBool && LESSTHAN == \"fruit\") {\r\n        //the veggie image is under the line AND under the line is veggie\r\n        fruitCount++;\r\n      } else if (!lessThanBool && LESSTHAN != \"fruit\") {\r\n        //veggie image is NOT under line and under the line is NOT veggie\r\n        //This implies: image is ABOVE the line, and above the line IS veggie\r\n        fruitCount++;\r\n      }\r\n    }\r\n  }\r\n  setText(\"veggieAccuracyLabel\", \"Veggie Accuracy: \" + veggieCount + \" / \" + veggieTotal + \" (\" + Math.round((veggieCount/veggieTotal)*100) + \"%)\");\r\n  setText(\"fruitAccuracyLabel\", \"Fruit Accuracy: \" + fruitCount + \" / \" + fruitTotal + \" (\" + Math.round((fruitCount/fruitTotal)*100) + \"%)\");\r\n  \r\n}\r\n\r\n\r\nonEvent(\"screen1\", \"mousemove\", function(e) {\r\n  if (clickedOnce == true) {\r\n    clearCanvas();\r\n    setStrokeColor(\"blue\");\r\n    line(startPt.x, startPt.y, e.pageX, e.pageY);\r\n  }\r\n});\r\nfunction drawGrid() {\r\n  setActiveCanvas(\"grid\");\r\n  setStrokeColor(\"#AAAAAA\");\r\n  for (var i = 1; i < GRIDLINES+1; i++) {\r\n    line(0, i*MULTIPLIER, CANVWIDTH, i*MULTIPLIER);\r\n    line(i*MULTIPLIER, 0, i*MULTIPLIER, CANVHEIGHT);\r\n  }\r\n}\r\n// function drawFoodsCanvas() {\r\n//   setActiveCanvas(\"grid\");\r\n//   for (var i = 0; i < foodName.length; i++) {\r\n//     if (foodLabel[i] == \"Fruit\") {\r\n//       setFillColor(\"orange\");\r\n//     } else if (foodLabel[i] == \"Veggie\") {\r\n//       setFillColor(\"purple\");\r\n//     }\r\n//     circle(__, __, 5);\r\n//   }\r\n// }\r\nfunction generateFoodLabels() {\r\n  for (var i = 0; i < foodName.length; i++) {\r\n    var imgId = \"img\"+i;\r\n    image(imgId, \"\");\r\n    if (foodLabel[i] == \"Fruit\") {\r\n      setImageURL(imgId, ORANGEURL);\r\n    } else if (foodLabel[i] == \"Veggie\") {\r\n      setImageURL(imgId, PURPLEURL);\r\n    }\r\n    setPosition(imgId, -50, -50, 10, 10);\r\n    onEvent(imgId, \"mouseover\", function(e) {\r\n      var imgIndex = e.currentTargetId.substring(3);\r\n      setText(\"hoverLabel\", foodName[imgIndex]);\r\n      if(foodLabel[imgIndex] == \"Fruit\") {\r\n        setProperty(\"hoverLabel\", \"background-color\", \"rgba(255, 165, 0, 0.3)\");\r\n      } else {\r\n        setProperty(\"hoverLabel\", \"background-color\", \"rgba(148, 0, 211, 0.3)\");\r\n      }\r\n    });\r\n    onEvent(imgId, \"mouseout\", function() {\r\n      setText(\"hoverLabel\", \"Hover over a dot to see its name\");\r\n      setProperty(\"hoverLabel\", \"background-color\", \"rgb(219, 219, 219)\");\r\n    });\r\n  }\r\n  \r\n  testId = \"testImg\";\r\n  testLabel[0] = \"TESTTESTTEST\";\r\n  image(testId, GRAYURL);\r\n  setPosition(testId, -50, -50, 10, 10);\r\n  onEvent(testId, \"mouseover\", function() {\r\n    setText(\"hoverLabel\", testLabel[0]);\r\n  });\r\n  onEvent(testId, \"mouseout\", function() {\r\n    setText(\"hoverLabel\", \"\");\r\n  });\r\n  \r\n}\r\n\r\n// function drawFoodsAllAtOnce() {\r\n//   for (var i = 0; i < foodName.length; i++) {\r\n//     var imgId = \"img\"+i;\r\n//     setPosition(imgId, foodFeature1[i]*MULTIPLIER- 5, CANVHEIGHT - foodFeature2[i]*MULTIPLIER - 5, 10, 10);\r\n//   }\r\n//}\r\nfunction drawFoods() {\r\n  showElement(\"dropdownCover\");\r\n  hideElement(\"instructionsLabel\");\r\n  setPosition(testId, -50, -50, 10, 10);\r\n  drawFoodsHelper(0);\r\n}\r\nfunction drawFoodsHelper(i) {\r\n  var imgId = \"img\"+i;\r\n  setPosition(imgId, foodFeature1[i]*MULTIPLIER- 5, CANVHEIGHT - foodFeature2[i]*MULTIPLIER - 5, 10, 10);\r\n  if (i+1 < foodName.length) {\r\n    setTimeout(function() {\r\n      drawFoodsHelper(i+1);\r\n    }, 200);\r\n  } else {\r\n    hideElement(\"dropdownCover\");\r\n    showElement(\"instructionsLabel\");\r\n  }\r\n}\r\n\r\nfunction update() {\r\n  setActiveCanvas(\"clicks\");\r\n\tclearCanvas();\r\n\tgetNewColumns();\r\n\tdrawFoods();\r\n}\r\n\r\nfunction getNewColumns() {\r\n  foodFeature1 = getColumn(\"Foods\", getText(\"feature1Dropdown\"));\r\n  foodFeature2 = getColumn(\"Foods\", getText(\"feature2Dropdown\"));\r\n}\r\nfunction resetScreen() {\r\n  setActiveCanvas(\"clicks\");\r\n  clearCanvas();\r\n  hideElement(\"bg\");\r\n  for (var i = 0; i < foodName.length; i++) {\r\n    var imgId = \"img\"+i;\r\n    setPosition(imgId, -50, -50, 10, 10);\r\n  }\r\n  setText(\"veggieAccuracyLabel\", \"Veggie Accuracy:\");\r\n  setText(\"fruitAccuracyLabel\", \"Fruit Accuracy:\");\r\n  showModelElts(false);\r\n}\r\n\r\nfunction drawTestImg() {\r\n  var testName = getText(\"zzFoodInput\");\r\n  var testVal1 = getNumber(\"zzScale1Slider\");\r\n  var testVal2 = getNumber(\"zzScale2Slider\");\r\n  \r\n  var imgX = testVal1*MULTIPLIER- 5;\r\n\tvar imgY = CANVHEIGHT - testVal2*MULTIPLIER - 5;\r\n\tsetPosition(testId, imgX, imgY, 10, 10);\r\n\ttestLabel[0] = testName;\r\n \r\n\tvar m = (endPt.y - startPt.y)/(endPt.x - startPt.x);\r\n\tvar lessThanBool = (imgY >= m*(imgX - startPt.x)+startPt.y);\r\n  \r\n  showModelResultsElts(true);\r\n  \r\n  if(lessThanBool && LESSTHAN == \"veggie\") {\r\n    //the veggie image is under the line AND under the line is veggie\r\n    setText(\"yyModelDecision\", \"Veggie!\");\r\n    setProperty(\"yyModelDecision\", \"background-color\", \"rgba(148,0,211, 0.3)\");\r\n  } else if (!lessThanBool && LESSTHAN != \"veggie\") {\r\n    //veggie image is NOT under line and under the line is NOT veggie\r\n    //This implies: image is ABOVE the line, and above the line IS veggie\r\n    setText(\"yyModelDecision\", \"Veggie!\");\r\n    setProperty(\"yyModelDecision\", \"background-color\", \"rgba(148,0,211, 0.3)\");\r\n  } else if(lessThanBool && LESSTHAN == \"fruit\") {\r\n    //the veggie image is under the line AND under the line is veggie\r\n    setText(\"yyModelDecision\", \"Fruit!\");\r\n    setProperty(\"yyModelDecision\", \"background-color\", \"rgba(255, 165, 0, 0.3)\");\r\n  } else if (!lessThanBool && LESSTHAN != \"fruit\") {\r\n    //veggie image is NOT under line and under the line is NOT veggie\r\n    //This implies: image is ABOVE the line, and above the line IS veggie\r\n    setText(\"yyModelDecision\", \"Fruit!\");\r\n    setProperty(\"yyModelDecision\", \"background-color\", \"rgba(255, 165, 0, 0.3)\");\r\n  }\r\n}\r\n\r\nonEvent(\"testBtn\", \"click\", function( ) {\r\n\tshowTestElts(true);\r\n});\r\n\r\nonEvent(\"feature1Dropdown\", \"change\", function( ) {\r\n\tresetScreen();\r\n\tupdate();\r\n});\r\n\r\nonEvent(\"feature2Dropdown\", \"change\", function( ) {\r\n\tresetScreen();\r\n\tupdate();\r\n});\r\nonEvent(\"flipColorsBtn\", \"click\", function( ) {\r\n\tif(LESSTHAN == \"veggie\") {\r\n\t  LESSTHAN = \"fruit\";\r\n\t} else if (LESSTHAN == \"fruit\") {\r\n\t  LESSTHAN = \"veggie\";\r\n\t}\r\n\tupdateGradient();\r\n\tcheckAccuracy(startPt.x, startPt.y, endPt.x, endPt.y);\r\n});\r\n\r\nfunction showTestElts(bool) {\r\n  var testEltsList = [\"zzBackground\", \"zzBottomCoverLabel\", \"zzTestEltDismiss\", \"zzNameLabel\", \"zzFoodInput\", \"zzScale1Label\", \"zzScale1DynamicLabel\", \"zzScale1Slider\", \"zzScale1ValueLabel\", \"zzScale2Label\", \"zzScale2DynamicLabel\", \"zzScale2Slider\", \"zzScale2ValueLabel\", \"zzTestSubmitBtn\"];\r\n  if(bool) {\r\n    setText(\"zzFoodInput\", \"\");\r\n    setNumber(\"zzScale1Slider\", 1);\r\n    setNumber(\"zzScale2Slider\", 1);\r\n    setText(\"zzScale1ValueLabel\", \"1\");\r\n    setText(\"zzScale2ValueLabel\", \"1\");\r\n    setText(\"zzScale1DynamicLabel\", getText(\"feature1Dropdown\"));\r\n    setText(\"zzScale2DynamicLabel\", getText(\"feature2Dropdown\"));\r\n    for(var i = 0; i < testEltsList.length; i++) {\r\n      showElement(testEltsList[i]);\r\n    }\r\n    for (var i = 0; i < foodName.length; i++) {\r\n      var imgId = \"img\"+i;\r\n      hideElement(imgId);\r\n    }\r\n    hideElement(testId);\r\n  } else {\r\n    for(var i = 0; i < testEltsList.length; i++) {\r\n      hideElement(testEltsList[i]);\r\n    }\r\n    for (var i = 0; i < foodName.length; i++) {\r\n      var imgId = \"img\"+i;\r\n      showElement(imgId);\r\n    }\r\n    showElement(testId);\r\n  }\r\n}\r\n\r\n\r\n//FOR FUTURE REFERENCE: How to move a canvas on the screen\r\n//createCanvas(\"canvas1\");\r\n//setPosition(\"canvas1\", 50, 50);\r\n\r\nonEvent(\"zzTestEltDismiss\", \"click\", function( ) {\r\n\tshowTestElts(false);\r\n});\r\nonEvent(\"zzScale1Slider\", \"input\", function( ) {\r\n\tsetText(\"zzScale1ValueLabel\", getNumber(\"zzScale1Slider\"));\r\n});\r\nonEvent(\"zzScale2Slider\", \"input\", function( ) {\r\n\tsetText(\"zzScale2ValueLabel\", getNumber(\"zzScale2Slider\"));\r\n});\r\nonEvent(\"zzTestSubmitBtn\", \"click\", function( ) {\r\n\tshowTestElts(false);\r\n\tdrawTestImg();\r\n});\r\n\r\nfunction showModelResultsElts(bool) {\r\n  var modelResultsElts = [\"zzBottomCoverLabel\", \"yyModelText1\", \"yyModelText2\", \"yyModelDecision\", \"yyModelContinueBtn\"];\r\n  if(bool) {\r\n    for(var i = 0; i < modelResultsElts.length; i++) {\r\n      showElement(modelResultsElts[i]);\r\n    }\r\n  } else {\r\n    for(var i = 0; i < modelResultsElts.length; i++) {\r\n      hideElement(modelResultsElts[i]);\r\n    }\r\n    setText(\"yyModelDecision\", \"\");\r\n    setProperty(\"yyModelDecision\", \"background-color\", \"rgba(255,255,255, 1)\");\r\n  }\r\n}\r\nonEvent(\"yyModelContinueBtn\", \"click\", function( ) {\r\n\tshowModelResultsElts(false);\r\n});\r\n",
