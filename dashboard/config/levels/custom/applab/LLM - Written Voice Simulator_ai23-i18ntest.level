n        \"truth\", \n        \"way\"\n    ], \n    \"that's\": [\n        \"what\"\n    ]\n  }\n};\n\n\nfunction getRandomKey(obj) {\n    var keys = Object.keys(obj);\n    return keys[Math.floor(Math.random() * keys.length)];\n}\n\nfunction updateButtons(word) {\n  //Iterate through the array with the buttonIds\n  for(var i = 0; i < buttonIds.length; i++) {\n    //Delete the element on the screen with that ID,\n    //allowing us to recreate later with the same ID if we want\n    deleteElement(buttonIds[i]);\n  }\n  //Reset the buttonIds array\n  buttonIds = [];\n  var words = dictionary[word];\n  for (i = 0; i < words.length; i++) {\n    if(words[i] == \"\") {\n      continue;\n    }\n    button(words[i], words[i]);\n    setProperty(words[i], \"y\", 225);\n    setProperty(words[i], \"height\", 30);\n    //...when generating the button, also add the id to the buttonIds array\n    buttonIds.push(words[i]);\n      onEvent(words[i], \"click\", function(event) {\n        story += event.currentTargetId + \" \";\n        setText(\"story\", story);\n        updateButtons(event.currentTargetId);\n      });\n  }\n}\n\nfunction initializeButtons() {\n    var initialWord = getRandomKey(dictionary);\n    story += initialWord + \" \";\n    setText(\"story\", story);\n    updateButtons(initialWord);\n}\n\nonEvent(\"delete\", \"click\", function( ) {\n  story = \"\";\n  setText(\"story\", story);\n  initializeButtons();\n});\n\nonEvent(\"save\", \"click\", function( ) {\n  setScreen(\"saveScreen\");\n  checkLoad()\n});\n\nonEvent(\"saveName\", \"click\", function( ) {\n  var savedName = getText(\"savedStoryName\");\n  \n  while (savedName == null || savedName == \"\"){\n    savedName = prompt(promptStoryText);\n  }\n    savedStoryNames.push(savedName);\n    setProperty(\"savedStoriesSelect\", \"options\", savedStoryNames);\n    savedStories[savedName]= [story, dictionary];\n    setScreen(\"storyScreen\");\n    checkLoad();\n    setProperty(\"savedStoriesSelect\", \"value\", savedName);\n    setText(\"savedStoryName\",\"\");\n  });\n\nonEvent(\"load\", \"click\", function() {\n  setScreen(\"loadSaveScreen\");\n  checkLoad();\n});\n\nfunction lastWord(fullString){\n  var separateWords = fullString.split(\" \");\n  return separateWords[separateWords.length -1];\n}\n\nonEvent(\"loadScreenButton\", \"click\", function() {\n  story = savedStories[getProperty(\"savedStoriesSelect\", \"value\")][0];\n  dictionary = savedStories[getProperty(\"savedStoriesSelect\", \"value\")][1];\n  setText(\"story\", story);\n  var finalWord = lastWord(story.trim());\n  setScreen(\"storyScreen\");\n  checkLoad();\n  updateButtons(finalWord);\n});\n\nonEvent(\"startButton\", \"click\", function( ) {\n  var story = \" \";\n  var voiceChoice = getProperty(\"whichVoice\", \"value\");\n  if (voiceChoice == whichVoiceOption11){\n    setText(\"newVoiceInput\",\"\");\n    setText(\"newVoiceId\",\"\");\n    setScreen(\"newVoiceModel\");\n    checkLoad();\n  } else {\n    dictionary = voiceModels[voiceChoice];\n    setText(\"currVoiceText\", voiceChoice);\n    setScreen(\"storyScreen\");\n    checkLoad();\n    initializeButtons();\n  }\n});\n\nonEvent(\"newVoice\", \"click\", function( ) {\n  setScreen(\"voiceChoiceScreen\");\n  checkLoad();\n});\n\nfunction remove_punctuation(word) {\n  return word.replace(/[^\\w\\s]|_/g, \"\").toLowerCase();\n}\n\nfunction checkIfInArray (arrayToCheck, itemToCheck){\n  for (var i = 0; i < arrayToCheck.length; i++){\n    if (arrayToCheck[i] == itemToCheck){\n      return true;\n    }\n  }\n  return false;\n}\n\nfunction create_dictionary(text) {\n  var words = text.split(/\\s+/);\n  var word_dict = {};\n  \n  if (words.length < 2){\n    words = [words[0], words[0]];\n  }\n\n  for (var i = 0; i < words.length; i++) {\n    var word = remove_punctuation(words[i]);\n    var next_word = remove_punctuation(words[(i + 1) % words.length]); //makes the text circular\n    if (!word_dict[word]) {\n      word_dict[word] = [];\n    }\n    if (!checkIfInArray(word_dict[word], next_word)){\n      word_dict[word].push(next_word);\n    }\n  }\n\n  for (var key in word_dict) {\n    word_dict[key] = word_dict[key].reduce(function(acc, cur) {\n      if (!acc[cur]) {\n        acc[cur] = 1;\n      } else {\n        acc[cur] += 1;\n      }\n      return acc;\n    }, {});\n\n    // Convert the object of followers to an array of entries\n    var entries = [];\n\n    for (var follower in word_dict[key]) {\n      entries.push([follower, word_dict[key][follower]]);\n    }\n\n    // Sort the entries by count in descending order\n    entries.sort(function(a, b) {\n      return b[1] - a[1];\n    });\n\n    // Extract the top 10 followers and assign them to the current key in the word dictionary\n    var top_followers = [];\n\n    for (var j = 0; j < 5 && j < entries.length; j++) {\n      top_followers.push(entries[j][0]);\n    }\n\n    word_dict[key] = top_followers;\n  }\n\n  // Return the completed word dictionary\n  return word_dict;\n}\n\nonEvent(\"enterNewVoice\", \"click\", function( ) {\n  var newVoiceId = getText(\"newVoiceId\");\n  while (newVoiceId == null || newVoiceId == \"\"){\n    newVoiceId = prompt(promptVoiceText);\n  }\n  setText(\"newVoiceId\", newVoiceId);\n  \n  var rawNewVoice = getText(\"newVoiceInput\");\n  while (rawNewVoice == \"\" || rawNewVoice == null){\n    rawNewVoice = prompt(promptRawText);\n  }\n  voices.push(newVoiceId);\n  setText(\"currVoiceText\", newVoiceId);\n  var dictNewVoice = create_dictionary(rawNewVoice);\n  //console.log(dictNewVoice);\n  voiceModels[newVoiceId] = dictNewVoice;\n  dictionary = voiceModels[newVoiceId];\n  setProperty(\"whichVoice\", \"options\", voices);\n  setScreen(\"storyScreen\");\n  checkLoad();\n  initializeButtons();\n});\n\nfunction checkLoad() {\n  if(savedStoryNames.length == 0) {\n    hideElement(\"load\");\n  } else {\n    showElement(\"load\");\n  }\n}\n\nonEvent(\"newBackButton\", \"click\", function( ) {\n\tsetScreen(\"voiceChoiceScreen\");\n});\n\nfunctio