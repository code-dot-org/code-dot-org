      "contents": "import unittest\nimport ast\n\nclass TestMagicalPotionIngredients(unittest.TestCase):\n    def setUp(self):\n        # Set up the initial context and parse the AST of the student's code.\n        with open('main.py', 'r') as f:\n            self.tree = ast.parse(f.read())\n\n    def detect_filter_notnull(self):\n        \"\"\"Detects filtering using notnull() on the 'Source' column.\"\"\"\n        for node in ast.walk(self.tree):\n            if isinstance(node, ast.Call) and isinstance(node.func, ast.Attribute):\n                if node.func.attr == 'notnull':\n                    if isinstance(node.func.value, ast.Subscript):\n                        if isinstance(node.func.value.slice, ast.Constant) and node.func.value.slice.value == 'Source':\n                            return True\n        return False\n\n    def detect_filter_effect_endswith(self):\n        \"\"\"Detects filtering using str.endswith('ing') on the 'Effect' column.\"\"\"\n        for node in ast.walk(self.tree):\n            if isinstance(node, ast.Call) and isinstance(node.func, ast.Attribute):\n                if node.func.attr == 'endswith':\n                    if isinstance(node.func.value, ast.Attribute):\n                        if isinstance(node.func.value.value, ast.Subscript) and node.func.value.value.slice.value == 'Effect':\n                            if isinstance(node.args[0], ast.Constant) and node.args[0].value == 'ing':\n                                return True\n        return False\n\n    def detect_filter_legendary_forest(self):\n        \"\"\"Detects filtering for legendary ingredients from the forest.\"\"\"\n        legendary_detected = False\n        forest_detected = False\n\n        for node in ast.walk(self.tree):\n            if isinstance(node, ast.Compare) and isinstance(node.left, ast.Subscript):\n                # Check for 'Rarity == legendary'\n                if node.left.slice.value == 'Rarity' and isinstance(node.comparators[0], ast.Constant):\n                    if node.comparators[0].value == 'legendary':\n                        legendary_detected = True\n                        print(f\"Detected: Rarity == 'legendary'\")\n                # Check for 'Source == forest'\n                elif node.left.slice.value == 'Source' and isinstance(node.comparators[0], ast.Constant):\n                    if node.comparators[0].value == 'forest':\n                        forest_detected = True\n                        print(f\"Detected: Source == 'forest'\")\n\n        # Pass the test if both conditions are detected, even if not combined in a single expression\n        if legendary_detected and forest_detected:\n            print(\"Condition Met: Legendary and Forest Detected\")\n            return True\n        else:\n            print(f\"Legendary Detected: {legendary_detected}, Forest Detected: {forest_detected}\")\n        return False\n\n    def detect_category_aggregation(self):\n        \"\"\"Detects groupby() and agg() operations on 'Category' for 'QuantityAvailable'.\"\"\"\n        for node in ast.walk(self.tree):\n            if isinstance(node, ast.Call) and isinstance(node.func, ast.Attribute):\n                if node.func.attr == 'agg':\n                    if isinstance(node.func.value, ast.Subscript) and isinstance(node.func.value.value, ast.Call):\n                        # Check if it's a groupby operation on 'Category'\n                        if isinstance(node.func.value.value.func, ast.Attribute) and node.func.value.value.func.attr == 'groupby':\n                            if isinstance(node.func.value.value.args[0], ast.Constant) and node.func.value.value.args[0].value == 'Category':\n                                # Check that sum and mean are used\n                                if isinstance(node.args[0], ast.List):\n                                    operations = {elt.value for elt in node.args[0].elts if isinstance(elt, ast.Constant)}\n                                    if {'sum', 'mean'}.issubset(operations):\n                                        return True\n        return False\n\n    def test_step_1_notnull_filter(self):\n        \"\"\"Filter for root ingredients that have a non-null 'Source'.\"\"\"\n        notnull_filter_detected = self.detect_filter_notnull()\n        self.assertTrue(notnull_filter_detected, \"Filter for root ingredients that have a non-null 'Source'.\")\n\n    def test_step_2_effect_endswith_filter(self):\n        \"\"\"Filter for ingredients whose 'Effect' ends with 'ing'.\"\"\"\n        endswith_filter_detected = self.detect_filter_effect_endswith()\n        self.assertTrue(endswith_filter_detected, \"Filter for ingredients whose 'Effect' ends with 'ing'.\")\n\n    def test_step_3_legendary_forest_filter(self):\n        \"\"\"Filter for legendary ingredients from the forest.\"\"\"\n        legendary_filter_detected = self.detect_filter_legendary_forest()\n        self.assertTrue(legendary_filter_detected, \"Filter for legendary ingredients from the forest.\")\n\n    def test_step_4_category_aggregation(self):\n        \"\"\"Calculate the total and median 'QuantityAvailable' for each Category.\"\"\"\n        aggregation_detected = self.detect_category_aggregation()\n        self.assertTrue(aggregation_detected, \"Calculate the total and median 'QuantityAvailable' for each Category.\")\n\nif __name__ == \"__main__\":\n    unittest.main()\n",
