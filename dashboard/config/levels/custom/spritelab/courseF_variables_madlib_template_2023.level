r\n      ]\r\n    },\r\n    \"1d3be8e0-893c-402c-ba58-3e30a0da4d22\": {\r\n      \"name\": \"yellow_shirt2\",\r\n      \"sourceUrl\": \"/api/v1/animation-library/spritelab/tD11NnW3UPWzOQkSsjzjVn0FDb1KJbQW/category_people/yellow_shirt2.png\",\r\n      \"frameSize\": {\r\n        \"x\": 202,\r\n        \"y\": 400\r\n      },\r\n      \"frameCount\": 1,\r\n      \"looping\": true,\r\n      \"frameDelay\": 2,\r\n      \"version\": \"tD11NnW3UPWzOQkSsjzjVn0FDb1KJbQW\",\r\n      \"categories\": [\r\n        \"people\"\r\n      ]\r\n    },\r\n    \"a7f36339-fc34-4d19-a83c-59d7e91e6f34\": {\r\n      \"name\": \"gradient_02_1\",\r\n      \"sourceUrl\": \"https://studio.code.org/media?u=https%3A%2F%2Fstudio.code.org%2Fapi%2Fv1%2Fanimation-library%2Fgamelab%2F3RTQ7JCCwARUka_u91BBU07yebnrpLNZ%2Fcategory_backgrounds%2Fgradient_02.png\",\r\n      \"frameSize\": {\r\n        \"x\": 400,\r\n        \"y\": 399\r\n      },\r\n      \"frameCount\": 1,\r\n      \"looping\": true,\r\n      \"frameDelay\": 2,\r\n      \"version\": \"3RTQ7JCCwARUka_u91BBU07yebnrpLNZ\",\r\n      \"categories\": [\r\n        \"backgrounds\"\r\n      ]\r\n    }\r\n  }\r\n}\r\n                \r\n              ",
