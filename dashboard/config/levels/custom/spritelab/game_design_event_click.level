                        <block type="gamelab_allSpritesWithAnimation">
                          <field name="ANIMATION">"game_power"</field>
                        </block>
                      </value>
                      <value name="BEHAVIOR">
                        <block type="gamelab_behavior_get">
                          <mutation behaviorId="wandering" name="wandering"/>
                          <field name="NAME">wandering</field>
                        </block>
                      </value>
                    </block>
                  </next>
                </block>
              </next>
            </block>
          </next>
        </block>
