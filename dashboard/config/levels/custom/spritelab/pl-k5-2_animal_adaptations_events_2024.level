    "authored_hints": "[{\"hint_class\":\"content\",\"hint_markdown\":\"Your `when clicked` event should look like this: \\n<xml><block type=\\\"gamelab_spriteClicked\\\">\\n  <field name=\\\"CONDITION\\\">\\\"when\\\"</field>\\n  <value name=\\\"SPRITE\\\">\\n    <block type=\\\"gamelab_allSpritesWithAnimation\\\">\\n      <field name=\\\"ANIMATION\\\">\\\"ladybug2\\\"</field>\\n    </block>\\n  </value>\\n  <next>\\n    <block type=\\\"adaptations_toggleBehaviorSimple\\\">\\n      <field name=\\\"TOGGLE\\\">\\\"stops\\\"</field>\\n      <value name=\\\"SPRITE\\\">\\n        <block type=\\\"gamelab_allSpritesWithAnimation\\\">\\n          <field name=\\\"ANIMATION\\\">\\\"ladybug2\\\"</field>\\n        </block>\\n      </value>\\n      <value name=\\\"BEHAVIOR\\\">\\n        <block type=\\\"adaptations_adaptationBehaviors\\\">\\n          <field name=\\\"BEHAVIOR\\\">new Behavior(spinning_right, [])</field>\\n        </block>\\n      </value>\\n    </block>\\n  </next>\\n</block></xml>\",\"hint_id\":\"hint1\",\"hint_type\":\"general\",\"hint_path\":\"\",\"hint_video\":\"\",\"tts_url\":\"https://tts.code.org/sharon22k/180/100/231f4dea6887d81b8ff82d11f1121fa6/csc_animal_adaptations_groups.mp3\"}]",
