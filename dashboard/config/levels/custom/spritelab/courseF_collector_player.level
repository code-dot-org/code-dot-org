    <toolbox_blocks>
      <xml>
        <category name="World">
          <block type="gamelab_setBackgroundImageAs">
            <field name="IMG">"grid_1"</field>
          </block>
          <block type="gamelab_setBackground">
            <value name="COLOR">
              <block type="colour_picker">
                <field name="COLOUR">#3333ff</field>
              </block>
            </value>
          </block>
          <block type="gamelab_randomColor"/>
