    "validation_code": "if (World.frameCount == 1) {\r\n  setFailTime(150);\r\n  setDelayTime(90);\r\n  setupPrevious();\r\n  \r\n  \r\n   //fail on no firefly at beginning of scene\r\n  addCriteria(function() {\r\n    return World.frameCount == 1 && spriteIds.length > 0;\r\n  }, \"cscAdaptationsMissingFireflies\");\r\n\r\n  //fail on wrong costume at beginning of scene\r\n  addCriteria(function() {\r\n  \treturn World.frameCount == 1 && countByAnimation({\"costume\": \"firefily_dark\"}) > 0;\r\n  }, \"cscAdaptationsDaytimeFireflies\"); \r\n\r\n  //fail on no down key \r\n  addCriteria(function() {\r\n    return member('whenPress: down', getEventLog());\r\n  }, \"cscAdaptationsDown\");\r\n  \r\n  //fail on the costume and background not changing sometime after the down key is pressed\r\n  addCriteria(function() {\r\n    //var giraffeID = getExactlyOneSpriteIdFromCostumes(giraffeCostumes);\r\n    //return checkNewSpriteCostumeThisFrame(giraffeID) && exactlyOneFromCostumes([\"giraffe_longer_neck\"]);\r\n    return anyCostumeChanged() && getBackground() === \"night_moon\";\r\n\r\n  }, \"cscAdaptationsChangeFirefliesCostume\");\r\n  \r\n  //todo: add specific test for background change\r\n  \r\n}\r\ngetHelperVars();\r\n//trackSpriteCostume(getExactlyOneSpriteIdFromCostumes(giraffeCostumes));\r\ncheck();\r\n//updatePrevSpriteCostume(getExactlyOneSpriteIdFromCostumes(giraffeCostumes));\r\nupdatePrevious();\r\n",
