    "validation_code": "var octopusCostumes = [\"octopus_green\", \"octopus_purple\", \"octopus_red\"];\r\n\r\nif (World.frameCount == 1) {\r\n  setFailTime(150);\r\n  setDelayTime(90);\r\n  setupPrevious();\r\n  \r\n  //Students have an octopus costume at the beginning of the program\r\n  addCriteria(function() {\r\n    return World.frameCount == 1 && atLeastOneFromCostumes(octopusCostumes);\r\n  }, \"cscAdaptationsMissingOctopus\");\r\n  \r\n  //Students have exactly one octopus costume\r\n  addCriteria(function() {\r\n    return World.frameCount == 1 && exactlyOneFromCostumes(octopusCostumes);\r\n  }, \"cscAdaptationsTooManyOctopuses\");\r\n\r\n\r\n  //Check that the octopus has been assigned the \"Moving with arrow keys\" behavior\r\n  addCriteria(function() {\r\n    var octopusId = getExactlyOneSpriteIdFromCostumes(octopusCostumes);\r\n    console.log(\"behavior:\");\r\n    console.log(checkSpriteHasBehavior(octopusId, \"moving_with_arrow_keys\"));\r\n    return checkSpriteHasBehavior(octopusId, \"moving_with_arrow_keys\");\r\n  }, \"cscAdaptationsOctopusNoBehavior\");\r\n\r\n  //Check if students have used the arrow keys\r\n  addCriteria(function() {\r\n    return isKeyPressed(\"up\") || isKeyPressed(\"down\") || isKeyPressed(\"left\") || isKeyPressed(\"right\");\r\n  }, \"cscAdaptationsMoveOctopusToPlant\");\r\n  \r\n\r\n  //Did two sprites ever touch, regardless of event? Check that the octopus sprite touched a plant (not a plant sprite touching another plant)\r\n  addCriteria(function() {\r\n    return anySpritesTouched();\r\n  }, \"cscAdaptationsOctopusTouchPlant\");\r\n\r\n  //[Is this possible?] Check if students don't have a when touches event block in their code using touchEventFound() Did a touch event get logged in the event log? ie. Students have added the when touches event block\r\n  addCriteria(function() {\r\n    //this is broken currently\r\n    return touchEventFound();\r\n  }, \"cscAdaptationsWhenTouchEvent\");\r\n\r\n  \r\n\r\n  //Did the octopus sprite touch a plant but the student forgot the change costume block?\r\n  addCriteria(function() {\r\n    return checkSpriteTouchedThisFrame() && anyCostumeChanged();\r\n  }, \"cscAdaptationsOctopusTouchNoCostumeChange\");\r\n\r\n  //Need criteria to check if change costume block added to event\r\n  addCriteria(function() {\r\n    var octopusId = getExactlyOneSpriteIdFromCostumes(octopusCostumes);\r\n    return touchEventFound() && checkNewSpriteCostumeThisFrame(octopusId);\r\n  }, \"cscAdaptationsChangeOctopusCostume\");\r\n\r\n  //Make sure the octopus changes to the correct color of the plant\r\n  addCriteria(function() {\r\n    var octopusId = getExactlyOneSpriteIdFromCostumes(octopusCostumes);\r\n    return checkSpriteTouchedThisFrame() && checkNewSpriteCostumeThisFrame(octopusId) &&\r\n      \t  (checkTheseTwoSpriteCostumesTouched(\"octopus_green\", \"underseadeco_34\") || \r\n          checkTheseTwoSpriteCostumesTouched(\"octopus_red\", \"underseadeco_25\") ||\r\n          checkTheseTwoSpriteCostumesTouched(\"octopus_purple\", \"underseadeco_24\"));\r\n\r\n  }, \"cscAdaptationsOctopusPlantColor\");\r\n\r\n}\r\n\r\n\r\nconsole.log(\"newTick\");\r\ngetHelperVars();\r\ntrackSpriteCostume(getExactlyOneSpriteIdFromCostumes(octopusCostumes));\r\ncheck();\r\nconsole.log(\"eventLog\");\r\nconsole.log(eventLog);\r\nconsole.log(\"touchEventFound\");\r\nconsole.log(touchEventFound());\r\nconsole.log(\"keyPresss?\");\r\nconsole.log(isKeyPressed(\"up\") || isKeyPressed(\"down\") || isKeyPressed(\"left\") || isKeyPressed(\"right\"));\r\nupdatePrevSpriteCostume(getExactlyOneSpriteIdFromCostumes(octopusCostumes));\r\nupdatePrevious();\r\n\r\nconsole.log(\"this ps\");\r\nconsole.log(this.p5);\r\nconsole.log(p5);",
