    "start_sources": "              {\r\n  \"files\": [\r\n    {\r\n      \"name\": \"index.html\",\r\n      \"data\": \"<!DOCTYPE html>\\n<html>\\n  <head>\\n    <link rel=\\\"stylesheet\\\" href=\\\"style.css\\\">\\n  </head>\\n  <body>\\n    \\n    <!-- ADD A DIV container with a class attribute around the 6 images -->\\n \\n      <img src=\\\"one.jpg\\\" alt=\\\"one\\\">\\n      <img src=\\\"two.jpg\\\" alt=\\\"two\\\">\\n      <img src=\\\"three.jpg\\\" alt=\\\"three\\\">\\n      <img src=\\\"four.jpg\\\" alt=\\\"four\\\">\\n      <img src=\\\"five.jpg\\\" alt=\\\"five\\\">\\n      <img src=\\\"six.jpg\\\" alt=\\\"six\\\">\\n\\n    \\n  </body>\\n</html>\"\r\n    },\r\n    {\r\n      \"name\": \"style.css\",\r\n      \"data\": \".container {\\n  /* We gave the div container a background color so you can see how the items are positioned within it */\\n  background: #b691d6;\\n  \\n  /* First, create a flex layout context */\\n\\n\\n\\n  /* Then, define the flow direction \\n  and if the items should wrap */\\n\\n \\n}\\n\\n\\nimg {\\n  border-style: solid;\\n  width: 100px;\\n}\"\r\n    },\r\n    {\r\n      \"name\": \"one.jpg\",\r\n      \"url\": \"https://images.code.org/6d03fd210c0f3527e02bf011f46575b5-one.jpg\"\r\n    },\r\n    {\r\n      \"name\": \"two.jpg\",\r\n      \"url\": \"https://images.code.org/553a4b7c78ab37b38695b095a5a12517-two.jpg\"\r\n    },\r\n    {\r\n      \"name\": \"three.jpg\",\r\n      \"url\": \"https://images.code.org/f3fe826f3692d138b269d153089d697b-three.jpg\"\r\n    },\r\n    {\r\n      \"name\": \"four.jpg\",\r\n      \"url\": \"https://images.code.org/efe517869f6385313ef456b740961174-four.jpg\"\r\n    },\r\n    {\r\n      \"name\": \"five.jpg\",\r\n      \"url\": \"https://images.code.org/babf3e1c199abdce5aeb270b66f16eb1-five.jpg\"\r\n    },\r\n    {\r\n      \"name\": \"six.jpg\",\r\n      \"url\": \"https://images.code.org/dbf46e859657df01200e8300e271b5da-six.jpg\"\r\n    }\r\n  ]\r\n}\r\n            ",
