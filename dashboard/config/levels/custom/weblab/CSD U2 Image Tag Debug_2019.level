    "start_sources": "{\r\n  \"files\": [\r\n    {\r\n      \"name\": \"index.html\",\r\n      \"data\": \"<!DOCTYPE html> \\n<html> \\n \\t<head>\\n\\t\\t<title>Colors in Nature</title>\\n\\t</head>\\n\\t<body>\\n\\t\\t<h1>Colors in Nature</h1>\\n\\t\\t<h2>Red</h2>\\n\\t\\t<img src=\\\"red.jpg\\\" alt=\\\"red sky\\\"> \\n\\t\\t<h2>Green</h2>\\n\\t\\t<img src=\\\"grass.jpg\\\" alt=\\\"green grass\\\"> \\n\\t\\t<h2>Blue</h2>\\n\\t\\t<img src=\\\"blu.jpg\\\" alt=\\\"blue trees\\\">\\n\\t</body>\\n</html>\"\r\n    },\r\n{\r\n      \"name\": \"red_sky.jpg\",\r\n      \"url\": \"https://images.code.org/2bf24034ecb0eee956ecf699e247cd9c-image-1493406103669.jpg\"\r\n    },\r\n{\r\n      \"name\": \"grass.JPG\",\r\n      \"url\": \"https://images.code.org/04f105e4bb9d3f38b711046e42756fbd-image-1493406146007.JPG\"\r\n    },\r\n{\r\n      \"name\": \"blue.jpg\",\r\n      \"url\": \"https://images.code.org/c907d2e83c6181058149ddca6c35e9f7-image-1493406190729.jpg\"\r\n    }\r\n  ]\r\n}",
