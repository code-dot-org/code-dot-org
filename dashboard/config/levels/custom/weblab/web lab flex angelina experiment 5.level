    "start_sources": "              {\r\n  \"files\": [\r\n    {\r\n      \"name\": \"index.html\",\r\n      \"data\": \"<!DOCTYPE html>\\n<html>\\n  <head>\\n    <link rel=\\\"stylesheet\\\" href=\\\"style.css\\\">\\n  </head>\\n  <body>\\n    \\n    <!-- container holding images -->\\n    <div class=\\\"container\\\">\\n      <img class=\\\"alien\\\" src=\\\"PurpleAlien.jpg\\\">\\n      <img class=\\\"alien\\\" src=\\\"RedAlien.png\\\">\\n    </div>\\n \\n    \\n  </body>\\n</html>\"\r\n    },\r\n    {\r\n      \"name\": \"style.css\",\r\n      \"data\": \"body {\\n  background-image: url(\\\"justify-content-center.png\\\");\\n  background-repeat: no-repeat;\\n  background-position: center top;\\n  background-attachment: fixed;\\n}\\n\\n.alien {\\n  width: 75px;\\n}\\n\\n\\n\\n.container {\\n  /* Add display property */\\n\\n  \\n  /* Add justify-content property */\\n\\n\\n}\"\r\n    },\r\n    {\r\n      \"name\": \"justify-content-center.png\",\r\n      \"url\": \"https://images.code.org/4970fbb9b3ed504624dee287f490b141-justify-content center.png\"\r\n    },\r\n    {\r\n      \"name\": \"PurpleAlien.jpg\",\r\n      \"url\": \"https://images.code.org/c845f6d0d9b4bb273669af5157bf8c13-PurpleAlien.png\"\r\n    },\r\n    {\r\n      \"name\": \"RedAlien.png\",\r\n      \"url\": \"https://images.code.org/60d024bcada4350fe7ff331a2765e440-RedAlien.png\"\r\n    }\r\n  ]\r\n}\r\n            ",
