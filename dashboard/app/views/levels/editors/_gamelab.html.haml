-# Gamelab-only fields for level edit page on levelbuilder
- content_for(:head) do
  %script{src: webpack_asset_path('js/levels/editors/_gamelab.js')}

= render partial: 'levels/editors/fields/encrypted_examples', locals: {f: f, level_type: 'gamelab'}
= render partial: 'levels/editors/fields/debugger', locals: {f: f}
= render partial: 'levels/editors/fields/control_buttons', locals: {f: f}
= render partial: 'levels/editors/fields/animation', locals: {f: f}

:javascript
  window.toggleBlock = function (elementId) {
    var element = document.getElementById(elementId);
    if (element) {
      element.style.display = element.style.display === 'none' ? 'block' : 'none';
    }
  };
