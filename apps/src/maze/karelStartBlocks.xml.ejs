    <statement name="STACK">
      <block type="controls_repeat" editable="false" deletable="false">
        <title name="TIMES"><%= n %></title>
        <statement name="DO">
          <block type="maze_dig" editable="false" deletable="false">
          </block>
        </statement>
       </block>
    </statement>
  </block>
<% }; %>

