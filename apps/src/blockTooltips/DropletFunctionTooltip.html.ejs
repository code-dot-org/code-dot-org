<% function getPrefixedName () {
  return locals.tipPrefix ? locals.tipPrefix + locals.functionName : locals.functionName;
} %>
<div class="function-name">
  <% if (locals.signatureOverride) { -%>
    <%= locals.signatureOverride %>
  <% } else if (locals.isProperty) { -%>
    <%= getPrefixedName() %>
  <% } else {
    /**
     * TODO(bjordan): would be nice to split the following line up, can't figure
     * out how to do so without inserting extraneous spaces between locals.parameters.
     */
    -%>
    <%= getPrefixedName() %>(<% for (var i = 0; i < locals.parameters.length; i++) { -%><%- locals.parameters[i].name -%><% if (i < locals.parameters.length - 1) { -%>, <% } -%><% } -%>)
  <% } %>
</div>
<% if (locals.functionShortDescription) { %><div><%= locals.functionShortDescription %></div><% } %>
<% if (locals.showExamplesLink) { %>
  <div class="tooltip-example-link">
    <a href="javascript:void(0);">See examples</a>
  </div>
<% } %>
