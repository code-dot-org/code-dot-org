{
  "presets": [
    ["env", {"modules": false}],
    "react"
  ],
  "plugins": [
    "syntax-async-functions",
    "transform-object-rest-spread",
    "transform-class-properties",
    ["transform-regenerator", {"async": true, "asyncGenerators": true}],
    // needed for IE 9/10 support
    ["transform-es2015-classes", {"loose": true}]
  ]
}
