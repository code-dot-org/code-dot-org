{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "lib": ["ES2019", "DOM"],
    "module": "es6",
    "rootDir": "./",
    "moduleResolution": "node",
    "outDir": "build",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
