// This tsconfig is used for development. It extends the tsconfig used for building apps.
{
  "extends": "./tsconfig.build.json",
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./test/**/*.ts",
    "./test/**/*.tsx",
    // For development, include js and jsx. This allows Intellisense to
    // detect js and jsx files for imports.
    "./src/**/*.js",
    "./src/**/*.jsx",
  ],
}
